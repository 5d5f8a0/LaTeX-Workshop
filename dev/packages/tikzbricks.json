{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tikz-3dplot"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "brick",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:length%plain}}{${2:width%plain}}"
      }
    },
    {
      "name": "brick",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:length%plain}}{${2:width%plain}}"
      }
    },
    {
      "name": "wallbrick",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:length%plain}}{${2:width%plain}}"
      }
    },
    {
      "name": "wallbrick",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:length%plain}}{${2:width%plain}}"
      }
    },
    {
      "name": "newrow"
    },
    {
      "name": "thebrickx",
      "unusual": true
    },
    {
      "name": "thebricky",
      "unusual": true
    },
    {
      "name": "thebrickz",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "wall"
    }
  ],
  "keys": {
    "\\brick#c,\\usepackage/tikzbricks#c": [
      "color=",
      "frontcolor=",
      "topcolor=",
      "sidecolor=",
      "studcolor=",
      "brickheight=${1:number}\r",
      "bricklength=${1:number}\r",
      "brickwidth=${1:number}\r",
      "studradius=${1:number}\r",
      "studheight=${1:number}\r",
      "studtext={${1:text}}\r"
    ],
    "\\wallbrick#c": [
      "color="
    ]
  },
  "args": [
    "\\brick#c,\\usepackage/tikzbricks#c"
  ]
}