{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "class-ctexart"
    },
    {
      "name": "xeCJK"
    },
    {
      "name": "geometry"
    },
    {
      "name": "fancyhdr"
    },
    {
      "name": "setspace"
    },
    {
      "name": "caption"
    },
    {
      "name": "booktabs"
    },
    {
      "name": "pdfpages"
    }
  ],
  "macros": [
    {
      "name": "BITSetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:键值对%keyvals}}"
      }
    },
    {
      "name": "MakeCover"
    },
    {
      "name": "MakeReviewTable"
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/bitreport#c": [
      "type=undergraduate_proposal\r"
    ],
    "\\BITSetup#c": [
      "cover={${1:键值列表}}\r",
      "cover/date=${1:任意字符串}\r",
      "cover/dilimiter=${1:任意字符串}\r",
      "cover/autoWidth",
      "cover/autoWidthPadding=",
      "cover/labelMaxWidth=",
      "cover/valueMaxWidth=",
      "cover/labelAlign=${1|c,l,r\r|}",
      "cover/valueAlign=${1|c,l,r\r|}",
      "cover/underlineThickness=",
      "cover/underlineOffset=",
      "info={${1:键值列表}}\r",
      "info/title=${1:字符串}\r",
      "info/school=${1:字符串}\r",
      "info/major=${1:字符串}\r",
      "info/class=${1:字符串}\r",
      "info/author=${1:字符串}\r",
      "info/studentId=${1:字符串}\r",
      "info/supervisor=${1:字符串}\r",
      "info/externalSupervisor=${1:字符串}\r",
      "misc={${1:键值列表}}\r",
      "misc/reviewTable=${1:.pdf file}\r"
    ]
  },
  "args": [
    "\\documentclass/bitreport#c"
  ]
}