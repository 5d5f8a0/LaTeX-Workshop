{
  "deps": [
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "colophon",
      "unusual": true
    },
    {
      "name": "endcolophon",
      "unusual": true
    },
    {
      "name": "colophontitle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title}}"
      }
    },
    {
      "name": "colophontitlestyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style cmds}}"
      }
    },
    {
      "name": "colophontitlesize",
      "arg": {
        "format": "{}",
        "snippet": "{${1:size%l}}"
      }
    },
    {
      "name": "colophonmidspace",
      "arg": {
        "format": "{}",
        "snippet": "{${1:length%l}}"
      }
    },
    {
      "name": "colophonpagestyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style}}"
      }
    },
    {
      "name": "colophontitlealign",
      "arg": {
        "format": "{}",
        "snippet": "{${1:alignment%keyvals}}",
        "keys": [
          "\\colophontitlealign"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "colophonpretitlehook",
      "unusual": true
    },
    {
      "name": "colophonposttitlehook",
      "unusual": true
    },
    {
      "name": "colophonparstyle",
      "unusual": true
    },
    {
      "name": "colophonparsize",
      "unusual": true
    },
    {
      "name": "colophonparlead",
      "unusual": true
    },
    {
      "name": "colophonnofirstindent",
      "unusual": true
    },
    {
      "name": "colophonpreparhook",
      "unusual": true
    },
    {
      "name": "colophonpostparhook",
      "unusual": true
    },
    {
      "name": "colophonparalign",
      "unusual": true
    },
    {
      "name": "colophonnofullpage",
      "unusual": true
    },
    {
      "name": "colophonnoclrdblpg",
      "unusual": true
    },
    {
      "name": "colophonclrpg",
      "unusual": true
    },
    {
      "name": "colophontopspace",
      "unusual": true
    },
    {
      "name": "colophonbotspace",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "colophon"
    }
  ],
  "keys": {
    "\\usepackage/colophon#c": [
      "title={${1:text}}\r",
      "titlestyle=${1:font csname}\r",
      "titlesize=",
      "aftertitle=",
      "titlealign=${1|l,c,r\r|}",
      "parstyle=${1:font csname}\r",
      "parsize=",
      "parlead=",
      "nofullpage\r",
      "noclrdblpg\r",
      "clrpg\r",
      "topspace=",
      "botspace=",
      "nofirstindent\r"
    ],
    "\\colophontitlealign": [
      "c\r",
      "l\r",
      "r\r"
    ]
  },
  "args": [
    "\\usepackage/colophon#c"
  ]
}