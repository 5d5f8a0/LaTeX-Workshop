{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-plot"
    },
    {
      "name": "multido"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "Oscillo"
    },
    {
      "name": "Oscillo",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "TriangleA"
    },
    {
      "name": "TriangleB"
    },
    {
      "name": "RectangleA"
    },
    {
      "name": "RectangleB"
    },
    {
      "name": "RDogToothA"
    },
    {
      "name": "RDogToothB"
    },
    {
      "name": "LDogToothA"
    },
    {
      "name": "LDogToothB"
    },
    {
      "name": "SinusA"
    },
    {
      "name": "SinusB"
    }
  ],
  "envs": [],
  "keys": {
    "\\Oscillo#c": [
      "period1=${1:number}\r",
      "period2=${1:number}\r",
      "periodmodulation1=${1:number}\r",
      "freqmod1=${1:number}\r",
      "periodmodulation2=${1:number}\r",
      "freqmod2=${1:number}\r",
      "amplitude1=${1:number}\r",
      "amplitude2=${1:number}\r",
      "CC1=${1:number}\r",
      "CC2=${1:number}\r",
      "timediv=${1:number}\r",
      "phase1=${1:number}\r",
      "phase2=${1:number}\r",
      "sensivity1=${1:number}\r",
      "sensivity2=${1:number}\r",
      "damping1=${1:number}\r",
      "damping2=${1:number}\r",
      "Fourier=${1:number}\r",
      "offset1=${1:number}\r",
      "offset2=${1:number}\r",
      "offset3=${1:number}\r",
      "plotstyle1=${1:style}\r",
      "plotstyle2=${1:style}\r",
      "plotstyle3=${1:style}\r",
      "plotstyle4=${1:style}\r",
      "operation=${1|add,sub,mul\r|}",
      "Wave1=${1|\\TriangleA,\\RectangleA,\\RDogToothA,\\LDogToothA,\\SinusA\r|}",
      "Wave2=${1|\\TriangleB,\\RectangleB,\\RDogToothB,\\LDogToothB,\\SinusB\r|}",
      "Lissajous",
      "AllColor",
      "combine",
      "plotpoints=${1:number}\r"
    ]
  },
  "args": []
}