    {
      "name": "drefkeys",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\drefset#c,\\drefsave#c,\\dref#c,\\dref*#c,\\drefcalc#c,\\drefcalc*#c,\\drefrel#c,\\drefrel*#c,\\drefrow#c,\\drefkeys#c": [
      "set=${1:key}",
      "save=${1:key}",
      "to macro=${1:macro}",
      "prefix=${1:key prefix}",
      "ignoremissing",
      "defaultvalue=${1:value}",
      "unit=${1:unit}",
      "unit/format=${1|false,plain,siunitx|}",
      "unit/format default=${1|false,plain,siunitx|}",
      "unit/new scala={${1:scala}}",
      "unit/scale to=${1:unit}",
      "unit/scale to auto",
      "unit/scale to auto=${1:number}",
      "scale by=${1:key or value}",
      "percent",
      "negate",
      "divide by=${1:key or value}",
      "abs",
      "factor of=${1:key or value}",
      "percent of=${1:key or value}",
      "increase from=${1:key or value}",
      "decrease from=${1:key or value}",
      "increase factor from=${1:key or value}",
      "decrease factor from=${1:key or value}",
      "increase percent from=${1:key or value}",
      "decrease percent from=${1:key or value}",
      "noassert",
      "annotate=${1|none,footnote,pdfcomment,typeout|}"
    ]
  },
  "args": []
}