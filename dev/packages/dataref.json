      "arg": {
        "format": "{}",
        "snippet": "{${1:key or value}}"
      }
    },
    {
      "name": "drefrel",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:key or value}}"
      }
    },
    {
      "name": "drefrel*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key or value}}"
      }
    },
    {
      "name": "drefrel*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:key or value}}"
      }
    },
    {
      "name": "drefrow",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:csv list}}{${2:key template}}"
      }
    },
    {
      "name": "drefrow",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:csv list}}{${2:key template}}"
      }
    },
    {
      "name": "drefrow*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:csv list}}{${2:key template}}"
