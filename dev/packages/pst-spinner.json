{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-node"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psFidgetSpinner"
    },
    {
      "name": "psFidgetSpinner(x,y)"
    },
    {
      "name": "psFidgetSpinner",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "psFidgetSpinner",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:options%keyvals}](${2:x},${3:y})"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psFidgetSpinner#c": [
      "R=${1:radius}\r",
      "color0=",
      "color1=",
      "color2=",
      "color3=",
      "colorMask=",
      "rotation=${1:degrees}\r",
      "mask",
      "customize",
      "customizeMask",
      "image=${1:imagefile}\r",
      "fillcolor=",
      "linecolor=",
      "linewidth="
    ]
  },
  "args": []
}