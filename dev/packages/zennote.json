{
  "deps": [
    {
      "name": "tcolorbox"
    },
    {
      "name": "tcolorboxlibrarymost"
    }
  ],
  "macros": [
    {
      "name": "titlebox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:settings%keyvals}}",
        "keys": [
          "\\titlebox"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "thenotenumber",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "noteframe"
    },
    {
      "name": "noteframe",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\titlebox": [
      "top-left={${1:text}}\r",
      "top-right={${1:text}}\r",
      "bottom-left={${1:text}}\r",
      "bottom-right={${1:text}}\r",
      "notenumber=${1:integer}\r",
      "topic={${1:text}}\r",
      "type={${1:text}}\r"
    ],
    "\\begin{noteframe}": [
      "color=",
      "style=${1|zero,one,two\r|}",
      "title={${1:text}}\r"
    ]
  },
  "args": []
}