{
  "deps": [
    {
      "name": "enumitem"
    },
    {
      "name": "environ"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "longtable"
    },
    {
      "name": "newfile"
    },
    {
      "name": "pgffor"
    },
    {
      "name": "xstring"
    }
  ],
  "macros": [
    {
      "name": "mctheversion",
      "unusual": true
    },
    {
      "name": "mcexamoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\mcexamoptions,\\usepackage/mcexam#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "question",
      "detail": "?",
      "doc": "Question mark"
    },
    {
      "name": "question",
      "arg": {
        "format": "[]",
        "snippet": "[${1:option%keyvals}]"
      },
      "detail": "?",
      "doc": "Question mark"
    },
    {
      "name": "answer"
    },
    {
      "name": "answer",
      "arg": {
        "format": "[]",
        "snippet": "[${1:mark}]"
      }
    },
    {
      "name": "answer",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:n}}{${2:answer}}"
      }
    },
    {
      "name": "answer",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:mark}]{${1:n}}{${2:answer}}"
      }
    },
    {
      "name": "answernum",
      "arg": {
        "format": "{}",
        "snippet": "{${1:n}}"
      }
    },
    {
      "name": "mcifoutput",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:output type%keyvals}}{${2:text}}",
        "keys": [
          "\\mcifoutput"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "mcifoutput",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:versions}]{${1:output type%keyvals}}{${2:text}}",
        "keys": [
          "\\mcifoutput"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "mcsetupConcept",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "mcsetupExam",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "mcsetupKey",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "mcsetupAnswers",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "mcsetupAnalysis",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "mcversionlabelfmt",
      "unusual": true
    },
    {
      "name": "mcquestionlabelfmt",
      "unusual": true
    },
    {
      "name": "mcanswerlabelfmt",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "mcquestions"
    },
    {
      "name": "mcanswerslist"
    },
    {
      "name": "mcanswerslist",
      "arg": {
        "format": "[]",
        "snippet": "[${1:option%keyvals}]"
      }
    },
    {
      "name": "mcanswers"
    },
    {
      "name": "mcanswers",
      "arg": {
        "format": "[]",
        "snippet": "[${1:option%keyvals}]"
      }
    },
    {
      "name": "mcquestioninstruction"
    },
    {
      "name": "mcexplanation"
    },
    {
      "name": "mcnotes"
    },
    {
      "name": "mcquestions",
      "if": "writeRfile=true",
      "arg": {
        "format": "[]",
        "snippet": "[${1:R file ext}]"
      }
    },
    {
      "name": "mcquestions",
      "if": "writeRfile",
      "arg": {
        "format": "[]",
        "snippet": "[${1:R file ext}]"
      }
    }
  ],
  "keys": {
    "\\mcexamoptions,\\usepackage/mcexam#c": [
      "output=${1|concept,exam,key,answers,analysis\r|}",
      "numberofversions=${1:integer}\r",
      "version=${1:integer}\r",
      "seed=${1:integer}\r",
      "randomizequestions",
      "randomizeanswers",
      "writeRfile"
    ],
    "\\question": [
      "follow\r"
    ],
    "\\begin{mcanswerslist}#c,\\begin{mcanswers}#c": [
      "permuteall\r",
      "ordinal\r",
      "fixlast\r",
      "permutenone\r"
    ],
    "\\mcifoutput": [
      "concept\r",
      "exam\r",
      "key\r",
      "answers\r",
      "analysis\r"
    ],
    "\\mcsetupConcept,\\mcsetupExam,\\mcsetupKey,\\mcsetupAnswers,\\mcsetupAnalysis": [
      "showPerVersion",
      "showQuestionPermutationtable",
      "showQuestionsAnalysis",
      "showQuestionList",
      "showCorrectAnswers",
      "showAnswerPoints",
      "showExplanation",
      "showAnswerPermutationTable",
      "showAnswersAnalysis",
      "showNotes",
      "showKeytable"
    ]
  },
  "args": [
    "\\mcexamoptions,\\usepackage/mcexam#c"
  ]
}