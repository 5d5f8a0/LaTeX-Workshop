{
  "deps": [],
  "macros": [
    {
      "name": "lcmd",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:command}}{${2:prefix}}{${3:list}}"
      }
    },
    {
      "name": "lmake",
      "arg": {
        "format": "[]",
        "snippet": "[${1:keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\lmake#c": [
      "p=${1:pattern}\r",
      "c=${1:separator}\r",
      "n=%{last index%>\r",
      "1=${1:first index}\r",
      "2=${1:second index}\r",
      "d=${1:dots}\r",
      "l={${1:sym1,sym2,...}}\r"
    ]
  },
  "args": []
}