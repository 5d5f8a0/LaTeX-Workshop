{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-node"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "rputover(x0,y0)(x1,y1)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:stuff}}"
      }
    },
    {
      "name": "rputover",
      "arg": {
        "format": "{}(){}",
        "snippet": "{${1:angle}}(${3:x0},${4:y0}){${2:stuff}}"
      }
    },
    {
      "name": "rputover",
      "arg": {
        "format": "{}()(){}",
        "snippet": "{${1:angle}}(${3:x0},${4:y0})(${5:x1},${6:y1}){${2:stuff}}"
      }
    },
    {
      "name": "rputover",
      "arg": {
        "format": "[]()(){}",
        "snippet": "[${2:options%keyvals}](${3:x0},${4:y0})(${5:x1},${6:y1}){${1:stuff}}"
      }
    },
    {
      "name": "rputover",
      "arg": {
        "format": "[]{}(){}",
        "snippet": "[${3:options%keyvals}]{${1:angle}}(${4:x0},${5:y0}){${2:stuff}}"
      }
    },
    {
      "name": "rputover",
      "arg": {
        "format": "[]{}()(){}",
        "snippet": "[${3:options%keyvals}]{${1:angle}}(${4:x0},${5:y0})(${6:x1},${7:y1}){${2:stuff}}"
      }
    },
    {
      "name": "coverable",
      "arg": {
        "format": "{}",
        "snippet": "{${1:stuff}}"
      }
    },
    {
      "name": "coverable",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:stuff}}"
      }
    },
    {
      "name": "pclineover(x0,y0)(x1,y1)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:stuff}}"
      }
    },
    {
      "name": "pclineover",
      "arg": {
        "format": "[]()(){}",
        "snippet": "[${2:options%keyvals}](${3:x0},${4:y0})(${5:x1},${6:y1}){${1:stuff}}"
      }
    },
    {
      "name": "pcarrowC(x0,y0)(x1,y1)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:stuff}}"
      }
    },
    {
      "name": "pcarrowC",
      "arg": {
        "format": "[]()(){}",
        "snippet": "[${2:options%keyvals}](${3:x0},${4:y0})(${5:x1},${6:y1}){${1:stuff}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\rputover#c,\\pclineover#c,\\pcarrowC#c": [
      "autoangle",
      "inverscl",
      "boxpos=${1:pos}\r",
      "fboxsep=",
      "npos=${1:number}\r"
    ],
    "\\coverable#c": [
      "inverscl"
    ],
    "\\pclineover#c,\\pcarrowC#c": [
      "absnodesep=\r",
      "angleadd=${1:degrees}\r",
      "arrows=${1:arrows}\r",
      "offset="
    ],
    "\\pcarrowC#c": [
      "addbars=${1:bars}\r",
      "baroffset=",
      "Cbarwidth=",
      "tbarsize="
    ]
  },
  "args": []
}