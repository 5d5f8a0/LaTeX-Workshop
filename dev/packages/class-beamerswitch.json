{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "xkvltxp"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xstring"
    },
    {
      "name": "shellesc"
    },
    {
      "name": "iftex"
    },
    {
      "name": "class-beamer"
    },
    {
      "name": "class-book",
      "if": "articleclass=book"
    },
    {
      "name": "class-report",
      "if": "articleclass=report"
    },
    {
      "name": "class-memoir",
      "if": "articleclass=memoir"
    },
    {
      "name": "beamerarticle",
      "if": "article"
    },
    {
      "name": "pgfpages",
      "if": "handout"
    }
  ],
  "macros": [
    {
      "name": "ArticleSuffix"
    },
    {
      "name": "BeamerSuffix"
    },
    {
      "name": "HandoutSuffix"
    },
    {
      "name": "TransSuffix"
    },
    {
      "name": "BeamerswitchSpawn",
      "arg": {
        "format": "{}",
        "snippet": "{${1:suffix}}"
      }
    },
    {
      "name": "SpawnedCompiler"
    },
    {
      "name": "SpawnedPDFTeX"
    },
    {
      "name": "SpawnedLuaTeX"
    },
    {
      "name": "SpawnedXeTeX"
    },
    {
      "name": "SpawnedTeX"
    },
    {
      "name": "handoutlayout",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\handoutlayout"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "articlelayout",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\articlelayout"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "JobName",
      "unusual": true
    },
    {
      "name": "handoutpnobaseline",
      "unusual": true
    },
    {
      "name": "pgfpageoptionborder",
      "unusual": true
    },
    {
      "name": "pgfpageoptionfirstshipout",
      "unusual": true
    },
    {
      "name": "pgfpageoptionheight",
      "unusual": true
    },
    {
      "name": "pgfpageoptionwidth",
      "unusual": true
    },
    {
      "name": "thehandoutpno",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/beamerswitch#c": [
      "articleclass=${1:class}\r",
      "articleoptions={${1:article class options}}\r",
      "beameroptions={${1:beamer class options}}\r",
      "article\r",
      "beamer\r",
      "handout\r",
      "trans\r",
      "alsoarticle\r",
      "alsobeamer\r",
      "alsohandout\r",
      "alsotrans\r",
      "also={${1:modes}}\r",
      "nohyperref\r"
    ],
    "\\handoutlayout": [
      "paper=${1:paper size}\r",
      "nup=${1|1,1plus,2,3,3plus,4,4plus,6,8\r|}",
      "pnos\r",
      "border=",
      "margin=",
      "align\r"
    ],
    "\\articlelayout": [
      "frametitles=${1|para,margin,none\r|}",
      "maketitle\r",
      "instbreak=${1:definition}\r"
    ]
  },
  "args": [
    "\\documentclass/beamerswitch#c"
  ]
}