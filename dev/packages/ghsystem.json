{
  "deps": [
    {
      "name": "chemmacros"
    },
    {
      "name": "translations"
    },
    {
      "name": "siunitx"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "longtable"
    },
    {
      "name": "ifpdf"
    }
  ],
  "macros": [
    {
      "name": "ghssetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\ghssetup,\\ghs,\\ghs*,\\ghslistall",
          "\\ghssetup,\\ghspic",
          "\\ghssetup",
          "\\ghssetup,\\ghslistall"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ghs",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type}}{${2:number}}"
      }
    },
    {
      "name": "ghs",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:type}}{${2:number}}"
      }
    },
    {
      "name": "ghs*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type}}{${2:number}}"
      }
    },
    {
      "name": "ghs*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:type}}{${2:number}}"
      }
    },
    {
      "name": "ghspic",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "ghspic",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:name}}"
      }
    },
    {
      "name": "ghslistall"
    },
    {
      "name": "ghslistall",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "GHSfahrenheit",
      "unusual": true
    },
    {
      "name": "GHScelsius",
      "unusual": true
    },
    {
      "name": "GHSkilogram",
      "unusual": true
    },
    {
      "name": "GHSpounds",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\ghssetup,\\ghs,\\ghs*,\\ghslistall": [
      "hide",
      "space=${1:space cmd}\r",
      "fill-in",
      "exposure={${1:text}}\r",
      "effect={${1:text}}\r",
      "organs={${1:text}}\r",
      "substance={${1:text}}\r",
      "text={${1:text}}\r",
      "dots={${1:text}}\r",
      "C-temperature={${1:num}}\r",
      "F-temperature={${1:num}}\r",
      "kg-mass={${1:num}}\r",
      "lbs-mass={${1:num}}\r"
    ],
    "\\ghssetup,\\ghspic": [
      "scale=${1:factor}\r",
      "includegraphics={${1:includegraphics keyval}}\r",
      "pic-type=${1|eps,pdf,jpg,png\r|}"
    ],
    "\\ghssetup": [
      "language=\r"
    ],
    "\\ghssetup,\\ghslistall": [
      "table-head-number={${1:text}}\r",
      "table-head-text={${1:text}}\r",
      "table-next-page={${1:text}}\r",
      "table-caption={${1:text}}\r",
      "table-caption-short={${1:text}}\r",
      "table-label={${1:text}}\r",
      "table-row-sep=",
      "table-rules=${1|default,booktabs,none\r|}",
      "table-top-head-rule=${1|default,booktabs,none\r|}",
      "table-head-rule=${1|default,booktabs,none\r|}",
      "table-foot-rule=${1|default,booktabs,none\r|}",
      "table-last-foot-rule=${1|default,booktabs,none\r|}"
    ]
  },
  "args": []
}