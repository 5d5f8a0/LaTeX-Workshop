{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "siunitx"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarycalc"
    }
  ],
  "macros": [
    {
      "name": "ENsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "ENsetup",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:module}]{${1:options%keyvals}}"
      }
    },
    {
      "name": "ENcurve",
      "arg": {
        "format": "{}",
        "snippet": "{${1:level1,level2,level3,...}}"
      }
    },
    {
      "name": "ENcurve",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:level1,level2,level3,...}}"
      }
    },
    {
      "name": "ShowNiveaus"
    },
    {
      "name": "ShowNiveaus",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "ShowGain"
    },
    {
      "name": "ShowGain",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "ShowEa"
    },
    {
      "name": "ShowEa",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "MakeOrigin"
    },
    {
      "name": "AddAxisLabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:(${3:point1})[${2:label1}];(${4:point2});...}}"
      }
    },
    {
      "name": "AddAxisLabel",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:(${4:point1})[${3:label1}];(${5:point2});...}}"
      }
    },
    {
      "name": "AddAxisLabel*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:level1[${2:label1}];level2;...}}"
      }
    },
    {
      "name": "AddAxisLabel*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:level1[${3:label1}];level2;...}}"
      }
    }
  ],
  "envs": [
    {
      "name": "endiagram"
    },
    {
      "name": "endiagram",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\ENsetup#c,\\begin{endiagram}": [
      "unit=",
      "scale=${1:factor}\r",
      "offset=${1:integer}\r",
      "r-offset=${1:integer}\r",
      "l-offset=${1:integer}\r",
      "axes=${1|xy,y,y-l,y-r,x,all,false\r|}",
      "x-axis={${1:TikZ keys}}\r",
      "y-axis={${1:TikZ keys}}\r",
      "x-label=${1|below,right\r|}",
      "y-label=${1|above,left\r|}",
      "x-label-pos=${1:value}\r",
      "y-label-pos=${1:value}\r",
      "x-label-offset=",
      "y-label-offset=",
      "x-label-angle=${1:degrees}\r",
      "y-label-angle=${1:degrees}\r",
      "x-label-text=${1:text}\r",
      "y-label-text=${1:text}\r",
      "debug",
      "draft",
      "final",
      "tikz={${1:TikZ keys}}\r",
      "ticks=${1|y,y-l,y-r,none\r|}",
      "ticks-step=${1:number}\r",
      "energy-unit=${1:unit}\r",
      "energy-step=${1:number}\r",
      "energy-zero=${1:number}\r",
      "energy-unit-separator=${1:separator}\r",
      "energy-round=${1:number}\r",
      "calculate"
    ],
    "\\ENcurve": [
      "step=${1:integer}\r",
      "looseness=${1:factor}\r",
      "minima",
      "tikz={${1:TikZ keys}}\r"
    ],
    "\\ShowNiveaus": [
      "length=${1:number}\r",
      "shift=${1:number}\r",
      "tikz={${1:TikZ keys}}\r",
      "niveau=${1:id}\r"
    ],
    "\\ShowGain": [
      "tikz={${1:TikZ keys}}\r",
      "connect={${1:TikZ keys}}\r",
      "connect-from-line",
      "offset=${1:number}\r",
      "label\r",
      "label=${1:text}\r",
      "label-side=${1|right,left\r|}",
      "label-pos=${1:value}\r",
      "label-tikz={${1:TikZ keys}}\r"
    ],
    "\\ShowEa": [
      "max=${1|first,all\r|}",
      "from={(${1:x1,y1}) to (${2:x2,y2})}\r",
      "tikz={${1:TikZ keys}}\r",
      "connect={${1:TikZ keys}}\r",
      "label\r",
      "label=${1:text}\r",
      "label-side=${1|right,left\r|}",
      "label-pos=${1:value}\r",
      "label-tikz={${1:TikZ keys}}\r"
    ],
    "\\AddAxisLabel,\\AddAxisLabel*": [
      "axis=${1|y-l,y-r,x\r|}",
      "connect={${1:TikZ keys}}\r",
      "font=${1:font commands}\r"
    ]
  },
  "args": []
}