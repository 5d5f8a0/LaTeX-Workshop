{
  "deps": [
    {
      "name": "ifpdf"
    },
    {
      "name": "pgfcore"
    },
    {
      "name": "pstricks",
      "if": "pstricks"
    },
    {
      "name": "pst-node",
      "if": "pstricks"
    }
  ],
  "macros": [
    {
      "name": "pstr",
      "if": "pstricks",
      "arg": {
        "format": "{}",
        "snippet": "{${1:string spec%formula}}"
      }
    },
    {
      "name": "pstr",
      "if": "pstricks",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:raise%l}]{${1:string spec%formula}}"
      }
    },
    {
      "name": "pstr",
      "if": "pstricks",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:raise%l}][${3:nodesep%l}]{${1:string spec%formula}}"
      }
    },
    {
      "name": "pstrSetLabelStyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style cmds}}"
      }
    },
    {
      "name": "pstrSetArrowColor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "pstrSetArrowLineWidth",
      "arg": {
        "format": "{}",
        "snippet": "{${1:width}}"
      }
    },
    {
      "name": "pstrSetArrowAngle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:angle}}"
      }
    },
    {
      "name": "pstrSetArrowLabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "pstrSetArrowLineStyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:line style%keyvals}}"
      }
    },
    {
      "name": "Pstr",
      "arg": {
        "format": "{}",
        "snippet": "{${1:string spec%formula}}"
      }
    },
    {
      "name": "Pstr",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:raise%l}]{${1:string spec%formula}}"
      }
    },
    {
      "name": "Pstr",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:raise%l}][${3:nodesep%l}]{${1:string spec%formula}}"
      }
    },
    {
      "name": "nd",
      "unusual": true
    },
    {
      "name": "arrow",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}{}{}",
        "snippet": "{${1:source}}{${2:target}}{${3:angle}}{${4:label}}{${5:linecolor}}{${6:linestyle}}"
      }
    },
    {
      "name": "txt",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pstring#c": [
      "pstricks\r",
      "pgf\r"
    ],
    "\\pstrSetArrowLineStyle#c": [
      "solid\r",
      "dotted\r",
      "dashed\r",
      "none\r"
    ]
  },
  "args": [
    "\\usepackage/pstring#c"
  ]
}