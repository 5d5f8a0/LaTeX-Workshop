{
  "deps": [
    {
      "name": "filehook"
    },
    {
      "name": "pgfkeys"
    }
  ],
  "macros": [
    {
      "name": "pgffilehook",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pgffilehook": [
      "EveryFile\r",
      "Files\r",
      "File\r",
      "Inputs\r",
      "InputFile\r",
      "Includes\r",
      "IncludeFile\r",
      "ClassFile\r",
      "PackageFile\r",
      "AtBegin={${1:code}}\r",
      "AtEnd={${1:code}}\r"
    ]
  },
  "args": []
}