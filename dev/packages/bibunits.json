    {
      "name": "bibliography*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bib file}}"
      }
    },
    {
      "name": "bibliographystyle*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style%keyvals}}"
