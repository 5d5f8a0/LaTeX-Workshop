{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "ProvideMathFix",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/mathfixs#c,\\ProvideMathFix"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "rfrac",
      "if": "rfrac",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:numerator}}{${2:denominator}}"
      }
    },
    {
      "name": "vfrac",
      "if": "vfrac",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:numerator}}{${2:denominator}}"
      }
    },
    {
      "name": "mathbold",
      "if": "mathbold",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    },
    {
      "name": "rfrac",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:numerator}}{${2:denominator}}"
      }
    },
    {
      "name": "vfrac",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:numerator}}{${2:denominator}}"
      }
    },
    {
      "name": "mathbold",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/mathfixs#c,\\ProvideMathFix": [
      "frac\r",
      "fracclass=${1:class}\r",
      "fracdelimclass=${1:class}\r",
      "rfrac\r",
      "rfrac=${1:command}\r",
      "vfrac\r",
      "vfrac=${1:command}\r",
      "vfracclass=${1:class}\r",
      "vfracskippre=${1:muskip}\r",
      "vfracskippost=${1:muskip}\r",
      "root\r",
      "rootclass=${1:class}\r",
      "rootskipend=${1:muskip}\r",
      "rootskippre=${1:muskip}\r",
      "rootskippost=${1:muskip}\r",
      "rootclose=",
      "multskip\r",
      "multskip=${1:muskip}\r",
      "greekcaps\r",
      "greekcaps=${1:prefix}\r",
      "greeklower\r",
      "greeklower=${1:prefix}\r",
      "autobold\r",
      "mathbold\r",
      "mathbold=${1:command}\r"
    ]
  },
  "args": [
    "\\usepackage/mathfixs#c,\\ProvideMathFix"
  ]
}