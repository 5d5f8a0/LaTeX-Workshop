{
  "deps": [
    {
      "name": "zref-base"
    },
    {
      "name": "zref-user"
    },
    {
      "name": "zref-abspage"
    },
    {
      "name": "ifdraft"
    },
    {
      "name": "zref-hyperref"
    }
  ],
  "macros": [
    {
      "name": "zcref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:labellist}}"
      }
    },
    {
      "name": "zcref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "zcref*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:labellist}}"
      }
    },
    {
      "name": "zcref*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "zcpageref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:labellist}}"
      }
    },
    {
      "name": "zcpageref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "zcpageref*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:labellist}}"
      }
    },
    {
      "name": "zcpageref*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "zcsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\zcref,\\zcref*,\\zcpageref,\\zcpageref*,\\zcsetup",
          "\\zcsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "zcRefTypeSetup",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type}}{${2:options%keyvals}}",
        "keys": [
          "\\zcRefTypeSetup"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "zcDeclareLanguage",
      "arg": {
        "format": "{}",
        "snippet": "{${1:language}}"
      }
    },
    {
      "name": "zcDeclareLanguage",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:language}}"
      }
    },
    {
      "name": "zcDeclareLanguageAlias",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:language alias}}{${2:aliased language}}"
      }
    },
    {
      "name": "zcLanguageSetup",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:language}}{${2:options%keyvals}}",
        "keys": [
          "\\zcLanguageSetup"
        ],
        "keyPos": 1
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\zcref,\\zcref*,\\zcpageref,\\zcpageref*,\\zcsetup": [
      "ref=${1|default,page,thecounter,title\r|}",
      "page\r",
      "typeset=${1|ref,name,both\r|}",
      "noname\r",
      "noref\r",
      "sort",
      "nosort\r",
      "typesort={${1:type list}}\r",
      "notypesort\r",
      "comp",
      "nocomp\r",
      "endrange=${1|ref,stripprefix,pagecomp,pagecomp2\r|}",
      "range",
      "rangetopair",
      "cap",
      "nocap\r",
      "capfirst",
      "abbrev",
      "noabbrev\r",
      "noabbrevfirst\r",
      "S\r",
      "hyperref=${1|auto,true,false\r|}",
      "nameinlink=${1|true,false,single,tsingle\r|}",
      "lang=${1:language}\r",
      "d=${1:declension case}\r",
      "nudge=${1|true,false,ifdraft,iffinal\r|}",
      "nudgeif=${1|multitype,comptosing,gender,all\r|}",
      "nonudge\r",
      "sg\r",
      "g=\r",
      "font=${1:font commands}\r",
      "note=${1:text}\r",
      "check={${1:checks}}\r",
      "countertype={${1:<counter>} = ${2:<type> list}}\r",
      "reftype=${1:type}\r",
      "counterresetters={${1:counter list}}\r",
      "counterresetby={${1:<counter=encl counter> list}}\r",
      "currentcounter=${1:counter}\r",
      "labelhook",
      "tpairsep={${1:separator}}\r",
      "tlistsep={${1:separator}}\r",
      "tlastsep={${1:separator}}\r",
      "notesep={${1:separator}}\r",
      "namesep={${1:separator}}\r",
      "pairsep={${1:separator}}\r",
      "listsep={${1:separator}}\r",
      "lastsep={${1:separator}}\r",
      "rangesep={${1:separator}}\r",
      "refbounds={${1:preref*,preref,postref,postref*}}\r",
      "namefont=${1:name}\r",
      "reffont=${1:name}\r"
    ],
    "\\zcsetup": [
      "nocompat\r",
      "nocompat={${1:module list}}\r"
    ],
    "\\zcRefTypeSetup": [
      "namesep={${1:separator}}\r",
      "pairsep={${1:separator}}\r",
      "listsep={${1:separator}}\r",
      "lastsep={${1:separator}}\r",
      "rangesep={${1:separator}}\r",
      "refbounds={${1:preref*,preref,postref,postref*}}\r",
      "Name-sg=${1:name}\r",
      "name-sg=${1:name}\r",
      "Name-pl=${1:name}\r",
      "name-pl=${1:name}\r",
      "Name-sg-ab=${1:name}\r",
      "name-sg-ab=${1:name}\r",
      "Name-pl-ab=${1:name}\r",
      "name-pl-ab=${1:name}\r",
      "namefont=${1:font commands}\r",
      "reffont=${1:font commands}\r",
      "cap",
      "abbrev",
      "endrange=${1|ref,stripprefix,pagecomp,pagecomp2\r|}",
      "rangetopair"
    ],
    "\\zcDeclareLanguage": [
      "declension={${1:cases list}}\r",
      "gender={${1:gender list}}\r",
      "allcaps\r"
    ],
    "\\zcLanguageSetup": [
      "tpairsep={${1:separator}}\r",
      "tlistsep={${1:separator}}\r",
      "tlastsep={${1:separator}}\r",
      "notesep={${1:separator}}\r",
      "namesep={${1:separator}}\r",
      "pairsep={${1:separator}}\r",
      "listsep={${1:separator}}\r",
      "lastsep={${1:separator}}\r",
      "rangesep={${1:separator}}\r",
      "refbounds={${1:preref*,preref,postref,postref*}}\r",
      "Name-sg=${1:name}\r",
      "name-sg=${1:name}\r",
      "Name-pl=${1:name}\r",
      "name-pl=${1:name}\r",
      "Name-sg-ab=${1:name}\r",
      "name-sg-ab=${1:name}\r",
      "Name-pl-ab=${1:name}\r",
      "name-pl-ab=${1:name}\r",
      "namefont=${1:font commands}\r",
      "reffont=${1:font commands}\r",
      "cap",
      "abbrev",
      "endrange=${1|ref,stripprefix,pagecomp,pagecomp2\r|}",
      "rangetopair",
      "type=${1:type}\r",
      "case=${1|N,A,D,G\r|}",
      "gender={${1:gender list}}\r"
    ]
  },
  "args": []
}