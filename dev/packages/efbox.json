{
  "deps": [
    {
      "name": "color"
    },
    {
      "name": "pgfkeys"
    }
  ],
  "macros": [
    {
      "name": "efbox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "efbox",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}",
        "keys": [
          "\\efbox,\\efboxsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "efboxsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\efbox,\\efboxsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\efbox,\\efboxsetup": [
      "leftline",
      "rightline",
      "topline",
      "bottomline",
      "hidealllines",
      "backgroundcolor=",
      "linecolor=",
      "linewidth=",
      "margin=",
      "font=",
      "baselinestretch="
    ]
  },
  "args": []
}