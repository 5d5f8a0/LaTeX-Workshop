{
  "deps": [],
  "macros": [
    {
      "name": "printbibshortfields"
    },
    {
      "name": "printbibshortfields",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "shortfieldswidth",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\printbibshortfields": [
      "heading=${1:name}\r",
      "title=${1:text}\r",
      "label=",
      "block=${1|none,space,par,nbpar,ragged\r|}",
      "prenote=${1:name}\r",
      "postnote=${1:name}\r",
      "section=${1:integer}\r",
      "segment=${1:integer}\r",
      "type=${1:entrytype}\r",
      "nottype=${1:entrytype}\r",
      "subtype=${1:subtype}\r",
      "notsubtype=${1:subtype}\r",
      "keyword=${1:keyword}\r",
      "notkeyword=${1:keyword}\r",
      "category=${1:category}\r",
      "notcategory=${1:category}\r",
      "filter=${1:name}\r",
      "resetnumbers=\r",
      "omitnumbers",
      "locallabelwidth"
    ]
  },
  "args": []
}