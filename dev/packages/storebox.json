{
  "deps": [
    {
      "name": "ifpdf"
    },
    {
      "name": "collectbox"
    }
  ],
  "macros": [
    {
      "name": "newstorebox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:boxname%cmd}}"
      }
    },
    {
      "name": "storebox",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:boxname}}{${2:content%text}}"
      }
    },
    {
      "name": "usestorebox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:boxname}}"
      }
    },
    {
      "name": "ifstorebox",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "storebox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:boxname}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/storebox#c": [
      "disable\r",
      "enable\r",
      "immediate\r",
      "delayed\r"
    ]
  },
  "args": [
    "\\usepackage/storebox#c"
  ]
}