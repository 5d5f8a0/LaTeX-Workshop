{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarymath"
    },
    {
      "name": "tikzlibrarydecorations.pathreplacing"
    },
    {
      "name": "tikzlibrarycalligraphy"
    },
    {
      "name": "tikzlibrarybackgrounds"
    }
  ],
  "macros": [
    {
      "name": "pmdEmpty"
    },
    {
      "name": "pmdBrick",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:U-edges}}{${2:L-edges}}{${3:T-edges}}"
      }
    },
    {
      "name": "pmdDiagram",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bricks}}"
      }
    },
    {
      "name": "pmdDiagram",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:bricks}}"
      }
    },
    {
      "name": "pmdProduct",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:A-edges}}{${2:T-bricks}}{${3:B-bricks}}"
      }
    },
    {
      "name": "pmdProduct",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:A-edges}}{${2:T-bricks}}{${3:B-bricks}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pmdBrick#c": [
      "degree=${1:integer}\r",
      "degree top=${1:integer}\r",
      "degree bottom=${1:integer}\r",
      "blank top=${1:integer}\r",
      "blank bottom=${1:integer}\r",
      "row sep=${1:integer}\r",
      "vertices={${1:\\draw options}}\r",
      "no vertex bottom 1=${1:integer}\r",
      "no vertex bottom 2=${1:integer}\r",
      "no vertex bottom 3=${1:integer}\r",
      "no vertex bottom 4=${1:integer}\r",
      "no vertex bottom 5=${1:integer}\r",
      "no vertex bottom 6=${1:integer}\r",
      "no vertex bottom 7=${1:integer}\r",
      "no vertex bottom 8=${1:integer}\r",
      "no vertex bottom 9=${1:integer}\r",
      "no vertex top 1=${1:integer}\r",
      "no vertex top 2=${1:integer}\r",
      "no vertex top 3=${1:integer}\r",
      "no vertex top 4=${1:integer}\r",
      "no vertex top 5=${1:integer}\r",
      "no vertex top 6=${1:integer}\r",
      "no vertex top 7=${1:integer}\r",
      "no vertex top 8=${1:integer}\r",
      "no vertex top 9=${1:integer}\r",
      "vertices options={${1:code}}\r",
      "labels\r",
      "labels top\r",
      "labels bottom\r",
      "labels start=${1:integer}\r",
      "labels top start=${1:integer}\r",
      "labels bottom start=${1:integer}\r",
      "no label bottom 1=${1:integer}\r",
      "no label bottom 2=${1:integer}\r",
      "no label bottom 3=${1:integer}\r",
      "no label bottom 4=${1:integer}\r",
      "no label bottom 5=${1:integer}\r",
      "no label bottom 6=${1:integer}\r",
      "no label bottom 7=${1:integer}\r",
      "no label bottom 8=${1:integer}\r",
      "no label bottom 9=${1:integer}\r",
      "no label top 1=${1:integer}\r",
      "no label top 2=${1:integer}\r",
      "no label top 3=${1:integer}\r",
      "no label top 4=${1:integer}\r",
      "no label top 5=${1:integer}\r",
      "no label top 6=${1:integer}\r",
      "no label top 7=${1:integer}\r",
      "no label top 8=${1:integer}\r",
      "no label top 9=${1:integer}\r",
      "labels top draw={${1:\\draw options}}\r",
      "labels top node={${1:\\node options}}\r",
      "labels bottom draw={${1:\\draw options}}\r",
      "labels bottom node={${1:\\node options}}\r",
      "labels top options={${1:code}}\r",
      "labels bottom options={${1:code}}\r",
      "no dots\r",
      "dots draw={${1:\\draw options}}\r",
      "dots node={${1:\\node options}}\r",
      "vertices top phantom\r",
      "vertices bottom phantom\r",
      "vertices top uncover={${1:overlay spec}}\r",
      "vertices bottom uncover={${1:overlay spec}}\r",
      "levels=${1:integer}\r",
      "levels top=${1:integer}\r",
      "levels bottom=${1:integer}\r",
      "levels sep=${1:number}\r",
      "levels sep top=${1:number}\r",
      "levels sep bottom=${1:number}\r",
      "edges={${1:\\draw options}}\r",
      "edges non-transversal={${1:\\draw options}}\r",
      "edges upper={${1:\\draw options}}\r",
      "edges lower={${1:\\draw options}}\r",
      "edges transversal={${1:\\draw options}}\r",
      "edges first\r",
      "transversals first\r",
      "edges phantom\r",
      "edges uncover={${1:overlay spec}}\r",
      "brace={${1:options}}\r",
      "brace draw={${1:\\draw options}}\r",
      "brace node={${1:\\node options}}\r",
      "decorate before={${1:code}}\r",
      "decorate after={${1:code}}\r"
    ],
    "\\pmdDiagram#c,\\pmdProduct#c": [
      "row sep=${1:integer}\r",
      "tikz={${1:TikZ options}}\r",
      "dots options={${1:code}}\r",
      "grid\r",
      "decorate before={${1:code}}\r",
      "decorate after={${1:code}}\r"
    ],
    "\\pmdProduct#c": [
      "diagram sep=${1:number}\r",
      "vertex sep=${1:number}\r",
      "edges added={${1:\\draw options}}\r",
      "edges added phantom\r",
      "edges added uncover={${1:overlay spec}}\r"
    ]
  },
  "args": []
}