{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-plot"
    },
    {
      "name": "multido"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "setpstgraph",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\begin{pstgraph},\\setpstgraph"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pstlabel",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:x-label}}{${2:y-label}}"
      }
    },
    {
      "name": "pstfileplot",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "pstfileplot",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:pstricks options}]{${1:file}}"
      }
    }
  ],
  "envs": [
    {
      "name": "pstgraph",
      "arg": {
        "format": "()",
        "snippet": "(${1:x0},${2:y0})"
      }
    },
    {
      "name": "pstgraph",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:options%keyvals}](${2:x0},${3:y0})"
      }
    }
  ],
  "keys": {
    "\\begin{pstgraph},\\setpstgraph": [
      "xmin=${1:number}\r",
      "xmax=${1:number}\r",
      "ymin=${1:number}\r",
      "ymax=${1:number}\r",
      "xgriddiv=${1:number}\r",
      "ygriddiv=${1:number}\r",
      "gridstyle=${1:style}\r",
      "gridcolor=",
      "gridwidth=",
      "xticksize=${1:number}\r",
      "yticksize=${1:number}\r",
      "xtagsep=${1:number}\r",
      "ytagsep=${1:number}\r",
      "xtagformat=${1:format commands}\r",
      "ytagformat=${1:format commands}\r",
      "xlabel=${1:text}\r",
      "ylabel=${1:text}\r",
      "xlabelsep=${1:number}\r",
      "ylabelsep=${1:number}\r",
      "framewidth=",
      "framecolor="
    ]
  },
  "args": []
}