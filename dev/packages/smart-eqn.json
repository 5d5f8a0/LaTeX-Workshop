{
  "deps": [
    {
      "name": "fancyvrb"
    }
  ],
  "macros": [
    {
      "name": "smesetsym",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:style csname}}{${2:symbols}}"
      }
    },
    {
      "name": "smeclearsym"
    },
    {
      "name": "makeatmath"
    },
    {
      "name": "smenewenv",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "smeraw",
      "arg": {
        "format": "{}",
        "snippet": "{${1:contents}}"
      }
    },
    {
      "name": "smeDefineVerbatimEnvironment",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:envname}}{${2:env type}}{${3:options}}"
      }
    }
  ],
  "envs": [],
  "keys": {},
  "args": []
}