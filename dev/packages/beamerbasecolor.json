{
  "deps": [],
  "macros": [
    {
      "name": "setbeamercolor",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:beamer-color name}}{${2:options%keyvals}}",
        "keys": [
          "\\setbeamercolor,\\setbeamercolor*"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "setbeamercolor*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:beamer-color name}}{${2:options%keyvals}}",
        "keys": [
          "\\setbeamercolor,\\setbeamercolor*"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "ifbeamercolorempty",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:beamer-color name}}{${2:if undefined}}{${3:if defined}}"
      }
    },
    {
      "name": "ifbeamercolorempty",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:fg or bg%keyvals}]{${1:beamer-color name}}{${2:if undefined}}{${3:if defined}}"
      }
    },
    {
      "name": "usebeamercolor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:beamer-color name}}"
      }
    },
    {
      "name": "usebeamercolor",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:fg or bg%keyvals}]{${1:beamer-color name}}"
      }
    },
    {
      "name": "usebeamercolor*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:beamer-color name}}"
      }
    },
    {
      "name": "usebeamercolor*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:fg or bg%keyvals}]{${1:beamer-color name}}"
      }
    },
    {
      "name": "donotcoloroutermaths",
      "unusual": true
    },
    {
      "name": "donotcolorouterdisplaymaths",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "beamercolorbox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:beamer color}}"
      }
    },
    {
      "name": "beamercolorbox",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:beamer color}}"
      }
    }
  ],
  "keys": {
    "\\setbeamercolor,\\setbeamercolor*": [
      "fg=",
      "bg=",
      "parent={${1:parent beamer colors}}\r",
      "use=${1:beamer color}\r"
    ],
    "\\ifbeamercolorempty,\\usebeamercolor,\\usebeamercolor*": [
      "fg\r",
      "bg\r"
    ],
    "\\begin{beamercolorbox}": [
      "wd=",
      "dp=",
      "ht=",
      "left\r",
      "right\r",
      "center\r",
      "leftskip=",
      "rightskip=",
      "sep=",
      "colsep=",
      "colsep*=",
      "shadow",
      "rounded",
      "ignorebg\r",
      "vmode\r"
    ]
  },
  "args": []
}