{
  "deps": [
    {
      "name": "zref-base"
    },
    {
      "name": "gettitlestring"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "ztitleref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "ztitlerefsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\ztitlerefsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\ztitlerefsetup": [
      "title=",
      "stripperiod",
      "expand",
      "cleanup={${1:code}}"
    ],
    "\\zref#c": [
      "title"
    ]
  },
  "args": []
}