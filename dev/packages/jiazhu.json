{
  "deps": [],
  "macros": [
    {
      "name": "jiazhu",
      "arg": {
        "format": "{}",
        "snippet": "{${1:夹注内容%text}}"
      }
    },
    {
      "name": "jiazhu",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:键值选项%keyvals}]{${1:夹注内容%text}}"
      }
    },
    {
      "name": "jiazhuset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:键值选项%keyvals}}",
        "keys": [
          "\\jiazhu,\\jiazhuset"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\jiazhu,\\jiazhuset": [
      "format=${1:格式命令}\r",
      "beforeskip=${1:弹性间距}\r",
      "afterskip=${1:弹性间距}\r",
      "opening=${1:前置内容}\r",
      "closing=${1:后置内容}\r",
      "ideohtratio=${1:数字}\r",
      "jzideohtratio=${1:数字}\r",
      "ratio=${1:数字}\r",
      "bracketratio=${1:数字}\r",
      "baselineshift=",
      "lines=${1:正整数}\r",
      "shortcut=${1:字符}\r",
      "shortcut-=${1:字符列表}\r",
      "halign=${1|justified,left,right,centered,distributed\r|}",
      "valign=${1|middle,top,bottom\r|}"
    ]
  },
  "args": []
}