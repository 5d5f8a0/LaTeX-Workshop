{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-plot"
    },
    {
      "name": "pst-node"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psthick",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:t1}}{${2:t2}}{${3:function%definition}}"
      }
    },
    {
      "name": "fonctionSinus",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:period}}{${2:amplitude}}"
      }
    },
    {
      "name": "CalculsCurves",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\psthick#c": [
      "E=${1:number}\r",
      "K=${1:number}\r",
      "stylethick=${1|vide,thickline,onlythecurvered,onlythecurveblue,thicklineblue,rainbow,solide,serpent\r|}",
      "curveonly",
      "stylecurve1=${1|vide,thickline,onlythecurvered,onlythecurveblue,thicklineblue,rainbow,solide,serpent\r|}",
      "stylecurve2=${1|vide,thickline,onlythecurvered,onlythecurveblue,thicklineblue,rainbow,solide,serpent\r|}"
    ]
  },
  "args": []
}