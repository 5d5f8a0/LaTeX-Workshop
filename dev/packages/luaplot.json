{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "luacode"
    },
    {
      "name": "tikz"
    },
    {
      "name": "luamplib"
    }
  ],
  "macros": [
    {
      "name": "luaplot",
      "arg": {
        "format": "{}",
        "snippet": "{${1:function%definition}}"
      }
    },
    {
      "name": "luaplot",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:function%definition}}"
      }
    },
    {
      "name": "luatikzpath",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:function%definition}}{${2:xmin}}{${3:xmax}}{${4:plot points}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\luaplot": [
      "xmin=${1:number}\r",
      "xmax=${1:number}\r",
      "ymin=${1:number}\r",
      "ymax=${1:number}\r",
      "plotpts=${1:integer}\r",
      "hor=",
      "ver=",
      "clr={'${1:color1,color2,...}'}\r",
      "plotsty={'${1:style1,style2,...}'}\r",
      "plotoptions={${1:MetaPost code}}\r"
    ]
  },
  "args": []
}