{
  "deps": [
    {
      "name": "platex"
    },
    {
      "name": "class-ltxdoc"
    }
  ],
  "macros": [
    {
      "name": "Lcount",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    },
    {
      "name": "Lopt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:option}}"
      }
    },
    {
      "name": "NFSS"
    },
    {
      "name": "dst"
    },
    {
      "name": "file",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file name}}"
      }
    },
    {
      "name": "mlineplus",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "pstyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    }
  ],
  "envs": [
    {
      "name": "tsample",
      "arg": {
        "format": "{}",
        "snippet": "{${1:height}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/jltxdoc#c": [
      "nocfg\r",
      "doc2\r",
      "a4paper\r",
      "b5paper\r",
      "letterpaper\r",
      "legalpaper\r",
      "executivepaper\r",
      "landscape\r",
      "10pt\r",
      "11pt\r",
      "12pt\r",
      "oneside\r",
      "twoside\r",
      "draft\r",
      "final\r",
      "titlepage\r",
      "notitlepage\r",
      "onecolumn\r",
      "twocolumn\r",
      "leqno\r",
      "fleqn\r",
      "openbib\r"
    ]
  },
  "args": [
    "\\documentclass/jltxdoc#c"
  ]
}