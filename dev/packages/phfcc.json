{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "marginnote"
    }
  ],
  "macros": [
    {
      "name": "phfMakeCommentingCommand",
      "arg": {
        "format": "{}",
        "snippet": "{${1:cmd name}}"
      }
    },
    {
      "name": "phfMakeCommentingCommand",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:cmd name}}"
      }
    },
    {
      "name": "phfDisableCommentingCommands"
    },
    {
      "name": "phfDefineCommentingStyle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:style name}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "phfSetDefaultCommentingStyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "phfccformatmargininitials",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:initials}}"
      }
    },
    {
      "name": "phfccmargininitialssep",
      "unusual": true
    },
    {
      "name": "phfccformatboxinitials",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:initials}}"
      }
    },
    {
      "name": "phfCCChangesBy",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:initials}}"
      }
    },
    {
      "name": "phfCommentingDefaultStartCmds",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultEndCmds",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultFont",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultSpacing",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultBegin",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultEnd",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultCFont",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultCSpacing",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultCBegin",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultCEnd",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultRmFont",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultRmSpacing",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultRmBegin",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultRmEnd",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultIFont",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultISpacing",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultIBegin",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultIEnd",
      "unusual": true
    },
    {
      "name": "phfccfootcommenttextstyle",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\phfMakeCommentingCommand#c,\\phfDefineCommentingStyle#c,\\phfSetDefaultCommentingStyle#c": [
      "color=",
      "initials={${1:initials}}\r",
      "formatinitials=${1:format cmds}\r",
      "formatinitials=${1|default,margin,box,nobox,hide,footnote\r|}",
      "style=${1|footcomments,rmstrikethrough\r|}",
      "font={${1:font cmds}}\r",
      "spacing=",
      "begin=\r",
      "end=\r",
      "cfont={${1:font cmds}}\r",
      "cspacing=",
      "cbegin=\r",
      "cend=\r",
      "rmfont={${1:font cmds}}\r",
      "rmspacing=",
      "rmbegin=\r",
      "rmend=\r",
      "ifont={${1:font cmds}}\r",
      "ispacing=",
      "ibegin=\r",
      "iend=\r"
    ]
  },
  "args": []
}