{
  "deps": [
    {
      "name": "marvosym"
    }
  ],
  "macros": [
    {
      "name": "affil",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "affilstyle",
      "arg": {
        "format": "[]",
        "snippet": "[${1:keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\author#c": [
      "name={${1:name1,name2,...}}\r",
      "mail={${1:email1,email2,...}}\r",
      "phone={${1:num1,num2,...}}\r",
      "url={${1:URL1,URL2,...}}\r",
      "affil={${1:affil1,affil2,...}}\r",
      "note={${1:note1,note2,...}\r"
    ],
    "\\affil#c": [
      "id=${1:id}\r",
      "div=${1:text}\r",
      "org=${1:text}\r",
      "addr=${1:text}\r",
      "street=${1:text}\r",
      "landmark=${1:text}\r",
      "pincode=${1:text}\r",
      "postbox=${1:text}\r",
      "city=${1:text}\r",
      "state=${1:text}\r",
      "country=${1:text}\r"
    ],
    "\\affilstyle#c": [
      "authfont=${1:font commands}\r",
      "affilfont=${1:font commands}\r",
      "mailfont=${1:font commands}\r",
      "urlfont=${1:font commands}\r",
      "phonefont=${1:font commands}\r",
      "notefont=${1:font commands}\r",
      "notenum=${1|alpha,fnsym,roman\r|}",
      "authspace=",
      "affilspace=",
      "affilnum=${1|alpha,Alpha,roman,Roman\r|}"
    ]
  },
  "args": []
}