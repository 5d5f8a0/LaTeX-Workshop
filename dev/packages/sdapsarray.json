{
  "deps": [
    {
      "name": "sdapsbase"
    }
  ],
  "macros": [
    {
      "name": "sdapsnested",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "sdapsarray"
    },
    {
      "name": "sdapsarray",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{sdapsarray}": [
      "flip",
      "layouter=${1|default,rotated\r|}",
      "angle=${1:angle}\r",
      "align=${1:string}\r",
      "keepenv\r",
      "no_header",
      "colsep=",
      "rowsep="
    ]
  },
  "args": []
}