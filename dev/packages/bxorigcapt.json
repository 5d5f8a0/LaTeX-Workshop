{
  "deps": [
    {
      "name": "etoolbox"
    }
  ],
  "macros": [
    {
      "name": "bxDebug",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:message}}"
      }
    },
    {
      "name": "bxorigcaptDeprecateCommand",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:csname1,csname2,...}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/bxorigcapt#c": [
      "main\r",
      "${1:language}\r",
      "warn\r",
      "nowarn\r"
    ]
  },
  "args": [
    "\\usepackage/bxorigcapt#c"
  ]
}