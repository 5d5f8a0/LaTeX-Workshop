{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "environ"
    }
  ],
  "macros": [
    {
      "name": "gmpoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/gmp#c,\\gmpoptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "usempxclass",
      "arg": {
        "format": "{}",
        "snippet": "{${1:class}}"
      }
    },
    {
      "name": "usempxclass",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:class}}"
      }
    },
    {
      "name": "usempxpackage",
      "arg": {
        "format": "{}",
        "snippet": "{${1:package}}"
      }
    },
    {
      "name": "usempxpackage",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:package}}"
      }
    },
    {
      "name": "resetmpxpackages"
    },
    {
      "name": "mpxcommands",
      "arg": {
        "format": "{}",
        "snippet": "{${1:commands}}"
      }
    },
    {
      "name": "resetmpxcommands"
    },
    {
      "name": "mpdim",
      "arg": {
        "format": "{}",
        "snippet": "{${1:dimen param}}"
      }
    },
    {
      "name": "usempost",
      "arg": {
        "format": "{}",
        "snippet": "{${1:string}}"
      }
    },
    {
      "name": "usempost",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:string}}"
      }
    },
    {
      "name": "btex"
    },
    {
      "name": "verbatimtex"
    }
  ],
  "envs": [
    {
      "name": "mpost"
    },
    {
      "name": "mpost",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "mpost*"
    },
    {
      "name": "mpost*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/gmp#c,\\gmpoptions": [
      "tex\r",
      "latex\r",
      "noshellescape\r",
      "shellescape\r",
      "nowrite\r",
      "write\r",
      "envname=${1:envname}\r",
      "extension=${1:extension}\r",
      "everymp={${1:metapost code}}\r",
      "clean=${1|none,aux,mp\r|}",
      "rmcommand=${1:cmd}\r",
      "postrmcommand=${1:cmd}\r"
    ],
    "\\usempost": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ],
    "\\begin{mpost},\\begin{mpost*}": [
      "name=${1:string}\r",
      "use",
      "mpmem=${1:metapost format}\r",
      "mpxprogram=${1|tex,latex\r|}",
      "mpxcommands=${1:commands}\r",
      "mpsettings={${1:metapost code}}\r",
      "runs=${1:number}\r"
    ]
  },
  "args": [
    "\\usepackage/gmp#c,\\gmpoptions"
  ]
}