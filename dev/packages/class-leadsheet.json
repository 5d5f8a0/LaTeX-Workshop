{
  "deps": [
    {
      "name": "class-scrartcl"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "leadsheets"
    },
    {
      "name": "scrlayer-scrpage"
    },
    {
      "name": "translations"
    },
    {
      "name": "zref-totpages"
    }
  ],
  "macros": [
    {
      "name": "instruction",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "choir",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "lsenparen",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "mkinstruction",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "mkchoir",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "mklsenparens",
      "unusual": true
    },
    {
      "name": "mklsenparen",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:open paren}}{${2:closing paren}}{${3:text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "prechorus"
    },
    {
      "name": "prechorus",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{prechorus}": [
      "format={${1:code}}\r",
      "label-format={${1:code}}\r",
      "class={${1:class name}}\r",
      "after-label={${1:code}}\r",
      "name=${1:name}\r",
      "template=${1:template}\r",
      "numbered",
      "named",
      "recall-chords={${1:env-class}}\r",
      "verses-format={${1:code}}\r",
      "verses-label-format={${1:code}}\r",
      "verses-after-label={${1:code}}\r"
    ]
  },
  "args": []
}