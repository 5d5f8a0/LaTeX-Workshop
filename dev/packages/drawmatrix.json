{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "drawmatrix",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "drawmatrix",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:label}}"
      }
    },
    {
      "name": "drawmatrixset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\drawmatrix#c,\\drawmatrixset#c": [
      "label text=${1:text}\r",
      "height=",
      "width=",
      "size=",
      "lower\r",
      "upper\r",
      "lower banded\r",
      "upper banded\r",
      "banded\r",
      "lower bandwidth=",
      "upper bandwidth=",
      "bandwidth=",
      "diag\r",
      "label base=${1:text}\r",
      "label base anchor=${1:anchor}\r",
      "exponent=${1:text}\r",
      "fill=",
      "draw=",
      "line width=",
      "ultra thin\r",
      "very thin\r",
      "thin\r",
      "semithick\r",
      "thick\r",
      "very thick\r",
      "ultra thick\r",
      "line cap=${1|round,rect,butt\r|}",
      "line join=${1|round,bevel,miter\r|}",
      "miter limit=${1:factor}\r",
      "dash pattern=${1:pattern}\r",
      "dash phase=",
      "dash=${1:dash pattern} phase ${2:dash phase}\r",
      "dash expand off\r",
      "solid\r",
      "dotted\r",
      "densely dotted\r",
      "loosely dotted\r",
      "dashed\r",
      "densely dashed\r",
      "loosely dashed\r",
      "dash dot\r",
      "densely dash dot\r",
      "loosely dash dot\r",
      "dash dot dot\r",
      "densely dash dot dot\r",
      "loosely dash dot dot\r",
      "bbox style={${1:TikZ options}}\r",
      "bbox height=",
      "bbox width=",
      "bbox size=",
      "offset height=",
      "offset width=",
      "offset=",
      "scale=${1:factor}\r",
      "x=",
      "y=",
      "label anchor=${1:anchor}\r",
      "label pos=bbox.${1:anchor}\r",
      "label pos=matrix.${1:anchor}\r",
      "baseline=${1:node.anchor}\r"
    ],
    "\\drawmatrixset#c": [
      "every picture/.append style={${1:TikZ options}}\r",
      "every bbox/.append style={${1:TikZ options}}\r",
      "every drawmatrix/.append style={${1:TikZ options}}\r",
      "every label/.append style={${1:TikZ options}}\r",
      "every node/.append style={${1:TikZ options}}\r",
      "externalize"
    ]
  },
  "args": []
}