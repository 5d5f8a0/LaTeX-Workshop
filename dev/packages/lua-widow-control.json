{
  "deps": [
    {
      "name": "luatex"
    },
    {
      "name": "microtype"
    }
  ],
  "macros": [
    {
      "name": "lwcsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\lwcsetup,\\usepackage/lua-widow-control#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\lwcsetup,\\usepackage/lua-widow-control#c": [
      "disable\r",
      "enable\r",
      "disablecmds={${1:\\macro1,\\macro2,...}}\r",
      "emergencystretch=",
      "widowpenalty=${1:integer}\r",
      "orphanpenalty=${1:integer}\r",
      "nobreak=${1|keep,split,warn\r|}",
      "max-cost=${1:integer}\r",
      "draft\r",
      "showcosts",
      "showcolours",
      "draftoffset=",
      "debug",
      "default\r",
      "strict\r",
      "balanced\r"
    ],
    "\\usepackage/lua-widow-control#c": [
      "microtype"
    ]
  },
  "args": [
    "\\lwcsetup,\\usepackage/lua-widow-control#c",
    "\\usepackage/lua-widow-control#c"
  ]
}