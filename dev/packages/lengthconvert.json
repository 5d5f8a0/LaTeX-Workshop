{
  "deps": [
    {
      "name": "l3keys2e"
    }
  ],
  "macros": [
    {
      "name": "Convert",
      "arg": {
        "format": "{}",
        "snippet": "{${1:length}}"
      }
    },
    {
      "name": "Convert",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:length}}"
      }
    },
    {
      "name": "Convertsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\Convert,\\Convertsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\Convert,\\Convertsetup": [
      "unit=${1|pt,pc,in,bp,cm,mm,dd,cc,sp\r|}",
      "pt\r",
      "pc\r",
      "in\r",
      "bp\r",
      "cm\r",
      "mm\r",
      "dd\r",
      "cc\r",
      "sp\r",
      "point\r",
      "pica\r",
      "inch\r",
      "big-point\r",
      "centimeter\r",
      "millimeter\r",
      "didot-point\r",
      "cicero\r",
      "scaled-point\r",
      "use-siunitx",
      "precision=${1:integer}\r",
      "number-only"
    ]
  },
  "args": []
}