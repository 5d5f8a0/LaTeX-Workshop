{
  "deps": [
    {
      "name": "atbegshi"
    },
    {
      "name": "etextools"
    },
    {
      "name": "fix-cm"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "ltxnew"
    },
    {
      "name": "picture"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "xifthen"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "draftmarksetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/draftmark#c,\\draftmarksetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/draftmark#c": [
      "draft",
      "final",
      "allpages",
      "evenpages",
      "oddpages",
      "pages=${1:num1-num2}\r",
      "page=${1:num}\r"
    ],
    "\\usepackage/draftmark#c,\\draftmarksetup": [
      "mark=${1:text}\r",
      "angle=${1:degrees}\r",
      "scale=${1:factor}\r",
      "grayness=${1:factor}\r",
      "color=",
      "fontfamily=${1:family}\r",
      "fontsize=",
      "xcoord=${1:number}\r",
      "ycoord=${1:number}\r",
      "coordunit=${1:unit}\r",
      "showcenter",
      "acceptrange"
    ]
  },
  "args": [
    "\\usepackage/draftmark#c",
    "\\usepackage/draftmark#c,\\draftmarksetup"
  ]
}