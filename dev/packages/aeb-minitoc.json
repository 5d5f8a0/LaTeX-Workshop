{
  "deps": [],
  "macros": [
    {
      "name": "TOPLevel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:level%keyvals}}"
      }
    },
    {
      "name": "BTMLevel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:level%keyvals}}"
      }
    },
    {
      "name": "TOCLevels",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:top level%keyvals}}{${2:btm level}}"
      }
    },
    {
      "name": "insertminitoc"
    },
    {
      "name": "insertminitoc",
      "arg": {
        "format": "[]",
        "snippet": "[${1:label}]"
      }
    },
    {
      "name": "mtoclabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "declaretocfmt",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:level%keyvals}}{${2:various}}"
      }
    },
    {
      "name": "mtocgobble",
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "miniorfulltoc",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:fmt}}{${2:toc-sec-heading%text}}"
      }
    },
    {
      "name": "FmtTOCEntry"
    },
    {
      "name": "NoFmtTOCEntry"
    },
    {
      "name": "mtocCL",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:contentsline%text}}{${2:entry-cnt}}"
      }
    },
    {
      "name": "mtocref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "ifMiniTocListings",
      "unusual": true
    },
    {
      "name": "MiniTocListingsfalse",
      "unusual": true
    },
    {
      "name": "MiniTocListingstrue",
      "unusual": true
    },
    {
      "name": "insertminitocNOT",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "numBoxWidth",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:length}}"
      }
    }
  ],
  "envs": [
    {
      "name": "minitocfmt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:command}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/aeb-minitoc#c": [
      "nominitocs\r",
      "!nominitocs\r"
    ],
    "\\TOPLevel#c,\\BTMLevel#c,\\TOCLevels#c,\\declaretocfmt#c": [
      "chapter\r",
      "section\r",
      "subsection\r",
      "subsubsection\r",
      "paragraph\r",
      "subparagraph\r"
    ],
    "\\declaretocfmt#c": [
      "chapter*\r",
      "section*\r",
      "subsection*\r",
      "subsubsection*\r",
      "paragraph*\r",
      "subparagraph*\r"
    ]
  },
  "args": [
    "\\usepackage/aeb-minitoc#c"
  ]
}