{
  "deps": [
    {
      "name": "luamplib"
    }
  ],
  "macros": [
    {
      "name": "wordcloud",
      "arg": {
        "format": "{}",
        "snippet": "{${1:(${2:word1},${3:weight1});(${4:word2},${5:weight2});...}}"
      }
    },
    {
      "name": "wordcloud",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:(${3:word1},${4:weight1});(${5:word2},${6:weight2});...}}"
      }
    },
    {
      "name": "wordcloudFile",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:file}}{${2:number of words}}"
      }
    },
    {
      "name": "wordcloudFile",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:file}}{${2:number of words}}"
      }
    },
    {
      "name": "wordcloudIgnoreWords",
      "arg": {
        "format": "{}",
        "snippet": "{${1:word1,word2,...}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\wordcloud,\\wordcloudFile": [
      "scale=${1:factor}\r",
      "margin=",
      "rotate=${1:degrees}\r",
      "usecolor\r",
      "colors={${1:color1,color2,...}}\r"
    ]
  },
  "args": []
}