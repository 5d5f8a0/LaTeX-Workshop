{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "pgfopts"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "fontspec"
    },
    {
      "name": "keyval"
    },
    {
      "name": "calc"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "metroset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usetheme/metropolis,\\metroset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "plain",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "plain",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:format%keyvals}]{${1:title%text}}"
      }
    },
    {
      "name": "mreducelistspacing",
      "unusual": true
    },
    {
      "name": "thefontsnotfound",
      "unusual": true
    },
    {
      "name": "checkfont",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:font name}}"
      }
    },
    {
      "name": "iffontsavailable",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:font list}}{${2:true}}{${3:false}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usetheme/metropolis,\\metroset": [
      "titleformat=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "titleformat plain=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "sectionpage=${1|none,simple,progressbar\r|}",
      "subsectionpage=${1|none,simple,progressbar\r|}",
      "numbering=${1|none,counter,fraction\r|}",
      "progressbar=${1|none,head,frametitle,foot\r|}",
      "block=${1|transparent,fill\r|}",
      "background=${1|dark,light\r|}",
      "titleformat title=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "titleformat subtitle=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "titleformat section=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "titleformat frame=${1|regular,smallcaps,allsmallcaps,allcaps\r|}"
    ],
    "\\plain": [
      "regular\r",
      "smallcaps\r",
      "allsmallcaps\r",
      "allcaps\r"
    ],
    "\\begin{axis}#c,\\begin{loglogaxis}#c,\\begin{semilogxaxis}#c,\\begin{semilogyaxis}#c,\\addplot#c,\\addplot3#c,\\addplot+#c,\\pgfplotsset#c,\\begin{groupplot}#c,\\begin{smithchart}#c,\\begin{polaraxis}#c,\\begin{ternaryaxis}#c": [
      "mlineplot\r",
      "mbarplot\r",
      "horizontal mbarplot\r",
      "disable thousands separator\r"
    ]
  },
  "args": []
}