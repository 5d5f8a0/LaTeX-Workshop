{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-solides3d"
    }
  ],
  "macros": [
    {
      "name": "psShell",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "psShell",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:options%keyvals}](${2:x},${3:y},${4:z})"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psShell": [
      "A=${1:number}\r",
      "a=${1:number}\r",
      "action=${1|none,draw,draw*,draw**,writesolid,writeobj,writeoff\r|}",
      "alpha=${1:degrees}\r",
      "b=${1:number}\r",
      "base=${1:base spec}\r",
      "beta=${1:degrees}\r",
      "D=${1:number}\r",
      "deactivatecolor\r",
      "fcol=${1:n1 (col1) n2 (col2) ...}\r",
      "file=${1:file name}\r",
      "fillcolor=",
      "hue=${1:hue spec}\r",
      "incolor=",
      "L=${1:number}\r",
      "lightsrc=${1:x y z}\r",
      "linecolor=",
      "linewidth=",
      "mu=${1:degrees}\r",
      "N=${1:number}\r",
      "ngrid=${1:n1 n2}\r",
      "Omega=${1:degrees}\r",
      "P=${1:number}\r",
      "phi=${1:degrees}\r",
      "precode=${1:PS code}\r",
      "rm=${1:face1 face2 ...}\r",
      "RotX=${1:degrees}\r",
      "RotY=${1:degrees}\r",
      "RotZ=${1:degrees}\r",
      "style=${1|Achatina,Ammonite,Argonauta,Codakia,Conus,Epiteonium,Escalaria,Helcion,Lyria,Natalina,Nautilus,Oxystele,Planorbis,Tonna,Turritella\r|}",
      "unit=${1:number}\r",
      "W1=${1:number}\r",
      "W2=${1:number}\r",
      "writedata"
    ]
  },
  "args": []
}