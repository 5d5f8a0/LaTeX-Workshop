{
  "deps": [
    {
      "name": "pgfopts"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "listings"
    },
    {
      "name": "tcolorbox"
    }
  ],
  "macros": [
    {
      "name": "ff",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code%definition}}"
      }
    }
  ],
  "envs": [
    {
      "name": "ffcode"
    },
    {
      "name": "ffcode",
      "arg": {
        "format": "[]",
        "snippet": "[${1:listings options}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/ffcode#c": [
      "noframes\r",
      "nobars\r",
      "nonumbers\r",
      "nocn\r",
      "bold\r",
      "sf\r"
    ]
  },
  "args": [
    "\\usepackage/ffcode#c"
  ]
}