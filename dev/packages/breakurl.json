{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "ifpdf"
    }
  ],
  "macros": [
    {
      "name": "burl",
      "arg": {
        "format": "{}",
        "snippet": "{${1:URL}}"
      }
    },
    {
      "name": "burlalt",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:URL}}{${2:text%plain}}"
      }
    },
    {
      "name": "urlalt",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:URL}}{${2:text%plain}}"
      }
    },
    {
      "name": "UrlRight",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg%URL}}"
      }
    },
    {
      "name": "UrlLeft",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg%URL}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/breakurl#c": [
      "hyphenbreaks\r",
      "anythingbreaks\r",
      "preserveurlmacro\r",
      "vertfit=${1:criterion}\r"
    ]
  },
  "args": [
    "\\usepackage/breakurl#c"
  ]
}