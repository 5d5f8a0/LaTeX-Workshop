{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "class-ctexbeamer"
    },
    {
      "name": "xeCJKfntef"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "CJKhl",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:color}}{${2:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/bitbeamer#c": [
      "titlegraphic=${1:imagefile}\r",
      "framelogo=${1:imagefile}\r",
      "GBK\r",
      "UTF8\r",
      "zhmap=${1|true,false,zhmCJK\r|}",
      "zihao=${1|-4,5,false\r|}",
      "heading",
      "scheme=${1|chinese,plain\r|}",
      "linespread=${1:number}\r",
      "fontset=${1|adobe,fandol,founder,mac,macnew,macold,ubuntu,windows,none\r|}",
      "punct=${1|quanjiao,banjiao,kaiming,CCT,plain\r|}",
      "space=${1|true,false,auto\r|}",
      "autoindent=\r",
      "usepdftitle",
      "envcountsect\r",
      "notheorems\r",
      "noamsthm\r",
      "compress\r",
      "t\r",
      "c\r",
      "leqno\r",
      "fleqn\r",
      "handout\r",
      "trans\r",
      "ignorenonframetext\r",
      "noamssymb\r",
      "bigger\r",
      "smaller\r",
      "8pt\r",
      "9pt\r",
      "10pt\r",
      "11pt\r",
      "12pt\r",
      "14pt\r",
      "17pt\r",
      "20pt\r",
      "draft\r",
      "onlytextwidth\r",
      "pgf={${1:pgf options}}\r",
      "hyperref={${1:hyperref options}}\r",
      "color={${1:color options}}\r",
      "xcolor={${1:xcolor options}}\r",
      "aspectratio=${2|2013,1610,169,149,141,54,43,32,${1:xxxx}\r|}"
    ]
  },
  "args": [
    "\\documentclass/bitbeamer#c"
  ]
}