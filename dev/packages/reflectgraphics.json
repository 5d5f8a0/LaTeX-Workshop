{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "keyval"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "calc"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibraryfadings"
    }
  ],
  "macros": [
    {
      "name": "reflectgraphics",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "reflectgraphics",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\reflectgraphics": [
      "width=",
      "height=",
      "scale=${1:factor}\r",
      "angle=${1:degrees}\r",
      "trim=${1:<left> <bot> <up> <right>}\r",
      "distance=",
      "length=${1:percent}\r",
      "opacity=${1:percent}\r"
    ]
  },
  "args": []
}