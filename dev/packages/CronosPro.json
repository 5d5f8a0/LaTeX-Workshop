{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "CronosPro-FontDef"
    },
    {
      "name": "microtype"
    },
    {
      "name": "fontaxes"
    }
  ],
  "macros": [
    {
      "name": "smallfrac",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:numerator}}{${2:denominator}}"
      }
    },
    {
      "name": "slantfrac",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:numerator}}{${2:denominator}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/CronosPro#c": [
      "scale=${1:factor}\r",
      "slides\r",
      "noopticals\r",
      "opticals\r",
      "smallfamily\r",
      "medfamily\r",
      "normalsize\r",
      "nonormalsize\r",
      "textosf\r",
      "textlf\r",
      "osf\r",
      "lf\r",
      "footnotefigures\r",
      "loosequotes\r"
    ]
  },
  "args": [
    "\\usepackage/CronosPro#c"
  ]
}