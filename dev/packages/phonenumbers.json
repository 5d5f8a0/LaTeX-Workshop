{
  "deps": [
    {
      "name": "l3keys2e"
    }
  ],
  "macros": [
    {
      "name": "phonenumber",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "phonenumber",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:number}}[${2:extension}]"
      }
    },
    {
      "name": "phonenumber",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:number}}"
      }
    },
    {
      "name": "phonenumber",
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:options%keyvals}]{${1:number}}[${3:extension}]"
      }
    },
    {
      "name": "setphonenumbers",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/phonenumbers#c,\\phonenumber,\\setphonenumbers,\\AreaCodesGeographic,\\AreaCodesNonGeographic"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "AreaCodesGeographic"
    },
    {
      "name": "AreaCodesGeographic",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "AreaCodesNonGeographic"
    },
    {
      "name": "AreaCodesNonGeographic",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "CountryCodes"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/phonenumbers#c,\\phonenumber,\\setphonenumbers,\\AreaCodesGeographic,\\AreaCodesNonGeographic": [
      "country=${1|AT,DE,FR,UK,US\r|}",
      "link=${1|on,off\r|}",
      "foreign=${1|international,american,european,off\r|}",
      "foreign-area-code-sep=${1|space,brackets\r|}",
      "home-country=${1|AT,DE,FR,UK,US,none\r|}",
      "home-area-code=${1:area code}\r",
      "group-min=${1|3,4,5,6,7\r|}",
      "area-code-sep=${1|slash,brackets,space,hyphen\r|}",
      "area-code=${1|number,place,place-and-number\r|}",
      "trunk-prefix=${1|on,off\r|}"
    ]
  },
  "args": [
    "\\usepackage/phonenumbers#c,\\phonenumber,\\setphonenumbers,\\AreaCodesGeographic,\\AreaCodesNonGeographic"
  ]
}