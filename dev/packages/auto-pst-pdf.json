{
  "deps": [
    {
      "name": "ifpdf"
    },
    {
      "name": "xkeyval"
    },
    {
      "name": "ifplatform"
    },
    {
      "name": "pst-pdf"
    },
    {
      "name": "pst-calculate",
      "if": "useCalc"
    },
    {
      "name": "pdfcolmk",
      "if": "fixpdftex"
    }
  ],
  "macros": [
    {
      "name": "OnlyIfFileExists",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:file}}{${2:code}}"
      }
    },
    {
      "name": "NotIfFileExists",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:file}}{${2:code}}"
      }
    },
    {
      "name": "matlabfig",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "matlabfig",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "mathfig",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "mathfig",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "psfragfig",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "psfragfig",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/auto-pst-pdf#c": [
      "off\r",
      "on\r",
      "crop=${1|on,off\r|}",
      "ext=${1:extension}\r",
      "latex={${1:latex options}}\r",
      "dvips={${1:dvips options}}\r",
      "pspdf={${1:pspdf options}}\r",
      "pdfcrop={${1:pdfcrop options}}\r",
      "cleanup={${1:extension list}}\r",
      "runs=${1:number}\r",
      "active\r",
      "inactive\r",
      "pstricks\r",
      "nopstricks\r",
      "draft\r",
      "final\r",
      "tightpage\r",
      "notightpage\r",
      "displaymath\r",
      "noxcolor\r",
      "useCalc\r",
      "gsfonts\r",
      "psfonts\r",
      "noEngineCheck\r",
      "pdf\r",
      "xetex\r",
      "distiller\r",
      "monochrome\r",
      "grayscale\r"
    ],
    "\\matlabfig,\\mathfig,\\psfragfig": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": [
    "\\usepackage/auto-pst-pdf#c"
  ]
}