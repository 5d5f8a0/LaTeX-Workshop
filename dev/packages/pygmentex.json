{
  "deps": [
    {
      "name": "fancyvrb"
    },
    {
      "name": "color"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "caption"
    },
    {
      "name": "shellesc"
    },
    {
      "name": "pgfkeys"
    },
    {
      "name": "efbox"
    },
    {
      "name": "mdframed"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "inputpygmented",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "inputpygmented",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "pyginline",
      "arg": {
        "format": "[]||",
        "snippet": "[${1:options}]|${2:code}|"
      }
    },
    {
      "name": "setpygmented",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "widest(string list)",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:code}}{${2:cmd}}"
      }
    },
    {
      "name": "VerbatimOutAppend",
      "unusual": true
    },
    {
      "name": "FormatLineNumber",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    }
  ],
  "envs": [
    {
      "name": "pygmented"
    },
    {
      "name": "pygmented",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "VerbatimOutAppend",
      "unusual": true
    }
  ],
  "keys": {
    "\\usepackage/pygmentex#c": [
      "force\r"
    ],
    "\\begin{pygmented}#c,\\inputpygmented#c,\\pyginline#c,\\setpygmented#c": [
      "lang=${1:language}\r",
      "sty=${1:pygments style}\r",
      "font=${1:font commands}\r",
      "colback=",
      "gobble=${1:integer}\r",
      "tabsize=${1:integer}\r",
      "linenos",
      "linenostart=${1:integer}\r",
      "linenostep=${1:integer}\r",
      "linenosep=",
      "caption=${1:caption}\r",
      "label=",
      "texcomments",
      "mathescape",
      "escapeinside=${1:two chars}\r",
      "force"
    ],
    "\\pyginline#c,\\setpygmented#c": [
      "inline method=${1:csname}\r"
    ],
    "\\begin{pygmented}#c,\\inputpygmented#c,\\setpygmented#c": [
      "boxing method=${1:envname}\r"
    ]
  },
  "args": [
    "\\usepackage/pygmentex#c"
  ]
}