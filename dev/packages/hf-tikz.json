{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibraryshadings"
    },
    {
      "name": "etoolbox"
    }
  ],
  "macros": [
    {
      "name": "tikzmarkin",
      "arg": {
        "format": "{}",
        "snippet": "{${1:marker-id}}"
      }
    },
    {
      "name": "tikzmarkin",
      "arg": {
        "format": "{}()()",
        "snippet": "{${1:marker-id}}(${2:x1},${3:y1})(${4:x2},${5:y2})"
      }
    },
    {
      "name": "tikzmarkin",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:marker-id}}"
      }
    },
    {
      "name": "tikzmarkend",
      "arg": {
        "format": "{}",
        "snippet": "{${1:marker-id}}"
      }
    },
    {
      "name": "thejumping",
      "if": "beamer",
      "unusual": true
    },
    {
      "name": "hfsetfillcolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "hfsetbordercolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "ifshowmarkers",
      "if": "markings",
      "unusual": true
    },
    {
      "name": "showmarkerstrue",
      "if": "markings",
      "unusual": true
    },
    {
      "name": "showmarkersfalse",
      "if": "markings",
      "unusual": true
    },
    {
      "name": "fcol",
      "unusual": true
    },
    {
      "name": "bcol",
      "unusual": true
    },
    {
      "name": "savepointas",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:point}}"
      }
    },
    {
      "name": "pgfsyspdfmark",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}{${3:arg3}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/hf-tikz#c": [
      "nofill\r",
      "customcolors\r",
      "shade\r",
      "beamer\r",
      "norndcorners\r",
      "markings\r"
    ],
    "\\tikzmarkin#c,\\tikzset#c": [
      "mark at=${1:position}\r",
      "use marker id=${1:id number}\r",
      "show markers\r",
      "marker size=",
      "marker color="
    ],
    "\\tikzset#c": [
      "offset definition/.style={${1:style specs}}\r"
    ]
  },
  "args": [
    "\\usepackage/hf-tikz#c"
  ]
}