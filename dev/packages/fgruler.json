      }
    },
    {
      "name": "rulerparams",
      "arg": {
        "format": "{}{}{}{}{}",
        "snippet": "{${1:markthick%l}}{${2:numfont}}{${3:color}}{${4:marklength%l}}{${5:numsep%l}}"
