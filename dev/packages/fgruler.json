  "envs": [],
  "keys": {
    "\\usepackage/fgruler#c,\\setfgruler,\\fgruler*": [
      "unit=${1|cm,in|}",
      "type=${1|upperleft,upperright,lowerleft,lowerright,upperleftT,upperrightT,lowerleftT,lowerrightT,alledges,alledges*,alledgesT,alledgesT*,none,user|}",
      "hshift=",
      "vshift=",
      "color=",
      "numsep=",
      "markthick=",
      "numfont=${1:font commands}",
      "showframe",
      "nonefgrulers"
    ],
    "\\fgruler,\\squareruler,\\squareruler*": [
      "upperleft",
      "upperright",
      "lowerleft",
      "lowerright"
    ],
    "\\ruler,\\ruler*": [
      "downright",
      "downleft",
      "upright",
      "upleft",
      "rightdown",
      "rightup",
      "leftdown",
      "leftup",
      "taperight",
      "tapeleft"
    ]
  },
  "args": [
    "\\usepackage/fgruler#c,\\setfgruler,\\fgruler*"
  ]
