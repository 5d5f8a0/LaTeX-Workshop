  "macros": [
    {
      "name": "setfgruler",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/fgruler#c,\\setfgruler,\\fgruler*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "fgruler*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:type}}"
      }
    },
    {
      "name": "fgruler*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:type}}",
        "keys": [
          "\\usepackage/fgruler#c,\\setfgruler,\\fgruler*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "fgruler",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:type%keyvals}}{${2:hshift%l}}{${3:vshift%l}}",
        "keys": [
          "\\fgruler,\\squareruler,\\squareruler*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "fgruler",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:unit}]{${1:type%keyvals}}{${2:hshift%l}}{${3:vshift%l}}",
        "keys": [
          "\\fgruler,\\squareruler,\\squareruler*"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "ruler",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type%keyvals}}{${2:length}}",
        "keys": [
          "\\ruler,\\ruler*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ruler",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:unit}]{${1:type%keyvals}}{${2:length}}",
        "keys": [
          "\\ruler,\\ruler*"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "ruler*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type%keyvals}}{${2:length}}",
        "keys": [
          "\\ruler,\\ruler*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ruler*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:unit}]{${1:type%keyvals}}{${2:length}}",
        "keys": [
          "\\ruler,\\ruler*"
        ],
        "keyPos": 1
      }
    },
    {
