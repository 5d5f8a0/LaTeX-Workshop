{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "pdftexcmds"
    },
    {
      "name": "xpatch"
    },
    {
      "name": "multicol"
    }
  ],
  "macros": [
    {
      "name": "makeindex",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "indexsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\indexsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "splitindexoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:splitindex option}}"
      }
    },
    {
      "name": "index",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:entry}}"
      }
    },
    {
      "name": "indexprologue",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "indexprologue",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:spacing}]{${1:text}}"
      }
    },
    {
      "name": "printindex"
    },
    {
      "name": "printindex",
      "arg": {
        "format": "[name",
        "snippet": "[name"
      }
    },
    {
      "name": "alsoname",
      "unusual": true
    },
    {
      "name": "innotenumber",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "innote",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "seealso",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text}}{${2:page}}"
      }
    },
    {
      "name": "seename",
      "unusual": true
    },
    {
      "name": "see",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text}}{${2:page}}"
      }
    },
    {
      "name": "nindex",
      "if": "innote",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:entry}}"
      }
    },
    {
      "name": "nindex",
      "if": "innote",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:entry}}"
      }
    },
    {
      "name": "nnumberindex",
      "if": "innote",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:entry}}"
      }
    },
    {
      "name": "nnumberindex",
      "if": "innote",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:entry}}"
      }
    },
    {
      "name": "nindex",
      "if": "notenumber",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:entry}}"
      }
    },
    {
      "name": "nindex",
      "if": "notenumber",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:entry}}"
      }
    },
    {
      "name": "nnumberindex",
      "if": "notenumber",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:entry}}"
      }
    },
    {
      "name": "nnumberindex",
      "if": "notenumber",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:entry}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/indextools#c": [
      "makeindex\r",
      "xindy\r",
      "texindy\r",
      "truexindy\r",
      "upmendex\r",
      "xindex\r",
      "noautomatic\r",
      "nonewpage\r",
      "quiet\r",
      "original\r",
      "afterindex\r",
      "innote\r",
      "notenumber\r",
      "splitindex\r"
    ],
    "\\makeindex": [
      "name=${1:name}\r",
      "title=${1:text}\r",
      "program=${1|makeindex,xindy,texindy,truexindy,upmendex,xindex\r|}",
      "options=${1:program options}\r",
      "noautomatic",
      "intoc",
      "columns=${1:integer}\r",
      "columnsep=",
      "columnseprule"
    ],
    "\\indexsetup": [
      "level=${1:section command}\r",
      "toclevel=${1:section csname}\r",
      "noclearpage",
      "firstpagestyle=${1:page style}\r",
      "headers={${1:left mark}}{${2:right mark}}\r",
      "othercode=${1:code}\r"
    ]
  },
  "args": [
    "\\usepackage/indextools#c"
  ]
}