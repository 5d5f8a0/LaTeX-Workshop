  "keys": {
    "\\usepackage/changes#c": [
      "draft",
      "final",
      "commandnameprefix=${1|none,ifneeded,always|}",
      "markup=${1|default,underlined,bfit,nocolor|}",
      "addedmarkup=${1|colored,uline,uuline,uwave,dashuline,dotuline,bf,it,sl,em|}",
      "deletedmarkup=${1|sout,xout,colored,uline,uuline,uwave,dashuline,dotuline,bf,it,sl,em|}",
      "highlightmarkup=${1|background,uuline,uwave|}",
      "commentmarkup=${1|todo,margin,footnote,uwave|}",
      "authormarkup=${1|superscript,subscript,brackets,footnote,none|}",
      "authormarkupposition=${1|right,left|}",
      "authormarkuptext=${1|id,name|}",
      "defaultcolor=",
      "todonotes={${1:todonotes options}}",
      "truncate={${1:truncate options}}",
      "ulem={${1:ulem options}}",
      "xcolor={${1:xcolor options}}"
    ],
    "\\definechangesauthor": [
      "name=${1:name}",
      "color="
    ],
    "\\added,\\deleted,\\replaced,\\highlight": [
      "id=",
      "comment=${1:comment}"
    ],
    "\\comment": [
      "id="
    ],
    "\\listofchanges": [
      "style=${1|list,summary,compactsummary|}",
      "title=${1:title}",
      "show=${1|all,added,deleted,replaced,highlight,comment|}"
    ]
  },
  "args": [
    "\\usepackage/changes#c"
  ]
