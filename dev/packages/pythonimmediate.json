{
  "deps": [
    {
      "name": "saveenv"
    },
    {
      "name": "currfile"
    },
    {
      "name": "currfile-abspath"
    },
    {
      "name": "precattl"
    }
  ],
  "macros": [
    {
      "name": "py",
      "arg": {
        "format": "{}",
        "snippet": "{${1:python expr%definition}}"
      }
    },
    {
      "name": "pyc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:python code%definition}}"
      }
    },
    {
      "name": "pycq",
      "arg": {
        "format": "{}",
        "snippet": "{${1:python code%definition}}"
      }
    },
    {
      "name": "pyfile",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "pythonimmediatecontinue",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    }
  ],
  "envs": [
    {
      "name": "pycode"
    },
    {
      "name": "pycodeq"
    }
  ],
  "keys": {
    "\\usepackage/pythonimmediate#c": [
      "args=${1:Python args}\r",
      "python-executable=${1:path}\r",
      "python-flags=${1:flags}\r",
      "child-process"
    ]
  },
  "args": [
    "\\usepackage/pythonimmediate#c"
  ]
}