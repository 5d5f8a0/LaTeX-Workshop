{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "keyval"
    },
    {
      "name": "verbatim"
    }
  ],
  "macros": [
    {
      "name": "mpostsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/mpostinl#c,\\mpostsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "mpostuse",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "mpostuse",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:label}}"
      }
    },
    {
      "name": "mpostgetname",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "mpostfigurename"
    },
    {
      "name": "mpostplaceholder",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "mpostplaceholder",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:type%keyvals}]{${1:name}}"
      }
    },
    {
      "name": "mpostdisplaylabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "mpostfilename"
    },
    {
      "name": "mpostfile",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "mpostfile",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "mpostdone"
    }
  ],
  "envs": [
    {
      "name": "mpostfig"
    },
    {
      "name": "mpostfig",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "mpostdef"
    },
    {
      "name": "mpostdef",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "mposttex"
    },
    {
      "name": "mposttex",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/mpostinl#c,\\mpostsetup": [
      "draft",
      "final\r",
      "write",
      "compile",
      "twice",
      "clean",
      "checksum",
      "fonts",
      "prologues=${1:value}\r",
      "lineno",
      "labelnames",
      "latex",
      "compiler\r",
      "compiler=${1:compiler}\r",
      "format\r",
      "format=${1:tag}\r",
      "class=${1:class}\r",
      "classopt={${1:class options}}\r",
      "mem=${1:mem}\r",
      "command=${1:command}\r",
      "now",
      "nowall",
      "nowkeep",
      "globaldef",
      "template=${1:template}\r",
      "extension=${1:ext}\r",
      "numberwithin=${1:counter}\r",
      "beamer",
      "warnunused"
    ],
    "\\begin{mpostfig}": [
      "show",
      "file=${1:file name}\r",
      "label=${1:label}\r",
      "opt={${1:includegraphics options}}\r",
      "now",
      "twice"
    ],
    "\\mpostuse": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ],
    "\\begin{mpostdef}": [
      "global",
      "tex"
    ],
    "\\begin{mposttex}": [
      "global",
      "dual"
    ],
    "\\mpostplaceholder": [
      "file\r",
      "label\r"
    ],
    "\\mpostfile": [
      "include"
    ]
  },
  "args": [
    "\\usepackage/mpostinl#c,\\mpostsetup"
  ]
}