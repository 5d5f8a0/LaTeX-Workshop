{
  "deps": [
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "doxitem",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:name}}{${2:envname}}{${3:index category}}"
      }
    },
    {
      "name": "doxitem",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:name}}{${2:envname}}{${3:index category}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/dox#c": [
      "noprint",
      "noindex",
      "macrolike",
      "idxtype=\r"
    ],
    "\\doxitem": [
      "idxtype=\r",
      "macrolike\r"
    ]
  },
  "args": [
    "\\usepackage/dox#c"
  ]
}