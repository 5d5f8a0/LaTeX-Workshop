{
  "deps": [
    {
      "name": "ifxetex"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "xkeyval"
    },
    {
      "name": "fontenc"
    },
    {
      "name": "fontaxes"
    },
    {
      "name": "mweights"
    }
  ],
  "macros": [
    {
      "name": "clear"
    },
    {
      "name": "clearlight"
    },
    {
      "name": "clearthin"
    },
    {
      "name": "textsfl",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "textsft",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "clearfamily",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/ClearSans#c": [
      "scale=${1:factor}\r",
      "scaled=${1:factor}\r",
      "sfdefault\r",
      "type1\r",
      "medium\r",
      "regular\r"
    ]
  },
  "args": [
    "\\usepackage/ClearSans#c"
  ]
}