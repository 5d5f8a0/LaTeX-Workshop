{
  "deps": [
    {
      "name": "pgfkeys"
    }
  ],
  "macros": [
    {
      "name": "interval",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:start}}{${2:end}}"
      }
    },
    {
      "name": "interval",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options}]{${1:start}}{${2:end}}"
      }
    },
    {
      "name": "ointerval",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options}]{${1:start}}{${2:end}}"
      }
    },
    {
      "name": "linterval",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options}]{${1:start}}{${2:end}}"
      }
    },
    {
      "name": "rinterval",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options}]{${1:start}}{${2:end}}"
      }
    },
    {
      "name": "intervalconfig",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\interval": [
      "open",
      "open left",
      "open right"
    ],
    "\\interval,\\ointerval,\\linterval,\\rinterval": [
      "scaled",
      "scaled=${1:scale command}"
    ],
    "\\intervalconfig": [
      "separator symbol=${1:symbol}",
      "left open fence=${1:symbol}",
      "left closed fence=${1:symbol}",
      "right open fence=${1:symbol}",
      "right closed fence=${1:symbol}",
      "soft open fences",
      "colorize=${1:color commands}"
    ]
  },
  "args": []
}