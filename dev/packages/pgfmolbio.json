{
  "deps": [
    {
      "name": "ifluatex"
    },
    {
      "name": "luatexbase"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarypositioning"
    },
    {
      "name": "tikzlibrarysvg.path"
    }
  ],
  "macros": [
    {
      "name": "pgfmolbioset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "pgfmolbioset",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:module}]{${1:options%keyvals}}"
      }
    },
    {
      "name": "pmbprotocolsizes",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "pmbchromatogram",
      "if": "chromatogram",
      "arg": {
        "format": "{}",
        "snippet": "{${1:scf file%file}}"
      }
    },
    {
      "name": "pmbchromatogram",
      "if": "chromatogram",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:scf file%file}}"
      }
    },
    {
      "name": "addfeature",
      "if": "domains",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:type}}{${2:start}}{${3:stop}}"
      }
    },
    {
      "name": "addfeature",
      "if": "domains",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:type}}{${2:start}}{${3:stop}}"
      }
    },
    {
      "name": "setfeaturestyle",
      "if": "domains",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type}}{${2:style list}}"
      }
    },
    {
      "name": "setfeaturestylealias",
      "if": "domains",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:new type}}{${2:existing type}}"
      }
    },
    {
      "name": "setfeatureshape",
      "if": "domains",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type}}{${2:TikZ code}}"
      }
    },
    {
      "name": "xLeft",
      "if": "domains"
    },
    {
      "name": "xMid",
      "if": "domains"
    },
    {
      "name": "xRight",
      "if": "domains"
    },
    {
      "name": "yMid",
      "if": "domains"
    },
    {
      "name": "pmbdomvalueof",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key%plain}}"
      }
    },
    {
      "name": "featureSequence",
      "if": "domains"
    },
    {
      "name": "residueNumber",
      "if": "domains"
    },
    {
      "name": "currentResidue",
      "if": "domains"
    },
    {
      "name": "setfeatureshapealias",
      "if": "domains",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:new type}}{${2:existing type}}"
      }
    },
    {
      "name": "setfeaturealias",
      "if": "domains",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:new type}}{${2:existing type}}"
      }
    },
    {
      "name": "setdisulfidefeatures",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key list}}"
      }
    },
    {
      "name": "adddisulfidefeatures",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key list}}"
      }
    },
    {
      "name": "removedisulfidefeatures",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key list}}"
      }
    },
    {
      "name": "setfeatureprintfunction",
      "if": "domains",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:key list}}{${2:Lua function}}"
      }
    },
    {
      "name": "removefeatureprintfunction",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key list}}"
      }
    },
    {
      "name": "pmbdomdrawfeature",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:type}}"
      }
    },
    {
      "name": "inputuniprot",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:Uniprot file%file}}"
      }
    },
    {
      "name": "inputgff",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:gff file%file}}"
      }
    }
  ],
  "envs": [
    {
      "name": "pmbdomains",
      "if": "domains",
      "arg": {
        "format": "{}",
        "snippet": "{${1:sequence length}}"
      }
    },
    {
      "name": "pmbdomains",
      "if": "domains",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:sequence length}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/pgfmolbio#c": [
      "chromatogram\r",
      "domains\r",
      "convert\r"
    ],
    "\\pgfmolbioset#c": [
      "output file name=${1:text}\r",
      "output file extension=${1:text}\r",
      "output code=${1|pgfmolbio,tikz\r|}",
      "include description"
    ],
    "\\pmbchromatogram#c,\\pgfmolbioset#c": [
      "sample range=${1:lower}-${2:upper}\r",
      "sample range=${1:lower}-${2:upper} step ${3:int}\r",
      "x unit=",
      "y unit=",
      "samples per line=${1:number}\r",
      "baseline skip=",
      "canvas style/.style={${1:TikZ options}}\r",
      "canvas height=",
      "trace A style/.style={${1:TikZ options}}\r",
      "trace C style/.style={${1:TikZ options}}\r",
      "trace G style/.style={${1:TikZ options}}\r",
      "trace T style/.style={${1:TikZ options}}\r",
      "trace style={${1:TikZ options}}\r",
      "traces drawn=\r",
      "tick A style/.style={${1:TikZ options}}\r",
      "tick C style/.style={${1:TikZ options}}\r",
      "tick G style/.style={${1:TikZ options}}\r",
      "tick T style/.style={${1:TikZ options}}\r",
      "tick style={${1:TikZ options}}\r",
      "tick length=",
      "ticks drawn=\r",
      "base label A text=${1:text}\r",
      "base label C text=${1:text}\r",
      "base label G text=${1:text}\r",
      "base label T text=${1:text}\r",
      "base label A style/.style={${1:TikZ options}}\r",
      "base label C style/.style={${1:TikZ options}}\r",
      "base label G style/.style={${1:TikZ options}}\r",
      "base label T style/.style={${1:TikZ options}}\r",
      "base label style={${1:TikZ options}}\r",
      "base labels drawn=\r",
      "show base numbers",
      "base number style/.style={${1:TikZ options}}\r",
      "base number range=${1:lower}-${2:upper}\r",
      "base number range=${1:lower}-${2:upper} step ${3:int}\r",
      "probability distance=",
      "probabilities drawn=\r",
      "probability style function=${1:Lua function}\r",
      "bases drawn=\r"
    ],
    "\\begin{pmbdomains}#c,\\addfeature#c,\\pgfmolbioset#c": [
      "name=${1:text}\r",
      "show name",
      "description=${1:text}\r",
      "x unit=",
      "y unit=",
      "residues per line=${1:number}\r",
      "baseline skip=${1:factor}\r",
      "residue numbering={${1:numbering scheme}}\r",
      "residue range=${1:lower}-${2:upper}\r",
      "enlarge left=",
      "enlarge right=",
      "enlarge top=",
      "enlarge bottom=",
      "style={${1:style list}}\r",
      "domain font=${1:font commands}\r",
      "level=${1:number}\r",
      "disulfide base distance=${1:number}\r",
      "disulfide level distance=${1:number}\r",
      "range font=${1:font commands}\r",
      "show ruler",
      "ruler range={${1:ruler range list}}\r",
      "default ruler step size=${1:number}\r",
      "ruler distance=${1:factor}\r",
      "sequence=${1:sequence}\r",
      "magnified sequence font=${1:font commands}\r",
      "show secondary structure",
      "secondary structure distance=${1:factor}\r",
      "sequence length=${1:number}\r"
    ]
  },
  "args": [
    "\\usepackage/pgfmolbio#c"
  ]
}