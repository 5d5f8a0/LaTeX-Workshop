      "group incomplete/.style={${1:TikZ options}}",
      "milestone incomplete/.style={${1:TikZ options}}",
      "progress label text=${1:text}",
      "bar progress label anchor=${1:anchor}",
      "bar progress label font=${1:font commands}",
      "bar progress label node/.style={${1:TikZ options}}",
      "group progress label anchor=${1:anchor}",
      "group progress label font=${1:font commands}",
      "group progress label node/.style={${1:TikZ options}}",
      "milestone progress label anchor=${1:anchor}",
      "milestone progress label font=${1:font commands}",
      "milestone progress label node/.style={${1:TikZ options}}",
      "on top fraction=${1:fraction}",
      "on bottom fraction=${1:fraction}",
      "on left fraction=${1:fraction}",
      "on right fraction=${1:fraction}",
      "name=${1:name}",
      "link/.style={${1:TikZ options}}",
