      "title left shift=${1:factor}",
      "title right shift=${1:factor}",
      "title top shift=${1:factor}",
      "title height=${1:factor}",
      "include title in canvas",
      "vrule offset=${1:number}",
      "vrule/.style={${1:TikZ options}}",
      "vrule label font=${1:font commands}",
      "vrule label node/.style={${1:TikZ options}}",
      "chart element start border=${1|left,right|}",
      "bar/.style={${1:TikZ options}}",
      "group/.style={${1:TikZ options}}",
      "milestone/.style={${1:TikZ options}}",
      "bar label text=${1:text}",
      "group label text=${1:text}",
      "milestone label text=${1:text}",
      "bar label font=${1:font commands}",
      "group label font=${1:font commands}",
      "milestone label font=${1:font commands}",
      "bar label node/.style={${1:TikZ options}}",
      "group label node/.style={${1:TikZ options}}",
      "milestone label node/.style={${1:TikZ options}}",
      "inline",
      "bar inline label anchor=${1:anchor}",
      "group inline label anchor=${1:anchor}",
      "milestone inline label anchor=${1:anchor}",
      "bar inline label node/.style={${1:TikZ options}}",
      "group inline label node/.style={${1:TikZ options}}",
      "milestone inline label node/.style={${1:TikZ options}}",
      "bar left shift=${1:factor}",
      "bar right shift=${1:factor}",
      "bar top shift=${1:factor}",
      "bar height=${1:factor}",
      "group left shift=${1:factor}",
      "group right shift=${1:factor}",
      "group top shift=${1:factor}",
      "group height=${1:factor}",
      "milestone left shift=${1:factor}",
      "milestone right shift=${1:factor}",
      "milestone top shift=${1:factor}",
      "milestone height=${1:factor}",
      "group right peak tip position=${1:fraction}",
      "group right peak width=${1:factor}",
      "group right peak height=${1:factor}",
      "group left peak tip position=${1:fraction}",
      "group left peak width=${1:factor}",
      "group left peak height=${1:factor}",
      "group peaks tip position=${1:fraction}",
      "group peaks width=${1:factor}",
      "group peaks height=${1:factor}",
      "progress=${2|none,today,${1:number}|}",
