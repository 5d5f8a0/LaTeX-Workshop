{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "thmboxoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "newtheorem",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:envname}}{${2:caption}}"
      }
    },
    {
      "name": "newtheorem",
      "arg": {
        "format": "[]{}{}[]",
        "snippet": "[${3:options%keyvals}]{${1:envname}}{${2:caption}}[${4:within}]"
      }
    },
    {
      "name": "examplename",
      "unusual": true
    },
    {
      "name": "proofname",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "thmbox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "thmbox",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:title%text}}"
      }
    },
    {
      "name": "leftbar"
    },
    {
      "name": "leftbar",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "proof"
    },
    {
      "name": "proof",
      "arg": {
        "format": "[]",
        "snippet": "[${1:postfix}]"
      }
    },
    {
      "name": "example"
    },
    {
      "name": "example",
      "arg": {
        "format": "[]",
        "snippet": "[${1:label text}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/thmbox#c": [
      "nothm\r",
      "nocut\r",
      "nounderline\r"
    ],
    "\\thmboxoptions#c,\\begin{thmbox}#c,\\begin{leftbar}#c,\\newtheorem#c": [
      "style=${1|S,M,L\r|}",
      "S\r",
      "M\r",
      "L\r",
      "cut",
      "underline",
      "headstyle=${1:code}\r",
      "titlestyle=${1:code}\r",
      "bodystyle=${1:code}\r",
      "leftmargin=",
      "rightmargin=",
      "hskip=",
      "vskip=",
      "thickness="
    ]
  },
  "args": [
    "\\usepackage/thmbox#c"
  ]
}