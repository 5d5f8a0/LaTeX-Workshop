{
  "deps": [
    {
      "name": "testidx"
    },
    {
      "name": "glossaries"
    },
    {
      "name": "glossaries-mcols"
    },
    {
      "name": "glossaries-extra",
      "if": "extra"
    },
    {
      "name": "glossaries-extra",
      "if": "bib2gls"
    },
    {
      "name": "glossaries-extra-bib2gls",
      "if": "bib2gls"
    }
  ],
  "macros": [
    {
      "name": "GlsSetXdyLanguage",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:language}}"
      }
    },
    {
      "name": "GlsSetXdyLanguage",
      "if": "xindy",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:glossary type}]{${1:language}}"
      }
    },
    {
      "name": "GlsSetXdyCodePage",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "GlsAddXdyCounters",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:counter list}}"
      }
    },
    {
      "name": "GlsAddXdyAttribute",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "GlsAddXdyLocation",
      "if": "xindy",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "GlsAddXdyLocation",
      "if": "xindy",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:prefix-location}]{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "GlsSetXdyLocationClassOrder",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:location names}}"
      }
    },
    {
      "name": "GlsSetXdyMinRangeLength",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:integer}}"
      }
    },
    {
      "name": "GlsSetXdyFirstLetterAfterDigits",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:letter}}"
      }
    },
    {
      "name": "GlsSetXdyNumberGroupOrder",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:relative location}}"
      }
    },
    {
      "name": "GlsAddLetterGroup",
      "if": "xindy",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "GlsAddSortRule",
      "if": "xindy",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "GlsAddXdyAlphabet",
      "if": "xindy",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "GlsAddXdyStyle",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:xindy style}}"
      }
    },
    {
      "name": "GlsSetXdyStyles",
      "if": "xindy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:xindy styles}}"
      }
    },
    {
      "name": "tstidxloadsamples"
    },
    {
      "name": "tstidxmakegloss"
    },
    {
      "name": "tstidxmakegloss",
      "unusual": true,
      "arg": {
        "format": "[]",
        "snippet": "[${1:options}]"
      }
    },
    {
      "name": "tstidxprintglossaries"
    },
    {
      "name": "tstidxprintglossary"
    },
    {
      "name": "tstidxprintglossary",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/testidx-glossaries#c": [
      "extra\r",
      "noextra\r",
      "makeindex\r",
      "xindy\r",
      "tex\r",
      "bib2gls\r",
      "manual\r",
      "seekey\r",
      "noseekey\r",
      "noglsnumbers\r",
      "glsnumbers\r",
      "desc\r",
      "nodesc\r",
      "ascii\r",
      "noascii\r",
      "german\r",
      "ngerman\r",
      "nogerman\r",
      "stripaccents\r",
      "nostripaccents\r",
      "sanitize\r",
      "nosanitize\r",
      "showmarks\r",
      "hidemarks\r",
      "noshowmarks\r",
      "verbose\r",
      "noverbose\r",
      "notestencaps\r",
      "testencaps\r",
      "prefix\r",
      "noprefix\r",
      "diglyphs\r",
      "nodiglyphs\r"
    ],
    "\\tstidxprintglossary#c": [
      "type=\r",
      "title=\r",
      "toctitle=\r",
      "style=\r",
      "numberedsection=${1|false,nolabel,autolabel\r|}",
      "nonumberlist",
      "nogroupskip",
      "nopostdot",
      "entrycounter",
      "subentrycounter",
      "sort=${1|word,letter,standard,use,def,nocase,case\r|}"
    ]
  },
  "args": [
    "\\usepackage/testidx-glossaries#c"
  ]
}