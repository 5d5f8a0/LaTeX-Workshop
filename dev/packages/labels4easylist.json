{
  "deps": [
    {
      "name": "easylist"
    }
  ],
  "macros": [
    {
      "name": "itemLabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "itemLabel",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:label}}[${2:skipA}]"
      }
    },
    {
      "name": "itemLabel",
      "arg": {
        "format": "{}[][]",
        "snippet": "{${1:label}}[${2:skipA}][${3:skipB}]"
      }
    }
  ],
  "envs": [
    {
      "name": "easylist",
      "if": "ampersand"
    },
    {
      "name": "easylist",
      "if": "ampersand",
      "arg": {
        "format": "[]",
        "snippet": "[${1:style%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/labels4easylist#c": [
      "pilcrow\r",
      "at\r",
      "sharp\r",
      "ampersand\r",
      "${1:number of counters}\r"
    ]
  },
  "args": [
    "\\usepackage/labels4easylist#c"
  ]
}