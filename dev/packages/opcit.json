{
  "deps": [
    {
      "name": "xspace"
    },
    {
      "name": "hyperref",
      "if": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "bibcase",
      "arg": {
        "format": "{}",
        "snippet": "{${1:word}}"
      }
    },
    {
      "name": "bibhereafter",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style}}"
      }
    },
    {
      "name": "biblastnames",
      "arg": {
        "format": "{}",
        "snippet": "{${1:last names}}"
      }
    },
    {
      "name": "bibpunctuation",
      "unusual": true
    },
    {
      "name": "bibref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bibid}}"
      }
    },
    {
      "name": "cite*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bibid}}"
      }
    },
    {
      "name": "cited",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keylist}}"
      }
    },
    {
      "name": "hereafter"
    },
    {
      "name": "idemtext",
      "unusual": true
    },
    {
      "name": "newBibCommand",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:command}}{${2:footnote style}}{${3:list style}}"
      }
    },
    {
      "name": "nobibliography",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bib file}}"
      }
    },
    {
      "name": "opcitends"
    },
    {
      "name": "opcitstart"
    },
    {
      "name": "opcittext",
      "unusual": true
    },
    {
      "name": "opcitwarning",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:warning%text}}"
      }
    },
    {
      "name": "QuoteOrNot",
      "unusual": true
    },
    {
      "name": "resetcites"
    },
    {
      "name": "sameauthors"
    },
    {
      "name": "sameauthorsrule",
      "unusual": true
    },
    {
      "name": "toomit",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "with",
      "arg": {
        "format": "{}",
        "snippet": "{${1:field}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/opcit#c": [
      "custombst\r",
      "nocustombst\r",
      "hyperref\r",
      "nohyperref\r",
      "omitting\r",
      "noomitting\r",
      "idem\r",
      "noidem\r",
      "checkidem\r",
      "nocheckidem\r"
    ]
  },
  "args": [
    "\\usepackage/opcit#c"
  ]
}