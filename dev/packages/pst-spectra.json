{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "multido"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psspectrum"
    },
    {
      "name": "psspectrum",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psspectrum": [
      "begin=${1:wavelength in nm}\r",
      "end=${1:wavelength in nm}\r",
      "gamma=${1:factor}\r",
      "brightness=${1:factor}\r",
      "numlines=${1:integer}\r",
      "lines=${1:wavelengths in nm}\r",
      "element=${1:text}\r",
      "emission",
      "absorption",
      "lwidth=${1:width}\r",
      "lmin=${1:factor}\r",
      "axe",
      "DI=${1:length in nm}\r",
      "axecolor=",
      "axewidth=${1:width}\r",
      "wlangle=${1:degrees}\r",
      "wlcmd=${1:font commands}\r"
    ]
  },
  "args": []
}