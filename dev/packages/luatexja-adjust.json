{
  "deps": [
    {
      "name": "luatexja"
    }
  ],
  "macros": [
    {
      "name": "ltjenableadjust"
    },
    {
      "name": "ltjenableadjust",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "ltjdisableadjust"
    },
    {
      "name": "ltjghostbeforejachar"
    },
    {
      "name": "ltjghostafterjachar"
    }
  ],
  "envs": [],
  "keys": {
    "\\ltjenableadjust": [
      "lineend=${1|false,true,extended\r|}",
      "priority",
      "profile",
      "linestep"
    ],
    "\\ltjsetparameter,\\ltjglobalsetparameter": [
      "stretch_priority={${1:keyval list}}\r",
      "shrink_priority={${1:keyval list}}\r",
      "linestep_factor=${1:real}\r",
      "profile_hgap_factor=${1:real}\r"
    ],
    "\\ltjgetparameter": [
      "stretch_priority\r",
      "shrink_priority\r",
      "linestep_factor\r",
      "profile_hgap_factor\r"
    ]
  },
  "args": []
}