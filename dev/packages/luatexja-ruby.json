{
  "deps": [
    {
      "name": "luatexja"
    }
  ],
  "macros": [
    {
      "name": "ltjruby",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:親|${3:文}|${4:字}}{${2:おや}|${5:も}|じ}}"
      }
    },
    {
      "name": "ltjruby",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:親|${3:文}|字}}"
      }
    },
    {
      "name": "ruby",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:親|${3:文}|${4:字}}{${2:おや}|${5:も}|じ}}"
      }
    },
    {
      "name": "ruby",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:親|${3:文}|字}}"
      }
    },
    {
      "name": "ltjsetruby",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\ltjruby,\\ruby,\\ltjsetruby,\\ltjkenten,\\kenten"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ltjkenten"
    },
    {
      "name": "ltjkenten",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "kenten"
    },
    {
      "name": "kenten",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\ltjruby,\\ruby,\\ltjsetruby,\\ltjkenten,\\kenten": [
      "pre=${1:real}\r",
      "post=${1:real}\r",
      "mode=\r",
      "intrude_jfmgk",
      "intrude_kanjiskip",
      "intrude_xkanjiskip",
      "stretchruby={${1:left}}{${2:middle}}{${3:right}}\r",
      "stretch={${1:left}}{${2:middle}}{${3:right}}\r",
      "stretchbol={${1:left}}{${2:middle}}{${3:right}}\r",
      "stretcheol={${1:left}}{${2:middle}}{${3:right}}\r",
      "maxmargin=${1:real}\r",
      "size=${1:real}\r",
      "intergap=${1:real}\r",
      "rubysmash",
      "ybaseheight=${1:real}\r",
      "tbaseheight=${1:real}\r",
      "baseheight=${1:real}\r",
      "yrubydepth=${1:real}\r",
      "trubydepth=${1:real}\r",
      "rubydepth=${1:real}\r",
      "kenten=${1:command}\r",
      "fontcmd=${1:command}\r",
      "naka\r",
      "kata\r"
    ],
    "\\ltjsetparameter,\\ltjglobalsetparameter": [
      "rubypreintrusion={${1:chr_code,pre_int}}\r",
      "rubypostintrusion={${1:chr_code,post_int}}\r"
    ],
    "\\ltjgetparameter": [
      "rubypreintrusion\r",
      "rubypostintrusion\r"
    ]
  },
  "args": []
}