{
  "deps": [
    {
      "name": "amstext"
    }
  ],
  "macros": [
    {
      "name": "textualicommafont",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:font family}}"
      }
    },
    {
      "name": "mathcomma",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/textualicomma#c": [
      "sffamily\r",
      "ttfamily\r"
    ]
  },
  "args": [
    "\\usepackage/textualicomma#c"
  ]
}