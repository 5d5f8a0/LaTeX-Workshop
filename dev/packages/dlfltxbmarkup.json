{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "ragged2e"
    },
    {
      "name": "dlfltxbmarkupbookkeys",
      "if": "loadsampleconfig"
    }
  ],
  "macros": [
    {
      "name": "markup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "markup",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}"
      }
    },
    {
      "name": "felineKeyGenerator",
      "arg": {
        "format": "{}{}{}{}{}",
        "snippet": "{${1:key%specialDef}}{${2:description}}{${3:margin code}}{${4:index code}}{${5:text code}}"
      }
    },
    {
      "name": "cs",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    },
    {
      "name": "css",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    },
    {
      "name": "felineWriteInMargin",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "ifNoMarginparAvail",
      "unusual": true
    },
    {
      "name": "NoMarginparAvailfalse"
    },
    {
      "name": "NoMarginparAvailtrue"
    },
    {
      "name": "felineMarginAdjustment",
      "unusual": true
    },
    {
      "name": "felineIndexCmd",
      "unusual": true
    },
    {
      "name": "itindex",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "felineStandardKey",
      "unusual": true
    },
    {
      "name": "felineMarkupDescription",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/dlfltxbmarkup#c": [
      "loadsampleconfig\r"
    ],
    "\\markup#c": [
      "notxt\r",
      "nowr\r",
      "nomk\r",
      "noidx\r",
      "idxit\r",
      "idxbf\r",
      "idxn\r",
      "addtospvrt=${1:number}\r",
      "vaddtosp=${1:number}\r",
      "forcesidepar\r",
      "%felineKey\r"
    ]
  },
  "args": [
    "\\usepackage/dlfltxbmarkup#c"
  ]
}