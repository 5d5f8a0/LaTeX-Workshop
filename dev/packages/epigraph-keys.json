{
  "deps": [
    {
      "name": "enumitem"
    },
    {
      "name": "pgfkeys"
    },
    {
      "name": "conditionals"
    },
    {
      "name": "microtype"
    }
  ],
  "macros": [
    {
      "name": "epigraph",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "epigraph",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:keyvals}]{${1:text}}"
      }
    },
    {
      "name": "qitem",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "qitem",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:keyvals}]{${1:text}}"
      }
    },
    {
      "name": "beforeepigraphskip",
      "unusual": true
    },
    {
      "name": "afterepigraphskip",
      "unusual": true
    },
    {
      "name": "epigraphtextindent",
      "unusual": true
    },
    {
      "name": "epigraphauthorsourceindent",
      "unusual": true
    },
    {
      "name": "epigraphtextwidth",
      "unusual": true
    },
    {
      "name": "epigraphstyle",
      "unusual": true
    },
    {
      "name": "epigraphdash",
      "unusual": true
    },
    {
      "name": "epigraphquotefont",
      "unusual": true
    },
    {
      "name": "epigraphtranslationfont",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "epigraphs"
    }
  ],
  "keys": {
    "\\epigraph,\\qitem": [
      "author={${1:author}}\r",
      "source={${1:source}}\r",
      "translation={${1:translation text}}\r",
      "etc={${1:text}}\r",
      "after skip=",
      "before skip=",
      "author and source indent=",
      "text indent=",
      "width=",
      "style=${1:font commands}\r",
      "quote style=${1:font commands}\r",
      "translation style=${1:font commands}\r",
      "dash={${1:dash code}}\r"
    ]
  },
  "args": []
}