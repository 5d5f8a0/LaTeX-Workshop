{
  "deps": [
    {
      "name": "sdapsbase"
    },
    {
      "name": "sdapsarray"
    }
  ],
  "macros": [
    {
      "name": "choice",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "choice",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}"
      }
    },
    {
      "name": "question",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      },
      "detail": "?",
      "doc": "Question mark"
    },
    {
      "name": "question",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}"
      },
      "detail": "?",
      "doc": "Question mark"
    },
    {
      "name": "range",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:question%text}}{${2:lower%text}}{${3:upper%text}}{${4:other%text}}"
      }
    },
    {
      "name": "range",
      "arg": {
        "format": "[]{}{}{}{}",
        "snippet": "[${5:options%keyvals}]{${1:question%text}}{${2:lower%text}}{${3:upper%text}}{${4:other%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "choicearray"
    },
    {
      "name": "choicearray",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "optionarray"
    },
    {
      "name": "optionarray",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "rangearray"
    },
    {
      "name": "rangearray",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{choicearray},\\begin{optionarray}": [
      "align=\r",
      "noalign",
      "horizontal",
      "layouter=${1|default,rotated\r|}",
      "angle=${1:angle}\r",
      "vertical",
      "type=${1|multichoice,singlechoice\r|}",
      "multichoice\r",
      "singlechoice\r"
    ],
    "\\begin{choicearray}": [
      "type=${1|multichoice,singlechoice\r|}",
      "multichoice\r",
      "singlechoice\r"
    ],
    "\\choice": [
      "text=${1:text}\r",
      "var=${1:variable}\r",
      "val=${1:value}\r"
    ],
    "\\question": [
      "text=${1:text}\r",
      "var=${1:variable}\r",
      "range=${1:list}\r"
    ],
    "\\begin{rangearray}": [
      "count=${1:integer}\r",
      "align=${1:string}\r",
      "other\r"
    ],
    "\\range": [
      "text=${1:text}\r",
      "var=${1:variable}\r",
      "upper=${1:text}\r",
      "lower=${1:text}\r",
      "other\r"
    ]
  },
  "args": []
}