      "problem/bottom-sep=",
      "problem/index=${1:integer}\r",
      "problem/label=${1:label}\r",
      "problem/points-prelabel=${1:points 前面的内容}\r",
      "problem/points-postlabel=${1:points 后面的内容}\r",
      "problem/label-align=${1|left,center,right\r|}",
