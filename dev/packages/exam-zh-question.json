      "question/hang",
      "question/points-prelabel=${1:points 前面的内容}\r",
      "question/points-postlabel=${1:points 后面的内容}\r",
      "question/label-align=${1|left,center,right\r|}",
      "problem={${1:键值列表}}\r",
      "problem/show-answer",
      "problem/points=${1:number}\r",
      "problem/show-points=${1|true,auto,false\r|}",
      "problem/points-separate-par",
