{
  "deps": [
    {
      "name": "amsthm"
    },
    {
      "name": "tcolorbox"
    },
    {
      "name": "tcolorboxlibrarymost"
    },
    {
      "name": "xeCJKfntef"
    },
    {
      "name": "enumitem"
    },
    {
      "name": "linegoal"
    },
    {
      "name": "tikzlibraryshapes.misc"
    }
  ],
  "macros": [
    {
      "name": "questionsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:键值列表%keyvals}}",
        "keys": [
          "\\begin{question},\\begin{problem},\\questionsetup",
          "\\begin{question},\\questionsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "paren"
    },
    {
      "name": "paren",
      "arg": {
        "format": "[]",
        "snippet": "[${1:答案}]"
      }
    },
    {
      "name": "AddQuestionCounter",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:LaTeX command%cmd}}{${2:internal command%definition}}"
      }
    },
    {
      "name": "circlednumber",
      "arg": {
        "format": "{}",
        "snippet": "{${1:数字或计数器名字}}"
      }
    },
    {
      "name": "circlednumber*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:数字或计数器名字}}"
      }
    },
    {
      "name": "fillin"
    },
    {
      "name": "fillin",
      "arg": {
        "format": "[]",
        "snippet": "[${1:答案}]"
      }
    },
    {
      "name": "fillin",
      "arg": {
        "format": "[][]",
        "snippet": "[${1:键值列表%keyvals}][${2:答案}]"
      }
    },
    {
      "name": "fillin*"
    },
    {
      "name": "fillin*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:答案}]"
      }
    },
    {
      "name": "fillin*",
      "arg": {
        "format": "[][]",
        "snippet": "[${1:键值列表%keyvals}][${2:答案}]"
      }
    },
    {
      "name": "fillinsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:键值列表%keyvals}}",
        "keys": [
          "\\fillin#c,\\fillin*#c,\\fillinsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "AddFillinCounter",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:LaTeX command%cmd}}{${2:internal command%definition}}"
      }
    },
    {
      "name": "score",
      "arg": {
        "format": "{}",
        "snippet": "{${1:分数}}"
      }
    }
  ],
  "envs": [
    {
      "name": "question"
    },
    {
      "name": "question",
      "arg": {
        "format": "[]",
        "snippet": "[${1:键值列表%keyvals}]"
      }
    },
    {
      "name": "problem"
    },
    {
      "name": "problem",
      "arg": {
        "format": "[]",
        "snippet": "[${1:键值列表%keyvals}]"
      }
    },
    {
      "name": "solution"
    },
    {
      "name": "solution",
      "arg": {
        "format": "[]",
        "snippet": "[${1:键值列表%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\examsetup,\\ExamPrintAnswerSet": [
