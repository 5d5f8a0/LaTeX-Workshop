      "fillin/no-answer-type=${1|blacktriangle,counter,none\r|}",
      "fillin/no-answer-counter-index=${1:integer}\r",
      "fillin/no-answer-counter-label=${1:label}\r",
      "fillin/show-answer",
      "fillin/width=",
      "fillin/width-type=${1|fill,normal\r|}",
      "fillin/box-color=",
      "fillin/text-color=",
      "fillin/paren-type=${1|banjiao,quanjiao\r|}",
      "fillin/depth=",
      "solution={${1:键值列表}}\r",
      "solution/show-solution=${1|hide,show-stay,show-move\r|}",
      "solution/parbreak",
      "solution/show-qed",
      "solution/qedsymbol=${1:symbol}\r",
      "solution/label-content=${1:content}\r",
      "solution/label-punct=${1:punct}\r",
      "solution/score-pre-content=${1:content}\r",
      "solution/score-post-content=${1:content}\r",
      "solution/score-format=${1:风格设置}\r",
      "solution/score-showleader",
      "solution/text-color=",
      "solution/top-sep=",
      "solution/bottom-sep=",
      "solution/blank-type=${1|none,manual,hide\r|}",
      "solution/blank-vsep="
    ],
    "\\begin{question},\\begin{problem},\\questionsetup": [
      "show-answer",
      "points=${1:number}\r",
      "show-points=${1|true,auto,false\r|}",
      "points-separate-par",
      "top-sep=",
      "bottom-sep=",
      "index=${1:integer}\r",
      "label=${1:label}\r",
      "points-prelabel=${1:points 前面的内容}\r",
      "points-postlabel=${1:points 后面的内容}\r",
      "label-align=${1|left,center,right\r|}"
    ],
    "\\begin{question},\\questionsetup": [
      "combine-fillin",
      "combine-fillin-args=${1:\\fillin 命令的参数}\r",
      "hang"
    ],
    "\\fillin#c,\\fillin*#c,\\fillinsetup": [
      "type=${1|line,paren,circle,rectangle,blank\r|}",
      "no-answer-type=${1|blacktriangle,counter,none\r|}",
      "no-answer-counter-index=${1:integer}\r",
      "no-answer-counter-label=${1:label}\r",
      "show-answer",
      "width=",
      "width-type=${1|fill,normal\r|}",
      "box-color=",
      "text-color=",
      "paren-type=${1|banjiao,quanjiao\r|}",
      "depth="
    ],
    "\\begin{solution}": [
      "show-solution=${1|hide,show-stay,show-move\r|}",
      "parbreak",
      "show-qed",
      "qedsymbol=${1:symbol}\r",
      "label-content=${1:content}\r",
      "label-punct=${1:punct}\r",
      "score-pre-content=${1:content}\r",
      "score-post-content=${1:content}\r",
      "score-format=${1:风格设置}\r",
      "score-showleader",
      "text-color=",
      "top-sep=",
      "bottom-sep=",
      "blank-type=${1|none,manual,hide\r|}",
      "blank-vsep="
