{
  "deps": [
    {
      "name": "catchfile"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "ninecolors"
    },
    {
      "name": "varwidth"
    },
    {
      "name": "iftex"
    }
  ],
  "macros": [
    {
      "name": "CodeHigh",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\begin{codehigh},\\begin{demohigh},\\CodeHigh,\\dochighinput"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "dochighinput",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "dochighinput",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "fakeverb",
      "arg": {
        "format": "{}",
        "snippet": "{${1:verbatim content%definition}}"
      }
    },
    {
      "name": "NewCodeHighEnv",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "NewCodeHighInput",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:command}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "AddCodeHighRule",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:type}}{${2:name}}{${3:regex%definition}}"
      }
    },
    {
      "name": "AddCodeHighRule",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:language}]{${1:type}}{${2:name}}{${3:regex%definition}}"
      }
    },
    {
      "name": "SetCodeHighStyle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type}}{${2:color}}"
      }
    },
    {
      "name": "SetCodeHighStyle",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:theme}]{${1:type}}{${2:color}}"
      }
    },
    {
      "name": "GetCodeHighStyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:type}}"
      }
    },
    {
      "name": "GetCodeHighStyle",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:theme}]{${1:type}}"
      }
    },
    {
      "name": "SetCodeHighTracing",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\SetCodeHighTracing"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [
    {
      "name": "codehigh"
    },
    {
      "name": "codehigh",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "demohigh"
    },
    {
      "name": "demohigh",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{codehigh},\\begin{demohigh},\\CodeHigh,\\dochighinput": [
      "language=${1|latex,latex/latex2,latex/latex3,latex/math,latex/table\r|}",
      "style/main=",
      "style/code=",
      "style/demo=",
      "boxsep=",
      "lite",
      "long",
      "demo",
      "theme=${1:theme}\r"
    ],
    "\\SetCodeHighTracing": [
      "+code\r",
      "-code\r",
      "+demo\r",
      "-demo\r",
      "+parser\r",
      "-parser\r",
      "all\r",
      "none\r"
    ]
  },
  "args": []
}