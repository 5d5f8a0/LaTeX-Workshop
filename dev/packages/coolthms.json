{
  "deps": [
    {
      "name": "amssymb"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "scrbase"
    },
    {
      "name": "letltxmacro"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "xargs"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "ntheorem"
    },
    {
      "name": "cleveref"
    }
  ],
  "macros": [
    {
      "name": "definetheorem",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:thm name}}"
      }
    },
    {
      "name": "definetheorem",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:numbered like}]{${1:envname}}{${2:thm name}}"
      }
    },
    {
      "name": "definetheorem",
      "arg": {
        "format": "{}{}[]",
        "snippet": "{${1:envname}}{${2:thm name}}[${3:counter within}]"
      }
    },
    {
      "name": "definetheorem",
      "unusual": true,
      "arg": {
        "format": "[]{}{}[]",
        "snippet": "[${3:numbered like}]{${1:envname}}{${2:thm name}}[${4:counter within}]"
      }
    },
    {
      "name": "definetheorem",
      "unusual": true,
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:envname}}[${3:thm plural name}]{${2:thm name}}"
      }
    },
    {
      "name": "Label",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key}}"
      }
    },
    {
      "name": "theoremmarkup",
      "arg": {
        "format": "[][][][][][]",
        "snippet": "[${1:header font}][${2:body font}][${3:symbol}][${4:indent}][${5:separator}][${6:numbering}]"
      }
    },
    {
      "name": "proofname",
      "unusual": true
    },
    {
      "name": "theoremsymbol",
      "arg": {
        "format": "{}",
        "snippet": "{${1:symbol}}"
      }
    }
  ],
  "envs": [
    {
      "name": "proof"
    }
  ],
  "keys": {
    "\\usepackage/coolthms#c": [
      "proofname=${1:text}\r",
      "proofsymbol=${1:symbol}\r",
      "proofcaptionstyle=${1:font commands}\r",
      "proofindent=",
      "separator=${1:symbol}\r",
      "indent=",
      "minskip=",
      "maxskip=",
      "externalchapters"
    ]
  },
  "args": [
    "\\usepackage/coolthms#c"
  ]
}