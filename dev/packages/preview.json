{
  "deps": [
    {
      "name": "luatex85"
    }
  ],
  "macros": [
    {
      "name": "PreviewBorder",
      "if": "tightpage"
    },
    {
      "name": "PreviewBbAdjust",
      "if": "tightpage"
    },
    {
      "name": "PreviewMacro",
      "arg": {
        "format": "{}",
        "snippet": "{${1:macro}}"
      }
    },
    {
      "name": "PreviewMacro",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:arg spec}]{${1:macro}}"
      }
    },
    {
      "name": "PreviewMacro*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:macro}}"
      }
    },
    {
      "name": "PreviewMacro*",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:arg spec}][${3:default}]{${1:macro}}"
      }
    },
    {
      "name": "PreviewEnvironment",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "PreviewEnvironment",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:arg spec}]{${1:envname}}"
      }
    },
    {
      "name": "PreviewEnvironment*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "PreviewEnvironment*",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:arg spec}][${3:default}]{${1:envname}}"
      }
    },
    {
      "name": "PreviewSnarfEnvironment",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "PreviewSnarfEnvironment",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:arg spec}]{${1:envname}}"
      }
    },
    {
      "name": "PreviewOpen"
    },
    {
      "name": "PreviewClose"
    },
    {
      "name": "ifPreview"
    }
  ],
  "envs": [
    {
      "name": "preview"
    },
    {
      "name": "nopreview"
    }
  ],
  "keys": {
    "\\usepackage/preview#c": [
      "active\r",
      "noconfig\r",
      "psfixbb\r",
      "dvips\r",
      "pdftex\r",
      "xetex\r",
      "displaymath\r",
      "floats\r",
      "textmath\r",
      "graphics\r",
      "sections\r",
      "delayed\r",
      "auctex\r",
      "showlabels\r",
      "tightpage\r",
      "lyx\r",
      "counters\r",
      "footnotes\r",
      "tracingall\r",
      "showbox\r"
    ]
  },
  "args": [
    "\\usepackage/preview#c"
  ]
}