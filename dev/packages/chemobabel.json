{
  "deps": [
    {
      "name": "verbatim"
    },
    {
      "name": "graphicx"
    }
  ],
  "macros": [
    {
      "name": "chemobabel",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:imagefile}}{${2:obabel options}}"
      }
    },
    {
      "name": "chemobabel",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:imagefile}}{${2:obabel options}}"
      }
    },
    {
      "name": "chemobabel*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "chemobabel*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    },
    {
      "name": "smilesobabel",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:SMILES notation%definition}}{${2:obabel options}}"
      }
    },
    {
      "name": "smilesobabel",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:SMILES notation%definition}}{${2:obabel options}}"
      }
    },
    {
      "name": "smilesobabel*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:SMILES notation%definition}}"
      }
    },
    {
      "name": "smilesobabel*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:SMILES notation%definition}}"
      }
    },
    {
      "name": "chemobabelimgdir",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/chemobabel#c": [
      "extract\r",
      "pdf\r",
      "eps\r",
      "inkscape-old\r",
      "inkscape\r",
      "librsvg\r",
      "nocrop\r"
    ],
    "\\chemobabel,\\chemobabel*,\\smilesobabel,\\smilesobabel*": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": [
    "\\usepackage/chemobabel#c"
  ]
}