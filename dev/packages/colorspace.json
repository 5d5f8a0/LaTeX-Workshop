{
  "deps": [
    {
      "name": "xcolor"
    }
  ],
  "macros": [
    {
      "name": "definespotcolor",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:name%specialDef}}{${2:PDF name}}{${3:CMYK-equivalent}}"
      }
    },
    {
      "name": "definespotcolor",
      "arg": {
        "format": "{}{}[]{}",
        "snippet": "{${1:name%specialDef}}{${2:PDF name}}[${4:model}]{${3:equivalent}}"
      }
    },
    {
      "name": "definespotcolor",
      "arg": {
        "format": "{}{}[]{}",
        "snippet": "{${1:name}}{${2:PDF name}}[${4:alt=lab}]{${3:lab-values}}"
      }
    },
    {
      "name": "definecolorspace",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:name}}{${2:mixed}}{${3:color-list}}"
      }
    },
    {
      "name": "definecolorspace*",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:name}}{${2:iccbased}}{${3:icc-file}}"
      }
    },
    {
      "name": "pagecolorspace",
      "arg": {
        "format": "{}",
        "snippet": "{${1:colorspace list}}"
      }
    },
    {
      "name": "resetpagecolorspace"
    },
    {
      "name": "overprintstate",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:state}}"
      }
    },
    {
      "name": "textoverprint",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "textoverprint",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:state}]{${1:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/colorspace#c": [
      "illuminant=${1|a,c,e,d50,d55,d65,d75\r|}",
      "overprint\r",
      "opm=${1|0,1\r|}",
      "knockout\r"
    ]
  },
  "args": [
    "\\usepackage/colorspace#c"
  ]
}