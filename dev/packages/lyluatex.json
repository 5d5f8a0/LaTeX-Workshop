    {
      "name": "lynewenvironment",
      "arg": {
        "format": "{}[]{}{}",
        "snippet": "{${1:envname}}[${4:args}]{${2:begdef}}{${3:enddef}}"
      }
    },
    {
      "name": "lynewenvironment",
      "arg": {
        "format": "{}[][]{}{}",
        "snippet": "{${1:envname}}[${4:args}][${5:default}]{${2:begdef}}{${3:enddef}}"
      }
