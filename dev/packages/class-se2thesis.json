{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "translations"
    },
    {
      "name": "class-scrbook"
    },
    {
      "name": "se2colors"
    },
    {
      "name": "se2fonts"
    },
    {
      "name": "scrlayer-scrpage"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "ntheorem"
    },
    {
      "name": "tcolorbox"
    },
    {
      "name": "class-scrartcl",
      "if": "class=scrartcl"
    },
    {
      "name": "class-scrreprt",
      "if": "class=scrreprt"
    }
  ],
  "macros": [
    {
      "name": "version",
      "arg": {
        "format": "{}",
        "snippet": "{${1:version}}"
      }
    },
    {
      "name": "degreeprogramme",
      "arg": {
        "format": "{}",
        "snippet": "{${1:programme-name}}"
      }
    },
    {
      "name": "matrnumber",
      "arg": {
        "format": "{}",
        "snippet": "{${1:matriculation-number}}"
      }
    },
    {
      "name": "supervisor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "cosupervisor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "advisor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "coadvisor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "department",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "institute",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "institute",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:short-name}]{${1:name}}"
      }
    },
    {
      "name": "external",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "location",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name-of-town}}"
      }
    },
    {
      "name": "authorshipDeclaration"
    },
    {
      "name": "authorshipDeclaration",
      "unusual": true,
      "arg": {
        "format": "[]",
        "snippet": "[${1:location}]"
      }
    },
    {
      "name": "signatureBox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:signature-name}}"
      }
    },
    {
      "name": "signatureBox",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:width}]{${1:signature-name}}"
      }
    },
    {
      "name": "summarybox",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:label text%text}}{${2:text}}"
      }
    },
    {
      "name": "headingdot",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "abstract",
      "arg": {
        "format": "[]",
        "snippet": "[${1:language}]"
      }
    },
    {
      "name": "acknowledgements"
    },
    {
      "name": "acknowledgements",
      "arg": {
        "format": "[]",
        "snippet": "[${1:language}]"
      }
    },
    {
      "name": "resq"
    },
    {
      "name": "resq",
      "arg": {
        "format": "[]",
        "snippet": "[${1:heading%text}]"
      }
    },
    {
      "name": "hyp"
    },
    {
      "name": "hyp",
      "arg": {
        "format": "[]",
        "snippet": "[${1:heading%text}]"
      }
    },
    {
      "name": "summary",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label text%text}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/se2thesis#c": [
      "class=${1|scrreprt,scrbook\r|}",
      "paper=${1|a4,b5\r|}",
      "logofile={${1:path-to-file}}\r",
      "thesistype=${1|bachelor,bachelorproposal,master,masterproposal,phd,phdeng,phdphil,phdproposal\r|}",
      "colormode=${1|cmyk,rgb,bw\r|}"
    ]
  },
  "args": [
    "\\documentclass/se2thesis#c"
  ]
}