{
  "deps": [
    {
      "name": "pgfopts"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "tikzlibrary3d"
    },
    {
      "name": "sourcesanspro"
    }
  ],
  "macros": [
    {
      "name": "trigonset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usetheme/trigon,\\trigonset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "headcol",
      "unusual": true
    },
    {
      "name": "txtcol",
      "unusual": true
    },
    {
      "name": "logbig",
      "unusual": true
    },
    {
      "name": "biglogo",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "biglogo",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    },
    {
      "name": "slidestyle",
      "unusual": true
    },
    {
      "name": "titlestyle",
      "unusual": true
    },
    {
      "name": "sectionstyle",
      "unusual": true
    },
    {
      "name": "leftTriangle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "rightTriangle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "topTriangle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "leftColorTriangle",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:color}}{${2:x}}{${3:y}}"
      }
    },
    {
      "name": "rightColorTriangle",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:color}}{${2:x}}{${3:y}}"
      }
    },
    {
      "name": "topColorTriangle",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:color}}{${2:x}}{${3:y}}"
      }
    },
    {
      "name": "titleframe"
    },
    {
      "name": "sectionframe"
    },
    {
      "name": "logsmall",
      "unusual": true
    },
    {
      "name": "smalllogo",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "smalllogo",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usetheme/trigon,\\trigonset": [
      "background=${1|light,dark\r|}",
      "titleformat=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "usesourcefonts",
      "sectionpage=${1|none,simple\r|}",
      "titlestyle=${1|plain,style1,style2\r|}",
      "sectionstyle=${1|plain,style1,style2,style3\r|}",
      "slidestyle=${1|blank,style1,cyber\r|}",
      "numbering=${1|none,counter,fraction\r|}",
      "block=${1|transparent,fill\r|}",
      "headingcolor=${1|default,theme\r|}",
      "textgcolor=${1|default,theme\r|}",
      "titleformat title=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "titleformat subtitle=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "titleformat section=${1|regular,smallcaps,allsmallcaps,allcaps\r|}",
      "titleformat frame=${1|regular,smallcaps,allsmallcaps,allcaps\r|}"
    ],
    "\\biglogo,\\smalllogo": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": []
}