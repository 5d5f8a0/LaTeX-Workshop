{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xstring"
    }
  ],
  "macros": [
    {
      "name": "smartdiagram",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:type%keyvals}]{${1:list of items}}"
      }
    },
    {
      "name": "smartdiagramanimated",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:type%keyvals}]{${1:list of items}}"
      }
    },
    {
      "name": "usesmartdiagramlibrary",
      "arg": {
        "format": "{}",
        "snippet": "{${1:library}}"
      }
    },
    {
      "name": "smartdiagramadd",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:type%keyvals}]{${1:list of items}}{${2:list of additions}}"
      }
    },
    {
      "name": "smartdiagramconnect",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arrow options}}{${2:start module/end module}}"
      }
    },
    {
      "name": "smartdiagramset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\smartdiagram,\\smartdiagramanimated,\\smartdiagramadd": [
      "circular diagram\r",
      "circular diagram:clockwise\r",
      "flow diagram\r",
      "flow diagram:horizontal\r",
      "descriptive diagram\r",
      "priority descriptive diagram\r",
      "bubble diagram\r",
      "constellation diagram\r",
      "connected constellation diagram\r",
      "sequence diagram\r"
    ],
    "\\usesmartdiagramlibrary#c": [
      "additions\r"
    ],
    "\\smartdiagramset#c": [
      "set color list={${1:color1,color2,...}}\r",
      "uniform color list=\r",
      "use predefined color list\r",
      "insert decoration=\r",
      "arrow line width=",
      "arrow tip=\r",
      "arrow style=\r",
      "uniform arrow color",
      "arrow color=",
      "module minimum width=",
      "module minimum height=",
      "module y sep=",
      "module x sep=",
      "module shape=\r",
      "text width=",
      "font=${1:cmd}\r",
      "border color=",
      "text color=",
      "circular distance=",
      "back arrow distance=",
      "back arrow disabled",
      "circular final arrow disabled",
      "descriptive items y sep=",
      "description title width=",
      "description title text width=",
      "description text width=",
      "description title font=${1:cmd}\r",
      "description font=${1:cmd}\r",
      "description width=",
      "priority arrow width=",
      "priority arrow head extend=",
      "priority tick size=",
      "priority arrow height advance=",
      "bubble center node size=",
      "bubble center node font=${1:cmd}\r",
      "bubble center node color=",
      "distance center/other bubbles=",
      "distance text center bubble=",
      "bubble fill opacity=\r",
      "bubble node size=",
      "bubble text opacity=\r",
      "bubble node font=${1:cmd}\r",
      "planet size=",
      "planet color=",
      "planet font=${1:cmd}\r",
      "distance planet-connection=",
      "distance planet-text=",
      "planet text width=",
      "satellite size=",
      "satellite font=${1:cmd}\r",
      "satellite fill opacity=\r",
      "satellite text opacity=\r",
      "satellite text width=",
      "distance satellite-connection=",
      "connection line width=",
      "distance planet-satellite=",
      "uniform connection color",
      "connection color=",
      "sequence item height=",
      "sequence item width=",
      "sequence item border color=",
      "sequence item border size=",
      "sequence item font size=",
      "sequence item fill opacity=\r",
      "sequence item text opacity=\r",
      "sequence item text width=",
      "sequence item text color=",
      "uniform sequence color",
      "sequence item uniform color=",
      "additions={${1:specs}}\r"
    ]
  },
  "args": []
}