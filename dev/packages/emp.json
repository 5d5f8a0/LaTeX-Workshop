{
  "deps": [
    {
      "name": "graphics"
    },
    {
      "name": "verbatim"
    }
  ],
  "macros": [
    {
      "name": "empuse",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "empTeX",
      "arg": {
        "format": "{}",
        "snippet": "{${1:LaTeX prelude%definition}}"
      }
    },
    {
      "name": "empaddtoTeX",
      "arg": {
        "format": "{}",
        "snippet": "{${1:LaTeX code%definition}}"
      }
    },
    {
      "name": "empprelude",
      "arg": {
        "format": "{}",
        "snippet": "{${1:METAPOST prelude%definition}}"
      }
    },
    {
      "name": "empaddtoprelude",
      "arg": {
        "format": "{}",
        "snippet": "{${1:METAPOST code%definition}}"
      }
    },
    {
      "name": "empwrite",
      "arg": {
        "format": "{}",
        "snippet": "{${1:METAPOST code%definition}}"
      }
    },
    {
      "name": "theempfig",
      "unusual": true
    },
    {
      "name": "theempfile",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "empfile"
    },
    {
      "name": "empfile",
      "arg": {
        "format": "[]",
        "snippet": "[${1:METAPOST file%file}]"
      }
    },
    {
      "name": "emp",
      "arg": {
        "format": "()",
        "snippet": "(${1:width},${2:height})"
      }
    },
    {
      "name": "emp",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:name}](${2:width},${3:height})"
      }
    },
    {
      "name": "empdef",
      "arg": {
        "format": "()",
        "snippet": "(${1:width},${2:height})"
      }
    },
    {
      "name": "empdef",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:name}](${2:width},${3:height})"
      }
    },
    {
      "name": "empcmds"
    },
    {
      "name": "empgraph",
      "arg": {
        "format": "()",
        "snippet": "(${1:width},${2:height})"
      }
    },
    {
      "name": "empgraph",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:name}](${2:width},${3:height})"
      }
    }
  ],
  "keys": {
    "\\usepackage/emp#c": [
      "unknownkeysallowed\r",
      "draft\r",
      "final\r",
      "hiresbb\r",
      "demo\r",
      "setpagesize\r",
      "nosetpagesize\r",
      "dvips\r",
      "xdvi\r",
      "dvipdf\r",
      "dvipdfm\r",
      "dvipdfmx\r",
      "xetex\r",
      "pdftex\r",
      "luatex\r",
      "dvisvgm\r",
      "dvipsone\r",
      "dviwindo\r",
      "emtex\r",
      "dviwin\r",
      "oztex\r",
      "textures\r",
      "pctexps\r",
      "pctexwin\r",
      "pctexhp\r",
      "pctex32\r",
      "truetex\r",
      "tcidvi\r",
      "vtex\r",
      "debugshow\r",
      "hiderotate\r",
      "hidescale\r"
    ]
  },
  "args": [
    "\\usepackage/emp#c"
  ]
}