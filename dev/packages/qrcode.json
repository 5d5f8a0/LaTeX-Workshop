{
  "deps": [
    {
      "name": "xcolor"
    }
  ],
  "macros": [
    {
      "name": "qrcode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:URL}}"
      }
    },
    {
      "name": "qrcode",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:URL}}"
      }
    },
    {
      "name": "qrcode*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:URL}}"
      }
    },
    {
      "name": "qrcode*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:URL}}"
      }
    },
    {
      "name": "qrset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\qrcode,\\qrset,\\qrcode*",
          "\\qrcode,\\qrset"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/qrcode#c": [
      "nolinks\r",
      "draft\r",
      "final\r",
      "forget\r"
    ],
    "\\qrcode,\\qrset,\\qrcode*": [
      "height=",
      "level=${1|L,M,Q,H\r|}",
      "version=\r",
      "tight\r",
      "padding\r",
      "draft\r"
    ],
    "\\qrcode,\\qrset": [
      "link\r",
      "nolink\r",
      "hyperlink\r"
    ]
  },
  "args": [
    "\\usepackage/qrcode#c"
  ]
}