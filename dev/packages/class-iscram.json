{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "pgfopts"
    },
    {
      "name": "geometry"
    },
    {
      "name": "microtype"
    },
    {
      "name": "newtxtext"
    },
    {
      "name": "newtxmath"
    },
    {
      "name": "titlesec"
    },
    {
      "name": "float"
    },
    {
      "name": "caption"
    },
    {
      "name": "booktabs"
    },
    {
      "name": "biblatex"
    },
    {
      "name": "nowidow"
    },
    {
      "name": "url"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "titleps"
    }
  ],
  "macros": [
    {
      "name": "iscramset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\documentclass/iscram#c,\\iscramset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "abstract",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "keywords",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keywords%text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/iscram#c,\\iscramset": [
      "draft\r",
      "anonymous\r",
      "first alone\r",
      "title=${1:title}\r",
      "short title=${1:short title}\r",
      "author={short name=${1:short name},full name=${2:full name},affiliation=${3:affiliation}}\r",
      "footer/line 1=${1:text}\r",
      "footer/line 2=${1:text}\r",
      "footer/line 3=${1:text}\r",
      "iscram 2017 footer\r",
      "WiPe Paper 2017=${1:track name}\r",
      "CoRe Paper 2017=${1:track name}\r",
      "iscram 2018 footer\r",
      "WiPe Paper 2018=${1:track name}\r",
      "CoRe Paper 2018=${1:track name}\r"
    ]
  },
  "args": [
    "\\documentclass/iscram#c,\\iscramset"
  ]
}