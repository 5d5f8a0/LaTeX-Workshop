{
  "deps": [
    {
      "name": "luatex"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "mdframed"
    },
    {
      "name": "fontspec"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "NodetreeSet",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/nodetree-embed#c,\\NodetreeSet,\\begin{NodetreeEmbedView},\\begin{NodetreeEmbedEnv},\\NodetreeEmbedCmd,\\NodetreeEmbedInput,\\nodetreeterminalemulator"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "NodetreeEmbedCmd",
      "arg": {
        "format": "{}",
        "snippet": "{${1:TeX markup}}"
      }
    },
    {
      "name": "NodetreeEmbedCmd",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:TeX markup}}"
      }
    },
    {
      "name": "NodetreeEmbedInput",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "NodetreeEmbedInput",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "NodetreeSetOption",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:value}}"
      }
    },
    {
      "name": "NodetreeResetOption",
      "arg": {
        "format": "{}",
        "snippet": "{${1:option%keyvals}}",
        "keys": [
          "\\NodetreeSetOption,\\nodetreeoption,\\NodetreeResetOption"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "NodetreeReset"
    },
    {
      "name": "NodetreeRegisterCallback",
      "arg": {
        "format": "{}",
        "snippet": "{${1:callbacks}}"
      }
    },
    {
      "name": "NodetreeUnregisterCallback",
      "arg": {
        "format": "{}",
        "snippet": "{${1:callbacks}}"
      }
    }
  ],
  "envs": [
    {
      "name": "NodetreeEmbedView"
    },
    {
      "name": "NodetreeEmbedView",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "NodetreeEmbedEnv"
    },
    {
      "name": "NodetreeEmbedEnv",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/nodetree-embed#c,\\NodetreeSet,\\begin{NodetreeEmbedView},\\begin{NodetreeEmbedEnv},\\NodetreeEmbedCmd,\\NodetreeEmbedInput,\\nodetreeterminalemulator": [
      "channel=${1:output channel}\r",
      "callback={${1:callbacks}}\r",
      "verbosity=${1:integer}\r",
      "color=${1|colored,no\r|}",
      "unit=${1|pt,pc,in,bp,cm,mm,dd,cc,sp,ex,em\r|}",
      "decimalplaces=${1:integer}\r",
      "theme=${1|bwdark,bwlight,monokaisoda\r|}",
      "thememode=${1|dark,light\r|}",
      "font=${1:font name}\r",
      "fontsize=${1:fontsize cmd}\r",
      "firstline=${1:integer}\r",
      "lastline=${1:integer}\r",
      "showmarkup"
    ],
    "\\NodetreeSetOption,\\nodetreeoption,\\NodetreeResetOption": [
      "channel\r",
      "callback\r",
      "verbosity\r",
      "color\r",
      "unit\r",
      "decimalplaces\r",
      "theme\r",
      "thememode\r",
      "font\r",
      "fontsize\r",
      "firstline\r",
      "lastline\r",
      "showmarkup\r"
    ]
  },
  "args": [
    "\\usepackage/nodetree-embed#c,\\NodetreeSet,\\begin{NodetreeEmbedView},\\begin{NodetreeEmbedEnv},\\NodetreeEmbedCmd,\\NodetreeEmbedInput,\\nodetreeterminalemulator"
  ]
}