{
  "deps": [
    {
      "name": "luatex"
    }
  ],
  "macros": [
    {
      "name": "barracuda",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:encoder}}{${2:data}}"
      }
    },
    {
      "name": "barracuda",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:encoder}}{${2:data}}"
      }
    },
    {
      "name": "barracudabox",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\barracuda#c": [
      "ax=${1:number}\r",
      "ay=${1:number}\r",
      "debug_bbox=${1|\"none\",\"enum\",\"symb\",\"qz\",\"qzsymb\"\r|}",
      "module=${1:number}\r",
      "ratio=${1:number}\r",
      "quietzone=${1:number}\r",
      "interspace=${1:number}\r",
      "height=${1:number}\r",
      "text_enabled",
      "text_vpos=${1|'top','bottom'\r|}",
      "text_hpos=${1|'left','center','right','spaced'\r|}",
      "text_gap=${1:number}\r",
      "text_star",
      "xdim=${1:number}\r",
      "ydim=${1:number}\r",
      "quietzone_factor=${1:number}\r",
      "mod=${1:number}\r",
      "quietzone_left_factor=${1:number}\r",
      "quietzone_right_factor=${1:number}\r",
      "bars_depth_factor=${1:number}\r",
      "text_enabled",
      "text_ygap_factor=${1:number}\r",
      "text_xgap_factor=${1:number}\r",
      "text_guard_enabled",
      "check_digit_policy=${1|'add','verify','none'\r|}",
      "check_digit_method=\r",
      "bearer_bars_enabled",
      "bearer_bars_thickness=${1:number}\r",
      "bearer_bars_layout=${1|'frame','hbar'\r|}"
    ]
  },
  "args": []
}