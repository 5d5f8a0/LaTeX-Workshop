{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-xkey"
    },
    {
      "name": "pst-plot"
    },
    {
      "name": "pst-node"
    }
  ],
  "macros": [
    {
      "name": "psRQBCmasse(x0,y0)(x1,y1)(x2,y2)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:w0,w1,w2}}"
      }
    },
    {
      "name": "psRQBCmasse",
      "arg": {
        "format": "[]()()(){}",
        "snippet": "[${2:options%keyvals}](${3:x0},${4:y0})(${5:x1},${6:y1})(${7:x2},${8:y2}){${1:w0,w1,w2}}"
      }
    },
    {
      "name": "pscalculate",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:expr%definition}}"
      }
    },
    {
      "name": "defopt",
      "unusual": true,
      "arg": {
        "format": "{}{}[]args{}",
        "snippet": "{${1:macro%cmd}}{${2:default}}[${4:optargs}]args{${3:definition}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psbcurve": [
      "bcurveTension=${1:value}\r",
      "dash=${1:a b}\r",
      "fillcolor=",
      "fillstyle=${1:style}\r",
      "linecolor=",
      "linestyle=${1:style}\r",
      "linewidth=",
      "showpoints"
    ],
    "\\psRQBCmasse": [
      "autoTrace",
      "linecolor=",
      "linewidth=",
      "nPoints=${1:number}\r",
      "showpoints",
      "showPolygon"
    ]
  },
  "args": []
}