{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "xifthen"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "helpgrid"
    },
    {
      "name": "helpgrid",
      "arg": {
        "format": "[]",
        "snippet": "[${1:color}]"
      }
    },
    {
      "name": "callout",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:center-coords}}{${2:note%text}}{${3:tip-coords}}"
      }
    },
    {
      "name": "note",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:coords}}{${2:note%text}}"
      }
    },
    {
      "name": "arrow",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:center-coords}}{${2:tip-coords}}"
      }
    },
    {
      "name": "focol",
      "unusual": true
    },
    {
      "name": "bgcol",
      "unusual": true
    },
    {
      "name": "arcol",
      "unusual": true
    },
    {
      "name": "xtic",
      "unusual": true
    },
    {
      "name": "ytic",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "annotate",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:object}}{${2:width-factor}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/callouts#c": [
      "bwr\r",
      "wby\r",
      "bww\r",
      "background=",
      "arrow="
    ]
  },
  "args": [
    "\\usepackage/callouts#c"
  ]
}