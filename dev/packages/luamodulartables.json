{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "luacode"
    }
  ],
  "macros": [
    {
      "name": "luaModularMult",
      "arg": {
        "format": "{}",
        "snippet": "{${1:integer}}"
      }
    },
    {
      "name": "luaModularMult",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:integer}}"
      }
    },
    {
      "name": "luaModularAdd",
      "arg": {
        "format": "{}",
        "snippet": "{${1:integer}}"
      }
    },
    {
      "name": "luaModularAdd",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:integer}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\luaModularMult": [
      "multlabel=${1:text}\r",
      "headline=${1:command}\r",
      "midline=${1:command}\r"
    ],
    "\\luaModularAdd": [
      "addlabel=${1:text}\r",
      "headline=${1:command}\r",
      "midline=${1:command}\r"
    ]
  },
  "args": []
}