{
  "deps": [
    {
      "name": "iftex"
    },
    {
      "name": "scrbase"
    },
    {
      "name": "pdftexcmds"
    },
    {
      "name": "trimspaces"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "shellesc"
    },
    {
      "name": "ifplatform"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "transparent"
    },
    {
      "name": "pgfsys"
    }
  ],
  "macros": [
    {
      "name": "svgsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "svgpath",
      "arg": {
        "format": "{}",
        "snippet": "{${1:path%URL}}"
      }
    },
    {
      "name": "includesvg",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "includesvg",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    },
    {
      "name": "includeinkscape",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "includeinkscape",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/svg#c": [
      "usexcolor=${1|true,on,yes,false,off,no\r|}",
      "usetransparent=${1|true,on,yes,false,off,no\r|}",
      "noxcolor\r",
      "notransparent\r"
    ],
    "\\usepackage/svg#c,\\svgsetup#c": [
      "inkscape=${2|false,off,no,true,on,yes,newer,onlynewer,forced,force,overwrite,pdf,eps,ps,png,latex,nolatex,drawing,page,${1:integer}dpi\r|}",
      "inkscapepath=${2|svgdir,svgpath,svgsubdir,svgsubpath,basedir,basepath,jobdir,jobpath,basesubdir,basesubpath,jobsubdir,jobsubpath,${1:path}\r|}",
      "inkscapeexe=${1:exec path}\r",
      "inkscapeversion=${1|0,1,auto\r|}",
      "inkscapename=${1:file name}\r",
      "inkscapeformat=${1|pdf,aps,ps,png\r|}",
      "inkscapelatex",
      "inkscapearea=${1|drawing,crop,page,nocrop\r|}",
      "inkscapedpi=${1:integer}\r",
      "inkscapeopt=${1:Inkscape option}\r",
      "svgextension=${1:extension}\r",
      "width=",
      "height=",
      "distort",
      "scale=${1:factor}\r",
      "pretex=${1:code}\r",
      "apptex=${1:code}\r",
      "draft",
      "lastpage"
    ],
    "\\includesvg": [
      "inkscape=${2|false,off,no,true,on,yes,newer,onlynewer,forced,force,overwrite,pdf,eps,ps,png,latex,nolatex,drawing,page,${1:integer}dpi\r|}",
      "inkscapeformat=${1|pdf,aps,ps,png\r|}",
      "inkscapelatex",
      "inkscapearea=${1|drawing,crop,page,nocrop\r|}",
      "inkscapedpi=${1:integer}\r",
      "inkscapeopt=${1:Inkscape option}\r",
      "svgextension=${1:extension}\r",
      "width=",
      "height=",
      "distort",
      "scale=${1:factor}\r",
      "pretex=${1:code}\r",
      "apptex=${1:code}\r",
      "draft",
      "lastpage",
      "angle=${1:degrees}\r",
      "origin=\r"
    ],
    "\\includeinkscape": [
      "inkscapeformat=${1|pdf,aps,ps,png\r|}",
      "inkscapelatex",
      "width=",
      "height=",
      "distort",
      "scale=${1:factor}\r",
      "pretex=${1:code}\r",
      "apptex=${1:code}\r",
      "draft",
      "lastpage",
      "angle=${1:degrees}\r",
      "origin=\r"
    ]
  },
  "args": [
    "\\usepackage/svg#c",
    "\\usepackage/svg#c,\\svgsetup#c"
  ]
}