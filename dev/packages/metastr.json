{
  "deps": [
    {
      "name": "hyperref"
    },
    {
      "name": "hyperxmp"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "metadef",
      "arg": {
        "format": "{}",
        "snippet": "{${1:reg}}"
      }
    },
    {
      "name": "metaset",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:reg}}{${2:def}}"
      }
    },
    {
      "name": "metaset",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:var}]{${1:reg}}{${2:def}}"
      }
    },
    {
      "name": "metasetlang",
      "arg": {
        "format": "{}",
        "snippet": "{${1:ln-CN}}"
      }
    },
    {
      "name": "metasetlang*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:ln-CN}}"
      }
    },
    {
      "name": "metaget",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:var}]{${1:reg}}{${2:def}}"
      }
    },
    {
      "name": "metaif",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:}]{${1:reg}}{${2:true}}{${3:false}}"
      }
    },
    {
      "name": "metaunset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:reg}}"
      }
    },
    {
      "name": "metaunset",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:var}]{${1:reg}}"
      }
    },
    {
      "name": "metaappend",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:reg}}{${2:def}}"
      }
    },
    {
      "name": "metaappend",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:var}]{${1:reg}}{${2:def}}"
      }
    },
    {
      "name": "metaprepend",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:reg}}{${2:def}}"
      }
    },
    {
      "name": "metaprepend",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:var}]{${1:reg}}{${2:def}}"
      }
    },
    {
      "name": "metaaddsep",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:reg}}{${2:sep}}{${3:def}}"
      }
    },
    {
      "name": "metaaddsep",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:var}]{${1:reg}}{${2:sep}}{${3:def}}"
      }
    },
    {
      "name": "metapick",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:var}]{${1:reg}}"
      }
    },
    {
      "name": "metaifpick",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:var}]{${1:reg}}{${2:true}}{${3:false}}"
      }
    },
    {
      "name": "metacompose",
      "arg": {
        "format": "[]{}{}{}{}",
        "snippet": "[${5:var}]{${1:reg}}{${2:prefix}}{${3:postfix}}{${4:empty}}"
      }
    },
    {
      "name": "metatitleline",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:var}]{${1:reg}}"
      }
    },
    {
      "name": "metatitlelinetwo",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "[${3:var1}]{${1:reg1}}[${4:var2}]{${2:reg2}}"
      }
    },
    {
      "name": "metawritepdfinfo"
    },
    {
      "name": "metawritepdfaux"
    },
    {
      "name": "metawritepdfpreamble"
    },
    {
      "name": "metawritepdfcontact"
    },
    {
      "name": "metawritepdfrights"
    },
    {
      "name": "metawritepdf"
    },
    {
      "name": "metacopyright",
      "arg": {
        "format": "{}",
        "snippet": "{${1:preset%keyvals}}",
        "keys": [
          "\\metacopyright"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "metalicense",
      "arg": {
        "format": "{}",
        "snippet": "{${1:preset%keyvals}}",
        "keys": [
          "\\metalicense"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "metalicensecc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:license%keyvals}}",
        "keys": [
          "\\metalicensecc"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "metaterm"
    },
    {
      "name": "metatranslate",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:ln}]{${1:term}}"
      }
    },
    {
      "name": "metasetterm",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:ln}]{${1:reg}}{${2:def}}"
      }
    },
    {
      "name": "metasetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "metacomma",
      "unusual": true
    },
    {
      "name": "metatilde",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/metastr#c": [
      "hyperref",
      "hyperxmp",
      "checkdef",
      "cclogo",
      "cclogocurr=${1|dollar,euro,yen\r|}",
      "cclogoshape=${1|box,slim\r|}",
      "xmppdfinfo",
      "draft",
      "titlematter",
      "course",
      "loadlang=${1|en,de,fr,es\r|}"
    ],
    "\\metacopyright": [
      "plain\r",
      "parts\r",
      "doc\r",
      "doc-parts\r",
      "reserved\r",
      "publicdomain\r"
    ],
    "\\metalicense": [
      "consent\r",
      "consent-noncom\r",
      "lppl\r"
    ],
    "\\metalicensecc": [
      "by\r",
      "by-sa\r",
      "by-nd\r",
      "by-nc\r",
      "by-nc-sa\r",
      "by-nc-nd\r",
      "zero\r",
      "pd\r"
    ],
    "\\metasetup#c": [
      "draft"
    ]
  },
  "args": [
    "\\usepackage/metastr#c"
  ]
}