{
  "deps": [
    {
      "name": "siunitx"
    },
    {
      "name": "pgfmath"
    },
    {
      "name": "pgfkeys"
    }
  ],
  "macros": [
    {
      "name": "SmartUnit",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "SmartUnitSettings",
      "arg": {
        "format": "{}",
        "snippet": "{${1:settings%keyvals}}",
        "keys": [
          "\\SmartUnit,\\SmartUnitSettings"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\SmartUnit,\\SmartUnitSettings": [
      "metric\r",
      "imperial\r",
      "metric imperial\r",
      "imperial metric\r",
      "figures=${1:integer}\r",
      "places=${1:integer}\r",
      "uk\r",
      "usa\r"
    ],
    "\\SmartUnit": [
      "km=${1:number}\r",
      "miles=${1:number}\r",
      "mi=${1:number}\r",
      "cm=${1:number}\r",
      "inches=${1:number}\r",
      "feet=${1:number}\r",
      "celsius=${1:number}\r",
      "C=${1:number}\r",
      "fahrenheit=${1:number}\r",
      "F=${1:number}\r",
      "hours=${1:number}\r",
      "minutes=${1:number}\r",
      "seconds=${1:number}\r",
      "am\r",
      "pm\r",
      "L=${1:number}\r",
      "l=${1:number}\r",
      "gal=${1:number}\r",
      "gallons=${1:number}\r",
      "kg=${1:number}\r",
      "pounds=${1:number}\r",
      "pound=${1:number}\r",
      "lbs=${1:number}\r"
    ]
  },
  "args": []
}