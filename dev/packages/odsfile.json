{
  "deps": [
    {
      "name": "luacode"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "includespread"
    },
    {
      "name": "includespread",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "includespread*"
    },
    {
      "name": "includespread*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "tabletemplate",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:code}}"
      }
    },
    {
      "name": "AddString",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text}}{${2:position}}"
      }
    },
    {
      "name": "AddNumber",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:number}}{${2:position}}"
      }
    },
    {
      "name": "loadodsfile",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "loadodsfile",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "savespreadsheet"
    },
    {
      "name": "OdsNl",
      "unusual": true
    },
    {
      "name": "OdsLastNl",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "AddRow"
    },
    {
      "name": "AddRow",
      "arg": {
        "format": "[]",
        "snippet": "[${1:row number}]"
      }
    }
  ],
  "keys": {
    "\\includespread#c,\\includespread*#c,\\loadodsfile#c": [
      "file=${1:file name}\r",
      "sheet=${1:sheet name}\r",
      "range=${1:cell1:cell2}\r",
      "columns=${2|head,top,{${1:list of headings}}\r|}",
      "columnbreak=${1:command}\r",
      "rowseparator=${2|tableline,hline,newline,${1:command}\r|}",
      "template=${3|${1:template name},{${2:code}}\r|}",
      "coltypes=${1:coltypes}\r",
      "rowtemplate=${3|${1:template name},{${2:code}}\r|}",
      "celltemplate=${3|${1:template name},{${2:code}}\r|}",
      "multicoltemplate=${3|${1:template name},{${2:code}}\r|}",
      "escape"
    ]
  },
  "args": []
}