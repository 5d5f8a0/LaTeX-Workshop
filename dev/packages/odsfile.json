{
  "includes": {
    "luacode": [],
    "xkeyval": []
  },
  "macros": {
    "includespread": {},
    "includespread[]": {
      "snippet": "includespread[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "includespread*": {},
    "includespread*[]": {
      "snippet": "includespread*[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "tabletemplate{}{}": {
      "snippet": "tabletemplate{${1:name}}{${2:code}}"
    },
    "AddString{}{}": {
      "snippet": "AddString{${1:text}}{${2:position}}"
    },
    "AddNumber{}{}": {
      "snippet": "AddNumber{${1:number}}{${2:position}}"
    },
    "loadodsfile{}": {
      "snippet": "loadodsfile{${1:file}}"
    },
    "loadodsfile[]{}": {
      "snippet": "loadodsfile[${2:options}]{${1:file}}"
    },
    "savespreadsheet": {},
    "OdsNl": {},
    "OdsLastNl": {}
  },
  "envs": {
    "AddRow": {},
    "AddRow[]": {
      "name": "AddRow",
      "snippet": "[${1:row number}]"
    }
  },
  "options": [],
  "keyvals": [
    [
      "file=${1:file name}",
      "sheet=${1:sheet name}",
      "range=${1:cell1:cell2}",
      "columns=${1|head,top,{${1:list of headings}}|}",
      "columnbreak=${1:command}",
      "rowseparator=${1|tableline,hline,newline,${1:command}|}",
      "template=${2|${1:template name},{${2:code}}|}",
      "coltypes=${1:coltypes}",
      "rowtemplate=${2|${1:template name},{${2:code}}|}",
      "celltemplate=${2|${1:template name},{${2:code}}|}",
      "multicoltemplate=${2|${1:template name},{${2:code}}|}",
      "escape"
    ]
  ]
}