{
  "deps": [
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "changefontsize",
      "arg": {
        "format": "{}",
        "snippet": "{${1:fontsize}}"
      }
    },
    {
      "name": "changefontsize",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:baselineskip}]{${1:fontsize}}"
      }
    },
    {
      "name": "generateclofile",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:fontsize}}"
      }
    },
    {
      "name": "generateclofile",
      "unusual": true,
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:name}}[${3:baselineskip}]{${2:fontsize}}"
      }
    },
    {
      "name": "printsamples",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:baselineskip}}{${2:fontsize}}"
      }
    },
    {
      "name": "printsamples",
      "unusual": true,
      "arg": {
        "format": "{}{}[]",
        "snippet": "{${1:baselineskip}}{${2:fontsize}}[${3:sizes}]"
      }
    },
    {
      "name": "sampletext",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
