{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tcolorbox"
    },
    {
      "name": "tcolorboxlibraryskins"
    }
  ],
  "macros": [
    {
      "name": "lecturename",
      "unusual": true
    },
    {
      "name": "autorecitazione",
      "unusual": true
    },
    {
      "name": "structureA",
      "unusual": true
    },
    {
      "name": "structureB",
      "unusual": true
    },
    {
      "name": "titlegraphic",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:imagefile}}{${2:TikZ options}}"
      }
    },
    {
      "name": "titlegraphic",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:imagefile}}{${2:TikZ options}}"
      }
    },
    {
      "name": "sidegraphics",
      "if": "sidebar",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:imagefile}}{${2:TikZ options}}"
      }
    },
    {
      "name": "sidegraphics",
      "if": "sidebar",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:imagefile}}{${2:TikZ options}}"
      }
    },
    {
      "name": "sidegraphics<overlay specification>",
      "if": "sidebar",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:imagefile}}{${2:TikZ options}}"
      }
    },
    {
      "name": "sidegraphics<overlay specification>",
      "if": "sidebar",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:imagefile}}{${2:TikZ options}}"
      }
    },
    {
      "name": "lectureinfoot",
      "unusual": true
    },
    {
      "name": "frametitlesidebar",
      "unusual": true
    },
    {
      "name": "mail",
      "arg": {
        "format": "{}",
        "snippet": "{${1:address%text}}"
      }
    },
    {
      "name": "datelecture",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "citazione",
      "unusual": true
    },
    {
      "name": "citazione",
      "unusual": true,
      "arg": {
        "format": "<>",
        "snippet": "<${1:action specification}>"
      }
    },
    {
      "name": "citazione",
      "unusual": true,
      "arg": {
        "format": "[]",
        "snippet": "[${1:author%text}]"
      }
    },
    {
      "name": "citazione",
      "unusual": true,
      "arg": {
        "format": "<>[]",
        "snippet": "<${2:action specification}>[${1:author%text}]"
      }
    },
    {
      "name": "quotation",
      "arg": {
        "format": "[]",
        "snippet": "[${1:author%text}]"
      }
    },
    {
      "name": "quotation",
      "arg": {
        "format": "<>[]",
        "snippet": "<${2:action specification}>[${1:author%text}]"
      }
    }
  ],
  "keys": {
    "\\usetheme/Verona#c": [
      "showheader\r",
      "red\r",
      "gray\r",
      "colorblocks\r",
      "graytitle\r",
      "sidebar\r",
      "noframetitlerule\r"
    ],
    "\\titlegraphic,\\sidegraphics": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": []
}