{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "varwidth"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarybending"
    },
    {
      "name": "footnote",
      "if": "footnote"
    },
    {
      "name": "footnotehyper",
      "if": "footnotehyper"
    }
  ],
  "macros": [
    {
      "name": "WithArrowsOptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\begin{WithArrows},\\WithArrowsOptions,\\WithArrowsNewStyle",
          "\\begin{WithArrows},\\begin{DispWithArrows},\\begin{DispWithArrows*},\\WithArrowsOptions,\\WithArrowsNewStyle",
          "\\begin{DispWithArrows},\\begin{DispWithArrows*},\\WithArrowsOptions,\\WithArrowsNewStyle",
          "\\WithArrowsOptions,\\WithArrowsNewStyle"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "Arrow",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:label%plain}}"
      }
    },
    {
      "name": "Arrow",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:label%plain}}"
      }
    },
    {
      "name": "MultiArrow",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:row1,row2,...}}{${2:label%plain}}"
      }
    },
    {
      "name": "WithArrowsLastEnv",
      "unusual": true
    },
    {
      "name": "tag",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label%plain}}"
      }
    },
    {
      "name": "tag*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label%plain}}"
      }
    },
    {
      "name": "notag"
    },
    {
      "name": "tagnextline",
      "unusual": true
    },
    {
      "name": "WithArrowsRightX",
      "unusual": true
    },
    {
      "name": "WithArrowsNewStyle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:style name%specialDef}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "WithArrowsNbLines",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "WithArrows"
    },
    {
      "name": "WithArrows",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "DispWithArrows"
    },
    {
      "name": "DispWithArrows",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "DispWithArrows*"
    },
    {
      "name": "DispWithArrows*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/witharrows#c": [
      "footnote\r",
      "footnotehyper\r"
    ],
    "\\begin{WithArrows},\\WithArrowsOptions,\\WithArrowsNewStyle": [
      "c\r",
      "b\r",
      "right-overlap"
    ],
    "\\begin{WithArrows},\\begin{DispWithArrows},\\begin{DispWithArrows*},\\WithArrowsOptions,\\WithArrowsNewStyle": [
      "tikz={${1:TikZ options}}\r",
      "xoffset=",
      "command-name=${1:csname}\r",
      "code-before=${1:code}\r",
      "code-after=${1:code}\r",
      "format=${1:cols}\r",
      "show-nodes\r",
      "show-node-names\r",
      "displaystyle\r",
      "ll\r",
      "lr\r",
      "rl\r",
      "rr\r",
      "i\r",
      "group\r",
      "groups\r",
      "up-and-down={${1:options}}\r",
      "jot=",
      "interline=",
      "tikz-code={${1:code}}\r",
      "start-adjust=",
      "end-adjust=",
      "adjust=",
      "ygap=",
      "ystart=",
      "max-length-of-arrow=",
      "%witharrowsstyle\r"
    ],
    "\\begin{WithArrows},\\begin{DispWithArrows},\\begin{DispWithArrows*},\\WithArrowsNewStyle": [
      "name=${1:node name}\r"
    ],
    "\\begin{DispWithArrows},\\begin{DispWithArrows*},\\WithArrowsOptions,\\WithArrowsNewStyle": [
      "wrap-lines\r",
      "tagged-lines={${1:line1,line2,...}}\r",
      "fleqn\r",
      "mathindent=",
      "subequations\r",
      "standard-behaviour-with-items\r"
    ],
    "\\begin{DispWithArrows},\\begin{DispWithArrows*},\\WithArrowsNewStyle": [
      "left-brace\r",
      "left-brace={${1:content}}\r",
      "replace-left-brace-by=\r"
    ],
    "\\WithArrowsOptions,\\WithArrowsNewStyle": [
      "xoffset-for-o-arrows=",
      "up-and-down/width=",
      "up-and-down/radius=",
      "no-arrows\r"
    ],
    "\\Arrow": [
      "jump=${1:integer}\r",
      "xoffset=",
      "tikz=\r",
      "tikz-code={${1:code}}\r",
      "ll\r",
      "lr\r",
      "rl\r",
      "rr\r",
      "i\r",
      "new-group\r",
      "o\r",
      "up\r",
      "up={${1:options}}\r",
      "down\r",
      "down={${1:options}}\r"
    ]
  },
  "args": [
    "\\usepackage/witharrows#c"
  ]
}