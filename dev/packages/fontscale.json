{
  "deps": [],
  "macros": [
    {
      "name": "fontscalesetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "CurrentFontStep"
    },
    {
      "name": "CurrentFontScale"
    },
    {
      "name": "CurrentFontSize"
    },
    {
      "name": "CurrentFontBaselineskip"
    },
    {
      "name": "SetFontStep",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "SetFontStep*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "SetFontScale",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "SetFontScale*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "SetFontSize",
      "arg": {
        "format": "{}",
        "snippet": "{${1:size}}"
      }
    },
    {
      "name": "SetFontSize*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:size}}"
      }
    },
    {
      "name": "ScaleFont",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "SetFontSizeBaselineskip",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:size}}{${2:skip%l}}"
      }
    },
    {
      "name": "PrintFontParameters"
    },
    {
      "name": "PrintAllFontParameters"
    },
    {
      "name": "PrintSampleText"
    },
    {
      "name": "PrintSampleText",
      "arg": {
        "format": "[]",
        "snippet": "[${1:text}]"
      }
    },
    {
      "name": "PrintSampleText*"
    },
    {
      "name": "PrintSampleText*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:text}]"
      }
    },
    {
      "name": "PrintFontSizeCommand"
    }
  ],
  "envs": [],
  "keys": {
    "\\fontscalesetup": [
      "reset\r",
      "reset=${1|initial,preamble\r|}",
      "ignore-order",
      "baselineskip-size-ratio=${1:number}\r",
      "typographic-scale=${1|classic-10pt,classic-11pt,classic-12pt,modular,musical\r|}",
      "classic-10pt\r",
      "classic-11pt\r",
      "classic-12pt\r",
      "modular\r",
      "musical\r",
      "modular/ratio=${1:number}\r",
      "musical/ratio=${1:number}\r",
      "musical/notes=${1:integer}\r",
      "tiny/scale=${1:number}\r",
      "tiny/size=${1:size}\r",
      "tiny/baselineskip=",
      "tiny=${1:size}\r",
      "tiny=${1:size}/${2:baselineskip}\r",
      "scriptsize/scale=${1:number}\r",
      "scriptsize/size=${1:size}\r",
      "scriptsize/baselineskip=",
      "scriptsize=${1:size}\r",
      "scriptsize=${1:size}/${2:baselineskip}\r",
      "footnotesize/scale=${1:number}\r",
      "footnotesize/size=${1:size}\r",
      "footnotesize/baselineskip=",
      "footnotesize=${1:size}\r",
      "footnotesize=${1:size}/${2:baselineskip}\r",
      "small/scale=${1:number}\r",
      "small/size=${1:size}\r",
      "small/baselineskip=",
      "small=${1:size}\r",
      "small=${1:size}/${2:baselineskip}\r",
      "normalsize/scale=${1:number}\r",
      "normalsize/size=${1:size}\r",
      "normalsize/baselineskip=",
      "normalsize=${1:size}\r",
      "normalsize=${1:size}/${2:baselineskip}\r",
      "large/scale=${1:number}\r",
      "large/size=${1:size}\r",
      "large/baselineskip=",
      "large=${1:size}\r",
      "large=${1:size}/${2:baselineskip}\r",
      "Large/scale=${1:number}\r",
      "Large/size=${1:size}\r",
      "Large/baselineskip=",
      "Large=${1:size}\r",
      "Large=${1:size}/${2:baselineskip}\r",
      "LARGE/scale=${1:number}\r",
      "LARGE/size=${1:size}\r",
      "LARGE/baselineskip=",
      "LARGE=${1:size}\r",
      "LARGE=${1:size}/${2:baselineskip}\r",
      "huge/scale=${1:number}\r",
      "huge/size=${1:size}\r",
      "huge/baselineskip=",
      "huge=${1:size}\r",
      "huge=${1:size}/${2:baselineskip}\r",
      "Huge/scale=${1:number}\r",
      "Huge/size=${1:size}\r",
      "Huge/baselineskip=",
      "Huge=${1:size}\r",
      "Huge=${1:size}/${2:baselineskip}\r"
    ]
  },
  "args": []
}