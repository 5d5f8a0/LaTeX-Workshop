{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "pict2e"
    },
    {
      "name": "calc"
    },
    {
      "name": "array"
    },
    {
      "name": "fp"
    }
  ],
  "macros": [
    {
      "name": "diagbox",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "diagbox",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "diagbox",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:left%text}}{${2:middle%text}}{${3:right%text}}"
      }
    },
    {
      "name": "backslashbox",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "backslashbox",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:width}]{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "backslashbox",
      "unusual": true,
      "arg": {
        "format": "[][]{}{}",
        "snippet": "[${3:width}][${4:trim%keyvals}]{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "slashbox",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "slashbox",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:width}]{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "slashbox",
      "unusual": true,
      "arg": {
        "format": "[][]{}{}",
        "snippet": "[${3:width}][${4:trim%keyvals}]{${1:left%text}}{${2:right%text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\diagbox": [
      "width=",
      "height=",
      "dir=${1|NW,NE,SW,SE\r|}",
      "innerwidth=",
      "innerleftsep=",
      "innerrightsep=",
      "outerleftsep=",
      "outerrightsep=",
      "leftsep=",
      "rightsep=",
      "trim=${1|l,r,lr,rl\r|}",
      "font=${1:font commands}\r",
      "linewidth=",
      "linecolor="
    ],
    "\\backslashbox,\\slashbox": [
      "l\r",
      "r\r",
      "lr\r",
      "rl\r"
    ]
  },
  "args": []
}