{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "simplekv"
    },
    {
      "name": "xintexpr"
    },
    {
      "name": "xinttools"
    },
    {
      "name": "xstring"
    },
    {
      "name": "listofitems"
    },
    {
      "name": "csvsimple-l3"
    }
  ],
  "macros": [
    {
      "name": "PixelArtTikz",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "PixelArtTikz",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:clés%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "PixelArtTikz",
      "arg": {
        "format": "[]<>{}",
        "snippet": "[${2:clés%keyvals}]<${3:TikZ options}>{${1:file}}"
      }
    },
    {
      "name": "PixelArtTikz*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "PixelArtTikz*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:clés%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "MiniPixelArt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:liste des couleurs}}"
      }
    },
    {
      "name": "MiniPixelArt",
      "arg": {
        "format": "{}<>",
        "snippet": "{${1:liste des couleurs}}<${2:TikZ options}>"
      }
    },
    {
      "name": "MiniPixelArt",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:clés%keyvals}]{${1:liste des couleurs}}"
      }
    },
    {
      "name": "MiniPixelArt",
      "arg": {
        "format": "[]{}<>",
        "snippet": "[${2:clés%keyvals}]{${1:liste des couleurs}}<${3:TikZ options}>"
      }
    },
    {
      "name": "PixlArtTikz",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "PixlArtTikz",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "PixlArtTikz",
      "unusual": true,
      "arg": {
        "format": "[]<>{}",
        "snippet": "[${2:options%keyvals}]<${3:TikZ options}>{${1:file}}"
      }
    },
    {
      "name": "PixlArtTikz*",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
