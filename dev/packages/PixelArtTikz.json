    {
      "name": "MiniPixlArt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:list of colors}}"
      }
    },
    {
      "name": "MiniPixlArt",
      "arg": {
        "format": "{}<>",
        "snippet": "{${1:list of colors}}<${2:TikZ options}>"
      }
    },
    {
      "name": "MiniPixlArt",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:list of colors}}"
      }
    },
    {
      "name": "MiniPixlArt",
      "arg": {
        "format": "[]{}<>",
        "snippet": "[${2:options%keyvals}]{${1:list of colors}}<${3:TikZ options}>"
      }
    }
  ],
  "envs": [
    {
      "name": "EnvPixelArtTikz",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "EnvPixelArtTikz",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:clés%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "EnvPixelArtTikz",
      "arg": {
        "format": "[]<>{}",
        "snippet": "[${2:clés%keyvals}]<${3:TikZ options}>{${1:file}}"
      }
    },
    {
      "name": "EnvPixlArtTikz",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "EnvPixlArtTikz",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "EnvPixlArtTikz",
      "unusual": true,
      "arg": {
        "format": "[]<>{}",
        "snippet": "[${2:options%keyvals}]<${3:TikZ options}>{${1:file}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/PixelArtTikz#c": [
      "csvii\r"
    ],
    "\\PixelArtTikz,\\PixelArtTikz*,\\begin{EnvPixelArtTikz}": [
      "Codes=${1:chaîne}\r",
      "Couleurs={${1:couleur1,couleur2,...}}\r",
      "Symboles={${1:symbol1,symbol2,...}}\r",
      "Correction",
      "Symb",
      "BordCases",
      "Style=${1:font commands}\r",
      "Unite=${1:nombre}\r"
    ],
    "\\MiniPixelArt": [
      "Unite=",
      "Bord"
    ],
    "\\PixlArtTikz,\\PixlArtTikz*,\\begin{EnvPixlArtTikz}": [
      "Codes=${1:string}\r",
      "Colors={${1:color1,color2,...}}\r",
      "Symbols={${1:symbol1,symbol2,...}}\r",
      "Correction",
      "Symb",
      "Border",
      "Style=${1:font commands}\r",
      "Unit=${1:number}\r"
    ],
    "\\MiniPixlArt": [
      "Unit=",
      "Border"
    ]
  },
  "args": [
    "\\usepackage/PixelArtTikz#c"
  ]
}