{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "color"
    }
  ],
  "macros": [
    {
      "name": "includecombinedgraphics",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "includecombinedgraphics",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/combinedgraphics#c": [
      "textfontcmd=${1:font csname}\r",
      "textcolorcmd=${1:color csname}\r",
      "vecscale=${1:factor}\r",
      "vecwidth=",
      "vecheight=",
      "vecinclude=${1:file name}\r",
      "vecfirst"
    ],
    "\\includecombinedgraphics": [
      "textfont=${1:font commands}\r",
      "textcolor=${1:color commands}\r",
      "vecscale=${1:factor}\r",
      "vecwidth=",
      "vecheight=",
      "vecinclude=${1|auto,true,false,overwrite\r|}",
      "vecfirst",
      "vecfile=${1:file name}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "x=\r",
      "y=\r",
      "units=\r",
      "scale=${1:factor}\r",
      "hscale=${1:factor}\r",
      "vscale=${1:factor}\r",
      "height=",
      "totalheight=",
      "width=",
      "keepaspectratio\r"
    ]
  },
  "args": [
    "\\usepackage/combinedgraphics#c"
  ]
}