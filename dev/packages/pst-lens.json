{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-grad"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "PstLens",
      "arg": {
        "format": "{}",
        "snippet": "{${1:stuff}}"
      }
    },
    {
      "name": "PstLens(x,y)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:stuff}}"
      }
    },
    {
      "name": "PstLens",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:stuff}}"
      }
    },
    {
      "name": "PstLens",
      "arg": {
        "format": "[](){}",
        "snippet": "[${2:options%keyvals}](${3:x},${4:y}){${1:stuff}}"
      }
    },
    {
      "name": "PstLensShape",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\PstLens": [
      "LensHandle",
      "LensStyleHandle=${1:style}\r",
      "LensHandleWidth=",
      "LensHandleHeight=",
      "LensShadow",
      "LensStyleGlass=${1:style}\r",
      "LensSize=",
      "LensMagnification=${1:factor}\r",
      "LensRotation=${1:degrees}\r"
    ]
  },
  "args": []
}