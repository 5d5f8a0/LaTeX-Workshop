  "keys": {
    "\\usepackage/ocgx2#c": [
      "dvipdfmx\r",
      "viewocg=${1|always,never,ifvisible\r|}",
      "printocg=${1|always,never,ifvisible\r|}",
      "exportocg=${1|always,never,ifvisible\r|}",
      "showingui=${1|true,false,always,never,iffirstuse\r|}",
      "tikz",
      "ocgcolorlinks"
    ],
    "\\begin{ocg}": [
      "viewocg=${1|always,never,ifvisible\r|}",
      "printocg=${1|always,never,ifvisible\r|}",
      "exportocg=${1|always,never,ifvisible\r|}",
      "showingui=${1|true,false,always,never,iffirstuse\r|}",
      "listintoolbar=${1|true,false,always,never,iffirstuse\r|}",
      "radiobtngrps={${1:group1,group2,...}}\r"
    ],
    "\\switchocg,\\switchocg*,\\showocg,\\showocg*,\\hideocg,\\hideocg*,\\actionsocg,\\actionsocg*": [
      "onmousenter\r",
      "onmouseexit\r",
      "onmousedown\r",
      "onmouseup\r",
      "onmouseall\r"
    ],
    "\\tikzset#c,\\begin{tikzpicture}#c,\\begin{scope}#c,\\node#c": [
      "ocg={${1:options}}\r",
      "ocmd={${1:options}}\r",
      "trigger ocg=${1|onmousenter,onmouseexit,onmousedown,onmouseup,onmouseall\r|}",
      "switch ocg={${1:ocg list}}\r",
      "show ocg={${1:ocg list}}\r",
      "hide ocg={${1:ocg list}}\r",
      "actions ocg={${1:ocg list1}}{${2:ocg list2}}{${3:ocg list3}}\r",
      "switch ocg with mark on={${1:ocg ref}}{${2:ocg list}}\r",
      "switch ocg with mark off={${1:ocg ref}}{${2:ocg list}}\r"
    ]
  },
  "args": [
    "\\usepackage/ocgx2#c"
  ]
