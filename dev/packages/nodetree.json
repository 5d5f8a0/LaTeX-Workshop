{
  "deps": [
    {
      "name": "luatex"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "NodetreeSet",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/nodetree#c,\\NodetreeSet,\\nodetreeset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "NodetreeSetOption",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:value}}"
      }
    },
    {
      "name": "NodetreeResetOption",
      "arg": {
        "format": "{}",
        "snippet": "{${1:option%keyvals}}",
        "keys": [
          "\\NodetreeSetOption,\\nodetreeoption,\\NodetreeResetOption"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "NodetreeReset"
    },
    {
      "name": "NodetreeRegisterCallback",
      "arg": {
        "format": "{}",
        "snippet": "{${1:callbacks}}"
      }
    },
    {
      "name": "NodetreeUnregisterCallback",
      "arg": {
        "format": "{}",
        "snippet": "{${1:callbacks}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/nodetree#c,\\NodetreeSet,\\nodetreeset": [
      "channel=${1:output channel}\r",
      "callback={${1:callbacks}}\r",
      "verbosity=${1:integer}\r",
      "color=${1|colored,no\r|}",
      "unit=${1|pt,pc,in,bp,cm,mm,dd,cc,sp,ex,em\r|}",
      "decimalplaces=${1:integer}\r"
    ],
    "\\NodetreeSetOption,\\nodetreeoption,\\NodetreeResetOption": [
      "channel\r",
      "callback\r",
      "verbosity\r",
      "color\r",
      "unit\r",
      "decimalplaces\r"
    ]
  },
  "args": [
    "\\usepackage/nodetree#c,\\NodetreeSet,\\nodetreeset"
  ]
}