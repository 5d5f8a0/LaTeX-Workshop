{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarycalc"
    }
  ],
  "macros": [
    {
      "name": "liftarm",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:point}}{${2:length%plain}}{${3:angle}}"
      }
    },
    {
      "name": "liftarm",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:point}}{${2:length%plain}}{${3:angle}}"
      }
    },
    {
      "name": "liftarmconstruct",
      "arg": {
        "format": "{}",
        "snippet": "{${1:commands}}"
      }
    },
    {
      "name": "liftarmconstructclear"
    },
    {
      "name": "liftarmanimate",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:frame rate}}{${2:list}}{${3:commands}}"
      }
    },
    {
      "name": "liftarmanimate",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:frame rate}}{${2:list}}{${3:commands}}"
      }
    }
  ],
  "envs": [
    {
      "name": "liftarmconnect"
    },
    {
      "name": "liftarmconnect",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\liftarm#c,\\begin{liftarmconnect}#c": [
      "axle holes={${1:values}}\r",
      "brick",
      "color={${1:number}}{${2:color}}\r",
      "color modulo=${1:number}\r",
      "contour",
      "contour style={${1:options}}\r",
      "coordinate={${1:num1/name1,num2/name2/...}}\r",
      "hole radius=${1:number}\r",
      "liftarm style={${1:options}}\r",
      "liftarm thickness=${1:number}\r",
      "mark holes={${1:values}}\r",
      "mark radius=${1:factor}\r",
      "mark style={${1:options}}\r",
      "origin=${1:number}\r",
      "scalefactor=${1:factor}\r",
      "screw angle=${1:degrees}\r",
      "screw holes={${1:values}}\r",
      "screw radius=${1:factor}\r",
      "screw style={${1:options}}\r",
      "trace={${1:num1/numframes1/code1,...}}\r",
      "type=${1|liftarm,line segment\r|}"
    ],
    "\\begin{liftarmconnect}#c": [
      "connect stop=${1|1-norm,2-norm,iterations\r|}"
    ],
    "\\liftarmanimate#c": [
      "label=${1:label}\r",
      "type=${1:file ext}\r",
      "poster\r",
      "poster=${2|first,last,none,${1:number}\r|}",
      "every=${1:number}\r",
      "autopause\r",
      "autoplay\r",
      "autoresume\r",
      "loop\r",
      "palindrome\r",
      "step\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio\r",
      "scale=${1:factor}\r",
      "bb=${1:llx lly urx ury}\r",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:left bottom right top}\r",
      "clip\r",
      "hiresbb\r",
      "interpolate\r",
      "pagebox=\r",
      "controls=${1|all,true,on,none,false,off\r|}",
      "controlsaligned=\r",
      "buttonsize=",
      "buttonbg=",
      "buttonfg=",
      "buttonalpha=${1:opacity}\r",
      "draft\r",
      "final\r",
      "nomouse\r",
      "method=${1|icon,widget,ocg\r|}",
      "measure\r",
      "alttext=${2|none,${1:alt description}\r|}",
      "begin={${1:begin code}}\r",
      "end={${1:end code}}\r",
      "timeline=${1:file}\r"
    ]
  },
  "args": []
}