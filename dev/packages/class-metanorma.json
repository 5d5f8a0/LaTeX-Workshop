{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "amsmath"
    },
    {
      "name": "subcaption"
    },
    {
      "name": "enumitem"
    },
    {
      "name": "verbatim"
    },
    {
      "name": "ulem"
    },
    {
      "name": "mdframed"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "set",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:attribute%specialDef}}{${2:value}}"
      }
    },
    {
      "name": "get",
      "arg": {
        "format": "{}",
        "snippet": "{${1:attribute%keyvals}}"
      }
    },
    {
      "name": "mn",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "alt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:term%text}}"
      }
    },
    {
      "name": "deprecated",
      "arg": {
        "format": "{}",
        "snippet": "{${1:term%text}}"
      }
    },
    {
      "name": "domain",
      "arg": {
        "format": "{}",
        "snippet": "{${1:domain%text}}"
      }
    },
    {
      "name": "mncite",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key}}"
      }
    },
    {
      "name": "mncite",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:add. text%text}]{${1:key}}"
      }
    },
    {
      "name": "mncite",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:key}}[${2:localities}]"
      }
    },
    {
      "name": "mncite",
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:add. text%text}]{${1:key}}[${3:localities}]"
      }
    }
  ],
  "envs": [
    {
      "name": "example"
    },
    {
      "name": "note"
    },
    {
      "name": "source"
    },
    {
      "name": "key"
    },
    {
      "name": "requirement"
    },
    {
      "name": "recommendation"
    },
    {
      "name": "permission"
    },
    {
      "name": "specification"
    },
    {
      "name": "verification"
    },
    {
      "name": "import"
    },
    {
      "name": "tip"
    },
    {
      "name": "important"
    },
    {
      "name": "caution"
    },
    {
      "name": "warning"
    }
  ],
  "keys": {
    "\\get#c": [
      "%metanormaattribute\r"
    ],
    "\\mn#c": [
      "heading=${1:heading}\r",
      "level=${1:level}\r",
      "obligation=${1|informative,normative\r|}",
      "language=${1:lang code}\r",
      "script=${1:script name}\r",
      "reviewer=\"${1:name}\"\r",
      "from=${1:anchor}\r",
      "to=${1:anchor}\r",
      "date=${1:date}\r"
    ]
  },
  "args": []
}