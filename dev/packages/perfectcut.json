{
  "deps": [
    {
      "name": "calc"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "scalerel"
    },
    {
      "name": "mathstyle",
      "if": "mathstyle"
    }
  ],
  "macros": [
    {
      "name": "perfectcut",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "perfectbra",
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "perfectket",
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "perfectcase",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "perfectbrackets",
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "perfectparens",
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "perfectunary",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:option%keyvals}}{${2:delim1}}{${3:delim2}}{${4:arg}}",
        "keys": [
          "\\perfectunary,\\perfectbinary"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "perfectbinary",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}{}{}",
        "snippet": "{${1:option%keyvals}}{${2:delim1}}{${3:delim2}}{${4:delim3}}{${5:arg1}}{${6:arg2}}",
        "keys": [
          "\\perfectunary,\\perfectbinary"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cutbarskip",
      "unusual": true
    },
    {
      "name": "cutangleskip",
      "unusual": true
    },
    {
      "name": "cutangleouterskip",
      "unusual": true
    },
    {
      "name": "cutinterbarskip",
      "unusual": true
    },
    {
      "name": "nthleft",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:n}}{${2:delim}}"
      }
    },
    {
      "name": "nthmiddle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:n}}{${2:delim}}"
      }
    },
    {
      "name": "nthright",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:n}}{${2:delim}}"
      }
    },
    {
      "name": "lenleft",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:length}}{${2:delim}}"
      }
    },
    {
      "name": "lenmiddle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:length}}{${2:delim}}"
      }
    },
    {
      "name": "lenright",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:length}}{${2:delim}}"
      }
    },
    {
      "name": "reallenleft",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:length}}{${2:delim}}"
      }
    },
    {
      "name": "reallenmiddle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:length}}{${2:delim}}"
      }
    },
    {
      "name": "reallenright",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:length}}{${2:delim}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/perfectcut#c": [
      "nomathstyle\r",
      "mathstyle\r",
      "realVert\r",
      "fixxits\r"
    ],
    "\\perfectunary,\\perfectbinary": [
      "IncreaseHeight\r",
      "CurrentHeight\r",
      "CurrentHeightPlusOne\r"
    ]
  },
  "args": [
    "\\usepackage/perfectcut#c"
  ]
}