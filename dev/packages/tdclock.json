    {
      "name": "pdfslash",
      "unusual": true
    },
    {
      "name": "pdfcolon",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/tdclock#c": [
      "font=${1|Helv,HelvB,HelvI,HelvBI,Times,TimesB,TimesI,TimesBI,Cour,CourB,CourI,CourBI\r|}",
      "timeinterval=${1:integer}\r",
      "timeduration=${1:minutes}\r",
      "timewarningfirst=${1:integer 0-100}\r",
      "timewarningsecond=${1:integer 0-100}\r",
      "timedeath=${1:0|1}\r",
      "colorwarningfirst=",
      "colorwarningsecond=",
      "fillcolorwarningfirst=",
      "fillcolorwarningsecond=",
      "resetatpages=${3|none,all,${1:integer},{${2:list of integers}}\r|}",
      "pageresetcontrol=${1|pdfpagelabels,pdfpagenumbers\r|}"
    ]
  },
  "args": [
    "\\usepackage/tdclock#c"
  ]
}