{
  "deps": [
    {
      "name": "tocbasic"
    },
    {
      "name": "floatrow"
    },
    {
      "name": "xpatch"
    }
  ],
  "macros": [
    {
      "name": "DeclareFloatType",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:options%keyvals}}",
        "keys": [
          "\\DeclareFloatType,\\ProvideFloatType,\\RedeclareFloatType"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "ProvideFloatType",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:options%keyvals}}",
        "keys": [
          "\\DeclareFloatType,\\ProvideFloatType,\\RedeclareFloatType"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "RedeclareFloatType",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:options%keyvals}}",
        "keys": [
          "\\DeclareFloatType,\\ProvideFloatType,\\RedeclareFloatType"
        ],
        "keyPos": 1
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\DeclareFloatType,\\ProvideFloatType,\\RedeclareFloatType": [
      "placement=${1:combination of t,b,h,p}\r",
      "name=${1:name}\r",
      "fileext=${1:ext}\r",
      "within=${1:counter}\r",
      "relatedcapstyle=${1|yes,no\r|}"
    ]
  },
  "args": []
}