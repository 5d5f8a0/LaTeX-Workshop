{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "njuemblem",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuemblem",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:选项%keyvals}]{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuname",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuname",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:选项%keyvals}]{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuname*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuname*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:选项%keyvals}]{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njumotto",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njumotto",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:color}]{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuspirit",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuspirit",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:color}]{${1:宽度}}{${2:高度}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/njuvisual#c": [
      "force\r"
    ],
    "\\njuemblem,\\njuname,\\njuname*": [
      "color=",
      "color*=",
      "%color\r",
      "department=${1:院系名称}\r"
    ]
  },
  "args": [
    "\\usepackage/njuvisual#c"
  ]
}