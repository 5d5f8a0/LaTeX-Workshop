{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "ProcessOptionsBeamer",
      "unusual": true
    },
    {
      "name": "ExecuteOptionsBeamer",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:options}}"
      }
    },
    {
      "name": "DeclareOptionBeamer",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:option}}{${2:definition}}"
      }
    },
    {
      "name": "DeclareOptionBeamer",
      "unusual": true,
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:option}}[${3:default}]{${2:definition}}"
      }
    },
    {
      "name": "defbeameroption",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:option%specialDef}}{${2:definition}}"
      }
    },
    {
      "name": "setbeameroption",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\setbeameroption#c": [
      "%beameroption\r"
    ]
  },
  "args": []
}