{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-plot"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "readpsbardata",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:macro%cmd}}{${2:filename%file}}"
      }
    },
    {
      "name": "readpsbardata",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:macro%cmd}}{${2:filename%file}}"
      }
    },
    {
      "name": "psbarchart",
      "arg": {
        "format": "{}",
        "snippet": "{${1:data macro}}"
      }
    },
    {
      "name": "psbarchart",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:data macro}}"
      }
    },
    {
      "name": "newpsbarstyle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "psbarlabel",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "psbarlabelsep",
      "unusual": true
    },
    {
      "name": "psbarscale(scale)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:PostScript code}}"
      }
    },
    {
      "name": "setbarstyle",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:style}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\readpsbardata#c": [
      "header"
    ],
    "\\psbarchart#c": [
      "chartstyle=${1|cluster,stack,block\r|}",
      "barstyle={${1:style list}}\r",
      "barcolsep=${1:factor}\r",
      "barsep=${1:factor}\r",
      "barlabelrot=${1:degrees}\r",
      "orientation=${1|vertical,horizontal\r|}"
    ]
  },
  "args": []
}