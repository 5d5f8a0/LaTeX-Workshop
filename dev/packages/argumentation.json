    },
    {
      "name": "pgfopts"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarypositioning"
    },
    {
      "name": "tikzlibrarydecorations.markings"
    }
  ],
  "macros": [
    {
      "name": "setargumentstyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style}}"
      }
    },
    {
      "name": "setattackstyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style}}"
      }
    },
    {
      "name": "setsupportstyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style}}"
      }
    },
    {
      "name": "setannotationstyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style}}"
      }
    },
    {
      "name": "argument",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "argument",
      "arg": {
        "format": "(){}",
        "snippet": "(${2:id}){${1:name}}"
      }
    },
    {
      "name": "argument",
      "arg": {
        "format": "[](){}",
        "snippet": "[${2:options}](${3:id}){${1:name}}"
