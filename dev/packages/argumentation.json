    {
      "name": "dualattack",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:id1}}{${2:id2}}"
      }
    },
    {
      "name": "dualattack",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options}]{${1:id1}}{${2:id2}}"
      }
    },
    {
      "name": "selfattack",
      "arg": {
        "format": "{}",
        "snippet": "{${1:id}}"
      }
    },
    {
      "name": "selfattack",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:id}}"
      }
