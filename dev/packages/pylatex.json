{
  "deps": [
    {
      "name": "comment"
    },
    {
      "name": "listings"
    },
    {
      "name": "keyval"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "pymacros"
    }
  ],
  "macros": [
    {
      "name": "pyverb",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    },
    {
      "name": "PySetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    }
  ],
  "envs": [
    {
      "name": "python"
    }
  ],
  "keys": {
    "\\PySetup": [
      "action=${1|show,hide,verbatim\r|}"
    ]
  },
  "args": []
}