{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-3dplot"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psdiffractionRectangle"
    },
    {
      "name": "psdiffractionRectangle",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "psdiffractionCircular"
    },
    {
      "name": "psdiffractionCircular",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "psdiffractionTriangle"
    },
    {
      "name": "psdiffractionTriangle",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psdiffractionRectangle": [
      "a=${1:area}\r",
      "Alpha=${1:number}\r",
      "colorMode=${1|0,1,2,3\r|}",
      "contrast=${1:number}\r",
      "f=${1:focal length}\r",
      "IIID",
      "k=${1:height}\r",
      "lambda=${1:wavelength in nm}\r",
      "pixel=${1:factor}\r",
      "s=${1:distance}\r",
      "twoSlit\r"
    ],
    "\\psdiffractionCircular": [
      "colorMode=${1|0,1,2,3\r|}",
      "contrast=${1:number}\r",
      "d=${1:distance}\r",
      "f=${1:focal length}\r",
      "IIID",
      "lambda=${1:wavelength in nm}\r",
      "pixel=${1:factor}\r",
      "r=${1:radius}\r",
      "twoHole\r"
    ],
    "\\psdiffractionTriangle": [
      "colorMode=${1|0,1,2,3\r|}",
      "contrast=${1:number}\r",
      "f=${1:focal length}\r",
      "h=${1:height}\r",
      "scale=${1:factor}\r",
      "IIID",
      "lambda=${1:wavelength in nm}\r"
    ]
  },
  "args": []
}