{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "iflang"
    }
  ],
  "macros": [
    {
      "name": "hpsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/hpstatement#c,\\hpsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "hpnumber",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "hpnumber",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:text1%text}]{${1:number}}"
      }
    },
    {
      "name": "hpnumber",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:text1%text}][${3:text2%text}]{${1:number}}"
      }
    },
    {
      "name": "hpstatement",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "hpstatement",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:text1%text}]{${1:number}}"
      }
    },
    {
      "name": "hpstatement",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:text1%text}][${3:text2%text}]{${1:number}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/hpstatement#c,\\hpsetup": [
      "date=${1:YYYY-MM-DD}\r"
    ]
  },
  "args": [
    "\\usepackage/hpstatement#c,\\hpsetup"
  ]
}