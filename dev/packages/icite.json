{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "datatool"
    },
    {
      "name": "usebib"
    }
  ],
  "macros": [
    {
      "name": "SetTitleStyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style code}}"
      }
    },
    {
      "name": "AuthorTitleDelim",
      "arg": {
        "format": "{}",
        "snippet": "{${1:delim}}"
      }
    },
    {
      "name": "TitlePageDelim",
      "arg": {
        "format": "{}",
        "snippet": "{${1:delim}}"
      }
    },
    {
      "name": "icite",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bibid}}"
      }
    },
    {
      "name": "icite",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:pre}]{${1:bibid}}"
      }
    },
    {
      "name": "icite",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:pre}][${3:post}]{${1:bibid}}"
      }
    },
    {
      "name": "icite",
      "arg": {
        "format": "[][]{}[]",
        "snippet": "[${2:pre}][${3:post}]{${1:bibid}}[${4:csname}]"
      }
    },
    {
      "name": "IndexSubtypeAs",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:subtype}}{${2:index name}}"
      }
    }
  ],
  "envs": [],
  "keys": {},
  "args": []
}