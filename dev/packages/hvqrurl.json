{
  "includes": {
    "qrcode": [],
    "xcolor": [],
    "marginnote": [],
    "url": []
  },
  "macros": {
    "hvqrset{}": {
      "snippet": "hvqrset{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "hvqrurl{}": {
      "snippet": "hvqrurl{${1:URL}}"
    },
    "hvqrurl[]{}": {
      "snippet": "hvqrurl[${2:options}]{${1:URL}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "hvqrurl*{}": {
      "snippet": "hvqrurl*{${1:URL}}"
    },
    "hvqrurl*[]{}": {
      "snippet": "hvqrurl*[${2:options}]{${1:URL}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "envs": {},
  "options": [
    "nohyperref",
    "forget"
  ],
  "keyvals": [
    [
      "qrlink=${1|nolink,link|}",
      "qrcolor=",
      "qradjust=",
      "qrheight=",
      "qrlevel=${1|L,M,Q,H|}",
      "qrreverse",
      "linktext=${1:text}"
    ]
  ]
}