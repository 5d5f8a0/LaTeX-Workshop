{
  "deps": [
    {
      "name": "qrcode"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "marginnote"
    },
    {
      "name": "url"
    }
  ],
  "macros": [
    {
      "name": "hvqrset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\hvqrset,\\hvqrurl,\\hvqrurl*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "hvqrurl",
      "arg": {
        "format": "{}",
        "snippet": "{${1:URL}}"
      }
    },
    {
      "name": "hvqrurl",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:URL}}"
      }
    },
    {
      "name": "hvqrurl*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:URL}}"
      }
    },
    {
      "name": "hvqrurl*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:URL}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/hvqrurl#c": [
      "nohyperref\r",
      "forget\r"
    ],
    "\\hvqrset,\\hvqrurl,\\hvqrurl*": [
      "qrlink=${1|nolink,link\r|}",
      "qrcolor=",
      "qradjust=",
      "qrheight=",
      "qrlevel=${1|L,M,Q,H\r|}",
      "qrreverse\r",
      "linktext=${1:text}\r"
    ]
  },
  "args": [
    "\\usepackage/hvqrurl#c"
  ]
}