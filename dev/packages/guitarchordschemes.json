{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibraryshapes.misc"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "cnltx-base"
    }
  ],
  "macros": [
    {
      "name": "chordscheme"
    },
    {
      "name": "chordscheme",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "scales"
    },
    {
      "name": "scales",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "setfingering",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:name}}{${2:fingers}}{${3:roots}}"
      }
    },
    {
      "name": "setchordscheme",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\setchordscheme,\\usepackage/guitarchordschemes#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "rootsymbol",
      "unusual": true
    },
    {
      "name": "showrootsymbol",
      "unusual": true
    },
    {
      "name": "ringingstring",
      "unusual": true
    },
    {
      "name": "mutedstring",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\chordscheme": [
      "fret-number=${1:number}\r",
      "name=${1:chord symbol}\r",
      "position=${1:position}\r",
      "finger=${1:fret}/${2:string}:${3:label}\r",
      "root=${1:fret}/${2:string}:${3:label}\r",
      "show-root=${1:fret}/${2:string}\r",
      "barre=${1:fret}/${2:string range}:${3:label}\r",
      "ring={${1:strings}}\r",
      "mute={${1:strings}}\r"
    ],
    "\\scales#c": [
      "fret-number=${1:number}\r",
      "name=${1:title}\r",
      "position=${1:position}\r",
      "finger=${1:fret}/${2:string}:${3:label}\r",
      "root=${1:fret}/${2:string}:${3:label}\r",
      "fingering=${1|type 1,type 1A,type 2,type 3,type 4\r|}",
      "fingering*=${1|type 1,type 1A,type 2,type 3,type 4\r|}",
      "fingering?=${1|type 1,type 1A,type 2,type 3,type 4\r|}"
    ],
    "\\setchordscheme,\\usepackage/guitarchordschemes#c": [
      "x-unit=",
      "y-unit=",
      "rotate=${1:angle}\r",
      "finger-format={${1:TeX code}}\r",
      "finger-format+={${1:TeX code}}\r",
      "position-format={${1:TeX code}}\r",
      "position-format+={${1:TeX code}}\r",
      "name-format={${1:TeX code}}\r",
      "name-format+={${1:TeX code}}\r",
      "name-below",
      "name-distance=",
      "chord-name-cs=${1:cs}\r",
      "scales-name-cs=${1:cs}\r",
      "string-name-format={${1:TeX code}}\r",
      "string-name-format+={${1:TeX code}}\r",
      "strings=${1:number}\r",
      "chord-frets=${1:number}\r",
      "scales-frets=${1:number}\r",
      "line-width=",
      "finger-radius=${1:number}\r",
      "finger-x-offset=${1:number}\r",
      "finger-y-offset=${1:number}\r",
      "finger-style={${1:TikZ style}}\r",
      "root-style={${1:TikZ style}}\r",
      "show-root-style={${1:TikZ style}}\r",
      "ringing-style={${1:TikZ style}}\r",
      "muted-style={${1:TikZ style}}\r",
      "tuning={${1:strings}}\r",
      "restrict-bounding-box"
    ]
  },
  "args": [
    "\\setchordscheme,\\usepackage/guitarchordschemes#c"
  ]
}