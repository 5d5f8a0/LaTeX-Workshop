{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "scsnowman"
    },
    {
      "name": "scsnowman",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "scsnowmandefault",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "scsnowmannumeral",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "scsnowmannumeral",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:number}}"
      }
    },
    {
      "name": "usescsnowmanlibrary",
      "arg": {
        "format": "{}",
        "snippet": "{${1:library%file}}"
      }
    },
    {
      "name": "makeitemsnowman",
      "unusual": true
    },
    {
      "name": "makeitemother",
      "unusual": true
    },
    {
      "name": "makeqedsnowman",
      "unusual": true
    },
    {
      "name": "makeqedother",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\scsnowman#c,\\scsnowmandefault#c,\\scsnowmannumeral#c": [
      "scale=${1:factor}\r",
      "adjustbaseline",
      "body\r",
      "body=",
      "eyes\r",
      "eyes=",
      "mouth\r",
      "mouth=",
      "nose\r",
      "nose=",
      "sweat\r",
      "sweat=",
      "arms\r",
      "arms=",
      "hat\r",
      "hat=",
      "muffler\r",
      "muffler=",
      "buttons\r",
      "buttons=",
      "snow\r",
      "snow=",
      "note\r",
      "note=",
      "broom\r",
      "broom=",
      "shape=\r",
      "mouthshape=${1|smile,tight,frown\r|}",
      "mikan=",
      "leaf="
    ],
    "\\pagenumbering#c": [
      "enumsnowman\r"
    ]
  },
  "args": []
}