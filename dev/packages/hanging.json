{
  "deps": [],
  "macros": [
    {
      "name": "hangpara",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:indent%l}}{${2:afternum}}"
      }
    },
    {
      "name": "nhpt",
      "unusual": true
    },
    {
      "name": "nhlq",
      "unusual": true
    },
    {
      "name": "nhrq",
      "unusual": true
    },
    {
      "name": "activatepunct",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "hangparas",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:indent%l}}{${2:afternum}}"
      }
    },
    {
      "name": "hangpunct"
    }
  ],
  "keys": {
    "\\usepackage/hanging#c": [
      "notcomma\r",
      "notperiod\r",
      "notquote\r",
      "notquery\r",
      "notexcl\r",
      "notcolon\r",
      "notscolon\r",
      "fine\r"
    ]
  },
  "args": [
    "\\usepackage/hanging#c"
  ]
}