{
  "deps": [
    {
      "name": "epic"
    },
    {
      "name": "graphicx"
    }
  ],
  "macros": [
    {
      "name": "setOverpic",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\begin{overpic},\\begin{Overpic},\\setOverpic"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [
    {
      "name": "overpic",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "overpic",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "Overpic",
      "arg": {
        "format": "{}",
        "snippet": "{${1:tex-code}}"
      }
    },
    {
      "name": "Overpic",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:tex-code}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/overpic#c": [
      "abs\r",
      "percent\r",
      "permil\r"
    ],
    "\\begin{overpic},\\begin{Overpic},\\setOverpic": [
      "abs",
      "percent",
      "permil",
      "rel=${1:number}\r",
      "grid",
      "tics=${1:number}\r",
      "unit=",
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": [
    "\\usepackage/overpic#c"
  ]
}