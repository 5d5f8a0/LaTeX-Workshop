  "envs": [],
  "keys": {
    "\\usepackage/digicap-pro#c": [
      "display1\r"
    ],
    "\\digiCap,\\digiCap*,\\dcFirstOpt": [
      "outerboxsep=",
      "vcaption=${1|b,c,t\r|}",
      "hcaption=${1|l,c,r\r|}",
      "inclgraphicx={${1:\\includegraphics keys}}\r",
      "rollovername=${1:name}\r"
    ],
    "\\opcolorbox,\\dcSecondOpt": [
      "borderwidth=",
      "fboxsep=",
      "width=",
      "bordercolor=",
      "bgcolor=",
      "borderop=${1:factor}\r",
      "bgop=${1:factor}\r",
      "textop=${1:factor}\r",
      "borderblendmode=${1:mode}\r",
      "bgblendmode=${1:mode}\r"
    ],
    "\\setThumbAppearances": [
      "normalop=${1:factor}\r",
      "rolloverop=${1:factor}\r",
      "downop=${1:factor}\r",
      "boundarywidth\r",
      "rgbcolor=${1:r g b}\r",
      "cmykcolor=${1:c m y k}\r"
    ]
  },
  "args": [
    "\\usepackage/digicap-pro#c"
  ]
}