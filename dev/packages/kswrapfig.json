{
  "includes": {
    "environ": [],
    "calc": [],
    "keycommand": [],
    "xifthen": [],
    "picinpar": [],
    "picins": [],
    "ksinsbox": [
      "insboxonly",
      "insbox"
    ]
  },
  "macros": {
    "kswrapfig{}{}": {
      "snippet": "kswrapfig{${1:imagefile}}{${2:text}}"
    },
    "kswrapfig[]{}{}": {
      "snippet": "kswrapfig[${3:options}]{${1:imagefile}}{${2:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "kswrapfigline[]{}{}": {
      "snippet": "kswrapfigline[${3:options}]{${1:imagefile}}{${2:text}}",
      "keyvalindex": 1,
      "keyvalpos": 0
    },
    "tightlist": {}
  },
  "envs": {
    "KSwrapfig{}{}": {
      "name": "KSwrapfig",
      "snippet": "{${1:options}}{${2:imagefile}}"
    },
    "KSwrapfigline{}{}": {
      "name": "KSwrapfigline",
      "snippet": "{${1:options}}{${2:imagefile}}"
    }
  },
  "options": [
    "insboxonly",
    "clscaption",
    "insbox"
  ],
  "keyvals": [
    [
      "Pos=${1|r,l|}",
      "Width=",
      "InPos=${1|r,c,l|}",
      "Sep=",
      "Indent=",
      "Lower=",
      "Caption={${1:text}}",
      "LastLineSkip=",
      "FirstLineSkip=",
      "List=${1:env name}",
      "VAdjust=",
      "UseBox"
    ],
    [
      "Line=${1:num lines}",
      "Pos=${1|r,l,c|}",
      "CaptionName=${1:env name}",
      "Width=",
      "InPos=${1|r,c,l|}",
      "Sep=",
      "Indent=",
      "Lower=",
      "Caption={${1:text}}",
      "LastLineSkip=",
      "FirstLineSkip=",
      "List=${1:env name}",
      "VAdjust=",
      "UseBox"
    ]
  ]
}