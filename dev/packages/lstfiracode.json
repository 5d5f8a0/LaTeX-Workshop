{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "listings"
    }
  ],
  "macros": [
    {
      "name": "ActivateVerbatimLigatures"
    },
    {
      "name": "DeactivateVerbatimLigatures"
    },
    {
      "name": "RestoreVerbatimBehavior"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/lstfiracode#c": [
      "verbatim"
    ],
    "\\lstset,\\lstinline,\\begin{lstlisting},\\lstinputlisting,\\lstMakeShortInline": [
      "style=FiraCodeStyle\r",
      "moreliterate=\r"
    ]
  },
  "args": [
    "\\usepackage/lstfiracode#c"
  ]
}