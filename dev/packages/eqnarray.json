{
  "deps": [
    {
      "name": "array"
    }
  ],
  "macros": [
    {
      "name": "yesnumber"
    },
    {
      "name": "eqnnum",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    }
  ],
  "envs": [
    {
      "name": "equationarray",
      "arg": {
        "format": "{}",
        "snippet": "{${1:preamble}}"
      }
    },
    {
      "name": "equationarray",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:alignment%keyvals}]{${1:preamble}}"
      }
    },
    {
      "name": "equationarray*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:alignment%keyvals}]{${1:preamble}}"
      }
    }
  ],
  "keys": {
    "\\begin{equationarray},\\begin{equationarray*}": [
      "l\r",
      "c\r",
      "r\r"
    ]
  },
  "args": []
}