{
  "deps": [
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "newref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:ref types}}"
      }
    },
    {
      "name": "newref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:ref types}}"
      }
    },
    {
      "name": "setrefstyle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:ref types}}{${2:options%keyvals}}",
        "keys": [
          "\\setrefstyle"
        ],
        "keyPos": 1
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\newref": [
      "namespace=${1:string}\r",
      "nonamespace\r",
      "scope=${1:counter}\r"
    ],
    "\\setrefstyle": [
      "refcmd=${1:command}\r",
      "sep=${1:command}\r",
      "last sep\r",
      "last sep=${1:command}\r",
      "prefix=${1:command}\r",
      "suffix=${1:command}\r"
    ]
  },
  "args": []
}