{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "ifplatform"
    },
    {
      "name": "graphicx"
    }
  ],
  "macros": [
    {
      "name": "autopdfoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/autopdf#c,\\autopdfoptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "autopdfinclude"
    },
    {
      "name": "autopdfendinclude"
    },
    {
      "name": "autopdfpsfrag",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:tag}}{${2:content%text}}"
      }
    },
    {
      "name": "autopdfpsfrag",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:tag}}{${2:content%text}}"
      }
    },
    {
      "name": "autopdfpsfoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\autopdfpsfrag,\\autopdfpsfoptions"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/autopdf#c,\\autopdfoptions": [
      "from=${1:file extension}\r",
      "to=${1:file extension}\r",
      "logfile\r",
      "nologfile\r",
      "showcmds",
      "cleanup",
      "scale",
      "margin=",
      "resolution=${1:dpi}\r",
      "gscmd=${1:GhostScript cmd}\r",
      "gmidentifycmd=${1:GraphicsMagick cmd}\r",
      "gmconvertcmd=${1:GraphicsMagick cmd}\r"
    ],
    "\\autopdfpsfrag,\\autopdfpsfoptions": [
      "texpos=${1:LaTeX ref point}\r",
      "epspos=${1:PS ref point}\r",
      "scale=${1:factor}\r",
      "angle=${1:degrees}\r",
      "add=${1:text}\r"
    ]
  },
  "args": [
    "\\usepackage/autopdf#c,\\autopdfoptions"
  ]
}