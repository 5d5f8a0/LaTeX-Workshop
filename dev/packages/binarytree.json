{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "BinaryTree",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:path spec}}{${2:depth}}"
      }
    },
    {
      "name": "btreeset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "btreesetexternal",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\BinaryTree#c,\\btreeset#c": [
      "default\r",
      "grow=${1|up,down,left,right,none\r|}",
      "root label anchor=\r",
      "left label anchor=\r",
      "right label anchor=\r",
      "final label anchor=\r",
      "root edge",
      "draw missing",
      "label on every edge",
      "math labels",
      "continue at path end",
      "continue after turn",
      "default color=",
      "default color after turn",
      "xscale=\r",
      "yscale=\r",
      "scale=\r",
      "label distance=",
      "sibling distance=",
      "level distance=",
      "sibling distance scales",
      "level distance scales",
      "top padding=",
      "bottom padding=",
      "left padding=",
      "right padding=",
      "framed",
      "separate",
      "external",
      "external/use automatic file name",
      "external/file name=${1:file}\r"
    ],
    "\\btreesetexternal#c": [
      "use automatic file name"
    ]
  },
  "args": []
}