{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "pgfopts"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "tikzlibrarydecorations"
    },
    {
      "name": "tikzlibrarydecorations.markings"
    },
    {
      "name": "tikzlibrarydecorations.pathmorphing"
    },
    {
      "name": "tikzlibrarydecorations.pathreplacing"
    },
    {
      "name": "tikzlibrarygraphs"
    },
    {
      "name": "tikzlibrarypatterns"
    },
    {
      "name": "tikzlibrarypositioning"
    },
    {
      "name": "tikzlibraryshapes.geometric"
    },
    {
      "name": "tikzlibrarygraphdrawing"
    }
  ],
  "macros": [
    {
      "name": "tikzfeynmanset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "diagram",
      "arg": {
        "format": "{}",
        "snippet": "{${1:contents}}"
      }
    },
    {
      "name": "diagram",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:contents}}"
      }
    },
    {
      "name": "diagram*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:contents}}"
      }
    },
    {
      "name": "diagram*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:contents}}"
      }
    }
  ],
  "envs": [
    {
      "name": "feynman"
    },
    {
      "name": "feynman",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\feynmandiagram#c,\\tikzfeynmanset#c,\\begin{feynman}#c,\\diagram#c,\\diagram*#c": [
      "execute at begin feynman={${1:code}}\r",
      "execute at end feynman={${1:code}}\r",
      "every feynman=${1:styles}\r",
      "inline=${1:node}\r",
      "baseline=${1:node}\r",
      "horizontal=${1:node1} to ${2:node2}\r",
      "horizontal'=${1:node1} to ${2:node2}\r",
      "vertical=${1:node1} to ${2:node2}\r",
      "vertical'=${1:node1} to ${2:node2}\r",
      "every diagram=${1:styles}\r",
      "small\r",
      "medium\r",
      "large\r",
      "spring layout\r",
      "spring electrical layout\r",
      "layered layout\r",
      "tree layout\r",
      "edges=${1:options}\r",
      "nodes=${1:options}\r",
      "vertex\r",
      "every small=${1:styles}\r",
      "every medium=${1:styles}\r",
      "every large=${1:styles}\r",
      "every vertex=${1:styles}\r",
      "every dot=${1:styles}\r",
      "every square dot=${1:styles}\r",
      "every empty dot=${1:styles}\r",
      "every crossed dot=${1:styles}\r",
      "every particle=${1:styles}\r",
      "every blob=${1:styles}\r",
      "every edge=${1:styles}\r",
      "every boson=${1:styles}\r",
      "every photon=${1:styles}\r",
      "every charges boson=${1:styles}\r",
      "every anti charged boson=${1:styles}\r",
      "every scalar=${1:styles}\r",
      "every anti charge scalar=${1:styles}\r",
      "every ghost=${1:styles}\r",
      "every fermion=${1:styles}\r",
      "every anti fermion=${1:styles}\r",
      "eveyr majorana=${1:styles}\r",
      "eveyr anti majorana=${1:styles}\r",
      "every gluon=${1:styles}\r"
    ],
    "\\vertex#c": [
      "right=${1:length} of ${2:node}\r",
      "below=${1:length} of ${2:node}\r",
      "above=${1:length} of ${2:node}\r",
      "left=${1:length} of ${2:node}\r",
      "above right=of ${1:node}\r",
      "above left=of ${1:node}\r",
      "below right=of ${1:node}\r",
      "below left=of ${1:node}\r"
    ],
    "\\tikzfeynmanset#c": [
      "compat=\r"
    ]
  },
  "args": []
}