{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "xintexpr"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "mathtools"
    }
  ],
  "macros": [
    {
      "name": "SetMathEnvironmentSinglespace",
      "arg": {
        "format": "{}",
        "snippet": "{${1:factor}}"
      }
    },
    {
      "name": "RestoreMathEnvironmentLeading",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "SetTextEnvironmentSinglespace",
      "arg": {
        "format": "{}",
        "snippet": "{${1:factor}}"
      }
    },
    {
      "name": "RestoreTextEnvironmentLeading",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/zhlineskip#c": [
      "bodytextleadingratio=${1:number}\r",
      "footnoteleadingratio=${1:number}\r",
      "restoremathleading",
      "UseMSWordMultipleLineSpacing",
      "MSWordLineSpacingMultiple=${1:number}\r",
      "MSWordSinglespaceRatio=${1:number}\r"
    ]
  },
  "args": [
    "\\usepackage/zhlineskip#c"
  ]
}