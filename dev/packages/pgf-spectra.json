{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "pgfspectra"
    },
    {
      "name": "pgfspectra",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "wlcolor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:wavelength}}"
      }
    },
    {
      "name": "pgfspectraStyle",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "pgfspectraStyleReset"
    },
    {
      "name": "pgfspectrashade(start,end)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "pgfspectrashade",
      "arg": {
        "format": "[](){}",
        "snippet": "[${2:h|v}](${3:start},${4:end}){${1:name}}"
      }
    },
    {
      "name": "usepgfspectralibrary",
      "arg": {
        "format": "{}",
        "snippet": "{${1:libraries%keyvals}}"
      }
    },
    {
      "name": "pgfspectradata",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\pgfspectradata"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfspectradata",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:options%keyvals}}",
        "keys": [
          "\\pgfspectradata"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "pgfspectratable"
    },
    {
      "name": "pgfspectratable",
      "arg": {
        "format": "{}",
        "snippet": "{${1:dataset names}}"
      }
    },
    {
      "name": "pgfspectratable",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "pgfspectratable",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:dataset names}}"
      }
    },
    {
      "name": "pgfspectrawrite"
    },
    {
      "name": "pgfspectrawrite",
      "arg": {
        "format": "{}",
        "snippet": "{${1:dataset names}}"
      }
    },
    {
      "name": "pgfspectrawrite",
      "arg": {
        "format": "[]",
        "snippet": "[${1:filename}]"
      }
    },
    {
      "name": "pgfspectrawrite",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:filename}]{${1:dataset names}}"
      }
    },
    {
      "name": "pgfspectraplotmap",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "pgfspectraplotmap",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:l|h}]{${1:name}}"
      }
    },
    {
      "name": "pgfspectraplotshade",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "pgfspectraplotshade",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:name}}"
      }
    },
    {
      "name": "tempercolor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:Kelvin}}"
      }
    },
    {
      "name": "pgfspectrarainbow",
      "arg": {
        "format": "{}",
        "snippet": "{${1:radius}}"
      }
    },
    {
      "name": "pgfspectrarainbow",
      "arg": {
        "format": "[](){}",
        "snippet": "[${2:TikZ options}](${3:options%keyvals}){${1:radius}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pgf-spectra#c": [
      "LSE\r",
      "NIST\r"
    ],
    "\\pgfspectra,\\pgfspectraStyle,\\pgfspectraplotshade": [
      "use visible shading",
      "width=",
      "height=",
      "element=${1:symbol(s)}\r",
      "charge=${1:<integer> or all}\r",
      "Imin=${1:factor}\r",
      "relative intensity",
      "relative intensity threshold=${1:factor}\r",
      "line intensity=${1:number}\r",
      "gamma=${1:number}\r",
      "brightness=${1:factor}\r",
      "back=",
      "backIRUV=",
      "IRcolor=",
      "UVcolor=",
      "lines={${1:wavelengths}}\r",
      "line width=",
      "begin=${1:wavelength}\r",
      "end=${1:wavelength}\r",
      "absorption",
      "axis",
      "axis step=${1:integer}\r",
      "axis ticks=${1:integer}\r",
      "axis unit=${1|nm,micron,A\r|}",
      "axis unit precision=${1:integer}\r",
      "axis label",
      "axis label text={${1:text}}\r",
      "axis label position=${1|left,center,right\r|}",
      "axis color=",
      "axis font=${1:font commands}\r",
      "axis font color=",
      "label",
      "label position=${1|north west,north,north east,west,east,south west,south,south east\r|}",
      "label font=${1:font commands}\r",
      "label font color=",
      "label before text=${1:text}\r",
      "label after text=${1:text}\r",
      "redshift=${1:value}\r",
      "show redshift value"
    ],
    "\\usepgfspectralibrary#c": [
      "data\r",
      "pgfplots\r",
      "tempercolor\r",
      "rainbow\r"
    ],
    "\\pgfspectradata": [
      "element=${1:symbol(s)}\r",
      "charge=${1:<integer> or all}\r",
      "Imin=${1:factor}\r",
      "redshift=${1:value}\r",
      "begin=${1:wavelength}\r",
      "end=${1:wavelength}\r",
      "relative intensity",
      "precision=${1:integer}\r",
      "unit=${1|nm,micron,A\r|}"
    ],
    "\\pgfspectratable": [
      "title={${1:text}}\r",
      "back color=",
      "data back color=",
      "text color=",
      "width=",
      "elements column width="
    ],
    "\\pgfspectraplotshade": [
      "shade begin=${1:value}\r",
      "shade end=${1:value}\r",
      "shade opacity=${1:factor}\r",
      "shade opacity color=",
      "logarithmic"
    ],
    "\\pgfspectrarainbow": [
      "rainbow start=${1:fraction}\r",
      "rainbow knock out=${1:value}\r",
      "rainbow fade=${1:fading}\r",
      "rainbow transparency=${1:factor}\r",
      "rainbow background="
    ]
  },
  "args": [
    "\\usepackage/pgf-spectra#c"
  ]
}