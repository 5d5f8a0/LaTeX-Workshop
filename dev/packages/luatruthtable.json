{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "amsmath"
    },
    {
      "name": "luacode"
    }
  ],
  "macros": [
    {
      "name": "luaTruthTable",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:var1,var2,...}}{${2:expr1,expr2,...}}"
      }
    },
    {
      "name": "luaTruthTable",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:var1,var2,...}}{${2:expr1,expr2,...}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\luaTruthTable": [
      "trtext=${1:text}\r",
      "fltext=${1:text}\r"
    ]
  },
  "args": []
}