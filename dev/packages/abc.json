{
  "deps": [
    {
      "name": "ifluatex"
    },
    {
      "name": "verbatim"
    },
    {
      "name": "keyval"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "shellesc"
    }
  ],
  "macros": [
    {
      "name": "abcinput",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "abcinput",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "abcwidth",
      "unusual": true
    },
    {
      "name": "normalabcoutputfile",
      "unusual": true
    },
    {
      "name": "normalmupoutputfile",
      "if": "mup",
      "unusual": true
    },
    {
      "name": "mupinput",
      "if": "mup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "mupinput",
      "if": "mup",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "mupwidth",
      "if": "mup",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "abc"
    },
    {
      "name": "abc",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "mup",
      "if": "mup"
    },
    {
      "name": "mup",
      "if": "mup",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/abc#c": [
      "shellescape\r",
      "noshellescape\r",
      "generate\r",
      "nogenerate\r",
      "ps2epsi\r",
      "ps2epsidos\r",
      "ps2eps\r",
      "nosaveall\r",
      "saveall\r",
      "mup\r"
    ],
    "\\begin{abc},\\abcinput,\\begin{mup},\\mupinput": [
      "name=${1:name}\r",
      "options={${1:abcm2ps options}}\r",
      "postoptions={${1:abcm2ps options}}\r",
      "program=${1:program}\r",
      "width=",
      "center",
      "extension=${1:extension}\r"
    ]
  },
  "args": [
    "\\usepackage/abc#c"
  ]
}