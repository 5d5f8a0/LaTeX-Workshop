{
  "deps": [
    {
      "name": "tcolorbox"
    },
    {
      "name": "simplekv"
    },
    {
      "name": "xstring"
    },
    {
      "name": "settobox"
    },
    {
      "name": "varwidth"
    },
    {
      "name": "tcolorboxlibraryskins"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "tikzlibrarypositioning"
    },
    {
      "name": "tikzlibrarydecorations"
    },
    {
      "name": "tikzlibrarydecorations.pathmorphing"
    },
    {
      "name": "tikzlibrarybabel"
    }
  ],
  "macros": [
    {
      "name": "MiniPostIt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:contenu%text}}"
      }
    },
    {
      "name": "MiniPostIt",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:color}]{${1:contenu%text}}"
      }
    },
    {
      "name": "MiniPostIt*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:contenu%text}}"
      }
    },
    {
      "name": "MiniPostIt*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:color}]{${1:contenu%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "PostIt"
    },
    {
      "name": "PostIt",
      "arg": {
        "format": "<>",
        "snippet": "<${1:options tcbox}>"
      }
    },
    {
      "name": "PostIt",
      "arg": {
        "format": "[]",
        "snippet": "[${1:clés%keyvals}]"
      }
    },
    {
      "name": "PostIt",
      "arg": {
        "format": "[]<>",
        "snippet": "[${1:clés%keyvals}]<${2:options tcbox}>"
      }
    },
    {
      "name": "PostItNote"
    },
    {
      "name": "PostItNote",
      "arg": {
        "format": "<>",
        "snippet": "<${1:tcbox options}>"
      }
    },
    {
      "name": "PostItNote",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "PostItNote",
      "arg": {
        "format": "[]<>",
        "snippet": "[${1:options%keyvals}]<${2:tcbox options}>"
      }
    }
  ],
  "keys": {
    "\\begin{PostIt}": [
      "RappelPostIt=${1:nom}\r",
      "Largeur=",
      "Couleur=",
      "Hauteur=",
      "Rendu=${1|tcbox,tikz,tikzv2\r|}",
      "Inclinaison=${1:nombre}\r",
      "Ombre",
      "Bordure",
      "Coin",
      "Attache=${1|Trombone,Punaise,Non,Scotch\r|}",
      "CouleurAttache=",
      "DecalAttache=${1:nombre}\r",
      "Titre=${1:texte}\r",
      "PoliceTitre=${1:font commands}\r",
      "ExtraMargeDroite=",
      "AlignementV=${1|top,center,bottom\r|}",
      "AlignementH=${1|left,center,right,justify\r|}",
      "AlignementPostIt=${1|top,center,bottom\r|}"
    ],
    "\\begin{PostItNote}": [
      "StorePostIt=${1:name}\r",
      "Width=",
      "Color=",
      "Height=",
      "Render=${1|tcbox,tikz,tikz2\r|}",
      "Rotate=${1:degrees}\r",
      "Shadow",
      "Border",
      "Corner",
      "Pin=${1|Paperclip,Pushpin,None,Scotch\r|}",
      "PinColor=",
      "PinScale=${1:factor}\r",
      "PinShift=${1:number}\r",
      "Title=${1:text}\r",
      "FontTitle=${1:font commands}\r",
      "ExtraRightMargin=",
      "AlignV=${1|top,center,bottom\r|}",
      "AlignH=${1|left,center,right,justify\r|}",
      "AlignPostIt=${1|top,center,bottom\r|}"
    ]
  },
  "args": []
}