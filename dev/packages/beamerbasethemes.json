{
  "deps": [],
  "macros": [
    {
      "name": "usetheme",
      "arg": {
        "format": "{}",
        "snippet": "{${1:beamertheme}}"
      }
    },
    {
      "name": "usetheme",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:beamertheme}}"
      }
    },
    {
      "name": "usecolortheme",
      "arg": {
        "format": "{}",
        "snippet": "{${1:beamer color theme}}"
      }
    },
    {
      "name": "usecolortheme",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:beamer color theme}}"
      }
    },
    {
      "name": "usefonttheme",
      "arg": {
        "format": "{}",
        "snippet": "{${1:beamer font theme}}"
      }
    },
    {
      "name": "usefonttheme",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:beamer font theme}}"
      }
    },
    {
      "name": "useoutertheme",
      "arg": {
        "format": "{}",
        "snippet": "{${1:beamer outer theme}}"
      }
    },
    {
      "name": "useoutertheme",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:beamer outer theme}}"
      }
    },
    {
      "name": "useinnertheme",
      "arg": {
        "format": "{}",
        "snippet": "{${1:beamer inner theme}}"
      }
    },
    {
      "name": "useinnertheme",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:beamer inner theme}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\useinnertheme/rounded": [
      "shadow"
    ],
    "\\useoutertheme/miniframes": [
      "footline=${1|empty,authorinstitute,authortitle,institutetitle,authorinstitutetitle\r|}",
      "subsection"
    ],
    "\\useoutertheme/smoothbars": [
      "subsection"
    ],
    "\\useoutertheme/sidebar": [
      "height=",
      "hideothersubsections\r",
      "hideallsubsections\r",
      "left\r",
      "right\r",
      "width="
    ],
    "\\useoutertheme/tree": [
      "hooks"
    ],
    "\\usecolortheme/structure": [
      "rgb={${1:r,g,b}}\r",
      "RGB={${1:r,g,b}}\r",
      "cmyk={${1:c,m,y,k}}\r",
      "cmy={${1:c,m,y}}\r",
      "hsb={${1:h,s,b}}\r",
      "named="
    ],
    "\\usecolortheme/albatross,\\usecolortheme/monarca": [
      "overlystylish\r"
    ],
    "\\usefonttheme/serif": [
      "stillsansserifmath\r",
      "stillsansserifsmall\r",
      "stillsansseriflarge\r",
      "stillsansseriftext\r",
      "onlymath\r"
    ],
    "\\usefonttheme/structurebold,\\usefonttheme/structureitalicserif,\\usefonttheme/structuresmallcapsserif": [
      "onlysmall\r",
      "onlylarge\r"
    ]
  },
  "args": []
}