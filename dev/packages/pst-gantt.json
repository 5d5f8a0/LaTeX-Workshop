{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-node"
    },
    {
      "name": "pst-grad"
    },
    {
      "name": "pst-xkey"
    },
    {
      "name": "multido"
    }
  ],
  "macros": [
    {
      "name": "PstGanttChart",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:vertical units}}{${2:horizontal units}}"
      }
    },
    {
      "name": "PstGanttChart",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:vertical units}}{${2:horizontal units}}"
      }
    },
    {
      "name": "endPstGanttChart",
      "unusual": true
    },
    {
      "name": "PstGanttTask",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:start}}{${2:end}}"
      }
    },
    {
      "name": "PstGanttTask",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:start}}{${2:end}}"
      }
    }
  ],
  "envs": [
    {
      "name": "PstGanttChart",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:vertical units}}{${2:horizontal units}}"
      }
    },
    {
      "name": "PstGanttChart",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:vertical units}}{${2:horizontal units}}"
      }
    }
  ],
  "keys": {
    "\\PstGanttChart#c,\\begin{PstGanttChart}#c": [
      "PstPicture",
      "ChartShowIntervals\r",
      "ChartStartInterval=${1:number}\r",
      "ChartModulo",
      "ChartModuloValue=${1:number}\r",
      "ChartUnitIntervalName=${1:name}\r",
      "ChartUnitBasicIntervalName=${1:name}\r",
      "TaskUnitIntervalValue=${1:number}\r",
      "TaskUnitType=${1:text}\r",
      "TaskOutsideLabelMaxSize=${1:number}\r",
      "unit=${1:number}\r",
      "xunit=${1:number}\r",
      "yunit=${1:number}\r"
    ],
    "\\PstGanttTask#c": [
      "TaskUnitType=${1:text}\r",
      "TaskOutsideLabel={${1:text}}\r",
      "TaskInsideLabel={${1:text}}\r",
      "TaskStyle=${1:style}\r"
    ]
  },
  "args": []
}