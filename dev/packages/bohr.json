{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "pgfopts"
    },
    {
      "name": "elements"
    }
  ],
  "macros": [
    {
      "name": "bohr",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:num of electrons}}{${2:atom name}}"
      }
    },
    {
      "name": "bohr",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:num of shells}]{${1:num of electrons}}{${2:atom name}}"
      }
    },
    {
      "name": "setbohr",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\setbohr"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\setbohr": [
      "insert-symbol",
      "insert-number",
      "insert-missing",
      "atom-style={${1:code}}\r",
      "name-options-set={${1:tikz-options}}\r",
      "name-options-add={${1:tikz-options}}\r",
      "nucleus-options-set={${1:tikz-options}}\r",
      "nucleus-options-add={${1:tikz-options}}\r",
      "nucleus-radius=",
      "electron-options-set={${1:tikz-options}}\r",
      "electron-options-add={${1:tikz-options}}\r",
      "electron-radius=",
      "shell-options-set={${1:tikz-options}}\r",
      "shell-options-add={${1:tikz-options}}\r",
      "shell-dist=",
      "distribution-method=${1|periodic,quantum\r|}"
    ]
  },
  "args": []
}