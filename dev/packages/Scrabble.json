  "envs": [
    {
      "name": "EnvScrabble"
    },
    {
      "name": "EnvScrabble",
      "arg": {
        "format": "[]",
        "snippet": "[${1:keys%keyvals}]"
      }
    },
    {
      "name": "EnvScrabble",
      "arg": {
        "format": "<>",
        "snippet": "<${1:language}>"
      }
    },
    {
      "name": "EnvScrabble",
      "arg": {
        "format": "<>[]",
