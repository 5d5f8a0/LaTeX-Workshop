        "format": "[]",
        "snippet": "[${1:clés%keyvals}]"
      }
    },
    {
      "name": "ScrabblePutWord",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:letters}}{${2:x,y}}"
      }
