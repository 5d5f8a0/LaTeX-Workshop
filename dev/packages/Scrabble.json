    "\\PlateauScrabble,\\begin{EnvScrabbleFR}": [
      "Echelle=${1:factor}\r",
      "EchelleLabels=${1:factor}\r",
      "Cadre",
      "Labels",
      "Aide"
    ],
    "\\ScrabblePutWord,\\ScrabblePlaceMot": [
