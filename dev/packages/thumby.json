{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "perltex"
    },
    {
      "name": "bophook"
    }
  ],
  "macros": [
    {
      "name": "thumbyNumberFormat",
      "arg": {
        "format": "{}",
        "snippet": "{${1:format}}"
      }
    },
    {
      "name": "thumbySides",
      "arg": {
        "format": "{}",
        "snippet": "{${1:one or two}}"
      }
    },
    {
      "name": "thumbyBackground",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "thumbyForeground",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "thumbyPageHeight",
      "arg": {
        "format": "{}",
        "snippet": "{${1:points}}"
      }
    },
    {
      "name": "thumbyThumbWidth",
      "arg": {
        "format": "{}",
        "snippet": "{${1:points}}"
      }
    },
    {
      "name": "thumbySetup"
    },
    {
      "name": "thumbyTotalChapters",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "thumbychapnum",
      "unusual": true
    },
    {
      "name": "thumbyprintthumb",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:number}}{${2:left or right}}"
      }
    },
    {
      "name": "thumbyprintthumbleft",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "thumbyprintthumbright",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/thumb#c": [
      "minitoc",
      "box",
      "filledbox",
      "oval",
      "filledoval"
    ]
  },
  "args": [
    "\\usepackage/thumb#c"
  ]
}