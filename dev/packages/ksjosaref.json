{
  "deps": [
    {
      "name": "amsmath"
    },
    {
      "name": "refcount"
    }
  ],
  "macros": [
    {
      "name": "josaref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "josaref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:type}]{${1:label}}"
      }
    },
    {
      "name": "josaref",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:type}][${3:delim%keyvals}]{${1:label}}"
      }
    },
    {
      "name": "ref",
      "if": "refcmd",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:type}]{${1:label}}"
      }
    },
    {
      "name": "ref",
      "if": "refcmd",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:type}][${3:delim}]{${1:label}}"
      }
    },
    {
      "name": "josarefcmds"
    },
    {
      "name": "nojosarefcmds"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/ksjosaref#c": [
      "refcmd"
    ],
    "\\josaref#c": [
      "paren",
      "bracket",
      "left=${1:delim}",
      "right=${1:delim}"
    ]
  },
  "args": [
    "\\usepackage/ksjosaref#c"
  ]
}