{
  "deps": [
    {
      "name": "eso-pic"
    },
    {
      "name": "zref-savepos"
    },
    {
      "name": "zref-abspage"
    }
  ],
  "macros": [
    {
      "name": "returntogrid"
    },
    {
      "name": "returntogrid",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "showdebugpagegrid"
    },
    {
      "name": "returntogridsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\returntogridsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\returntogrid": [
      "save=${1:name}\r",
      "use=${1:name}\r",
      "label=${1:name}\r",
      "strut=${1:integer}\r",
      "debug-vgrid",
      "tab\r",
      "tab=${1:name}\r"
    ],
    "\\returntogridsetup": [
      "active",
      "step=",
      "reference=${1:point name}\r",
      "offset=",
      "settabpositions={${1:name}}{${2:dim-expr1,dim-expr2,...}}\r",
      "tab-list=${1:name}\r",
      "hfill",
      "twoside",
      "debug-tab"
    ]
  },
  "args": []
}