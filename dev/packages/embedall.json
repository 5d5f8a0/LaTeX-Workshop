{
  "deps": [
    {
      "name": "embedfile"
    },
    {
      "name": "filehook"
    },
    {
      "name": "currfile"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "letltxmacro",
      "if": "compat"
    }
  ],
  "macros": [
    {
      "name": "embedsource"
    },
    {
      "name": "embedsource",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "embedinput",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "embedinput",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/embedall#c": [
      "all\r",
      "notall\r",
      "main\r",
      "input\r",
      "include\r",
      "graphicx\r",
      "csvsimple\r",
      "compat\r"
    ],
    "\\embedsource#c,\\embedinput#c": [
      "filespec=\r",
      "ucfilespec=\r",
      "filesystem=\r",
      "mimetype=\r",
      "desc=${1:description}\r",
      "afrelationship=${1:PDF name}\r",
      "stringmethod=${1|psd,escape\r|}",
      "id=\r"
    ]
  },
  "args": [
    "\\usepackage/embedall#c"
  ]
}