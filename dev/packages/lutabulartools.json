  "envs": [],
  "keys": {
    "\\settabular": [
      "nopad",
      "tbrule",
      "rowsep=${1:integer}\r",
      "colsep=${1:integer}\r"
    ],
    "\\midruleX,\\midruleX*": [
      "step=${1:integer}\r",
      "rule=\r",
      "reset",
      "resetnum=${1:integer}\r",
      "cntr=${1:integer}\r"
    ]
  },
  "args": []
