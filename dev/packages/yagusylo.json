{
  "deps": [
    {
      "name": "xifthen"
    },
    {
      "name": "suffix"
    },
    {
      "name": "xargs"
    },
    {
      "name": "xcolor",
      "if": "color=true"
    },
    {
      "name": "colortbl",
      "if": "XcolorOptions=table"
    },
    {
      "name": "pdfcolmk",
      "if": "XcolorOptions=fixpdftex"
    }
  ],
  "macros": [
    {
      "name": "setyagusylokeys",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "yagding",
      "arg": {
        "format": "{}",
        "snippet": "{${1:char-num}}"
      }
    },
    {
      "name": "yagding",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:char-num}}[${2:color}]"
      }
    },
    {
      "name": "yagding",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:symfam%keyvals}]{${1:char-num}}"
      }
    },
    {
      "name": "yagding",
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:symfam%keyvals}]{${1:char-num}}[${3:color}]"
      }
    },
    {
      "name": "defdingname",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:char-num}}{${2:ding-name}}"
      }
    },
    {
      "name": "defdingname",
      "arg": {
        "format": "{}{}[]",
        "snippet": "{${1:char-num}}{${2:ding-name}}[${3:color}]"
      }
    },
    {
      "name": "defdingname",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:symfam%keyvals}]{${1:char-num}}{${2:ding-name}}"
      }
    },
    {
      "name": "defdingname",
      "arg": {
        "format": "[]{}{}[]",
        "snippet": "[${3:symfam%keyvals}]{${1:char-num}}{${2:ding-name}}[${4:color}]"
      }
    },
    {
      "name": "defdingname",
      "arg": {
        "format": "[][]{}{}",
        "snippet": "[${3:symfam%keyvals}][${4:defext}]{${1:char-num}}{${2:ding-name}}"
      }
    },
    {
      "name": "defdingname",
      "arg": {
        "format": "[][]{}{}[]",
        "snippet": "[${3:symfam%keyvals}][${4:defext}]{${1:char-num}}{${2:ding-name}}[${5:color}]"
      }
    },
    {
      "name": "yagding*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:ding-name}}"
      }
    },
    {
      "name": "yagfill",
      "arg": {
        "format": "{}",
        "snippet": "{${1:char-num}}"
      }
    },
    {
      "name": "yagfill",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:char-num}}"
      }
    },
    {
      "name": "yagfill*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:ding-name}}"
      }
    },
    {
      "name": "yagfill*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:ding-name}}"
      }
    },
    {
      "name": "setyagline",
      "arg": {
        "format": "{}",
        "snippet": "{${1:head-value}}"
      }
    },
    {
      "name": "setyagline",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:head-value}}[${2:tail-value}]"
      }
    },
    {
      "name": "yagline",
      "arg": {
        "format": "{}",
        "snippet": "{${1:char-num}}"
      }
    },
    {
      "name": "yagline",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:char-num}}"
      }
    },
    {
      "name": "yagline*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:ding-name}}"
      }
    },
    {
      "name": "yagline*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:ding-name}}"
      }
    },
    {
      "name": "setyagitemize",
      "arg": {
        "format": "{}",
        "snippet": "{${1:symfam1, number1, color1. symfam2, number2, color2. ...}}"
      }
    },
    {
      "name": "setyagitemize*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:dingname1. dingname2. ...}}"
      }
    },
    {
      "name": "newenumpattern",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:keyvals}}"
      }
    },
    {
      "name": "setyagenumeratekeys",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "yagnumber",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:symfam}}{${2:counter}}{${3:color}}"
      }
    }
  ],
  "envs": [
    {
      "name": "yagitemize",
      "arg": {
        "format": "{}",
        "snippet": "{${1:char-num}}"
      }
    },
    {
      "name": "yagitemize",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:char-num}}[${2:color}]"
      }
    },
    {
      "name": "yagitemize",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:symfam%keyvals}]{${1:char-num}}"
      }
    },
    {
      "name": "yagitemize",
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:symfam%keyvals}]{${1:char-num}}[${3:color}]"
      }
    },
    {
      "name": "yagitemize*"
    },
    {
      "name": "yagitemize*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:ding-name}]"
      }
    },
    {
      "name": "yagenumerate"
    },
    {
      "name": "yagenumerate",
      "arg": {
        "format": "[]",
        "snippet": "[${1:keyvals}]"
      }
    },
    {
      "name": "notyagenum"
    }
  ],
  "keys": {
    "\\usepackage/yagusylo#c": [
      "info=${1|mute,normal,verbose\r|}",
      "onerror=${1|nice,tough\r|}",
      "color",
      "XcolorOptions={${1:xcolor options}}\r",
      "configfile"
    ],
    "\\setyagusylokeys,\\defdingname,\\setyagenumeratekeys": [
      "*\r"
    ],
    "\\setyagusylokeys,\\yagfill,\\yagfill*,\\yagfill+,\\yagline,\\yagline*,\\yagline+": [
      "symfam=${1|marvosym,fourier,wasysym,bbding,dingbat,ark,ifsym,ifsymgeo,ifsymgeonarrow,ifsymgeowide,ifsymclock,ifsymweather,pifont\r|}",
      "symcolor=",
      "leadtype=${1|l,c,x\r|}",
      "symplace=${1|c,r,l,a,n\r|}",
      "sympos=${1:integer 0-1000}\r",
      "boxwidth=",
      "before=",
      "after=",
      "head=",
      "tail="
    ],
    "\\yagding,\\defdingname,\\begin{yagitemize}": [
      "marvosym\r",
      "fourier\r",
      "wasysym\r",
      "bbding\r",
      "dingbat\r",
      "ark\r",
      "ifsym\r",
      "ifsymgeo\r",
      "ifsymgeonarrow\r",
      "ifsymgeowide\r",
      "ifsymclock\r",
      "ifsymweather\r",
      "pifont\r"
    ],
    "\\newenumpattern,\\begin{yagenumerate},\\setyagenumeratekeys": [
      "symfam=${1|marvosym,fourier,wasysym,bbding,dingbat,ark,ifsym,ifsymgeo,ifsymgeonarrow,ifsymgeowide,ifsymclock,ifsymweather,pifont\r|}",
      "symcolor=",
      "firstitemnum=${1:integer 0-255}\r",
      "enumlength=${1:integer}\r",
      "enumpattern=${1:name}\r"
    ]
  },
  "args": [
    "\\usepackage/yagusylo#c"
  ]
}