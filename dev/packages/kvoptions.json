{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "ltxcmds"
    },
    {
      "name": "kvsetkeys"
    },
    {
      "name": "kvoptions-patch",
      "if": "patch"
    }
  ],
  "macros": [
    {
      "name": "ProcessKeyvalOptions",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:family}}"
      }
    },
    {
      "name": "ProcessKeyvalOptions*",
      "unusual": true
    },
    {
      "name": "ProcessLocalKeyvalOptions",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:family}}"
      }
    },
    {
      "name": "ProcessLocalKeyvalOptions*",
      "unusual": true
    },
    {
      "name": "SetupKeyvalOptions",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\SetupKeyvalOptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "DeclareStringOption",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:key%plain}}"
      }
    },
    {
      "name": "DeclareStringOption",
      "unusual": true,
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:init}]{${1:key%plain}}[${3:default}]"
      }
    },
    {
      "name": "DeclareBoolOption",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:key%plain}}"
      }
    },
    {
      "name": "DeclareBoolOption",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:init}]{${1:key%plain}}"
      }
    },
    {
      "name": "DeclareComplementaryOption",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:key%plain}}{${2:parent}}"
      }
    },
    {
      "name": "DeclareVoidOption",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:key%plain}}{${2:code}}"
      }
    },
    {
      "name": "DeclareDefaultOption",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "DeclareLocalOption",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:option}}"
      }
    },
    {
      "name": "DeclareLocalOptions",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:options}}"
      }
    },
    {
      "name": "DisableKeyvalOption",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:family}}{${2:key%plain}}"
      }
    },
    {
      "name": "DisableKeyvalOption",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:family}}{${2:key%plain}}"
      }
    },
    {
      "name": "AddToKeyvalOption",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:family}}{${2:key%plain}}{${3:code}}"
      }
    },
    {
      "name": "AddToKeyvalOption*",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:key%plain}}{${2:code}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/kvoptions#c": [
      "debugshow\r",
      "patch\r"
    ],
    "\\SetupKeyvalOptions": [
      "family=\r",
      "prefix=\r",
      "setkeys=\r"
    ],
    "\\DisableKeyvalOption": [
      "action=${1|undef,warning,error,ignore\r|}",
      "global\r",
      "local\r",
      "package=\r",
      "class=\r"
    ]
  },
  "args": [
    "\\usepackage/kvoptions#c"
  ]
}