{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "fancyvrb"
    },
    {
      "name": "color"
    },
    {
      "name": "listings"
    }
  ],
  "macros": [
    {
      "name": "checklistings",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/checklistings#c,\\checklistings"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "chklistingcmd"
    },
    {
      "name": "chklistingmsg",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "chklistingerr",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "ChkListingErr",
      "unusual": true
    },
    {
      "name": "chklistingfalse",
      "unusual": true
    },
    {
      "name": "chklistingfile",
      "unusual": true
    },
    {
      "name": "ChkListingMsg",
      "unusual": true
    },
    {
      "name": "chklistingtrue",
      "unusual": true
    },
    {
      "name": "ifchklisting",
      "unusual": true
    },
    {
      "name": "setchklistingcmd",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:command}}"
      }
    },
    {
      "name": "thechklisting",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "chklisting"
    },
    {
      "name": "chklisting",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "ChkListingErr",
      "unusual": true
    },
    {
      "name": "ChkListingMsg",
      "unusual": true
    }
  ],
  "keys": {
    "\\usepackage/checklistings#c,\\checklistings": [
      "withresult",
      "withoutresult",
      "showproblems",
      "hideproblems",
      "skipone\r",
      "skiptwo\r",
      "codestyle={${1:fancyvrb options}}\r",
      "msgstyle={${1:fancyvrb options}}\r",
      "errstyle={${1:fancyvrb options}}\r",
      "codelst={${1:listings options}}\r",
      "msglst={${1:listings options}}\r",
      "errlst={${1:listings options}}\r",
      "emptyoption=${1:string}\r",
      "subdir=${1:directory}\r",
      "prefix=${1:prefix}\r",
      "ext=${1:extension}\r",
      "prompt=${1:prompt}\r",
      "compiler=${1:path}\r",
      "compilerflags=${1:flags}\r",
      "lastflags=${1:flags}\r",
      "includecmd=${1:command}\r",
      "html=${1:path}\r"
    ],
    "\\begin{chklisting}": [
      "continue\r",
      "include=${1:label}\r",
      "fail\r",
      "label=${1:label}\r",
      "skipnone\r",
      "skipone\r",
      "skiptwo\r",
      "hide\r",
      "withresult\r",
      "withoutresult\r",
      "showproblems\r",
      "hideproblems\r"
    ]
  },
  "args": [
    "\\usepackage/checklistings#c,\\checklistings"
  ]
}