{
  "deps": [
    {
      "name": "tagpdf"
    }
  ],
  "macros": [
    {
      "name": "PDFManagementAdd",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:resource path}}{${2:name}}{${3:value}}"
      }
    },
    {
      "name": "AddToDocumentProperties",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:property}}{${2:value}}"
      }
    },
    {
      "name": "AddToDocumentProperties",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:label}]{${1:property}}{${2:value}}"
      }
    },
    {
      "name": "GetDocumentProperties",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label/property}}"
      }
    },
    {
      "name": "ShowDocumentProperties"
    },
    {
      "name": "url",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:URL}}"
      }
    },
    {
      "name": "hrefurl",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:URL}}{${2:text}}"
      }
    },
    {
      "name": "hrefurl",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:URL}}{${2:text}}"
      }
    },
    {
      "name": "hrefpdf",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:file}}{${2:text}}"
      }
    },
    {
      "name": "hrefpdf",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:file}}{${2:text}}"
      }
    },
    {
      "name": "hrefrun",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:launch link%URL}}{${2:text}}"
      }
    },
    {
      "name": "hrefrun",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:launch link%URL}}{${2:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\url": [
      "protocol=${1:prefix}\r",
      "format=${1:format commands}\r"
    ],
    "\\hrefurl": [
      "urlencode\r",
      "protocol=${1:prefix}\r",
      "ismap\r"
    ],
    "\\hrefpdf": [
      "destination=${1:dest name}\r",
      "page=${1:number}\r",
      "pdfremotestartview=${1|Fit,FitB,FitH,FitV,FitBH,FitBV\r|}",
      "afrelationship=${1:PDF name}\r"
    ],
    "\\hrefrun": [
      "run-parameter=${1:parameter}\r"
    ],
    "\\hypersetup": [
      "href/urlencode\r",
      "href/protocol=${1:prefix}\r",
      "href/destination=${1:dest name}\r",
      "href/format=${1:format commands}\r",
      "pdfcopyright=${1:copyright text}\r",
      "pdftype=${1|Collection,Dataset,Event,Image,InteractiveResource,MovingImage,PhysicalObject,Service,Software,Sound,StillImage,Text\r|}",
      "pdflicenseurl=${1:URL}\r",
      "pdfauthortitle=${1:author title}\r",
      "pdfcaptionwriter=${1:caption writer name}\r",
      "pdfmetalang=${1:meta language}\r",
      "pdfsource=${1:source file name}\r",
      "pdfdocumentid=uuid:${1:id num}\r",
      "pdfinstanceid=uuid:${1:id num}\r",
      "pdfversionid=${1:version id}\r",
      "pdfrendition=${1|default,draft,low-res,proof,screen,thumbnail\r|}",
      "pdfpublication=${1:publication}\r",
      "pdfpubtype=${1:publication type}\r",
      "pdfbytes=${1:file size}\r",
      "pdfnumpages=${1:number of pages}\r",
      "pdfissn=${1:print ISSN}\r",
      "pdfeissn=${1:electronic ISSN}\r",
      "pdfisbn=${1:ISBN}\r",
      "pdfbookedition=${1:book edition}\r",
      "pdfpublisher=${1:publisher}\r",
      "pdfvolumenum=${1:volume number}\r",
      "pdfissuenum=${1:issue number}\r",
      "pdfpagerange={${1:page ranges}}\r",
      "pdfdoi=${1:DOI}\r",
      "pdfurl=${1:URL}\r",
      "pdfidentifier=${1:identifier}\r",
      "pdfsubtitle=${1:subtitle}\r",
      "pdfpubstatus=${1:publication status}\r",
      "pdfcontactaddress=${1:street address}\r",
      "pdfcontactcity=${1:city}\r",
      "pdfcontactregion=${1:state or province}\r",
      "pdfcontactpostcode=${1:postal code}\r",
      "pdfcontactcountry=${1:country}\r",
      "pdfcontactphone=${1:phone number}\r",
      "pdfcontactemail=${1:email}\r",
      "pdfcontacturl=${1:URL}\r",
      "pdfdate=${1:date}\r",
      "link",
      "url",
      "file",
      "run",
      "menu",
      "nested-links"
    ]
  },
  "args": []
}