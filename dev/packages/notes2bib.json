{
  "deps": [
    {
      "name": "l3keys2e"
    }
  ],
  "macros": [
    {
      "name": "bibnote",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "bibnote",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:text}}"
      }
    },
    {
      "name": "bibnotemark"
    },
    {
      "name": "bibnotemark",
      "arg": {
        "format": "[]",
        "snippet": "[${1:name}]"
      }
    },
    {
      "name": "bibnotetext",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "bibnotetext",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:text}}"
      }
    },
    {
      "name": "printbibnotes"
    },
    {
      "name": "bibnotesetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\bibnotesetup,\\usepackage/notes2bib#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "recordnotes",
      "unusual": true
    },
    {
      "name": "TotalNotes",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "NotesAfterCitations",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:note-list}}"
      }
    },
    {
      "name": "NotesBeforeCitations",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:note-list}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\bibnotesetup,\\usepackage/notes2bib#c": [
      "note-name=${1:text}",
      "refsection-name=${1:text}",
      "refsection-separator=${1:symbol}",
      "cite-function=${1:cite command}",
      "file-name=${1:file name}",
      "record-type=${1:type}",
      "note-field=${1:field}",
      "keyword-entry=${1:text}",
      "placement=${1|before,after,mixed|}",
      "presort-before=${1:string}",
      "presort-mixed=${1:string}",
      "presort-after=${1:string}",
      "sort-key-before=${1:string}",
      "sort-key-mixed=${1:string}",
      "sort-key-after=${1:string}",
      "convert-endnotes",
      "convert-footnotes",
      "use-sort-key"
    ]
  },
  "args": [
    "\\bibnotesetup,\\usepackage/notes2bib#c"
  ]
}