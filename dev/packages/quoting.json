{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "quotingsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\begin{quoting},\\quotingsetup,\\usepackage/quoting#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "quotingfont",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "quoting"
    },
    {
      "name": "quoting",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{quoting},\\quotingsetup,\\usepackage/quoting#c": [
      "indentfirst=${1|auto,true,false\r|}",
      "font={${1:format names}}\r",
      "font+={${1:format names}}\r",
      "begintext=${1:text}\r",
      "endtext=${1:text}\r",
      "leftmargin=",
      "rightmargin=",
      "vskip=",
      "listvskip",
      "noorphanfirst",
      "noorphanafter",
      "noorphans"
    ]
  },
  "args": [
    "\\begin{quoting},\\quotingsetup,\\usepackage/quoting#c"
  ]
}