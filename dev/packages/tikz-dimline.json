{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "pgfplots"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "tikzlibrarydecorations.markings"
    }
  ],
  "macros": [
    {
      "name": "dimline",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:start}}{${2:end}}{${3:label}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\dimline": [
      "color=",
      "line style=\r",
      "label style=\r",
      "extension start length=",
      "extension end length=",
      "extension start angle=${1:degrees}\r",
      "extension end angle=${1:degrees}\r",
      "extension start style=\r",
      "extension end style=\r",
      "extension start path=\r",
      "extension end path=\r",
      "arrows=${1|dimline-dimline,dimline reverse-dimline reverse\r|}"
    ]
  },
  "args": []
}