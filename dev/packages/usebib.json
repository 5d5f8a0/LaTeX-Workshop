{
  "deps": [
    {
      "name": "url"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "bibinput",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bib file}}"
      }
    },
    {
      "name": "newbibfield",
      "arg": {
        "format": "{}",
        "snippet": "{${1:field}}"
      }
    },
    {
      "name": "newbibignore",
      "arg": {
        "format": "{}",
        "snippet": "{${1:type}}"
      }
    },
    {
      "name": "usebibentry",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:citekey}}{${2:field%keyvals}}"
      }
    },
    {
      "name": "usebibentryurl",
      "arg": {
        "format": "{}",
        "snippet": "{${1:citekey}}"
      }
    },
    {
      "name": "usebibentryurl",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:delim}]{${1:citekey}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usebibentry#c": [
      "title",
      "year",
      "note",
      "isbn"
    ]
  },
  "args": []
}