    {
      "name": "sseq",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:x-range}}{${2:y-range}}"
      }
    },
    {
      "name": "sseq",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:x-range}}{${2:y-range}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/luasseq#c": [
      "debug"
    ],
    "\\begin{sseq}": [
      "grid=${1|none,crossword,go,dots,chess\r|}",
      "gridstroke=",
      "gapsize=",
      "xgapsize=",
      "ygapsize=",
      "entrysize=",
      "labels=${2|none,numbers,{${1:x1;x2;...}}\r|}",
      "xlabels=${2|none,numbers,{${1:x1;x2;...}}\r|}",
      "ylabels=${2|none,numbers,{${1:x1;x2;...}}\r|}",
      "labelstep=${1:integer}\r",
      "xlabelstep=${1:integer}\r",
      "ylabelstep=${1:integer}\r",
      "leak=",
      "xleak=",
      "yleak=",
      "arrows=${1:arrow type}\r",
      "packing=${1|auto,horizontal,vertical,diagonal\r|}"
    ],
    "\\ssdrop,\\ssdropextension": [
      "circled\r",
      "boxed\r",
      "color=",
      "name=${1:name}\r"
    ],
    "\\ssdroplabel": [
      "color=",
      "pos=${1|U,LU,L,LD,D,RD,R,RU\r|}",
      "U\r",
      "LU\r",
      "L\r",
      "LD\r",
      "D\r",
      "RD\r",
      "R\r",
      "RU\r"
    ],
    "\\ssstroke,\\ssline,\\ssarrow,\\ssvoidline": [
      "color=",
      "curve=${1:value}\r",
      "dashed\r",
      "dashed=${1:dashing type}\r",
      "dotted\r",
      "dotted=${1:dashing type}\r",
      "arrowfrom\r",
      "arrowfrom=${1:dashing type}\r",
      "arrowto\r",
      "arrowto=${1:dashing type}\r",
      "void\r"
    ]
  },
  "args": [
    "\\usepackage/luasseq#c"
  ]
