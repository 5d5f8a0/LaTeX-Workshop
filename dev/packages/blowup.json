    {
      "name": "keyval"
    },
    {
      "name": "graphics"
    },
    {
      "name": "typearea"
    },
    {
      "name": "iftex"
    }
  ],
  "macros": [
    {
      "name": "blowUp",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "tPaperWidth",
      "unusual": true
    },
    {
      "name": "tPaperHeight",
      "unusual": true
    },
    {
      "name": "oPaperWidth",
      "unusual": true
    },
    {
      "name": "oPaperWidth",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\blowUp": [
      "target=${1:paper size}\r",
      "origin=${1:paper size}\r",
      "h-mirroring",
      "v-mirroring",
      "landscape",
      "noscale",
      "pos=${1|left,right,inside,outside,top,bottom\r|}",
      "onepage"
    ]
  },
  "args": []
}