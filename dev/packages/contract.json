    },
    {
      "name": "ellipsispar"
    },
    {
      "name": "ellipsispar",
      "arg": {
        "format": "[]",
        "snippet": "[${1:number}]"
      }
    },
    {
      "name": "parellipsis",
      "unusual": true
    },
    {
      "name": "thesentence",
      "unusual": true
