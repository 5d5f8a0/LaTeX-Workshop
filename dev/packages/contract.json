      "unusual": true
    },
    {
      "name": "Sentence"
    },
    {
      "name": "refL",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "refS",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "refN",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
