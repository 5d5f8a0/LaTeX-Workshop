    {
      "name": "refParN",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "refParN",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:number format}]{${1:label}}"
