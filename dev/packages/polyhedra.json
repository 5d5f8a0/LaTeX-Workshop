{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "xcolor-solarized"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikz-3dplot"
    },
    {
      "name": "xstring"
    },
    {
      "name": "tikzlibraryshapes"
    },
    {
      "name": "tikzlibrarypatterns"
    }
  ],
  "macros": [
    {
      "name": "point",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:(${3:x},${4:y},${5:z})}}{${2:name}}"
      }
    },
    {
      "name": "axes",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "vertex",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "edge",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "ray",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "cone",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "polygon",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "halfspace",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "hyperplane",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    }
  ],
  "envs": [
    {
      "name": "polyhedron",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    }
  ],
  "keys": {
    "\\begin{polyhedron}": [
      "dim=${1:integer}\r",
      "phi=${1:degrees}\r",
      "theta=${1:degrees}\r"
    ],
    "\\axes": [
      "x={${1:num1,num2}}\r",
      "y={${1:num1,num2}}\r",
      "z={${1:num1,num2}}\r",
      "xlabel=${1:text}\r",
      "ylabel=${1:text}\r",
      "zlabel=${1:text}\r",
      "labels=${1|true,false\r|}"
    ],
    "\\vertex,\\edge,\\ray,\\cone,\\polygon,\\halfspace,\\hyperplane": [
      "color=",
      "status=${1|normal,focus,alert,open\r|}",
      "opacity=${1:factor}\r",
      "thickness=${1:number}\r",
      "pattern={${1:pattern}}\r"
    ],
    "\\vertex": [
      "point={${1:point}}\r",
      "text=${1:text}\r",
      "textcolor=",
      "anchora=${1|north,south,east,west\r|}",
      "anchorb=${1|above,below,left,right\r|}"
    ],
    "\\edge,\\ray": [
      "points={${1:point1,point2}}\r"
    ],
    "\\cone": [
      "vertex={${1:point}}\r",
      "generators={${1:list of point}}\r"
    ],
    "\\polygon": [
      "points={${1:list of point}}\r",
      "boundary=${1|true,false\r|}"
    ],
    "\\halfspace,\\hyperplane": [
      "points={${1:list of point}}\r",
      "patterncolor="
    ]
  },
  "args": []
}