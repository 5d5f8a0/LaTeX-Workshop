{
  "deps": [
    {
      "name": "ocgbase"
    },
    {
      "name": "pdfbase"
    }
  ],
  "macros": [
    {
      "name": "includemedia",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:poster text%text}}{${2:file or URL%file}}"
      }
    },
    {
      "name": "includemedia",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:poster text%text}}{${2:file or URL%file}}"
      }
    },
    {
      "name": "addmediapath",
      "arg": {
        "format": "{}",
        "snippet": "{${1:directory}}"
      }
    },
    {
      "name": "mediabutton",
      "arg": {
        "format": "{}",
        "snippet": "{${1:button text%text}}"
      }
    },
    {
      "name": "mediabutton",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:button text%text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/media9#c": [
      "dvipdfmx\r",
      "xetex\r",
      "bigfiles\r"
    ],
    "\\usepackage/media9#c,\\includemedia#c": [
      "draft\r",
      "final\r",
      "playbutton=${1|fancy,plain,none\r|}",
      "noplaybutton\r",
      "activate=${1|onclick,pageopen,pagevisible\r|}",
      "deactivate=${1|onclick,pageclose,pageinvisible\r|}",
      "windowed=\r",
      "transparent\r",
      "passcontext\r",
      "attachfiles\r",
      "3Dplaytype=${1|linear,oscillating\r|}",
      "3Dplaycount=${1:integer}\r",
      "3Dplayspeed=${1:number}\r",
      "3Dtoolbar\r",
      "3Dnavpane\r",
      "3Dpartsattrs=${1|restore,keep\r|}",
      "3Dmenu\r",
      "3Dbg=${1:<r> <g> <b>}\r",
      "3Dlights=${1|None,White,Day,Night,Hard,Primary,Blue,Red,Cube,CAD,Headlamp\r|}",
      "3Drender=${1|Solid,SolidWireframe,Transparent,TransparentWireframe,BoundingBox,TransparentBoundingBox,TransparentBoundingBoxOutline,Wireframe,ShadedWireframe,HiddenWireframe,Vertices,ShadedVertices,SolidOutline,Illustration,ShadedIllustration\r|}"
    ],
    "\\includemedia#c": [
      "label=${1:label text}\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio\r",
      "scale=${1:factor}\r",
      "addresource=${1:file}\r",
      "flashvars={${1:var1=val1&var2=val2&...}}\r",
      "3Dcoo=${1:<x> <y> <z>}\r",
      "3Dc2c=${1:<x> <y> <z>}\r",
      "3Droll=${1:roll}\r",
      "3Dc2w=${1:camera-to-world matrix}\r",
      "3Dpsob=${1|Min,Max,W,H\r|}",
      "3Droo=${1:number}\r",
      "3Daac=${1:angle}\r",
      "3Dortho=${1:factor}\r",
      "3Dviews=${1:views file}\r",
      "add3Djscript=${1:3D JavaScript file}\r"
    ],
    "\\mediabutton#c": [
      "overface=${1:text or graphic}\r",
      "downface=${1:text or graphic}\r",
      "tooltip=${1:tip text}\r",
      "3Dgotoview=${1:label text}\r",
      "3Dgotoview=${1:label text}:${2:view spec}\r",
      "mediacommand=${1:label text}\r",
      "mediacommand=${1:label text}:${2:command}\r",
      "mediacommand=${1:label text}:${2:command}[(${3:arg1})${4:(arg2)...}]\r",
      "jsaction={${1:JavaScript code}}\r",
      "jsaction=${1:label text}:{${2:JavaScript code}}\r",
      "draft\r",
      "final\r"
    ]
  },
  "args": [
    "\\usepackage/media9#c",
    "\\usepackage/media9#c,\\includemedia#c"
  ]
}