{
  "deps": [
    {
      "name": "xintbinhex"
    },
    {
      "name": "xintcore"
    }
  ],
  "macros": [
    {
      "name": "bnumeval",
      "arg": {
        "format": "{}",
        "snippet": "{${1:integer-expr list}}"
      }
    },
    {
      "name": "evaltohex",
      "arg": {
        "format": "{}",
        "snippet": "{${1:hex-expr list}}"
      }
    },
    {
      "name": "bnumsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\bnumsetup,\\bnumexprsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "bnumhextodec",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:hex integer}}"
      }
    },
    {
      "name": "bnumprintone",
      "unusual": true
    },
    {
      "name": "bnumprintonetohex",
      "unusual": true
    },
    {
      "name": "bnumprintonesep",
      "unusual": true
    },
    {
      "name": "bnumdefinfix",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:operator}}{${2:macro}}{${3:L-prec}}{${4:R-prec}}"
      }
    },
    {
      "name": "bnumdefpostfix",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:operator}}{${2:macro}}{${3:L-prec}}"
      }
    },
    {
      "name": "BNErestorecatcodes",
      "unusual": true
    },
    {
      "name": "bnumexpro",
      "unusual": true
    },
    {
      "name": "bnebareeval",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:expr}}"
      }
    },
    {
      "name": "XINTfstop",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/bnumexpr#c": [
      "custom\r"
    ],
    "\\bnumsetup,\\bnumexprsetup": [
      "add=${1:macro}\r",
      "sub=${1:macro}\r",
      "mul=${1:macro}\r",
      "divround=${1:macro}\r",
      "div=${1:macro}\r",
      "mod=${1:macro}\r",
      "pow=${1:macro}\r",
      "fac=${1:macro}\r",
      "opp=${1:macro}\r"
    ]
  },
  "args": [
    "\\usepackage/bnumexpr#c"
  ]
}