{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "fancyvrb"
    },
    {
      "name": "framed"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "calc"
    }
  ],
  "macros": [
    {
      "name": "plset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\begin{pyglist},\\plset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "listofpyglistings"
    },
    {
      "name": "listofpyglistingsname",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "pyglist"
    },
    {
      "name": "pyglist",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{pyglist},\\plset": [
      "abovecaptionskip=",
      "belowcaptionskip=",
      "bgcolor=",
      "caption={${1:text}}\r",
      "captionbgcolor=",
      "captionfont=${1:font commands}\r",
      "encoding=${1:encoding}\r",
      "fontsize=\r",
      "fvset={${1:fancyvrb options}}\r",
      "gobble=${1:integer}\r",
      "language=${1:language}\r",
      "label=",
      "listingname=${1:name}\r",
      "listingnamefont=${1:font commands}\r",
      "mathescape",
      "numbers=${1|none,left,right\r|}",
      "numbersep=",
      "showspaces",
      "style=${1:Pygments style}\r",
      "texcl"
    ]
  },
  "args": []
}