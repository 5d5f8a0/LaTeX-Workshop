{
  "deps": [
    {
      "name": "qrcode"
    },
    {
      "name": "xstring"
    },
    {
      "name": "calculator"
    }
  ],
  "macros": [
    {
      "name": "epcqr",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\epcqr": [
      "bic=${1:BIC}\r",
      "name=${1:text}\r",
      "iban=${1:IBAN}\r",
      "amount=${1:number}\r",
      "reason=${1:text}\r",
      "ref=${1:text}\r",
      "text=${1:text}\r",
      "information=${1:text}\r"
    ]
  },
  "args": []
}