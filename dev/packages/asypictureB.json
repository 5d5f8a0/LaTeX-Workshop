{
  "deps": [
    {
      "name": "fancyvrb"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "pgfkeys"
    },
    {
      "name": "ifplatform"
    }
  ],
  "macros": [
    {
      "name": "asyset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "getfontsize"
    },
    {
      "name": "asylistingfile"
    },
    {
      "name": "RequireAsyRecompile"
    },
    {
      "name": "AsyCompileIfNecessary"
    },
    {
      "name": "ASYPICcomparefiles",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:file1%file}}{${2:file2%file}}"
      }
    },
    {
      "name": "copyfile",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:file1%file}}{${2:file2%file}}"
      }
    },
    {
      "name": "deletefile",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:file%file}}"
      }
    }
  ],
  "envs": [
    {
      "name": "asypicture",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "asyheader"
    }
  ],
  "keys": {
    "\\begin{asypicture}#c,\\asyset#c": [
      "name=${1:name}\r",
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": []
}