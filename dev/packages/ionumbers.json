{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "ionumbersstyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/ionumbers#c,\\ionumbersstyle"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ionumbersresetstyle"
    },
    {
      "name": "newionumbersthousands",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:value}}{${2:definition}}"
      }
    },
    {
      "name": "newionumbersdecimal",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:value}}{${2:definition}}"
      }
    },
    {
      "name": "newionumbersthousandths",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:value}}{${2:definition}}"
      }
    },
    {
      "name": "newionumbersexponent",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:value}}{${2:definition}}"
      }
    },
    {
      "name": "renewionumbersthousands",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:value}}{${2:definition}}"
      }
    },
    {
      "name": "renewionumbersdecimal",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:value}}{${2:definition}}"
      }
    },
    {
      "name": "renewionumbersthousandths",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:value}}{${2:definition}}"
      }
    },
    {
      "name": "renewionumbersexponent",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:value}}{${2:definition}}"
      }
    },
    {
      "name": "ionumbers"
    },
    {
      "name": "endionumbers"
    },
    {
      "name": "ionumbersoff",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/ionumbers#c,\\ionumbersstyle": [
      "comma=${1|ignore,decimal,thousands,default|}",
      "point=${1|ignore,decimal,thousands,default|}",
      "thousands=${1|none,point,comma,punctpoint,punctcomma,apostrophe,phantom,space,default|}",
      "decimal=${1|none,point,comma,punctpoint,punctcomma,apostrophe,phantom,space,default|}",
      "thousandths=${1|none,point,comma,punctpoint,punctcomma,apostrophe,phantom,space,default|}",
      "exponent=${1|none,original,ite,itE,rme,rmE,timestento,cdottento,wedge,default|}",
      "autothousands",
      "autothousandths",
      "grplenthousands=${1:number}",
      "grplenthousandths=${1:number}"
    ]
  },
  "args": [
    "\\usepackage/ionumbers#c,\\ionumbersstyle"
  ]
}