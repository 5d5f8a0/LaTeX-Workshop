{
  "deps": [
    {
      "name": "calc"
    },
    {
      "name": "xkeyval"
    },
    {
      "name": "picture"
    }
  ],
  "macros": [
    {
      "name": "h",
      "unusual": true
    },
    {
      "name": "labyrinthset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\begin{labyrinth},\\labyrinthset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "putsymbol(x,y)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:symbol}}"
      }
    },
    {
      "name": "plus",
      "unusual": true,
      "detail": "+",
      "doc": "Plus sign"
    },
    {
      "name": "minus",
      "unusual": true,
      "detail": "−",
      "doc": "Minus sign"
    },
    {
      "name": "ast",
      "unusual": true,
      "detail": "∗",
      "doc": "Asterisk operator (hodge star operator)"
    },
    {
      "name": "labyrinthsolution(x,y)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:solution code}}"
      }
    },
    {
      "name": "labyrinthsolution",
      "arg": {
        "format": "[](){}",
        "snippet": "[${2:options%keyvals}](${3:x},${4:y}){${1:solution code}}"
      }
    },
    {
      "name": "solutionset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\labyrinthsolution,\\solutionset,\\autosolution"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "autosolution(x1,y1)(x2,y2)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:first direction}}"
      }
    },
    {
      "name": "autosolution",
      "arg": {
        "format": "[]()(){}",
        "snippet": "[${2:options%keyvals}](${3:x1},${4:y1})(${5:x2},${6:y2}){${1:first direction}}"
      }
    },
    {
      "name": "solutionpath",
      "unusual": true
    },
    {
      "name": "truncdiv",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:num1}}{${2:num2}}"
      }
    }
  ],
  "envs": [
    {
      "name": "labyrinth",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:width%plain}}{${2:height%plain}}"
      }
    },
    {
      "name": "labyrinth",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:width%plain}}{${2:height%plain}}"
      }
    }
  ],
  "keys": {
    "\\begin{labyrinth},\\labyrinthset": [
      "unit=",
      "thickness=",
      "centered"
    ],
    "\\labyrinthsolution,\\solutionset,\\autosolution": [
      "hidden",
      "thicklines",
      "up=${1:symbol}\r",
      "left=${1:symbol}\r",
      "down=${1:symbol}\r",
      "right=${1:symbol}\r",
      "hcorr=",
      "vcorr=",
      "font=${1:font commands}\r"
    ]
  },
  "args": []
}