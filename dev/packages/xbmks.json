{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "xbmksetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\xbmksetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pdfbookmarkx",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "pdfbookmarkx",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:level}]{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "pdfbookmarkx",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:text}}[${3:keyvals}]{${2:name}}"
      }
    },
    {
      "name": "pdfbookmarkx",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "[${3:level}]{${1:text}}[${4:keyvals}]{${2:name}}"
      }
    },
    {
      "name": "currentpdfbookmarkx",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "currentpdfbookmarkx",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:text}}[${3:keyvals}]{${2:name}}"
      }
    },
    {
      "name": "subpdfbookmarkx",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "subpdfbookmarkx",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:text}}[${3:keyvals}]{${2:name}}"
      }
    },
    {
      "name": "belowpdfbookmarkx",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "belowpdfbookmarkx",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:text}}[${3:keyvals}]{${2:name}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/xbmks#c": [
      "dvipsone\r",
      "dvips\r",
      "pdftex\r",
      "luatex\r",
      "xetex\r"
    ],
    "\\xbmksetup": [
      "docbundle={${1:name1,name2,...}}\r",
      "colors={${1:int=<color>,ext=<color>}}\r",
      "styles={${1:style1,style2,...}}\r"
    ],
    "\\pdfbookmarkx,\\currentpdfbookmarkx,\\subpdfbookmarkx,\\belowpdfbookmarkx": [
      "action={${1:PDF action}}\r",
      "color=",
      "style=\r"
    ]
  },
  "args": [
    "\\usepackage/xbmks#c"
  ]
}