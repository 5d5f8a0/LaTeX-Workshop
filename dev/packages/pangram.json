{
  "deps": [],
  "macros": [
    {
      "name": "pangram"
    },
    {
      "name": "pangram",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "PangramSetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\pangram,\\PangramSetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "NewPangramClass",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text class}}{${2:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pangram,\\PangramSetup": [
      "textclass=${1:text-class}\r",
      "sizes={${1:size1,size2,...}}\r",
      "linegap=",
      "tagskip=",
      "tagfont=${1:font commands}\r",
      "font=${1:font commands}\r",
      "before=${1:code}\r",
      "after=${1:code}\r"
    ]
  },
  "args": []
}