      "onecounter",
      "layout=${1|list,table,hidden\r|}",
      "input-dates=${1|d.m.y,m/d/y,y-m-d\r|}",
      "output-dates=${1|d.m.y,m/d/y,y-m-d,d.m.yy,m/d/yy,yy-m-d,d.m.,m/d,m-d,same,datetime\r|}",
      "strict-dates"
    ],
    "\\begin{CheckList},\\CheckListSet": [
      "layout=${1|list,table,hidden\r|}",
      "input-dates=${1|d.m.y,m/d/y,y-m-d\r|}",
      "output-dates=${1|d.m.y,m/d/y,y-m-d,d.m.yy,m/d/yy,yy-m-d,d.m.,m/d,m-d,same,datetime\r|}",
      "strict-dates"
    ],
    "\\Goal,\\Task,\\Artifact,\\Milestone": [
      "who={${1:text}}\r",
      "deadline=${1:date}\r",
      "label=",
      "%checklistentryoption\r"
    ]
