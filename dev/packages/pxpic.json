{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "expkv"
    }
  ],
  "macros": [
    {
      "name": "pxpic",
      "arg": {
        "format": "{}",
        "snippet": "{${1:pixel list}}"
      }
    },
    {
      "name": "pxpic",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:pixel list}}"
      }
    },
    {
      "name": "pxpicsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\pxpic,\\pxpicsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pxpicnewmode",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "pxpicsetmode",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "pxpicnewcolorlist",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:color list}}"
      }
    },
    {
      "name": "pxpicsetcolorlist",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:color list}}"
      }
    },
    {
      "name": "pxpicaddcolorlist",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:color list}}"
      }
    },
    {
      "name": "pxpicforget",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:px}}"
      }
    },
    {
      "name": "px"
    },
    {
      "name": "pxskip"
    },
    {
      "name": "pxpicHT",
      "unusual": true
    },
    {
      "name": "pxpicWD",
      "unusual": true
    },
    {
      "name": "pxpiclogo",
      "unusual": true
    },
    {
      "name": "pxpiclogo",
      "unusual": true,
      "arg": {
        "format": "[]",
        "snippet": "[${1:size}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pxpic,\\pxpicsetup": [
      "colors=${1:color list}",
      "colours=${1:color list}",
      "color-list=${1:choice}",
      "colour-list=${1:choice}",
      "exp=${1|full,none|}",
      "expansion=${1|full,none|}",
      "file",
      "gap-hack=",
      "ht=",
      "lines=${1|group,space,csv|}",
      "mode=${1|px,named,rgb,cmy,cmyk,hsb,Hsb,tHsb,gray,RGB,HTML,HSB,Gray,wave|}",
      "size=",
      "skip=",
      "wd="
    ]
  },
  "args": []
}