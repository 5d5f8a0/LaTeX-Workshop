{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "wires",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:connectivity}}{${2:loose ends}}"
      }
    },
    {
      "name": "wires",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:TikZ keys}]{${1:connectivity}}{${2:loose ends}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\node#c": [
      "box\r",
      "box ports north=${1:integer}\r",
      "box ports east=${1:integer}\r",
      "box ports south=${1:integer}\r",
      "box ports west=${1:integer}\r",
      "box ports=${1:integer1}/${2:integer2}/${3:integer3}/${4:integer4}\r",
      "dot\r"
    ]
  },
  "args": []
}