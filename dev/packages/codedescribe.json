{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "pifont"
    },
    {
      "name": "codelisting"
    }
  ],
  "macros": [
    {
      "name": "defgroupfmt",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:format-group}}{${2:format-keys%keyvals}}"
      }
    },
    {
      "name": "defobjectfmt",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:obj-type}}{${2:format-group}}{${3:format-keys%keyvals}}"
      }
    },
    {
      "name": "describe",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:item-name}}{${2:item-description%text}}"
      }
    },
    {
      "name": "typesetobj",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:csv-list}}"
      }
    },
    {
      "name": "typesetobj",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:obj-type%keyvals}]{${1:csv-list}}"
      }
    },
    {
      "name": "tsobj",
      "arg": {
        "format": "{}",
        "snippet": "{${1:csv-list}}"
      }
    },
    {
      "name": "tsobj",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:obj-type%keyvals}]{${1:csv-list}}"
      }
    },
    {
      "name": "typesetargs",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:csv-list}}"
      }
    },
    {
      "name": "typesetargs",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:obj-type%keyvals}]{${1:csv-list}}"
      }
    },
    {
      "name": "tsargs",
      "arg": {
        "format": "{}",
        "snippet": "{${1:csv-list}}"
      }
    },
    {
      "name": "tsargs",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:obj-type%keyvals}]{${1:csv-list}}"
      }
    },
    {
      "name": "typesetmacro",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:macro-list}}{${2:csv-list}}"
      }
    },
    {
      "name": "typesetmacro",
      "unusual": true,
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:macro-list}}[${3:oargs-list}]{${2:margs-list}}"
      }
    },
    {
      "name": "tsmacro",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:macro-list%definition}}{${2:csv-list}}"
      }
    },
    {
      "name": "tsmacro",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:macro-list%definition}}[${3:oargs-list}]{${2:margs-list}}"
      }
    },
    {
      "name": "typesetmeta",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "tsmeta",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "typesetverb",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:verbatim text%definition}}"
      }
    },
    {
      "name": "typesetverb",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:obj-type%keyvals}]{${1:verbatim text%definition}}"
      }
    },
    {
      "name": "tsverb",
      "arg": {
        "format": "{}",
        "snippet": "{${1:verbatim text%definition}}"
      }
    },
    {
      "name": "tsverb",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:obj-type%keyvals}]{${1:verbatim text%definition}}"
      }
    },
    {
      "name": "typesetmarginnote",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:note%text}}"
      }
    },
    {
      "name": "tsmarginnote",
      "arg": {
        "format": "{}",
        "snippet": "{${1:note%text}}"
      }
    },
    {
      "name": "typesettitle",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:title-keys%keyvals}}",
        "keys": [
          "\\typesettitle,\\tstitle"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "tstitle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title-keys%keyvals}}",
        "keys": [
          "\\typesettitle,\\tstitle"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "typesetdate",
      "unusual": true
    },
    {
      "name": "tsdate"
    }
  ],
  "envs": [
    {
      "name": "codedescribe",
      "arg": {
        "format": "{}",
        "snippet": "{${1:csv-list}}"
      }
    },
    {
      "name": "codedescribe",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:obj-type%keyvals}]{${1:csv-list}}"
      }
    },
    {
      "name": "codesyntax"
    },
    {
      "name": "describelist",
      "arg": {
        "format": "{}",
        "snippet": "{${1:obj-type%keyvals}}"
      }
    },
    {
      "name": "describelist",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:item-indent}]{${1:obj-type%keyvals}}"
      }
    },
    {
      "name": "describelist*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:obj-type%keyvals}}"
      }
    },
    {
      "name": "describelist*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:item-indent}]{${1:obj-type%keyvals}}"
      }
    },
    {
      "name": "tsremark"
    },
    {
      "name": "tsremark",
      "arg": {
        "format": "[]",
        "snippet": "[${1:NB}]"
      }
    },
    {
      "name": "typesetabstract",
      "unusual": true
    },
    {
      "name": "tsabstract"
    }
  ],
  "keys": {
    "\\usepackage/codedescribe#c": [
      "nolisting\r"
    ],
    "\\defgroupfmt#c,\\defobjectfmt#c": [
      "meta\r",
      "xmeta\r",
      "verb\r",
      "xverb\r",
      "code\r",
      "nofmt\r",
      "slshape\r",
      "itshape\r",
      "noshape\r",
      "lbracket=${1:symbol}\r",
      "rbracket=${1:symbol}\r",
      "color="
    ],
    "\\begin{codedescribe}#c,\\begin{describelist}#c,\\begin{describelist*}#c,\\typesetobj#c,\\tsobj#c,\\typesetargs#c,\\tsargs#c,\\typesetverb#c,\\tsverb#c": [
      "arg\r",
      "meta\r",
      "verb\r",
      "xverb\r",
      "marg\r",
      "oarg\r",
      "parg\r",
      "xarg\r",
      "code\r",
      "macro\r",
      "function\r",
      "syntax\r",
      "keyval\r",
      "key\r",
      "keys\r",
      "value\r",
      "option\r",
      "defaultval\r",
      "env\r",
      "pkg\r",
      "pack\r"
    ],
    "\\begin{codedescribe}#c": [
      "rulecolor=${1:color}\r",
      "new=${1:date}\r",
      "update=${1:date}\r",
      "note=${1:text}\r",
      "EXP\r",
      "rEXP\r"
    ],
    "\\typesetobj#c,\\tsobj#c": [
      "sep=${1:separator}\r"
    ],
    "\\typesettitle,\\tstitle": [
      "title=${1:text}\r",
      "author=${1:name}\r",
      "date=${1:date}\r"
    ]
  },
  "args": [
    "\\usepackage/codedescribe#c"
  ]
}