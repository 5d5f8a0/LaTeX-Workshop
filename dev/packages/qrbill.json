{
  "deps": [
    {
      "name": "iftex"
    },
    {
      "name": "l3keys2e"
    },
    {
      "name": "fontspec"
    },
    {
      "name": "anyfontsize"
    },
    {
      "name": "scrbase"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "numprint"
    },
    {
      "name": "qrcode"
    },
    {
      "name": "luatex",
      "if": "qrmode=lua"
    },
    {
      "name": "marvosym",
      "if": "separate=symbol"
    },
    {
      "name": "marvosym",
      "if": "vrule=symbol"
    }
  ],
  "macros": [
    {
      "name": "QRbill"
    },
    {
      "name": "QRbill",
      "arg": {
        "format": "[]",
        "snippet": "[${1:data setup%keyvals}]"
      }
    },
    {
      "name": "QRbill*"
    },
    {
      "name": "QRbill*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:data setup%keyvals}]"
      }
    },
    {
      "name": "qrbillsetdata",
      "arg": {
        "format": "{}",
        "snippet": "{${1:data setup%keyvals}}",
        "keys": [
          "\\QRbill,\\QRbill*,\\SetupQrBill,\\qrbillsetdata,\\qrbillsetdata*,\\qrbillsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "qrbillsetdata*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:data setup%keyvals}}",
        "keys": [
          "\\QRbill,\\QRbill*,\\SetupQrBill,\\qrbillsetdata,\\qrbillsetdata*,\\qrbillsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "SetupQrBill",
      "arg": {
        "format": "{}",
        "snippet": "{${1:data setup%keyvals}}",
        "keys": [
          "\\QRbill,\\QRbill*,\\SetupQrBill,\\qrbillsetdata,\\qrbillsetdata*,\\qrbillsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "qrbillsetup",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:data setup%keyvals}}",
        "keys": [
          "\\QRbill,\\QRbill*,\\SetupQrBill,\\qrbillsetdata,\\qrbillsetdata*,\\qrbillsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "QRbillParseDate",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:year}}{${2:month}}{${3:day}}"
      }
    },
    {
      "name": "QRbillAddCustomReplacement",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:string}}{${2:replacement}}"
      }
    },
    {
      "name": "insertcreditor"
    },
    {
      "name": "insertcurrency"
    },
    {
      "name": "insertdebtor"
    },
    {
      "name": "SetStaticData",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "SetBillingInfoScheme",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:scheme data}}"
      }
    },
    {
      "name": "SetQrScheme",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:scheme data}}"
      }
    },
    {
      "name": "qrbillfont",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/qrbill#c": [
      "billinginfo",
      "creditorprefix=${1:string}\r",
      "debtorprefix=${1:string}\r",
      "data-to-string",
      "font=${1:font name}\r",
      "frame=${1|true,false,top,bottom,none\r|}",
      "ibanseparator=${1:token list}\r",
      "icon=${2|swiss-cross,${1:file name}\r|}",
      "iconwidth=",
      "ignore-if-empty={${1:comma list}}\r",
      "qrmode=${1|package,lua\r|}",
      "qrsize=",
      "qrscheme=${1:name}\r",
      "referenceseparator=${1:token list}\r",
      "sep-iban=${1:integer}\r",
      "sep-reference=${1:integer}\r",
      "separate=${1|false,text,symbol\r|}",
      "replace-tilde",
      "replace-umlauts",
      "vrule=${1|true,false,symbol\r|}"
    ],
    "\\QRbill,\\QRbill*,\\SetupQrBill,\\qrbillsetdata,\\qrbillsetdata*,\\qrbillsetup": [
      "QRType=\r",
      "Version=\r",
      "CodingType=\r",
      "Account=\r",
      "CR-AddressType=\r",
      "CR-Name=\r",
      "CR-Address1=\r",
      "CR-Address2=\r",
      "CR-PostalCode=\r",
      "CR-City=\r",
      "CR-Country=\r",
      "UCR-AddressType=\r",
      "UCR-Name=\r",
      "UCR-Address1=\r",
      "UCR-Address2=\r",
      "UCR-ZIP=\r",
      "UCR-City=\r",
      "UCR-Country=\r",
      "Amount=\r",
      "Currency=\r",
      "UD-AddressType=\r",
      "UD-Name=\r",
      "UD-Address1=\r",
      "UD-Address2=\r",
      "UD-PostalCode=\r",
      "UD-City=\r",
      "UD-Country=\r",
      "ReferenceType=\r",
      "Reference=\r",
      "Message=\r",
      "Trailer=\r",
      "BillingInfo=\r",
      "AV1-Parameters=\r",
      "AV2-Parameters=\r",
      "creditor=\r",
      "creditor*=\r",
      "debtor=\r",
      "debtor*=\r",
      "AV1=\r",
      "AV2=\r",
      "invoicenum=\r",
      "invoicedate=\r",
      "customerref=\r",
      "vat=\r",
      "vatdate=\r",
      "vatdetails=\r",
      "importvat=\r",
      "conditions=\r"
    ]
  },
  "args": [
    "\\usepackage/qrbill#c"
  ]
}