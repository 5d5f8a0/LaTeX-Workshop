{
  "deps": [
    {
      "name": "iftex"
    },
    {
      "name": "luatexbase"
    }
  ],
  "macros": [
    {
      "name": "LPTNewTable",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:name%specialDef}}{${2:num cols}}{${3:preamble}}"
      }
    },
    {
      "name": "LPTNewTable",
      "arg": {
        "format": "{}{}{}[]",
        "snippet": "{${1:name%specialDef}}{${2:num cols}}{${3:preamble}}[${4:options%keyvals}]"
      }
    },
    {
      "name": "LPTSetCurrentTable",
      "arg": {
        "format": "{}",
        "snippet": "{${1:table name%keyvals}}"
      }
    },
    {
      "name": "LPTGetCurrentTable"
    },
    {
      "name": "LPTAddRow"
    },
    {
      "name": "LPTAddRow",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "LPTSetRowProp",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:index expr}}{${2:row options%keyvals}}",
        "keys": [
          "\\LPTAddRow,\\LPTSetRowProp"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "LPTUseTable"
    },
    {
      "name": "LPTDeleteTable",
      "arg": {
        "format": "{}",
        "snippet": "{${1:table name%keyvals}}"
      }
    },
    {
      "name": "LPTSetCell",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:index expr}}{${2:content}}"
      }
    },
    {
      "name": "LPTSetCell",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:index expr}}[${3:shape}]{${2:content}}"
      }
    },
    {
      "name": "LPTFill",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:index expr}}{${2:content}}"
      }
    },
    {
      "name": "LPTGetTableNames"
    },
    {
      "name": "LPTGetTableShape"
    },
    {
      "name": "LPTGetCellData",
      "arg": {
        "format": "{}",
        "snippet": "{${1:index expr}}"
      }
    },
    {
      "name": "LPTGetCellShape",
      "arg": {
        "format": "{}",
        "snippet": "{${1:index expr}}"
      }
    },
    {
      "name": "LPTGetCellParent",
      "arg": {
        "format": "{}",
        "snippet": "{${1:index expr}}"
      }
    },
    {
      "name": "LPTGetCellMetaIndex",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    }
  ],
  "envs": [
    {
      "name": "lptview",
      "arg": {
        "format": "{}",
        "snippet": "{${1:index expr}}"
      }
    },
    {
      "name": "lptfill",
      "arg": {
        "format": "{}",
        "snippet": "{${1:index expr}}"
      }
    }
  ],
  "keys": {
    "\\LPTNewTable": [
      "backend=${1:envname}\r",
      "default before line=${1:code}\r",
      "default after line=${1:code}\r",
      "default after spacing=${1:code}\r",
      "input method=${1|file,stringbuffer\r|}",
      "nrows=${1:integer}\r"
    ],
    "\\LPTSetCurrentTable#c,\\LPTDeleteTable#c": [
      "%LPTtablename\r"
    ],
    "\\LPTAddRow,\\LPTSetRowProp": [
      "before line\r",
      "after line\r",
      "after spacing\r"
    ]
  },
  "args": []
}