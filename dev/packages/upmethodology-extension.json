{
  "deps": [
    {
      "name": "upmethodology-p-common"
    }
  ],
  "macros": [
    {
      "name": "Ifdefined",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg}}{${2:code}}"
      }
    },
    {
      "name": "Ifelsedefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:arg}}{${2:true code}}{${3:false code}}"
      }
    },
    {
      "name": "Ifundefined",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg}}{${2:code}}"
      }
    },
    {
      "name": "Ifelseundefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:arg}}{${2:true code}}{${3:false code}}"
      }
    },
    {
      "name": "Get",
      "arg": {
        "format": "{}",
        "snippet": "{${1:variable}}"
      }
    },
    {
      "name": "GetLang",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:variable}}{${2:language}}"
      }
    },
    {
      "name": "Set",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:variable}}{${2:name}}"
      }
    },
    {
      "name": "Set",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:language}]{${1:variable}}{${2:name}}"
      }
    },
    {
      "name": "Append",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:variable}}{${2:text}}"
      }
    },
    {
      "name": "Append",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:language}]{${1:variable}}{${2:text}}"
      }
    },
    {
      "name": "Unset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:variable}}"
      }
    },
    {
      "name": "Unset",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:language}]{${1:variable}}"
      }
    },
    {
      "name": "DeclareCopyright",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:extname}}{${2:year}}{${3:copyrighter}}{${4:text}}"
      }
    },
    {
      "name": "DeclareCopyright",
      "arg": {
        "format": "[]{}{}{}{}",
        "snippet": "[${5:language}]{${1:extname}}{${2:year}}{${3:copyrighter}}{${4:text}}"
      }
    },
    {
      "name": "Put",
      "arg": {
        "format": "(){}",
        "snippet": "(${2:x},${3:y}){${1:stuff}}"
      }
    },
    {
      "name": "UseExtension",
      "arg": {
        "format": "{}",
        "snippet": "{${1:extname}}"
      }
    }
  ],
  "envs": [],
  "keys": {},
  "args": []
}