  "keys": {
    "\\njustsetup": [
      "logo=${1:logo code}\r",
      "cover-title=${1:text}\r",
      "author=${1:name}\r",
      "id=${1:id number}\r",
      "department=${1:department}\r",
      "discipline=${1:discipline}\r",
      "title=${1:text}\r",
      "subtitle=${1:text}\r",
      "supervisor=${1:name}\r",
      "associate-supervisor=${1:name}\r",
      "judge=${1:name}\r",
      "supervisor-title=${1:text}\r",
      "associate-supervisor-title=${1:text}\r",
      "judge-title=${1:text}\r",
      "year=${1:year}\r",
      "month=${1:month}\r",
      "day=${1:day}\r",
      "signature=${1:signature}\r",
      "signature*=${1:English signature}\r",
      "abstract-prefix=\r",
      "title*=${1:English text}\r",
      "subtitle*=${1:English text}\r",
      "header-title=${1:text}\r"
    ]
  },
  "args": []
}