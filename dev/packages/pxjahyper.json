{
  "deps": [
    {
      "name": "platex"
    },
    {
      "name": "keyval"
    },
    {
      "name": "ltxcmds"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "bxjatoucs"
    },
    {
      "name": "pxjahyper-enc",
      "if": "tounicode"
    },
    {
      "name": "uplatex",
      "if": "bigcode"
    }
  ],
  "macros": [
    {
      "name": "pxjahypersetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "Ux",
      "arg": {
        "format": "{}",
        "snippet": "{${1:Unicode 符号値 16 進}}"
      }
    },
    {
      "name": "pxDeclarePdfTextCommand",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:cmd}}{${2:jis}}{${3:ucs}}"
      }
    },
    {
      "name": "pxDeclarePdfTextComposite",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:cmd}}{${2:arg}}{${3:jis}}{${4:ucs}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pxjahyper#c": [
      "dvipdfmx\r",
      "dvipdfm\r",
      "dvips\r",
      "nodvidriver\r",
      "resetdvidriver\r",
      "autodvidriver\r",
      "tounicode\r",
      "notounicode\r",
      "out2uni\r",
      "noout2uni\r",
      "convbkmk\r",
      "noconvbkmk\r",
      "papersize\r",
      "nopapersize\r",
      "charcmds\r",
      "nocharcmds\r",
      "bigcode\r",
      "nobigcode\r",
      "jacommentline\r",
      "nojacommentline\r",
      "disablecmds\r",
      "nodisablecmds\r",
      "otfutf\r",
      "nootfutf\r",
      "otfcid\r",
      "nootfcid\r",
      "otfmacros\r",
      "nootfmacros\r",
      "force-unicode\r"
    ],
    "\\pxjahypersetup#c,\\usepackage/pxjahyper#c": [
      "fallback=${1|geta,delete\r|}",
      "extjis",
      "ajhankaku-chars"
    ]
  },
  "args": [
    "\\usepackage/pxjahyper#c",
    "\\pxjahypersetup#c,\\usepackage/pxjahyper#c"
  ]
}