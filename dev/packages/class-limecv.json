    "\\documentclass/limecv#c": [
      "print",
      "path=${1:file path}\r",
      "sansfont=${1:font name}\r",
      "monofont=${1:font name}\r"
    ],
    "\\cvSetLanguage#c": [
      "chinese\r",
      "dutch\r",
      "english\r",
      "french\r",
      "german\r",
      "italian\r",
      "spanish\r"
    ],
    "\\cvProject": [
      "image=${1:imagefile}\r",
      "link=${1:URL}\r",
      "width=",
      "height="
    ],
    "\\cvAddReference,\\cvAddReference*": [
      "name=\r",
      "company=\r",
      "position=\r",
      "address line 1=\r",
      "address line 2=\r",
      "address line 3=\r",
      "mobile phone=\r",
      "work phone=\r",
      "email=\r"
    ],
    "\\cvBeneficiary": [
      "name=\r",
      "position=\r",
      "company=\r",
      "address line 1=\r",
      "address line 2=\r",
      "address line 3=\r"
    ],
    "\\tikzset#c": [
      "interesticon/.style={${1:TikZ styles}}\r",
      "interesttext/.style={${1:TikZ styles}}\r",
      "eventdottext/.style={${1:TikZ styles}}\r",
      "invisibletimedot/.style={${1:TikZ styles}}\r",
      "timedot/.style={${1:TikZ styles}}\r",
      "eventdot/.style={${1:TikZ styles}}\r",
      "contactIcon/.style={${1:TikZ styles}}\r",
      "contactText/.style={${1:TikZ styles}}\r",
      "headerIcon/.style={${1:TikZ styles}}\r",
      "skillLevel/.style={${1:TikZ styles}}\r",
      "languageText/.style={${1:TikZ styles}}\r",
      "skillText/.style={${1:TikZ styles}}\r",
      "progressArea/.style={${1:TikZ styles}}\r",
      "progressBar/.style={${1:TikZ styles}}\r",
      "sectionTitle/.style={${1:TikZ styles}}\r",
      "sectionEduText/.style={${1:TikZ styles}}\r"
    ]
  },
  "args": [
    "\\documentclass/limecv#c"
  ]
