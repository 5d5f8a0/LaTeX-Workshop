{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "multido"
    }
  ],
  "macros": [
    {
      "name": "setTileBgGraphic",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "setTileBgGraphic",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    },
    {
      "name": "disableTiling"
    },
    {
      "name": "enableTiling"
    },
    {
      "name": "maxiterations",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "autosetScreensizeWithMargins",
      "arg": {
        "format": "{}{}{}{}{}{}",
        "snippet": "{${1:rows}}{${2:cols}}{${3:left}}{${4:right}}{${5:top}}{${6:bottom}}"
      }
    },
    {
      "name": "placeTilesinLayers",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "placeTilesinLayers",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:keyvals}}"
      }
    },
    {
      "name": "theReqHeight",
      "unusual": true
    },
    {
      "name": "theReqWidth",
      "unusual": true
    },
    {
      "name": "tileboxheight",
      "unusual": true
    },
    {
      "name": "tileboxwidth",
      "unusual": true
    },
    {
      "name": "tileheight",
      "unusual": true
    },
    {
      "name": "tilewidth",
      "unusual": true
    },
    {
      "name": "turnOffTiling",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/aeb_tilebg#c": [
      "uselltiling\r",
      "draft\r",
      "ignoreforpaper\r"
    ],
    "\\setTileBgGraphic": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r",
      "name=${1:name}\r",
      "transparency={${1:specs}}\r",
      "presp={${1:PS code}}\r",
      "postsp={${1:PS code}}\r"
    ]
  },
  "args": [
    "\\usepackage/aeb_tilebg#c"
  ]
}