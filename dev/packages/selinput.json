{
  "deps": [
    {
      "name": "inputenc"
    },
    {
      "name": "kvsetkeys"
    },
    {
      "name": "stringenc"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "SelectInputEncodingList",
      "arg": {
        "format": "{}",
        "snippet": "{${1:encoding list}}"
      }
    },
    {
      "name": "SelectInputMappings",
      "arg": {
        "format": "{}",
        "snippet": "{${1:mapping pairs}}"
      }
    },
    {
      "name": "SelectInputDefineMapping",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/selinput#c": [
      "warning\r",
      "ucs\r",
      "utf8=${1:file name}\r"
    ]
  },
  "args": [
    "\\usepackage/selinput#c"
  ]
}