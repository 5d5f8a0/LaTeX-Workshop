{
  "deps": [
    {
      "name": "iftex"
    }
  ],
  "macros": [
    {
      "name": "greekfontfamily",
      "unusual": true
    },
    {
      "name": "greektxt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:greek text}}"
      }
    },
    {
      "name": "greektxt",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:font family}]{${1:greek text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "greek"
    },
    {
      "name": "greek",
      "arg": {
        "format": "[]",
        "snippet": "[${1:font family}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/begingreek#c": [
      "artemisia\r",
      "gfsbaskerville\r",
      "bodoni\r",
      "complutum\r",
      "udidot\r",
      "neohellenic\r",
      "porson\r",
      "solomos\r",
      "txr\r",
      "mak\r",
      "llcmss\r",
      "lmr\r"
    ]
  },
  "args": [
    "\\usepackage/begingreek#c"
  ]
}