{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "color",
      "if": "form"
    },
    {
      "name": "pifont",
      "if": "form"
    },
    {
      "name": "color",
      "if": "form=true"
    },
    {
      "name": "pifont",
      "if": "form=true"
    }
  ],
  "macros": [
    {
      "name": "vpesetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "VPE",
      "unusual": true
    },
    {
      "name": "vpeentry",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "vpesystem",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg1}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/vpe#c": [
      "debug\r"
    ],
    "\\usepackage/vpe#c,\\vpesetup#c": [
      "active\r",
      "inactive\r",
      "dupes\r",
      "nodupes\r",
      "linux\r",
      "unix\r",
      "win\r",
      "dos\r",
      "system=${1|linux,win\r|}",
      "form\r",
      "noform\r",
      "acroform\r",
      "noacroform\r",
      "command=${1:script}\r",
      "progname=${1:program}\r",
      "application=${1:application}\r",
      "parameters=${1:parameters}\r",
      "width=${1:integer}\r",
      "height=${1:integer}\r",
      "depth=${1:integer}\r",
      "color=${1:r g b}\r",
      "border=${1:r g b}\r",
      "flag=${1:flag}\r",
      "attr=${1:attr}\r",
      "everyhbox\r",
      "noeveryhbox\r"
    ]
  },
  "args": [
    "\\usepackage/vpe#c",
    "\\usepackage/vpe#c,\\vpesetup#c"
  ]
}