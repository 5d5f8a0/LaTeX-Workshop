{
  "deps": [
    {
      "name": "xcolor"
    }
  ],
  "macros": [
    {
      "name": "keyboard",
      "arg": {
        "format": "{}",
        "snippet": "{${1:notes}}"
      }
    },
    {
      "name": "keyboard",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:notes}}"
      }
    },
    {
      "name": "keyboardsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\keyboard,\\keyboardsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\keyboard,\\keyboardsetup": [
      "color=",
      "single",
      "ext",
      "size=",
      "height=${1:number}\r",
      "ratio=${1:number}\r",
      "numbers",
      "font=${1:font commands}\r",
      "numbercolor=",
      "10=${1:code}\r",
      "11=${1:code}\r"
    ]
  },
  "args": []
}