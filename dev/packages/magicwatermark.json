{
  "deps": [
    {
      "name": "everypage-1x"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "PageSetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content for watermark%text}}"
      }
    },
    {
      "name": "PageSetup",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:content for watermark%text}}"
      }
    },
    {
      "name": "PageSetup*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content for watermark%text}}"
      }
    },
    {
      "name": "Watermark"
    },
    {
      "name": "EvenPageSetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content for watermark%text}}"
      }
    },
    {
      "name": "EvenPageSetup",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:content for watermark%text}}"
      }
    },
    {
      "name": "EvenPageSetup*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content for watermark%text}}"
      }
    },
    {
      "name": "EvenWatermark"
    },
    {
      "name": "OddPageSetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content for watermark%text}}"
      }
    },
    {
      "name": "OddPageSetup",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:content for watermark%text}}"
      }
    },
    {
      "name": "OddPageSetup*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content for watermark%text}}"
      }
    },
    {
      "name": "OddWatermark"
    },
    {
      "name": "NewWatermark",
      "arg": {
        "format": "{}",
        "snippet": "{${1:watermark name}}"
      }
    },
    {
      "name": "MyPageSetup",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:watermark name}}{${2:content for watermark%text}}"
      }
    },
    {
      "name": "MyPageSetup",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:watermark name}}{${2:content for watermark%text}}"
      }
    },
    {
      "name": "MyWatermark",
      "arg": {
        "format": "{}",
        "snippet": "{${1:watermark name}}"
      }
    },
    {
      "name": "ClearWatermark",
      "arg": {
        "format": "{}",
        "snippet": "{${1:watermark list}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\PageSetup,\\EvenPageSetup,\\OddPageSetup,\\MyPageSetup": [
      "scale=${1:factor}\r",
      "opacity=${1:factor}\r",
      "shift={(${1:x,y})}\r",
      "color=",
      "rotate=${1:degrees}\r",
      "align=${1|center,left,right\r|}",
      "showframe"
    ]
  },
  "args": []
}