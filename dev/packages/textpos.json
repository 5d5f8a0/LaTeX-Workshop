{
  "deps": [
    {
      "name": "everyshi"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "TPoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\TPoptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "TPGrid",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:nhoriz}}{${2:nvert}}"
      }
    },
    {
      "name": "TPGrid",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:x,y}]{${1:nhoriz}}{${2:nvert}}"
      }
    },
    {
      "name": "TPShowGrid",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:nhoriz}}{${2:nvert}}"
      }
    },
    {
      "name": "TPMargin",
      "arg": {
        "format": "{}",
        "snippet": "{${1:size%l}}"
      }
    },
    {
      "name": "TPMargin*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:size%l}}"
      }
    },
    {
      "name": "TPReferencePosition",
      "arg": {
        "format": "{}",
        "snippet": "{${1:val1,val2}}"
      }
    },
    {
      "name": "TPHorizModule"
    },
    {
      "name": "TPVertModule"
    },
    {
      "name": "ifTPshowboxes",
      "unusual": true
    },
    {
      "name": "TPshowboxestrue"
    },
    {
      "name": "TPshowboxesfalse"
    },
    {
      "name": "TPboxrulesize"
    },
    {
      "name": "textblocklabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label%plain}}"
      }
    },
    {
      "name": "showtextsize"
    },
    {
      "name": "textblockorigin",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:hpos}}{${2:vpos}}"
      }
    },
    {
      "name": "textblockcolour",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "textblockcolor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "textblockrulecolour",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "textblockrulecolor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "tekstblokkulur",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "tekstblokroolkulur",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    }
  ],
  "envs": [
    {
      "name": "textblock",
      "arg": {
        "format": "{}()",
        "snippet": "{${1:hsize}}(${2:hpos},${3:vpos})"
      }
    },
    {
      "name": "textblock",
      "arg": {
        "format": "{}[]()",
        "snippet": "{${1:hsize}}[${2:ho,vo}](${3:hpos},${4:vpos})"
      }
    },
    {
      "name": "textblock*",
      "arg": {
        "format": "{}()",
        "snippet": "{${1:hsize}}(${2:hpos},${3:vpos})"
      }
    },
    {
      "name": "textblock*",
      "arg": {
        "format": "{}[]()",
        "snippet": "{${1:hsize}}[${2:ho,vo}](${3:hpos},${4:vpos})"
      }
    }
  ],
  "keys": {
    "\\usepackage/textpos#c": [
      "showboxes\r",
      "noshowtext\r",
      "absolute\r",
      "relative\r",
      "overlay\r",
      "quiet\r",
      "verbose\r",
      "discardcontent\r"
    ],
    "\\TPoptions": [
      "absolute",
      "overlay",
      "verbose",
      "showboxes",
      "showtext",
      "discardcontent"
    ]
  },
  "args": [
    "\\usepackage/textpos#c"
  ]
}