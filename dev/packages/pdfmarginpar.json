{
  "deps": [
    {
      "name": "pgfkeys"
    }
  ],
  "macros": [
    {
      "name": "pdfmarginpar",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "pdfmarginpar",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}"
      }
    },
    {
      "name": "pdfmarginparset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pdfmarginpar#c,\\pdfmarginparset#c": [
      "Name=${1|Comment,Key,Note,Help,NewParagraph,Paragraph,Insert,none\r|}",
      "Comment\r",
      "Key\r",
      "Note\r",
      "Help\r",
      "NewParagraph\r",
      "Paragraph\r",
      "Insert\r",
      "Open",
      "color=[${1:R G B}]\r",
      "opacity=${1:factor}\r",
      "Subject={${1:subject}}\r",
      "voffset=",
      "hoffset=",
      "Subtype=Text\r",
      "Subtype/Other=${1:pdf name}\r",
      "caption=${1:text}\r",
      "width=",
      "height=",
      "depth="
    ]
  },
  "args": []
}