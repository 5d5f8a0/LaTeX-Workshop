    {
      "name": "luatex"
    }
  ],
  "macros": [
    {
      "name": "mplibtextextlabel",
      "if": "luamplib",
      "arg": {
        "format": "{}",
        "snippet": "{${1:choice%keyvals}}"
      }
    },
    {
      "name": "mplibshowlog",
      "if": "luamplib",
      "arg": {
        "format": "{}",
        "snippet": "{${1:choice%keyvals}}"
      }
    },
    {
      "name": "mplibcodeinherit",
      "if": "luamplib",
      "arg": {
        "format": "{}",
        "snippet": "{${1:choice%keyvals}}"
      }
    },
    {
      "name": "mplibsetformat",
      "if": "luamplib",
      "arg": {
        "format": "{}",
        "snippet": "{${1:format name%keyvals}}"
      }
    },
    {
      "name": "mplibnumbersystem",
      "if": "luamplib",
      "arg": {
        "format": "{}",
        "snippet": "{${1:type%keyvals}}"
      }
    },
    {
      "name": "everymplib",
      "if": "luamplib",
      "arg": {
        "format": "{}",
        "snippet": "{${1:metapost code}}"
      }
    },
    {
      "name": "everymplib",
      "if": "luamplib",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:instance name}]{${1:metapost code}}"
      }
    },
    {
      "name": "everyendmplib",
      "if": "luamplib",
      "arg": {
        "format": "{}",
        "snippet": "{${1:metapost code%definition}}"
      }
    },
    {
      "name": "everyendmplib",
      "if": "luamplib",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:instance name}]{${1:metapost code}}"
      }
    },
    {
      "name": "newmetapostenvironment",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "newmetapostenvironment",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:envname}}"
      }
    },
    {
      "name": "mpcolor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "directmetapost",
      "arg": {
        "format": "{}",
        "snippet": "{${1:metapost code}}"
      }
    },
    {
      "name": "directmetapost",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:metapost code}}"
      }
    },
    {
      "name": "newmetapostinstance",
      "arg": {
        "format": "{}",
        "snippet": "{${1:cmd}}"
      }
    },
    {
      "name": "newmetapostinstance",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:cmd}}"
      }
    },
    {
      "name": "voidbox",
      "unusual": true
    },
    {
      "name": "ignore",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "unbrace",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "firstoftwo",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "secondoftwo",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "nextif",
      "unusual": true
    },
    {
      "name": "nextifx",
      "unusual": true
    },
    {
      "name": "nextifcat",
      "unusual": true
    },
    {
      "name": "withoptions",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:default}]{${1:code}}"
      }
    },
    {
      "name": "splitcommalist",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:code}}{${2:list}}"
      }
    },
    {
      "name": "unset",
      "unusual": true
    },
    {
      "name": "setluachunkname",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:cmd}}{${2:actual name}}"
      }
    }
  ],
  "envs": [
    {
      "name": "metapost"
    },
    {
      "name": "metapost",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/minim-mp#c": [
      "luamplib"
    ],
    "\\mplibtextextlabel#c,\\mplibshowlog#c,\\mplibcodeinherit#c": [
      "enable",
      "disable"
    ],
    "\\mplibsetformat#c": [
      "plain",
      "metafun"
    ],
    "\\mplibnumbersystem#c": [
      "scaled",
      "double",
      "decimal"
    ]
  },
  "args": [
    "\\usepackage/minim-mp#c"
  ]
}