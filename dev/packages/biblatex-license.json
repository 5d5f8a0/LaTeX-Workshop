{
  "deps": [
    {
      "name": "biblatex"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "biblicenseintrotext",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/biblatex-license#c": [
      "license=${1|short,full,off\r|}",
      "url",
      "link",
      "introtext={${1:text}}\r"
    ]
  },
  "args": [
    "\\usepackage/biblatex-license#c"
  ]
}