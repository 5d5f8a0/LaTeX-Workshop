{
  "deps": [
    {
      "name": "calc"
    },
    {
      "name": "keyval"
    },
    {
      "name": "eso-pic"
    }
  ],
  "macros": [
    {
      "name": "typogridsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/typogrid#c,\\typogridsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "typogrid"
    },
    {
      "name": "gridwidth"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/typogrid#c": [
      "draft\r",
      "final\r",
      "colorgrid\r",
      "grid\r"
    ],
    "\\usepackage/typogrid#c,\\typogridsetup": [
      "columns=${1:number}\r",
      "headings",
      "margin"
    ]
  },
  "args": [
    "\\usepackage/typogrid#c",
    "\\usepackage/typogrid#c,\\typogridsetup"
  ]
}