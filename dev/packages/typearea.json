{
  "deps": [
    {
      "name": "scrkbase"
    },
    {
      "name": "scrlogo"
    }
  ],
  "macros": [
    {
      "name": "PaperNameToSize",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "ProvideUnknownPaperSizeError",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type}}{${2:code}}"
      }
    },
    {
      "name": "isopaper",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "isopaper",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:letter}]{${1:number}}"
      }
    },
    {
      "name": "AfterCalculatingTypearea",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
