    },
    {
      "name": "AfterCalculatingTypearea*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "activateareas"
    },
    {
      "name": "typearea",
      "arg": {
        "format": "{}",
        "snippet": "{${1:DIV}}"
      }
    },
    {
      "name": "typearea",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:BCOR}]{${1:DIV}}"
      }
    },
    {
      "name": "recalctypearea"
    },
    {
      "name": "storeareas",
      "arg": {
        "format": "{}",
        "snippet": "{${1:command}}"
      }
    },
    {
      "name": "BeforeRestoreareas",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "BeforeRestoreareas*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "AfterRestoreareas",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "AfterRestoreareas*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "areaset",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:width}}{${2:height}}"
      }
    },
    {
      "name": "areaset",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:BCOR}]{${1:width}}{${2:height}}"
      }
    },
    {
      "name": "AfterSettingArea",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "AfterSettingArea*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "footheight",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\KOMAoptions#c": [
      "BCOR=${1:correction}\r",
      "DIV=${1|areaset,calc,classic,current,default,last\r|}",
      "twoside=${1|true,on,yes,false,off,no,semi\r|}",
      "twocolumn=${1|true,on,yes,false,off,no\r|}",
      "headinclude=${1|true,on,yes,false,off,no\r|}",
      "footinclude=${1|true,on,yes,false,off,no\r|}",
      "mpinclude=${1|true,on,yes,false,off,no\r|}",
      "headlines=${1:number of lines}\r",
      "headheight=",
      "footlines=${1:number of lines}\r",
      "footheight=",
      "paper=${1|letter,legal,executive,A0,A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,B0,B1,B2,B3,B4,B5,B6,B7,B8,B9,B10,B11,B12,C0,C1,C2,C3,C4,C5,C6,C7,C8,C9,C10,landscape,seascape,portrait\r|}",
      "pagesize\r",
      "pagesize=${1|auto,automedia,false,no,off,dvipdfmx,dvips,pdftex,luatex\r|}",
      "headsepline=${1|true,on,yes,false,off,no\r|}",
      "footsepline=${1|true,on,yes,false,off,no\r|}",
      "areasetadvanced=${1|true,on,yes,false,off,no\r|}",
      "usegeometry\r"
    ]
  },
  "args": []
}