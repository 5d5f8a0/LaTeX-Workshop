{
  "deps": [
    {
      "name": "processkv"
    }
  ],
  "macros": [
    {
      "name": "colchunk",
      "arg": {
        "format": "{}",
        "snippet": "{${1:chunk%text}}"
      }
    },
    {
      "name": "colchunk",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:column}]{${1:chunk%text}}"
      }
    },
    {
      "name": "colplacechunks"
    }
  ],
  "envs": [
    {
      "name": "parcolumns",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number of cols}}"
      }
    },
    {
      "name": "parcolumns",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:number of cols}}"
      }
    }
  ],
  "keys": {
    "\\begin{parcolumns}": [
      "colwidths={${1:col=width,...}}\r",
      "distance=",
      "rulebetween",
      "nofirstindent",
      "sloppy",
      "sloppyspaces"
    ]
  },
  "args": []
}