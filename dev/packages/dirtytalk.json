{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "ifthen"
    }
  ],
  "macros": [
    {
      "name": "say",
      "arg": {
        "format": "{}",
        "snippet": "{${1:quotation%text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/dirtytalk#c": [
      "left=${1:symbol}\r",
      "right=${1:symbol}\r",
      "leftsub=${1:symbol}\r",
      "rightsub=${1:symbol}\r"
    ]
  },
  "args": [
    "\\usepackage/dirtytalk#c"
  ]
}