{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "eforms"
    }
  ],
  "macros": [
    {
      "name": "rtpara",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name%specialDef}}{${2:richtext paragraph%text}}"
      }
    },
    {
      "name": "rtpara",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:name%specialDef}}{${2:richtext paragraph%text}}"
      }
    },
    {
      "name": "span",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:options%keyvals}}{${2:richtext string%text}}",
        "keys": [
          "\\rtpara,\\span,\\setDefaultStyle"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "useRV",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}",
        "keys": [
          "\\useRV,\\useV,\\displayRV,\\displayV"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "useV",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}",
        "keys": [
          "\\useRV,\\useV,\\displayRV,\\displayV"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "sub",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "sup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "spc"
    },
    {
      "name": "br"
    },
    {
      "name": "RV",
      "arg": {
        "format": "{}",
        "snippet": "{${1:rich value}}"
      }
    },
    {
      "name": "DS",
      "arg": {
        "format": "{}",
        "snippet": "{${1:default style}}"
      }
    },
    {
      "name": "useDefaultDS"
    },
    {
      "name": "setDefaultStyle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name%specialDef}}{${2:options%keyvals}}",
        "keys": [
          "\\rtpara,\\span,\\setDefaultStyle"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "useDS",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}",
        "keys": [
          "\\useDS"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "useRVContent",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}",
        "keys": [
          "\\useRVContent,\\useVContent"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "useVContent",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}",
        "keys": [
          "\\useRVContent,\\useVContent"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "displayRV",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}"
      }
    },
    {
      "name": "displayV",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}"
      }
    },
    {
      "name": "contName",
      "unusual": true
    },
    {
      "name": "displayRtParaName",
      "unusual": true
    },
    {
      "name": "makePDFSp",
      "unusual": true
    },
    {
      "name": "makeTeXSp",
      "unusual": true
    },
    {
      "name": "makeTeXSpPrnt",
      "unusual": true
    },
    {
      "name": "resetRtFontKeys",
      "unusual": true
    },
    {
      "name": "rtpdfSPDef",
      "unusual": true
    },
    {
      "name": "rtpdfSPDefPrnt",
      "unusual": true
    },
    {
      "name": "rvorvstring",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    }
  ],
  "envs": [
    {
      "name": "displayRtPara",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "displayRtPara*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    }
  ],
  "keys": {
    "\\useRV,\\useV,\\displayRV,\\displayV": [
      "%rtpara\r"
    ],
    "\\useDS": [
      "%rtdefaultstyle\r"
    ],
    "\\useRVContent,\\useVContent": [
      "%rvvcontent\r"
    ],
    "\\rtpara,\\span,\\setDefaultStyle": [
      "font=${1:font name}\r",
      "size=${1:number}\r",
      "raise=${1:number}\r",
      "ulstyle=${1|none,ul,2ul,wul,2wul\r|}",
      "style={${1:bold, italic, and/or strikeit}}\r",
      "color=${1:rrggbb}\r",
      "url={${1:URL}}\r",
      "raw=${1:CSS2 markup}\r"
    ],
    "\\rtpara": [
      "halign=${1|left,center,right,justify\r|}",
      "valign=${1|top,middle,bottom\r|}",
      "margleft=${1:number}\r",
      "margright=${1:number}\r",
      "indent=${1|none,first,hanging\r|}",
      "indentby=${1:number}\r",
      "margtop=${1:number}\r",
      "margbottom=${1:number}\r",
      "linespacing=${1|single,oneandhalf,double,exact\r|}",
      "lineheight=${1:number}\r"
    ]
  },
  "args": []
}