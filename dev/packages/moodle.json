  "keys": {
    "\\usepackage/moodle#c": [
      "draft\r",
      "handout\r",
      "final\r",
      "nostamp\r",
      "tikz\r",
      "svg\r",
      "section\r",
      "section*\r",
      "subsection\r",
      "subsection*\r",
      "feedbackleft\r",
      "feedbackright\r",
      "samepage\r",
      "LMS=${1:major.minor}\r",
      "pluginfile\r"
    ],
    "\\begin{quiz},\\moodleset,\\begin{truefalse},\\begin{multi},\\begin{numerical},\\begin{shortanswer},\\begin{essay},\\begin{matching},\\begin{cloze}": [
      "points=${1:number}\r",
      "default grade=${1:number}\r",
      "penalty=${1:fraction}\r",
      "fraction=${1:number}\r",
      "fractiontol=${1:value}\r",
      "feedback={${1:text}}\r",
      "tags={${1:tag1,tag2,...}}\r",
      "shuffle",
      "numbering=${1|alph,Alph,arabic,roman,Roman,none\r|}",
      "single",
      "sanction=${1:number}\r",
      "multiple",
      "allornothing",
      "tolerance=${1:value}\r",
      "case sensitive",
      "usecase",
      "response required\r",
      "response format=${1|html,file,html+file,text,monospaced\r|}",
      "response field lines=${1|5,10,15,20,25,30,25,40\r|}",
      "attachments allowed=${1|0,1,2,3,unlimited\r|}",
      "attachments required=${1|0,1,2,3\r|}",
      "template={${1:text}}\r",
      "drag and drop",
      "dd",
      "vertical\r",
      "horizontal\r",
      "inline\r",
      "ppi=${1:pixels per inch}\r"
    ],
    "\\includegraphics": [
      "ppi=${1:pixels per inch}\r"
    ],
    "\\VerbatimInput,\\BVerbatimInput,\\LVerbatimInput,\\inputminted": [
      "gobble=${1:integer}\r",
      "autogobble",
      "tabsize=${1:integer}\r",
      "numbers=${1|none,left,right,both\r|}",
      "firstnumber=${1:auto|last|<integer>}\r",
      "firstline=${1:integer}\r",
      "lastline=${1:integer}\r",
      "numberblanklines",
      "highlightlines={${1:line ranges}}\r",
      "style=${1:style name}\r"
    ]
  },
  "args": [
    "\\usepackage/moodle#c"
  ]
