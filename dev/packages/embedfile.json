{
  "deps": [
    {
      "name": "infwaerr"
    },
    {
      "name": "iftex"
    },
    {
      "name": "pdftexcmds"
    },
    {
      "name": "ltxcmds"
    },
    {
      "name": "kvsetkeys"
    },
    {
      "name": "kvdefinekeys"
    },
    {
      "name": "pdfescape"
    }
  ],
  "macros": [
    {
      "name": "embedfile",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "embedfile",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}"
      }
    },
    {
      "name": "embedfilesetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "embedfilefinish",
      "unusual": true
    },
    {
      "name": "embedfilefield",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:key%plain}}{${2:options%keyvals}}",
        "keys": [
          "\\embedfilefield"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "embedfilesort",
      "arg": {
        "format": "{}",
        "snippet": "{${1:key-sort-list}}"
      }
    },
    {
      "name": "embedfileifobjectexists",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:id}}{${2:type%keyvals}}{${3:then}}{${4:else}}",
        "keys": [
          "\\embedfileifobjectexists,\\embedfilegetobject"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "embedfilegetobject",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:id}}{${2:type%keyvals}}",
        "keys": [
          "\\embedfileifobjectexists,\\embedfilegetobject"
        ],
        "keyPos": 1
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\embedfile#c,\\embedfilesetup#c": [
      "filespec=\r",
      "ucfilespec=\r",
      "filesystem=\r",
      "mimetype=\r",
      "desc=${1:description}\r",
      "afrelationship=${1:PDF name}\r",
      "stringmethod=${1|psd,escape\r|}",
      "id=\r"
    ],
    "\\embedfilesetup#c": [
      "view=${1|details,tile,hidden\r|}",
      "initialfile=\r"
    ],
    "\\embedfilefield": [
      "type=${1|text,date,number,file,desc,moddate,size\r|}",
      "title=\r",
      "visible",
      "edit"
    ],
    "\\embedfileifobjectexists,\\embedfilegetobject": [
      "EmbeddedFile\r",
      "Filespec\r"
    ]
  },
  "args": []
}