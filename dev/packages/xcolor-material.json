{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "printcolorvalue",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "printcolorvalue",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:model}]{${1:color}}"
      }
    },
    {
      "name": "colorsample",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "colorsample",
      "arg": {
        "format": "[][][]{}[]",
        "snippet": "[${2:model}][${3:textcolor%color}][${4:box width%l}]{${1:color}}[${5:title%text}]"
      }
    },
    {
      "name": "colorpalette",
      "arg": {
        "format": "{}",
        "snippet": "{${1:color}}"
      }
    },
    {
      "name": "colorpalette",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:color}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/xcolor-material#c": [
      "prefix=${1:prefix}\r"
    ],
    "\\colorpalette": [
      "width=",
      "height=",
      "shape=${1:shape}\r",
      "title=",
      "title text color=",
      "init text color=",
      "altern text color=",
      "variation font=\r",
      "value font=\r",
      "title font=\r",
      "colorbox sep=",
      "first colorbox sep=",
      "title colorbox height=",
      "primary palette=\r",
      "secondary palette=\r",
      "percent char=\r",
      "primary variation=\r",
      "primary toggle at=\r",
      "secondary toggle at=\r",
      "color model=\r",
      "both\r",
      "primary\r",
      "basic\r"
    ]
  },
  "args": [
    "\\usepackage/xcolor-material#c"
  ]
}