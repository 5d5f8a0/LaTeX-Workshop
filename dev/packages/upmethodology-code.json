{
  "deps": [
    {
      "name": "upmethodology-p-common"
    }
  ],
  "macros": [
    {
      "name": "upmcodelang",
      "arg": {
        "format": "{}",
        "snippet": "{${1:language%keyvals}}"
      }
    },
    {
      "name": "jclass",
      "arg": {
        "format": "{}",
        "snippet": "{${1:class%plain}}"
      }
    },
    {
      "name": "jinterface",
      "arg": {
        "format": "{}",
        "snippet": "{${1:interface}}"
      }
    },
    {
      "name": "jpackage",
      "arg": {
        "format": "{}",
        "snippet": "{${1:package%plain}}"
      }
    },
    {
      "name": "jfunc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:function}}"
      }
    },
    {
      "name": "jclazz"
    },
    {
      "name": "jvoid"
    },
    {
      "name": "jint"
    },
    {
      "name": "jlong"
    },
    {
      "name": "jfloat"
    },
    {
      "name": "jboolean"
    },
    {
      "name": "jdouble"
    },
    {
      "name": "jchar"
    },
    {
      "name": "jstring"
    },
    {
      "name": "jarray",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    },
    {
      "name": "jcollection",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    },
    {
      "name": "jset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%plain}}"
      }
    },
    {
      "name": "jtrue"
    },
    {
      "name": "jfalse"
    },
    {
      "name": "jop",
      "arg": {
        "format": "{}",
        "snippet": "{${1:operator}}"
      }
    },
    {
      "name": "jcall",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:function}}{${2:params}}"
      }
    },
    {
      "name": "jcode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/upmethodology-code#c,\\upmcodelang#c": [
      "uml",
      "java",
      "cpp"
    ]
  },
  "args": [
    "\\usepackage/upmethodology-code#c,\\upmcodelang#c"
  ]
}