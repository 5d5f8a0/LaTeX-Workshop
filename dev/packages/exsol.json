    {
      "name": "informulacollection"
    },
    {
      "name": "informulacollectiononly"
    }
  ],
  "keys": {
    "\\usepackage/exsol#c": [
      "local",
      "external",
      "inline",
      "nolabels",
      "exercisesfontsize=${1:fontsize csname}\r",
      "exerciseaslist",
      "copyexercisesinsolutions",
      "minipage",
      "usesolutionserieslabels"
    ],
    "\\begin{exercises},\\begin{exerciseseries}": [
      "columns=${1:integer}\r",
      "exsubrule\r",
      "solsubrule\r",
      "subrule\r"
    ]
  },
  "args": [
    "\\usepackage/exsol#c"
  ]
