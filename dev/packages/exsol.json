    },
    {
      "name": "varwidth"
    }
  ],
  "macros": [
    {
      "name": "exercisename"
    },
    {
      "name": "exercisesname"
    },
    {
      "name": "seriesname"
    },
    {
      "name": "solutionname"
    },
    {
      "name": "solutionsname"
    },
    {
      "name": "loadSolutions"
    }
  ],
  "envs": [
    {
      "name": "exercises"
    },
    {
