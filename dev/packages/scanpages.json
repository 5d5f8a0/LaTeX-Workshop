{
  "deps": [
    {
      "name": "ifpdf"
    },
    {
      "name": "pgffor"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "xkeyval"
    },
    {
      "name": "fp-basic"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "etoolbox"
    }
  ],
  "macros": [
    {
      "name": "scanpage",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "scanpage",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    },
    {
      "name": "initviewport",
      "arg": {
        "format": "{}{}{}{}{}",
        "snippet": "{${1:scale}}{${2:llx%l}}{${3:lly%l}}{${4:w%l}}{${5:h%l}}"
      }
    },
    {
      "name": "whitesq(x,y)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:scale}}"
      }
    },
    {
      "name": "whitesq(x,y)",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:x-scale}}[${2:y-scale}]"
      }
    },
    {
      "name": "whitecirc(x,y)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:scale}}"
      }
    },
    {
      "name": "whitecirc(x,y)",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:x-scale}}[${2:y-scale}]"
      }
    },
    {
      "name": "origpgcmd",
      "unusual": true
    },
    {
      "name": "origpgnum",
      "unusual": true
    },
    {
      "name": "putn(x,y)",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:stuff}}"
      }
    },
    {
      "name": "thegrid",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:width}}{${2:height}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/scanpages#c": [
      "nogrid",
      "scale=${1:factor}\r",
      "llx=",
      "lly=",
      "w=",
      "h="
    ],
    "\\scanpage": [
      "origpgnum=${1:page number}\r",
      "origindest",
      "origpgnumoff",
      "dx=",
      "dy=",
      "page=${1:page number}\r",
      "rot=${1:degrees}\r",
      "tanrot=${1:tangent}\r"
    ]
  },
  "args": [
    "\\usepackage/scanpages#c"
  ]
}