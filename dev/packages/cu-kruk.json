{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "cu-kinovar"
    }
  ],
  "macros": [
    {
      "name": "cuKruk",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:neume}}{${2:text}}"
      }
    },
    {
      "name": "cuKruk",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:neume}}{${2:text}}"
      }
    },
    {
      "name": "cuKrukPara",
      "arg": {
        "format": "{}",
        "snippet": "{${1:neumes \\\\ ... \\\\ lyrics}}"
      }
    },
    {
      "name": "cuKrukPara",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:neumes \\\\ ... \\\\ lyrics}}"
      }
    },
    {
      "name": "cuSup",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:sup text}}{${2:base text%text}}"
      }
    },
    {
      "name": "cuSup",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:sup text}}{${2:base text%text}}"
      }
    },
    {
      "name": "cuKrukFont",
      "unusual": true
    },
    {
      "name": "cuKrukSylSpace",
      "unusual": true
    },
    {
      "name": "cuKrukTopMargin",
      "unusual": true
    },
    {
      "name": "cuKrukSylRuleHeight",
      "unusual": true
    },
    {
      "name": "cuKrukRaise",
      "unusual": true
    },
    {
      "name": "cuSupRaise",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/cu-kruk#c": [
      "autocolormarks\r",
      "noautocolormarks\r"
    ],
    "\\cuKruk,\\cuKrukPara": [
      "krukFont=${1:font}\r",
      "sylSpace=",
      "topMargin=",
      "krukRaise=",
      "sylRuleHeight="
    ],
    "\\cuSup": [
      "raise="
    ]
  },
  "args": [
    "\\usepackage/cu-kruk#c"
  ]
}