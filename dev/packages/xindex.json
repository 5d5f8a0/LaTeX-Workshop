{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "makeidx"
    },
    {
      "name": "imakeidx",
      "if": "imakeidx"
    }
  ],
  "macros": [
    {
      "name": "writeidx",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "braceLeft",
      "unusual": true
    },
    {
      "name": "braceRight",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/xindex#c": [
      "imakeidx\r",
      "xindy\r",
      "texindy\r",
      "truexindy\r",
      "makeindex\r",
      "noautomatic\r",
      "nonewpage\r",
      "splitindex\r",
      "original\r",
      "quiet\r"
    ]
  },
  "args": [
    "\\usepackage/xindex#c"
  ]
}