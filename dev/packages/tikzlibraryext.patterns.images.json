{
  "deps": [],
  "macros": [
    {
      "name": "pgfsetupimageaspattern",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:imagefile}}"
      }
    },
    {
      "name": "pgfsetupimageaspattern",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:name}}{${2:imagefile}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pgfsetupimageaspattern": [
      "height=",
      "width=",
      "page=${1:page number}\r",
      "interpolate",
      "mask=${1:mask name}\r"
    ],
    "\\node#c": [
      "image as pattern={${1:options}}\r"
    ]
  },
  "args": []
}