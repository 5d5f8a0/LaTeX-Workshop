{
  "deps": [
    {
      "name": "multido"
    },
    {
      "name": "amsmath"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "pgf"
    },
    {
      "name": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "title",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:short title%text}]{${1:text}}"
      }
    },
    {
      "name": "author",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:short author}]{${1:names}}"
      }
    },
    {
      "name": "slidesmag",
      "arg": {
        "format": "{}",
        "snippet": "{${1:factor}}"
      }
    },
    {
      "name": "slidestyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style%keyvals}}"
      }
    },
    {
      "name": "fromslide",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "fromslide*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "toslide",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "toslide*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "onlyslide",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "onlyslide*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "tableofcontents",
      "arg": {
        "format": "[]",
        "snippet": "[${1:<fromsec>.<${2:fromsubsec}>-<${3:tosec}>.<${4:tosubsec}>}]"
      }
    },
    {
      "name": "slidewidth",
      "unusual": true
    },
    {
      "name": "slideheight",
      "unusual": true
    },
    {
      "name": "theslide",
      "unusual": true
    },
    {
      "name": "thesubslide",
      "unusual": true
    },
    {
      "name": "theslidelabel",
      "unusual": true
    },
    {
      "name": "slidesadjust",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:hadjust}}{${2:vadjust}}"
      }
    }
  ],
  "envs": [
    {
      "name": "slide",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "slide",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:style%keyvals}]{${1:title%text}}"
      }
    },
    {
      "name": "notes"
    },
    {
      "name": "multislide",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:sub-slides}}{${2:title%text}}"
      }
    },
    {
      "name": "multislide",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:style%keyvals}]{${1:sub-slides}}{${2:title%text}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/talk#c": [
      "notes\r",
      "slides\r",
      "screen\r",
      "rotate\r",
      "norotate\r",
      "a4paper\r",
      "a5paper\r",
      "b5paper\r",
      "letterpaper\r",
      "legalpaper\r",
      "executivepaper\r",
      "landscape\r",
      "10pt\r",
      "11pt\r",
      "12pt\r",
      "oneside\r",
      "twoside\r",
      "draft\r",
      "final\r",
      "titlepage\r",
      "notitlepage\r",
      "onecolumn\r",
      "twocolumn\r",
      "leqno\r",
      "fleqn\r",
      "openbib\r"
    ]
  },
  "args": [
    "\\documentclass/talk#c"
  ]
}