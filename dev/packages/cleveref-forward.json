{
  "deps": [
    {
      "name": "cleveref"
    },
    {
      "name": "iflang"
    },
    {
      "name": "regexpatch"
    },
    {
      "name": "crefthe",
      "if": "crefthe"
    },
    {
      "name": "crefthe",
      "if": "crefthe=true"
    }
  ],
  "macros": [
    {
      "name": "cref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "cref*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "Cref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "Cref*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "labelcref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "SetUsedOnMessageText",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "SetUsedByMessageText",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "SetUsedByAndOnMessageText",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "SetForwardReferenceStyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "SetForwardReferenceStyleInsideMath",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "SetForwardReferenceStyleOutsideMath",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "SetForwardReferenceRefForm",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "SetForwardReferencePagerefForm",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "restorelabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/cleveref-forward#c": [
      "default=${1|used on,used by,used by and on\r|}",
      "crefthe"
    ],
    "\\cref#c,\\cref*#c,\\Cref#c,\\Cref*#c,\\labelcref#c": [
      "used on\r",
      "used by\r",
      "used by and on\r",
      "no use\r"
    ]
  },
  "args": [
    "\\usepackage/cleveref-forward#c"
  ]
}