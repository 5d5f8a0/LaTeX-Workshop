{
  "deps": [
    {
      "name": "array"
    },
    {
      "name": "grabbox"
    }
  ],
  "macros": [
    {
      "name": "DefaultAnimal",
      "arg": {
        "format": "{}",
        "snippet": "{${1:animal%keyvals}}"
      }
    },
    {
      "name": "DucksayOptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "AnimalOptions",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:animal}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "AnimalOptions*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:animal}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "ducksay",
      "arg": {
        "format": "{}",
        "snippet": "{${1:message}}"
      }
    },
    {
      "name": "ducksay",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:message}}"
      }
    },
    {
      "name": "duckthink",
      "arg": {
        "format": "{}",
        "snippet": "{${1:message}}"
      }
    },
    {
      "name": "duckthink",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:message}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\DucksayOptions#c,\\AnimalOptions#c,\\AnimalOptions*#c,\\ducksay#c,\\duckthink#c": [
      "ligatures=${1:tokenlist}",
      "no-tail",
      "random",
      "say",
      "schroedinger",
      "tail-1=${1:tokenlist}",
      "tail-2=${1:tokenlist}",
      "think"
    ],
    "\\AnimalOptions#c,\\AnimalOptions*#c": [
      "tail-count=",
      "tail-symbol="
    ],
    "\\DucksayOptions#c,\\ducksay#c,\\duckthink#c": [
      "arg=${1|box,tab,tab*|}",
      "b",
      "body=${1:font}",
      "body*=${1:font}",
      "body-align=${1|l,c,r|}",
      "body-bigger=${1:count}",
      "body-mirrored",
      "body-to-msg=",
      "body-x=",
      "body-y=",
      "bubble=${1:font}",
      "bubble*=${1:font}",
      "bubble-bot-kern=",
      "bubble-delim-left-1=${1:tokenlist}",
      "bubble-delim-left-2=${1:tokenlist}",
      "bubble-delim-left-3=${1:tokenlist}",
      "bubble-delim-left-4=${1:tokenlist}",
      "bubble-delim-right-1=${1:tokenlist}",
      "bubble-delim-right-2=${1:tokenlist}",
      "bubble-delim-right-3=${1:tokenlist}",
      "bubble-delim-right-4=${1:tokenlist}",
      "bubble-delim-top=${1:tokenlist}",
      "bubble-side-kern=",
      "bubble-top-kern=",
      "c",
      "col=",
      "hpad=${1:count}",
      "ht=${1:count}",
      "ignore-body",
      "msg=${1:font}",
      "msg*=${1:font}",
      "MSG=${1:font}",
      "MSG*=${1:font}",
      "msg-align=${1|l,c,r,j|}",
      "msg-align-c=${1:tokenlist}",
      "msg-align-l=${1:tokenlist}",
      "msg-align-r=${1:tokenlist}",
      "msg-align-j=${1:tokenlist}",
      "msg-to-body=",
      "no-bubble",
      "none",
      "out-h=",
      "out-v=",
      "out-x=",
      "out-y=",
      "strip-spaces",
      "t",
      "vpad=${1:count}",
      "wd=${1:count}",
      "wd*=",
      "wd-eq-body"
    ],
    "\\DefaultAnimal#c,\\ducksay#c,\\duckthink#c": [
      "animal=",
      "duck",
      "small-duck",
      "duck-family",
      "small-rabbit",
      "squirrel",
      "cow",
      "tux",
      "head-in",
      "platypus",
      "pig",
      "turtle",
      "frog",
      "snowman",
      "bunny",
      "dragon",
      "sodomized",
      "hedgehog",
      "kangaroo",
      "small-horse",
      "dog",
      "sheep",
      "rabbit",
      "snail",
      "whale",
      "snake",
      "cat",
      "sleepy-cat",
      "schroedinger-dead",
      "schroedinger-alive",
      "crusader",
      "knight",
      "fairy",
      "ghost",
      "unicorn",
      "r2d2",
      "vader",
      "yoda-head",
      "small-yoda",
      "yoda",
      "only-tail",
      "only-tail3"
    ]
  },
  "args": []
}