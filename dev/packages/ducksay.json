{
  "deps": [
    {
      "name": "array"
    },
    {
      "name": "grabbox"
    }
  ],
  "macros": [
    {
      "name": "DefaultAnimal",
      "arg": {
        "format": "{}",
        "snippet": "{${1:animal%keyvals}}"
      }
    },
    {
      "name": "DucksayOptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "AnimalOptions",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:animal}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "AnimalOptions*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:animal}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "ducksay",
      "arg": {
        "format": "{}",
        "snippet": "{${1:message}}"
      }
    },
    {
      "name": "ducksay",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:message}}"
      }
    },
    {
      "name": "duckthink",
      "arg": {
        "format": "{}",
        "snippet": "{${1:message}}"
      }
    },
    {
      "name": "duckthink",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:message}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\DucksayOptions#c,\\AnimalOptions#c,\\AnimalOptions*#c,\\ducksay#c,\\duckthink#c": [
      "ligatures=${1:tokenlist}\r",
      "no-tail\r",
      "random",
      "say\r",
      "schroedinger\r",
      "tail-1=${1:tokenlist}\r",
      "tail-2=${1:tokenlist}\r",
      "think\r"
    ],
    "\\AnimalOptions#c,\\AnimalOptions*#c": [
      "tail-count=\r",
      "tail-symbol=\r"
    ],
    "\\DucksayOptions#c,\\ducksay#c,\\duckthink#c": [
      "arg=${1|box,tab,tab*\r|}",
      "b\r",
      "body=${1:font}\r",
      "body*=${1:font}\r",
      "body-align=${1|l,c,r\r|}",
      "body-bigger=${1:count}\r",
      "body-mirrored",
      "body-to-msg=\r",
      "body-x=",
      "body-y=",
      "bubble=${1:font}\r",
      "bubble*=${1:font}\r",
      "bubble-bot-kern=",
      "bubble-delim-left-1=${1:tokenlist}\r",
      "bubble-delim-left-2=${1:tokenlist}\r",
      "bubble-delim-left-3=${1:tokenlist}\r",
      "bubble-delim-left-4=${1:tokenlist}\r",
      "bubble-delim-right-1=${1:tokenlist}\r",
      "bubble-delim-right-2=${1:tokenlist}\r",
      "bubble-delim-right-3=${1:tokenlist}\r",
      "bubble-delim-right-4=${1:tokenlist}\r",
      "bubble-delim-top=${1:tokenlist}\r",
      "bubble-side-kern=",
      "bubble-top-kern=",
      "c\r",
      "col=\r",
      "hpad=${1:count}\r",
      "ht=${1:count}\r",
      "ignore-body",
      "msg=${1:font}\r",
      "msg*=${1:font}\r",
      "MSG=${1:font}\r",
      "MSG*=${1:font}\r",
      "msg-align=${1|l,c,r,j\r|}",
      "msg-align-c=${1:tokenlist}\r",
      "msg-align-l=${1:tokenlist}\r",
      "msg-align-r=${1:tokenlist}\r",
      "msg-align-j=${1:tokenlist}\r",
      "msg-to-body=\r",
      "no-bubble",
      "none",
      "out-h=\r",
      "out-v=\r",
      "out-x=",
      "out-y=",
      "strip-spaces",
      "t\r",
      "vpad=${1:count}\r",
      "wd=${1:count}\r",
      "wd*=",
      "wd-eq-body"
    ],
    "\\DefaultAnimal#c,\\ducksay#c,\\duckthink#c": [
      "animal=\r",
      "duck\r",
      "small-duck\r",
      "duck-family\r",
      "small-rabbit\r",
      "squirrel\r",
      "cow\r",
      "tux\r",
      "head-in\r",
      "platypus\r",
      "pig\r",
      "turtle\r",
      "frog\r",
      "snowman\r",
      "bunny\r",
      "dragon\r",
      "sodomized\r",
      "hedgehog\r",
      "kangaroo\r",
      "small-horse\r",
      "dog\r",
      "sheep\r",
      "rabbit\r",
      "snail\r",
      "whale\r",
      "snake\r",
      "cat\r",
      "sleepy-cat\r",
      "schroedinger-dead\r",
      "schroedinger-alive\r",
      "crusader\r",
      "knight\r",
      "fairy\r",
      "ghost\r",
      "unicorn\r",
      "r2d2\r",
      "vader\r",
      "yoda-head\r",
      "small-yoda\r",
      "yoda\r",
      "only-tail\r",
      "only-tail3\r"
    ]
  },
  "args": []
}