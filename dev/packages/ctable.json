{
  "deps": [
    {
      "name": "ifpdf"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "array"
    },
    {
      "name": "tabularx"
    },
    {
      "name": "booktabs"
    },
    {
      "name": "rotating"
    },
    {
      "name": "transparent"
    }
  ],
  "macros": [
    {
      "name": "setupctable",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\setupctable,\\ctable",
          "\\setupctable"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ctable",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:preamble}}{${2:tnote cmds}}{${3:table}}"
      }
    },
    {
      "name": "ctable",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:preamble}}{${2:tnote cmds}}{${3:table}}"
      }
    },
    {
      "name": "tnote",
      "arg": {
        "format": "{}",
        "snippet": "{${1:footnote text}}"
      }
    },
    {
      "name": "tnote",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:label}]{${1:footnote text}}"
      }
    },
    {
      "name": "tmark"
    },
    {
      "name": "tmark",
      "arg": {
        "format": "[]",
        "snippet": "[${1:label}]"
      }
    },
    {
      "name": "NN"
    },
    {
      "name": "FL"
    },
    {
      "name": "ML"
    },
    {
      "name": "LL"
    }
  ],
  "envs": [],
  "keys": {
    "\\setupctable,\\ctable": [
      "bgopacity=\r",
      "botcap\r",
      "captionskip=",
      "center\r",
      "continued\r",
      "continued=\r",
      "doinside=${1:code}\r",
      "figure\r",
      "footerwidth=",
      "framebg=${1:r g b}\r",
      "framefg=${1:r g b}\r",
      "framerule=",
      "framesep=",
      "left\r",
      "maxwidth=",
      "mincapwidth=",
      "nonotespar\r",
      "nosideways\r",
      "nostar\r",
      "nosuper\r",
      "notespar\r",
      "pos=\r",
      "right\r",
      "sidecap\r",
      "sideways\r",
      "star\r",
      "super\r",
      "table\r",
      "topcap\r",
      "width="
    ],
    "\\setupctable": [
      "captionsleft\r",
      "captionsright\r",
      "captionsinside\r"
    ],
    "\\ctable": [
      "caption=${1:text}\r",
      "cap=${1:short caption text}\r",
      "label="
    ]
  },
  "args": []
}