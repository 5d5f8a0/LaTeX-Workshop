{
  "deps": [
    {
      "name": "tikzlibraryspath3"
    }
  ],
  "macros": [
    {
      "name": "DefineTile",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:name%specialDef}}{${2:sides}}{${3:vertices}}"
      }
    },
    {
      "name": "BakeTile",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}"
      }
    },
    {
      "name": "TilingDecomposition",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:type%keyvals}}{${2:level}}{${3:seed}}"
      }
    },
    {
      "name": "TilingDecomposition",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:TikZ keys}]{${1:type%keyvals}}{${2:level}}{${3:seed}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\BakeTile#c,\\pic#c": [
      "%penrosetilename\r"
    ],
    "\\TilingDecomposition#c": [
      "kite\r",
      "rhombus\r",
      "pentagon\r",
      "ktriangle\r",
      "rtriangle\r"
    ],
    "\\begin{tikzpicture}#c,\\tikz#c,\\begin{scope}#c,\\tikzset#c,\\scoped#c": [
      "every tile/.style={${1:TikZ keys}}\r",
      "tile ${1:<n>}/.style={${2:TikZ keys}}\r",
      "every circle arc/.style={${1:TikZ keys}}\r",
      "every long arc/.style={${1:TikZ keys}}\r",
      "tiling step=",
      "first tile/.style={${1:TikZ keys}}\r"
    ],
    "\\pic#c": [
      "align with=${1:tile} along ${2:edge}\r",
      "align with=${1:tile} along ${2:edge} using ${3:number}\r"
    ],
    "\\path#c": [
      "save tiling path=${1:edge}\r"
    ],
    "\\tikzset#c": [
      "clone tiling side path={${1:target}}{${2:source}}\r",
      "clone tile path={${1:target}}{${2:source}}\r"
    ]
  },
  "args": []
}