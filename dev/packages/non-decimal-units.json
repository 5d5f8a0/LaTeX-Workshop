{
  "deps": [],
  "macros": [
    {
      "name": "nduKeys",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "nduValue",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:unit group}}{${2:value}}"
      }
    },
    {
      "name": "nduValue",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:unit group}}[${3:options%keyvals}]{${2:value}}"
      }
    },
    {
      "name": "VALUE"
    },
    {
      "name": "SYMBOL"
    },
    {
      "name": "nduMath",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:unit group}}{${2:variable}}{${3:operator}}{${4:value}}"
      }
    },
    {
      "name": "nduMath",
      "arg": {
        "format": "{}[]{}{}{}",
        "snippet": "{${1:unit group}}[${5:options%keyvals}]{${2:variable}}{${3:operator}}{${4:value}}"
      }
    },
    {
      "name": "nduResult",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:unit group}}{${2:variable}}"
      }
    },
    {
      "name": "nduResult",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:unit group}}[${3:options%keyvals}]{${2:variable}}"
      }
    },
    {
      "name": "nduNormalize",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:unit group}}{${2:amount}}{${3:unit}}"
      }
    },
    {
      "name": "nduNormalize",
      "arg": {
        "format": "{}[]{}{}",
        "snippet": "{${1:unit group}}[${4:options%keyvals}]{${2:amount}}{${3:unit}}"
      }
    },
    {
      "name": "nduHeader",
      "arg": {
        "format": "{}",
        "snippet": "{${1:unit name}}"
      }
    },
    {
      "name": "nduHeader",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:unit name}}[${2:options%keyvals}]"
      }
    },
    {
      "name": "nduSymbol",
      "arg": {
        "format": "{}",
        "snippet": "{${1:unit group}}"
      }
    },
    {
      "name": "nduFactor",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:unit group}}{${2:unit name}}"
      }
    },
    {
      "name": "nduNewBaseUnit",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:unit group}}{${2:keyvals}}"
      }
    },
    {
      "name": "nduNewUnitGroup",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:unit group}}{${2:base units}}"
      }
    },
    {
      "name": "nduNewUnitGroup",
      "arg": {
        "format": "{}{}[]",
        "snippet": "{${1:unit group}}{${2:base units}}[${3:cmd}]"
      }
    },
    {
      "name": "nduNewUnitGroup",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:keyvals}]{${1:unit group}}{${2:base units}}"
      }
    },
    {
      "name": "nduNewUnitGroup",
      "arg": {
        "format": "[]{}{}[]",
        "snippet": "[${3:keyvals}]{${1:unit group}}{${2:base units}}[${4:cmd}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/non-decimal-units#c": [
      "british\r",
      "danish\r",
      "german\r"
    ],
    "\\nduKeys#c,\\nduValue#c,\\nduMath#c,\\nduResult#c,\\nduNormalize#c": [
      "display=${1|values only,formatted,symbols only\r|}",
      "format=${1:code}\r",
      "replace nil with=${1:code}\r",
      "treat zero as nil\r",
      "unit depth=${1:unit name}\r",
      "unit separator=${1:symbol}\r",
      "use numprint\r",
      "add to variable=${1:code}\r",
      "subtract from variable=${1:code}\r",
      "normalize\r"
    ],
    "\\nduKeys#c,\\nduHeader#c": [
      "aligned\r",
      "cell widths=",
      "set aligned for environment=${1:name}\r",
      "tabularray column type=${1:letter}\r"
    ]
  },
  "args": [
    "\\usepackage/non-decimal-units#c"
  ]
}