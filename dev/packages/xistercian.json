{
  "deps": [
    {
      "name": "pgf"
    },
    {
      "name": "expkv-opt"
    }
  ],
  "macros": [
    {
      "name": "cistercian",
      "arg": {
        "format": "{}",
        "snippet": "{${1:counter}}"
      }
    },
    {
      "name": "cisterciannum",
      "arg": {
        "format": "{}",
        "snippet": "{${1:integer}}"
      }
    },
    {
      "name": "cisterciannumE",
      "arg": {
        "format": "{}",
        "snippet": "{${1:integer expression}}"
      }
    },
    {
      "name": "cisterciansetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c",
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cistercianredraw",
      "unusual": true
    },
    {
      "name": "cistercianredrawlazy",
      "unusual": true
    },
    {
      "name": "cistercianstyle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:keyname%specialDef}}{${2:options%keyvals}}",
        "keys": [
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c",
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "cistercianstyle*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:keyname%specialDef}}{${2:options%keyvals}}",
        "keys": [
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c",
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*"
        ],
        "keyPos": 1
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c": [
      "width=",
      "wd=",
      "height=",
      "ht=",
      "bound-x=${1:factor}\r",
      "bx=${1:factor}\r",
      "bound-y=${1:factor}\r",
      "by=${1:factor}\r",
      "baseline=${1:factor}\r",
      "bs=${1:factor}\r",
      "stroke-v=",
      "sv=",
      "stroke-h=",
      "sh=",
      "stroke-du=",
      "sdu=",
      "stroke-dd=",
      "sdd=",
      "strokes=",
      "s=",
      "fraction=${1:fraction}\r",
      "fr=${1:fraction}\r",
      "orientation=${1|horizontal,h,vertical,v\r|}",
      "o=${1|horizontal,h,vertical,v\r|}",
      "horizontal\r",
      "vertical\r",
      "alternate-5=${1:choice}\r",
      "5=${1:choice}\r",
      "alternate-6=${1:choice}\r",
      "6=${1:choice}\r",
      "alternate-9=${1:choice}\r",
      "9=${1:choice}\r",
      "font={${1:font setup}}\r",
      "f={${1:font setup}}\r",
      "%cisterciankey\r"
    ],
    "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*": [
      "redraw={${1:feature list}}\r",
      "redraw +={${1:feature list}}\r",
      "redraw\r",
      "redraw!\r",
      "r={${1:feature list}}\r",
      "r +={${1:feature list}}\r",
      "r\r",
      "r!\r"
    ],
    "\\usepackage/xistercian#c": [
      "debug\r"
    ]
  },
  "args": [
    "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c",
    "\\usepackage/xistercian#c"
  ]
}