{
  "deps": [
    {
      "name": "ifthen"
    },
    {
      "name": "fancyhdr"
    },
    {
      "name": "extramarks"
    },
    {
      "name": "titlesec"
    },
    {
      "name": "enumitem"
    },
    {
      "name": "draftwatermark",
      "if": "draft"
    },
    {
      "name": "scrtime",
      "if": "draft"
    },
    {
      "name": "prelim2e",
      "if": "draft"
    }
  ],
  "macros": [
    {
      "name": "gbsection",
      "arg": {
        "format": "{}",
        "snippet": "{${1:id%labeldef}}"
      }
    },
    {
      "name": "gbturn",
      "arg": {
        "format": "{}",
        "snippet": "{${1:id%ref}}"
      }
    },
    {
      "name": "gbturntext",
      "unusual": true
    },
    {
      "name": "gbitem",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text}}{${2:id%ref}}"
      }
    },
    {
      "name": "gbvillain",
      "arg": {
        "format": "{}{}{}{}{}",
        "snippet": "{${1:name}}{${2:skill%text}}{${3:skill level%text}}{${4:life%text}}{${5:life level%text}}"
      }
    },
    {
      "name": "gbheader"
    },
    {
      "name": "gbheadtext",
      "unusual": true
    },
    {
      "name": "gbdebugx",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "gbdebug",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    }
  ],
  "envs": [
    {
      "name": "gbturnoptions"
    },
    {
      "name": "gbturnoptions",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "gbtabbing",
      "unusual": true
    }
  ],
  "keys": {
    "\\usepackage/gamebook#c": [
      "debug\r",
      "draft\r"
    ],
    "\\begin{gbturnoptions}": [
      "topsep=",
      "partopsep=",
      "parsep=",
      "itemsep=",
      "leftmargin=",
      "rightmargin=",
      "listparindent=",
      "labelwidth=",
      "labelsep=",
      "itemindent=",
      "label=${1:commands}\r",
      "label*=${1:commands}\r",
      "ref=${1:commands}\r",
      "font=${1:commands}\r",
      "format=${1:commands}\r",
      "align=${1|left,right,parleft\r|}",
      "labelindent=",
      "left=${1:labelindent}\r",
      "left=${1:labelindent} .. ${2:leftmargin}\r",
      "widest\r",
      "widest=${1:string}\r",
      "widest*=${1:integer}\r",
      "labelsep*=",
      "labelindent*=",
      "start=${1:integer}\r",
      "resume\r",
      "resume=${1:series name}\r",
      "resume*\r",
      "resume*=${1:series name}\r",
      "series=${1:series name}\r",
      "beginpenalty=${1:integer}\r",
      "midpenalty=${1:integer}\r",
      "endpenalty=${1:integer}\r",
      "before=${1:code}\r",
      "before*=${1:code}\r",
      "after=${1:code}\r",
      "after*=${1:code}\r",
      "first=${1:code}\r",
      "first*=${1:code}\r",
      "style=${1|standard,unboxed,nextline,sameline,multiline\r|}",
      "noitemsep\r",
      "nosep\r",
      "wide\r",
      "wide=${1:parindent}\r",
      "itemjoin=${1:string}\r",
      "itemjoin*=${1:string}\r",
      "afterlabel=${1:string}\r",
      "mode=${1|unboxed,boxed\r|}"
    ]
  },
  "args": [
    "\\usepackage/gamebook#c"
  ]
}