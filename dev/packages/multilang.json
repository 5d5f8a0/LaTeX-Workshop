{
  "deps": [
    {
      "name": "environ"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "pgfkeys"
    },
    {
      "name": "pgfopts"
    }
  ],
  "macros": [
    {
      "name": "NewMultilangCmd",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:command}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "NewMultilangEnv",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "NewMultilangType",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:type name}}{${2:format%definition}}"
      }
    },
    {
      "name": "NewMultilangType",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:arg count}]{${1:type name}}{${2:format%definition}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/multilang#c": [
      "languages={${1:lang1,lang2,...}}\r"
    ],
    "\\NewMultilangCmd#c": [
      "command=${1:command}\r",
      "margs={${1:arg1,arg2,...}}\r",
      "oargs={${1:arg1,arg2,...}}\r",
      "starred",
      "disablable",
      "defaults={${1:arg1=val1,arg2=val2,...}}\r",
      "alias/${1:name}={${2:arg1,arg2,...}}\r"
    ],
    "\\NewMultilangEnv#c": [
      "environment=${1:environment}\r",
      "margs={${1:arg1,arg2,...}}\r",
      "oargs={${1:arg1,arg2,...}}\r",
      "disablable",
      "defaults={${1:arg1=val1,arg2=val2,...}}\r",
      "alias/${1:name}={${2:arg1,arg2,...}}\r"
    ]
  },
  "args": [
    "\\usepackage/multilang#c"
  ]
}