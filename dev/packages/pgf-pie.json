{
  "includes": {
    "tikz": [],
    "scalefnt": []
  },
  "macros": {
    "pie{}": {
      "snippet": "pie{${1:num1/text1,${2:num2/text2},...}}"
    },
    "pie[]{}": {
      "snippet": "pie[${2:options}]{${1:num1/text1,${3:num2/text2},...}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "envs": {},
  "options": [],
  "keyvals": [
    [
      "pos={${1:x,y}}",
      "rotate=${1:degrees}",
      "radius=${1:number}",
      "color=${2|${1:color},{${2:color1,color2,...}}|}",
      "explode=${1:number}",
      "sum=${1|auto,${1:number}|}",
      "change direction",
      "before number={${1:code}}",
      "after number={${1:code}}",
      "hide number",
      "scale font",
      "text=${1|label,pin,inside,legend|}",
      "style={${1:TikZ options}}",
      "polar",
      "square",
      "cloud"
    ]
  ]
}