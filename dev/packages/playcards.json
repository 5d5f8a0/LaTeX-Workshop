{
  "deps": [
    {
      "name": "simplekv"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarypatterns"
    },
    {
      "name": "tikzlibraryshadows"
    },
    {
      "name": "contour"
    },
    {
      "name": "anttor",
      "if": "anttor"
    }
  ],
  "macros": [
    {
      "name": "drawcardsrecto",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "drawcardsrecto",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}",
        "keys": [
          "\\drawcardsrecto"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "drawcardsverso",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "drawcardsverso",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}",
        "keys": [
          "\\drawcardsverso"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/playcards#c": [
      "anttor"
    ],
    "\\drawcardsrecto": [
      "borders",
      "trame",
      "corners",
      "backgroundImg",
      "textColor=",
      "backgroundColor=",
      "contentsFontSize=${1:number}",
      "circleRay=${1:number}"
    ],
    "\\drawcardsverso": [
      "backgroundImg",
      "trame",
      "contentsFontSize=${1:number}"
    ]
  },
  "args": [
    "\\usepackage/playcards#c"
  ]
}