{
  "deps": [
    {
      "name": "verbatim"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "makecmds"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "ifthen"
    }
  ],
  "macros": [
    {
      "name": "sympy",
      "arg": {
        "format": "{}",
        "snippet": "{${1:Sympy code%definition}}"
      }
    },
    {
      "name": "percent",
      "detail": "%",
      "doc": "Percent sign"
    },
    {
      "name": "sympyplot",
      "arg": {
        "format": "{}",
        "snippet": "{${1:graphics obj, keyword args%definition}}"
      }
    },
    {
      "name": "sympyplot",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:graphics obj, keyword args%definition}}"
      }
    },
    {
      "name": "sympyplot",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:options%keyvals}][${3:fmt}]{${1:graphics obj, keyword args%definition}}"
      }
    },
    {
      "name": "sympytexindent",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "sympyblock"
    },
    {
      "name": "sympysilent"
    },
    {
      "name": "sympyverbatim"
    },
    {
      "name": "comment"
    }
  ],
  "keys": {
    "\\usepackage/sympytex#c": [
      "imagemagick\r"
    ],
    "\\sympyplot": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": [
    "\\usepackage/sympytex#c"
  ]
}