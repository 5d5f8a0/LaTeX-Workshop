{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "petiteannonce",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:phone number}}{${2:text}}"
      }
    },
    {
      "name": "petiteannonce",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:phone number}}{${2:text}}",
        "keys": [
          "\\petiteannonce"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "petiteannoncewidth",
      "arg": {
        "format": "{}",
        "snippet": "{${1:factor}}"
      }
    },
    {
      "name": "petiteannonceaddtowidth",
      "arg": {
        "format": "{}",
        "snippet": "{${1:dimen%l}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/petiteannonce#c": [
      "10pt",
      "11pt",
      "12pt",
      "margin="
    ],
    "\\petiteannonce": [
      "cols=${1:integer}",
      "count=${1:integer}",
      "cutvspace=",
      "skip=${1:integer}",
      "telcolsep=",
      "telrulewidth=",
      "telvspace=",
      "textvspace=",
      "texthspace=",
      "width="
    ]
  },
  "args": [
    "\\documentclass/petiteannonce#c"
  ]
}