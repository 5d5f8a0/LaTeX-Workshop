{
  "deps": [
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "barcode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "barcode",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:code}}"
      }
    },
    {
      "name": "HPlabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "ITFbarcode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "ITFbarcode",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:code}}"
      }
    },
    {
      "name": "BarcodeSanitize",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/makebarcode#c": [
      "help"
    ],
    "\\usepackage/makebarcode#c,\\barcode": [
      "X=",
      "H=",
      "bcorr=",
      "ratio=${1:ratio}\r",
      "K=${1:number}\r",
      "MtoXratio=${1:ratio}\r",
      "code=${1|2/5-Industrial,2/5-IATA,2/5-Matrix,2/5-Datalogic,2/5-Inverted,2/5-Interleaved,Code39\r|}",
      "S1.2\r",
      "S1.1\r",
      "S1\r",
      "S.9\r",
      "S.8\r",
      "S.7\r",
      "S.6\r",
      "lines",
      "frame\r"
    ],
    "\\ITFbarcode": [
      "lines",
      "frame\r",
      "S1.2\r",
      "S1.1\r",
      "S1\r",
      "S.9\r",
      "S.8\r",
      "S.7\r",
      "S.6\r"
    ]
  },
  "args": [
    "\\usepackage/makebarcode#c",
    "\\usepackage/makebarcode#c,\\barcode"
  ]
}