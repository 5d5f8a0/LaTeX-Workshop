{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "graphicx"
    }
  ],
  "macros": [
    {
      "name": "coverimage",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "coverimage",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:imagefile}}"
      }
    },
    {
      "name": "epubpage"
    },
    {
      "name": "DeclareLanguageEbook",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:lang name}}{${2:lang code}}"
      }
    },
    {
      "name": "GetLanguage",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\coverimage": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": []
}