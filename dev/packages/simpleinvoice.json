{
  "deps": [
    {
      "name": "advdate"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "url"
    },
    {
      "name": "tabularx"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "colortbl"
    },
    {
      "name": "babel"
    }
  ],
  "macros": [
    {
      "name": "setinvoicetitle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title}}"
      }
    },
    {
      "name": "setinvoicenumber",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "setreceivername",
      "arg": {
        "format": "{}",
        "snippet": "{${1:reciever's name}}"
      }
    },
    {
      "name": "setreceiveraddress",
      "arg": {
        "format": "{}",
        "snippet": "{${1:reciever's address}}"
      }
    },
    {
      "name": "setname",
      "arg": {
        "format": "{}",
        "snippet": "{${1:your name}}"
      }
    },
    {
      "name": "setaddress",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:your address (${3:multiline})}}{${2:your address (${4:single line})}}"
      }
    },
    {
      "name": "setphonenumber",
      "arg": {
        "format": "{}",
        "snippet": "{${1:your phone number}}"
      }
    },
    {
      "name": "setemail",
      "arg": {
        "format": "{}",
        "snippet": "{${1:your email}}"
      }
    },
    {
      "name": "setyourref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "setourref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "setinvoicedate",
      "arg": {
        "format": "{}",
        "snippet": "{${1:date}}"
      }
    },
    {
      "name": "setdeadline",
      "arg": {
        "format": "{}",
        "snippet": "{${1:days from now}}"
      }
    },
    {
      "name": "additem",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:description}}{${2:price excluding vat}}{${3:vat percentage}}{${4:price including vat}}"
      }
    },
    {
      "name": "setsubtotal",
      "arg": {
        "format": "{}",
        "snippet": "{${1:total excluding vat}}"
      }
    },
    {
      "name": "setvat",
      "arg": {
        "format": "{}",
        "snippet": "{${1:total vat}}"
      }
    },
    {
      "name": "settotal",
      "arg": {
        "format": "{}",
        "snippet": "{${1:total including vat}}"
      }
    },
    {
      "name": "setaccountnumber",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "makeinvoice"
    },
    {
      "name": "linesep",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/simpleinvoice#c": [
      "american",
      "english",
      "UKenglish",
      "USenglish",
      "norsk",
      "norwegian",
      "bokmaal"
    ]
  },
  "args": [
    "\\usepackage/simpleinvoice#c"
  ]
}