      "separator/array right=${1:string}\r",
      "zero-based",
      "replace/true=${1:string}\r",
      "replace/false=${1:string}\r",
      "replace/null=${1:string}\r"
    ],
    "\\JSONParseSet,\\JSONParseValue,\\JSONParseArrayValues,\\JSONParseArrayValuesMap": [
      "replace/backspace=${1:string}\r",
      "replace/formfeed=${1:string}\r",
      "replace/linefeed=${1:string}\r",
      "replace/carriage return=${1:string}\r",
      "replace/horizontal tab=${1:string}\r",
      "escape={${1:list}}\r",
      "rescan"
    ]
  },
  "args": [
    "\\usepackage/jsonparse#c,\\JSONParseSet,\\JSONParse,\\JSONParseFromFile"
