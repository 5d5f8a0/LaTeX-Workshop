{
  "deps": [
    {
      "name": "amsmath"
    }
  ],
  "macros": [
    {
      "name": "intertext",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:distance%l}]{${1:word or phrase%text}}"
      }
    },
    {
      "name": "dmulticolumn",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:count}}{${2:preamble}}{${3:formula}}"
      }
    },
    {
      "name": "useshortskip"
    },
    {
      "name": "nr"
    },
    {
      "name": "nr",
      "arg": {
        "format": "[]",
        "snippet": "[${1:dist%l}]"
      }
    },
    {
      "name": "nr*"
    },
    {
      "name": "nr*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:dist%l}]"
      }
    },
    {
      "name": "mrel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:formula}}"
      }
    },
    {
      "name": "underrel",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:base}}{${2:bottom}}"
      }
    },
    {
      "name": "medmath",
      "arg": {
        "format": "{}",
        "snippet": "{${1:formula}}"
      }
    },
    {
      "name": "medop",
      "arg": {
        "format": "{}",
        "snippet": "{${1:operator}}"
      }
    },
    {
      "name": "medint",
      "arg": {
        "format": "{}",
        "snippet": "{${1:operator}}"
      }
    },
    {
      "name": "medintcorr",
      "arg": {
        "format": "{}",
        "snippet": "{${1:length}}"
      }
    },
    {
      "name": "mfrac",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:num}}{${2:den}}"
      }
    },
    {
      "name": "mbinom",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:above}}{${2:below}}"
      }
    },
    {
      "name": "eq",
      "arg": {
        "format": "{}",
        "snippet": "{${1:formula}}"
      }
    },
    {
      "name": "eq*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:formula}}"
      }
    },
    {
      "name": "eqs",
      "arg": {
        "format": "{}",
        "snippet": "{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqs",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:dist%l}]{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqs*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqs*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:dist%l}]{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqalign",
      "arg": {
        "format": "{}",
        "snippet": "{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqalign*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:formulas%formula}}"
      }
    }
  ],
  "envs": [
    {
      "name": "fleqn"
    },
    {
      "name": "fleqn",
      "arg": {
        "format": "[]",
        "snippet": "[${1:margin}]"
      }
    },
    {
      "name": "ceqn"
    },
    {
      "name": "darray",
      "arg": {
        "format": "{}",
        "snippet": "{${1:cols}}"
      }
    },
    {
      "name": "darray",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:pos%keyvals}]{${1:cols}}"
      }
    },
    {
      "name": "medsize"
    },
    {
      "name": "mmatrix"
    }
  ],
  "keys": {
    "\\usepackage/nccmath#c": [
      "mediummath\r",
      "intlimits\r",
      "nointlimits\r",
      "sumlimits\r",
      "nosumlimits\r",
      "namelimits\r",
      "nonamelimits\r",
      "leqno\r",
      "reqno\r",
      "centertags\r",
      "tbtags\r",
      "cmex10\r",
      "fleqn\r",
      "alignedleftspaceyes\r",
      "alignedleftspaceno\r",
      "alignedleftspaceyesifneg\r"
    ],
    "\\begin{darray}": [
      "l\r",
      "c\r",
      "r\r"
    ]
  },
  "args": [
    "\\usepackage/nccmath#c"
  ]
}