      "start ribbons={${1:ribbon1,ribbon2,...}}\r",
      "finish boxes={${1:name1,name2,...}}\r",
      "start boxes={${1:box1,box2,...}}\r",
      "extra left=${1:integer}\r",
      "extra right=${1:integer}\r"
    ],
    "\\startblock": [
      "fit ribbons={${1:name1,name2,...}}\r",
      "fit boxes={${1:name1,name2,...}}\r"
    ],
    "\\continueblock": [
      "height=${1:integer}\r",
      "start ribbons={${1:ribbon1,ribbon2,...}}\r",
      "start boxes={${1:box1,box2,...}}\r",
