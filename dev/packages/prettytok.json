{
  "deps": [
    {
      "name": "precattl"
    }
  ],
  "macros": [
    {
      "name": "prettyN",
      "arg": {
        "format": "{}",
        "snippet": "{${1:token list}}"
      }
    },
    {
      "name": "prettyX",
      "arg": {
        "format": "{}",
        "snippet": "{${1:token list}}"
      }
    },
    {
      "name": "prettyO",
      "arg": {
        "format": "{}",
        "snippet": "{${1:token list}}"
      }
    },
    {
      "name": "prettyV",
      "arg": {
        "format": "{}",
        "snippet": "{${1:token list}}"
      }
    },
    {
      "name": "prettyshowC",
      "arg": {
        "format": "{}",
        "snippet": "{${1:csname}}"
      }
    },
    {
      "name": "prettyeN",
      "arg": {
        "format": "{}",
        "snippet": "{${1:token list}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/prettytok#c": [
      "mode=${1|term-8bit,term-shell,html\r|}",
      "html-file-name=${1:file name}\r",
      "html-refresh-strategy=${1:value 0-4}\r",
      "html-refresh-duration=${1:milliseconds}\r",
      "term-prefix=${1:string}\r",
      "term-prefix-more=${1:string}\r",
      "term-wrap-limit=${1:number}\r",
      "term-shell-decode-cmd=${1:command}\r",
      "term-shell-decode-cmd-print\r"
    ]
  },
  "args": [
    "\\usepackage/prettytok#c"
  ]
}