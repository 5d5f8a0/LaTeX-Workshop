{
  "deps": [
    {
      "name": "uplatex"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "cjkcategorymode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:mode%keyvals}}"
      }
    },
    {
      "name": "cjkcategory",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:block}}{${2:category}}"
      }
    },
    {
      "name": "showcjkcategory",
      "unusual": true
    },
    {
      "name": "getcjktokenmode"
    },
    {
      "name": "thecjktokenmode"
    },
    {
      "name": "setcjktokenmode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:value}}"
      }
    },
    {
      "name": "withcjktokendisabled",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "withcjktokenenabled",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "withcjktokenforced",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "withcjktokendisabledex",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "withcjktokenenabledex",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "withcjktokenforcedex",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pxcjkcat#c": [
      "ccv1\r",
      "ccv2\r",
      "ccv3\r",
      "ccv4\r",
      "ccv+\r",
      "real\r",
      "nomode\r"
    ],
    "\\cjkcategorymode#c,\\usepackage/pxcjkcat#c": [
      "forcecjk\r",
      "default\r",
      "prefercjk\r",
      "prefercjkvar\r",
      "japanese\r",
      "japanesevar\r",
      "prefernoncjk\r"
    ]
  },
  "args": [
    "\\usepackage/pxcjkcat#c",
    "\\cjkcategorymode#c,\\usepackage/pxcjkcat#c"
  ]
}