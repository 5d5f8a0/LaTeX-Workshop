{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "class-beamer"
    },
    {
      "name": "beamerthemeBFH"
    },
    {
      "name": "bfhmodule"
    },
    {
      "name": "bfhpub",
      "if": "beamerarticle"
    },
    {
      "name": "beamerarticle",
      "if": "beamerarticle"
    },
    {
      "name": "bfhpub",
      "if": "beamerarticle=true"
    },
    {
      "name": "beamerarticle",
      "if": "beamerarticle=true"
    },
    {
      "name": "handoutWithNotes",
      "if": "printNotes"
    },
    {
      "name": "handoutWithNotes",
      "if": "paperNotes"
    },
    {
      "name": "colortbl"
    }
  ],
  "macros": [
    {
      "name": "BFHarraystretch",
      "unusual": true
    },
    {
      "name": "BFHarrayrulewidth",
      "unusual": true
    },
    {
      "name": "setupBfhTabular"
    }
  ],
  "envs": [
    {
      "name": "bfhTabular",
      "arg": {
        "format": "{}",
        "snippet": "{${1:preamble}}"
      }
    },
    {
      "name": "bfhTabular",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:pos}]{${1:preamble}}"
      }
    },
    {
      "name": "bfhTblr",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:preamble}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/bfhbeamer#c": [
      "authorontitle",
      "logofile=${1:file}\r",
      "enforce-title-logo=\r",
      "beamerarticle",
      "printNotes\r",
      "tabletNotes\r",
      "paperNotes\r",
      "usepdftitle",
      "envcountsect\r",
      "notheorems\r",
      "noamsthm\r",
      "compress\r",
      "t\r",
      "c\r",
      "leqno\r",
      "fleqn\r",
      "handout\r",
      "trans\r",
      "pdftex\r",
      "nativepdf\r",
      "pdfmark\r",
      "dvips\r",
      "dviwindo\r",
      "dvipsone\r",
      "vtex\r",
      "ps2pdf\r",
      "ignorenonframetext\r",
      "noamssymb\r",
      "bigger\r",
      "smaller\r",
      "8pt\r",
      "9pt\r",
      "10pt\r",
      "11pt\r",
      "12pt\r",
      "14pt\r",
      "17pt\r",
      "20pt\r",
      "draft\r",
      "CJK\r",
      "cjk\r",
      "onlytextwidth\r",
      "pgf={${1:pgf options}}\r",
      "hyperref={${1:hyperref options}}\r",
      "color={${1:color options}}\r",
      "xcolor={${1:xcolor options}}\r",
      "ucs\r",
      "utf8x\r",
      "utf8\r",
      "aspectratio=${2|2013,1610,169,149,141,54,43,32,${1:xxxx}\r|}"
    ]
  },
  "args": [
    "\\documentclass/bfhbeamer#c"
  ]
}