{
  "deps": [
    {
      "name": "amsmath"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "l3keys2e"
    }
  ],
  "macros": [
    {
      "name": "obmathleading",
      "arg": {
        "format": "{}",
        "snippet": "{${1:value}}"
      }
    }
  ],
  "envs": [
    {
      "name": "obMathLeading"
    },
    {
      "name": "obMathLeading",
      "arg": {
        "format": "[]",
        "snippet": "[${1:value}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/ob-mathleading#c": [
      "default",
      "mathleading=${1:value}\r",
      "exception={${1:envname1,envname2,...}}\r",
      "noallowdisplaybreaks",
      "badm"
    ]
  },
  "args": [
    "\\usepackage/ob-mathleading#c"
  ]
}