{
  "deps": [
    {
      "name": "atbegshi"
    }
  ],
  "macros": [
    {
      "name": "absposset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "absput",
      "arg": {
        "format": "{}",
        "snippet": "{${1:contents%text}}"
      }
    },
    {
      "name": "absput",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:contents%text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\absposset#c,\\absput#c,\\absputcoffin#c": [
      "angle=${1:degrees}\r",
      "h=${1|l,hc,r\r|}",
      "pg=${1:page-coffin}\r",
      "pg-h=${1|l,hc,r\r|}",
      "pg-v=${1|t,vc,b,H,T,B\r|}",
      "scale=${1:factor}\r",
      "v=${1|t,vc,b,H,T,B\r|}",
      "width=",
      "x=",
      "y="
    ]
  },
  "args": []
}