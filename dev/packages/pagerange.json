{
  "deps": [
    {
      "name": "lastpage"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "pagerangeoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/pagerange#c,\\pagerangeoptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pagerange",
      "arg": {
        "format": "{}",
        "snippet": "{${1:page range}}"
      }
    },
    {
      "name": "pagestart"
    },
    {
      "name": "pageend"
    },
    {
      "name": "getpagenumber",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pagerange#c,\\pagerangeoptions": [
      "tie=${1:character}\r",
      "acceptempty",
      "defaultfirstpage=${1:number}\r",
      "defaultlastpage=${1:number}\r"
    ]
  },
  "args": [
    "\\usepackage/pagerange#c,\\pagerangeoptions"
  ]
}