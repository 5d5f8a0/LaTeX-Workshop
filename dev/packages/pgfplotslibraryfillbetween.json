{
  "deps": [
    {
      "name": "pgfplotslibrarydecorations.softclip"
    }
  ],
  "macros": [
    {
      "name": "tikzsegmentindex",
      "unusual": true
    },
    {
      "name": "tikzfillbetween",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:draw style}}"
      }
    },
    {
      "name": "tikzgetnamedpath",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:path name}}"
      }
    },
    {
      "name": "tikznamecurrentpath",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "tikzpathintersectionsegments",
      "unusual": true,
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\addplot#c,\\addplot+#c": [
      "name path=${1:name}\r"
    ],
    "\\path#c,\\draw#c,\\fill#c": [
      "intersection segments={${1:options}}\r"
    ],
    "\\tikzfillbetween#c": [
      "split",
      "soft clip={${1:specification}}\r",
      "soft clip first={${1:specification}}\r",
      "soft clip second={${1:specification}}\r",
      "every segment/.style={${1:TikZ keys}}\r",
      "every odd segment/.style={${1:TikZ keys}}\r",
      "every even segment/.style={${1:TikZ keys}}\r",
      "every segment no ${1:<n>}/.style={${2:TikZ keys}}\r",
      "every last segment/.style={${1:TikZ keys}}\r",
      "of=${1:first} and ${2:second}\r",
      "reverse=${1|auto,true,false\r|}",
      "on layer=${1:layer name}\r",
      "inner moveto=${1|connect,keep\r|}"
    ],
    "\\tikzpathintersectionsegments#c": [
      "of=${1:first} and ${2:second}\r",
      "sequence={${1:series spec}}\r",
      "reverse"
    ]
  },
  "args": []
}