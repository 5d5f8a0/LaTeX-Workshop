{
  "deps": [
    {
      "name": "etoolbox"
    }
  ],
  "macros": [
    {
      "name": "DefineCodeSection",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "DefineCodeSection",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:true|false%keyvals}]{${1:name}}"
      }
    },
    {
      "name": "SetCodeSection",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:true|false%keyvals}}",
        "keys": [
          "\\DefineCodeSection,\\SetCodeSection"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "BeginCodeSection",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "EndCodeSection",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\DefineCodeSection,\\SetCodeSection": [
      "true\r",
      "false\r"
    ]
  },
  "args": []
}