  "envs": [],
  "keys": {
    "\\ch": [
      "kroeger-vink=${1|true,false\r|}",
      "kv-positive-style={${1:TikZ}}\r",
      "kv-positive-radius={${1:dim}}\r",
      "kv-positive-hshift={${1:dim}}\r",
      "kv-positive-vshift={${1:dim}}\r",
      "kv-positive-offset={${1:dim}}\r",
      "kv-positive-symbol={${1:TeX code}}\r"
    ],
    "\\setchemformula": [
      "kroeger-vink=${1|true,false\r|}",
      "kv-positive-style={${1:TikZ}}\r",
      "kv-positive-radius={${1:dim}}\r",
      "kv-positive-hshift={${1:dim}}\r",
      "kv-positive-vshift={${1:dim}}\r",
      "kv-positive-offset={${1:dim}}\r",
      "kv-positive-symbol={${1:TeX code}}\r"
    ],
    "\\chcpd": [
      "format={${1:code}}\r",
      "atom-format={${1:code}}\r",
      "font-family={${1:family}}\r",
      "font-series={${1:series}}\r",
      "font-shape={${1:shape}}\r",
      "font-spec={${1:font}}\r",
      "font-spec={[${1:options}]${2:font}}\r"
    ],
    "\\chlewis": [
      "lewis-default=${1|.,:,|,o,single,pair,{pair (dotted)},{pair (line)},empty\r|}",
      "lewis-distance={${1:dim}}\r",
      "lewis-line-length={${1:dim}}\r",
      "lewis-line-width={${1:dim}}\r",
      "lewis-offset={${1:dim}}5ex\r"
    ]
  },
  "args": []
