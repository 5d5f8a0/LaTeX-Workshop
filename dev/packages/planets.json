{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "pgfkeys"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarydecorations.pathmorphing"
    },
    {
      "name": "xstring"
    }
  ],
  "macros": [
    {
      "name": "planet",
      "arg": {
        "format": "[]",
        "snippet": "[${1:keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\planet": [
      "surface=${1|mercury,venus,earth,mars,jupiter,saturn,uranus,neptune,pluto,sun,moon\r|}",
      "rings\r",
      "ring=${1:radius}\r",
      "tilt=${1:degrees}\r",
      "rotation\r",
      "retrograde\r",
      "phase=${1|new,first crescent,first half,waxing gibbous,full,waning gibbous,last half,last crescent\r|}",
      "scale=\r",
      "centerx=${1:x-coord}\r",
      "centery=${1:y-coord}\r",
      "center={${1:(x,y)}}\r"
    ]
  },
  "args": []
}