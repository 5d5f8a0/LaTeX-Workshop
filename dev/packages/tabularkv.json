{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "tabularht"
    }
  ],
  "macros": [],
  "envs": [
    {
      "name": "tabularkv",
      "arg": {
        "format": "{}",
        "snippet": "{${1:preamble}}"
      }
    },
    {
      "name": "tabularkv",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:preamble}}"
      }
    },
    {
      "name": "tabularkv",
      "arg": {
        "format": "[][]{}",
        "snippet": "[${2:options%keyvals}][${3:pos}]{${1:preamble}}"
      }
    }
  ],
  "keys": {
    "\\begin{tabularkv}": [
      "width=",
      "x=",
      "height=",
      "valign=${1|top,bottom,center\r|}"
    ]
  },
  "args": []
}