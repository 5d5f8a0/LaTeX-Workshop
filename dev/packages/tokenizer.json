{
  "deps": [
    {
      "name": "ifthen"
    }
  ],
  "macros": [
    {
      "name": "GetTokens",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:csname1}}{${2:csname2}}{${3:source}}"
      }
    },
    {
      "name": "TrimSpaces",
      "arg": {
        "format": "{}",
        "snippet": "{${1:source}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/tokenizer#c": [
      "trim\r",
      "notrim\r"
    ]
  },
  "args": [
    "\\usepackage/tokenizer#c"
  ]
}