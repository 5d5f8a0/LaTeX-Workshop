{
  "deps": [
    {
      "name": "ifpdf"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "ifvtex"
    },
    {
      "name": "luatex85"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "pstricks"
    },
    {
      "name": "preview"
    },
    {
      "name": "environ"
    },
    {
      "name": "pst-calculate",
      "if": "useCalc"
    },
    {
      "name": "pdfcolmk",
      "if": "fixpdftex"
    }
  ],
  "macros": [
    {
      "name": "PDFcontainer"
    },
    {
      "name": "savepicture",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "usepicture",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "usepicture",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:name}}"
      }
    },
    {
      "name": "thepspicture",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "postscript"
    },
    {
      "name": "postscript",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/pst-pdf#c": [
      "active\r",
      "inactive\r",
      "pstricks\r",
      "nopstricks\r",
      "draft\r",
      "final\r",
      "tightpage\r",
      "notightpage\r",
      "displaymath\r",
      "noxcolor\r",
      "useCalc\r",
      "gsfonts\r",
      "psfonts\r",
      "noEngineCheck\r",
      "pdf\r",
      "xetex\r",
      "distiller\r",
      "monochrome\r",
      "grayscale\r"
    ],
    "\\includegraphics,\\usepicture,\\begin{postscript}": [
      "frame",
      "innerframe",
      "ignore",
      "showname",
      "namefont=${1:font commands}\r"
    ],
    "\\usepicture,\\begin{postscript}": [
      "alt={${1:alt text}}\r",
      "bb=${1:llx lly urx ury}\r",
      "bbllx=\r",
      "bblly=\r",
      "bburx=\r",
      "bbury=\r",
      "natwidth=\r",
      "natheight=\r",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox\r|}",
      "viewport=${1:llx lly urx ury}\r",
      "trim=${1:llx lly urx ury}\r",
      "angle=${1:degrees}\r",
      "origin=\r",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}\r",
      "clip",
      "draft",
      "type=${1:file type}\r",
      "ext=${1:file extension}\r",
      "read=${1:read-file extension}\r",
      "command=\r",
      "quiet\r",
      "page=${1:page number}\r",
      "interpolate",
      "decodearray={${1:color array}}\r"
    ]
  },
  "args": [
    "\\usepackage/pst-pdf#c"
  ]
}