{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "xeCJK"
    },
    {
      "name": "unicode-math"
    }
  ],
  "macros": [
    {
      "name": "xdusetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\xdusetup#c": [
      "style={${1:keyvals}}\r",
      "style/cjk-font=${1|adobe,fandol,founder,hanyi,sinotype,win,none\r|}",
      "style/cjk-fake-bold=${1:伪粗体粗细程度}\r",
      "style/cjk-fake-slant=${1:伪斜体倾斜程度}\r",
      "style/latin-font=${1|gyre,tac,tacn,tcc,thcs,tll,none\r|}",
      "style/latin-sans-scale=${1|upper,lower,off\r|}",
      "style/latin-mono-scale=${1|upper,lower,off\r|}",
      "style/math-font=${1|asana,cambria,cm,concrete,erewhon,euler,fira,garamond,gfsneohellenic,kp,libertinus,lm,newcm,stix2,stix,xcharter,xits,bonum,dejavu,pagella,schola,termes,none\r|}",
      "style/unicode-math={${1:unicode-math宏包选项}}\r",
      "style/font-type=${1|font,file\r|}",
      "style/font-path={${1:路径}}\r"
    ]
  },
  "args": []
}