{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-xkey"
    },
    {
      "name": "multido"
    }
  ],
  "macros": [
    {
      "name": "psmoire(x,y)"
    },
    {
      "name": "psmoire",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:options%keyvals}](${2:x},${3:y})"
      }
    },
    {
      "name": "addtomoirelisttype",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "psRandomDotPatterns"
    },
    {
      "name": "psRandomDotPatterns",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "psRandomDot(x,y)"
    },
    {
      "name": "psRandomDot",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:options%keyvals}](${2:x},${3:y})"
      }
    },
    {
      "name": "psGlassPattern"
    },
    {
      "name": "psGlassPattern",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "variablesMoirages",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\psmoire#c": [
      "type=${1|Fresnel,linear,radial,circle,square,Newton,Bouasse,Gauss,dot,chess\r|}",
      "rotate=${1:degrees}\r",
      "Rmax=${1:length in cm}\r",
      "scale=${1:factor}\r",
      "Alpha=${1:degrees}\r",
      "E=${1:distance in cm}\r",
      "n=${1:number}\r",
      "T=${1:distance in mm}\r",
      "linewidth=",
      "linecolor=",
      "dotstyle=${1:style}\r",
      "dotsize="
    ],
    "\\psRandomDotPatterns#c": [
      "NumberDots=${1:integer}\r",
      "coefficients=${1:a b Î¸ x0 y0}\r",
      "srand=${1:random seed}\r",
      "layer",
      "revlayer",
      "linecolor=",
      "fillcolor=",
      "unit=${1:number}\r"
    ],
    "\\psRandomDot#c": [
      "hole=${1|round,square\r|}",
      "r=${1:radius in pts}\r",
      "p=${1:distance in pts}\r",
      "rotate=${1:degrees}\r",
      "k=${1:dispersion factor}\r",
      "symbole=${1:digit or letter}\r",
      "PSfont=${1:PS font name}\r",
      "fontsize=${1:size in pts}\r",
      "vadjust=${1:number}\r",
      "hadjust=${1:number}\r"
    ],
    "\\psGlassPattern#c": [
      "function=${1:function}\r",
      "layers",
      "linecolor=",
      "fillcolor=",
      "dotsize=",
      "dotstyle=${1:style}\r",
      "unit=${1:number}\r",
      "algebraic\r"
    ]
  },
  "args": []
}