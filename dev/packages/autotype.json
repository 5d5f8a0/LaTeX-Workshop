{
  "deps": [
    {
      "name": "luatex"
    }
  ],
  "macros": [
    {
      "name": "autotypelangoptions",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:language}}{${2:options%keyvals}}",
        "keys": [
          "\\autotypelangoptions"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "autotypefontoptions",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:font}}{${2:options%keyvals}}",
        "keys": [
          "\\autotypefontoptions"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "noligbreak",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "autotypelongs"
    },
    {
      "name": "autotyperounds"
    }
  ],
  "envs": [],
  "keys": {
    "\\autotypelangoptions": [
      "hyphenation=${1|default,primary,weighted\r|}",
      "mark-hyph=${1|on,off\r|}",
      "ligbreak=${1|on,off\r|}",
      "long-s=${1|on,off\r|}"
    ],
    "\\autotypefontoptions": [
      "long-s-codepoint=${1:codepoint}\r",
      "round-s-codepoint=${1:codepoint}\r",
      "final-round-s-codepoint=${1:codepoint}\r"
    ]
  },
  "args": []
}