{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "newfloatsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\newfloatsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "DeclareFloatingEnvironment",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "DeclareFloatingEnvironment",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:envname}}",
        "keys": [
          "\\DeclareFloatingEnvironment,\\SetupFloatingEnvironment"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "SetupFloatingEnvironment",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:options%keyvals}}",
        "keys": [
          "\\DeclareFloatingEnvironment,\\SetupFloatingEnvironment"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "ForEachFloatingEnvironment",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code}}"
      }
    },
    {
      "name": "PrepareListOf",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:code}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\newfloatsetup": [
      "within=${1:<counter> or none}",
      "chapterlistsgap="
    ],
    "\\DeclareFloatingEnvironment,\\SetupFloatingEnvironment": [
      "fileext=${1:file extension}",
      "listname=${1:list name}",
      "name=${1:name}",
      "autorefname=${1:name}",
      "legendname=${1:name}",
      "placement=${1:combination of htbp}",
      "within=${1:<counter> or none}",
      "chapterlistsgaps=${1|on,off|}",
      "without",
      "planb",
      "planb-fileext=${1:file extension}"
    ]
  },
  "args": []
}