{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "inferrule",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:premises}}{${2:conclusions}}"
      }
    },
    {
      "name": "inferrule",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:label%text}]{${1:premises}}{${2:conclusions}}"
      }
    },
    {
      "name": "inferrule*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:premises}}{${2:conclusions}}"
      }
    },
    {
      "name": "infer",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:premises}}{${2:conclusions}}"
      }
    },
    {
      "name": "infer",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:label%text}]{${1:premises}}{${2:conclusions}}"
      }
    },
    {
      "name": "infer*",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:premises}}{${2:conclusions}}"
      }
    },
    {
      "name": "mprset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "MathparLineskip",
      "unusual": true
    },
    {
      "name": "MathparNormalpar",
      "unusual": true
    },
    {
      "name": "MathparBindings",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "mathpar"
    }
  ],
  "keys": {
    "\\inferrule*,\\infer*": [
      "width=",
      "narrower=${1:scale}",
      "before=${1:tex}",
      "lab=${1:label}",
      "lskip=",
      "left=${1:label}",
      "Left=${1:label}",
      "right=${1:label}",
      "Right=${1:label}",
      "style=${1:tex}",
      "leftstyle=${1:tex}",
      "rightstyle=${1:tex}",
      "leftskip=",
      "rightskip=",
      "vdots=",
      "after=${1:tex}"
    ],
    "mprset": [
      "sep=",
      "flushleft",
      "center",
      "rewrite=",
      "fraction=${1:tail}${2:body}${3:head}",
      "fractionaboveskip=",
      "fractionbelowskip=",
      "myfraction=${1:tex}",
      "vskip=",
      "vcenter"
    ]
  },
  "args": []
}