      "unusual": true
    },
    {
      "name": "enigmaisrunningplainfalse",
      "unusual": true
    },
    {
      "name": "luastringsep",
      "unusual": true
    },
    {
      "name": "enigmasetupcatcodes",
      "unusual": true
    },
    {
      "name": "escapecatcode",
      "unusual": true
    },
    {
      "name": "begingroupcatcode",
      "unusual": true
    },
    {
      "name": "endgroupcatcode",
      "unusual": true
    },
    {
      "name": "spacecatcode",
      "unusual": true
    },
    {
      "name": "lettercatcode",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\setupenigma": [
      "other_chars=${1:boolean}\r",
