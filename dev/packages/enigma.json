{
  "deps": [
    {
      "name": "luatexbase"
    }
  ],
  "macros": [
    {
      "name": "defineenigma",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "setupenigma",
      "arg": {
