      "fillCells: ${1:color}\r",
      "fillBanner: ${1:color}\r",
      "textBanner: ${1:color}\r",
      "textBoard: ${1:color}\r",
      "fillInstructions: ${1:color}\r",
      "fillGameBoard: ${1:color}\r",
      "fillQuestions: ${1:color}\r",
      "dollarColor: ${1:color}\r",
      "linkColor: ${1:color}\r"
    ],
    "\\GameDesign#c": [
      "Cat: ${1:category}\r",
      "Cat: [${1:font cmds}] ${2:category}\r",
      "NumQuestions: ${1:integer}\r",
      "Goal: ${1:color}\r",
      "GoalPercentage: ${1:number}\r",
      "CellWidth: ${1:length}\r",
      "CellHeight: ${1:length}\r",
      "ExtraWidth: ${1:length}\r",
      "ExtraHeight: ${1:length}\r",
      "Champion: ${1:text}\r"
    ],
    "\\APHidden#c": [
      "Champion: ${1:text}\r"
    ],
    "\\APHidden#c,\\APDollar#c,\\APRight#c,\\APWrong#c,\\APScore#c": [
      "Font: ${1:font name}\r",
      "Size: ${1:number}\r",
      "TextColor: ${1:r g b}\r"
    ],
    "\\APHidden#c,\\APDollar#c,\\APScore#c": [
      "BorderColor: ${1:r g b}\r",
      "FillColor: ${1:r g b}\r"
    ],
    "\\APRight#c,\\APWrong#c": [
      "Message: ${1:text}\r"
    ],
    "\\APScore#c": [
      "CellHeight: ${1:length}\r",
      "CellWidth: ${1:length}\r",
      "AutoPlacement: ${1:true|false}\r",
      "Score: \"${1:text}\"\r",
      "Currency: \"${1:symbol}\"\r",
      "align: ${1:l|c|r}\r",
      "tpScaledCellWidth: ${1:factor}\r",
      "tpHighlightBorderColor: ${1:r g b}\r"
    ],
    "\\setTwoPlayerOptions#c": [
      "player1: ${1:name}\r",
      "player2: ${1:name}\r",
      "playerFmt: ${1:font commands}\r",
      "checkbox: ${1:true|false}\r"
    ]
  },
