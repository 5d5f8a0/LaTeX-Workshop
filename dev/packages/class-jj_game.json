      "fillBanner: ${1:color}",
      "textBanner: ${1:color}",
      "textBoard: ${1:color}",
      "fillInstructions: ${1:color}",
      "fillGameBoard: ${1:color}",
      "fillQuestions: ${1:color}",
      "dollarColor: ${1:color}",
      "linkColor: ${1:color}"
    ],
    "\\GameDesign#c": [
      "Cat: ${1:category}",
      "Cat: [${1:font cmds}] ${2:category}",
      "NumQuestions: ${1:integer}",
      "Goal: ${1:color}",
      "GoalPercentage: ${1:number}",
      "CellWidth: ${1:length}",
      "CellHeight: ${1:length}",
      "ExtraWidth: ${1:length}",
      "ExtraHeight: ${1:length}",
      "Champion: ${1:text}"
    ],
    "\\APHidden#c": [
      "Champion: ${1:text}"
    ],
    "\\APHidden#c,\\APDollar#c,\\APRight#c,\\APWrong#c,\\APScore#c": [
      "Font: ${1:font name}",
      "Size: ${1:number}",
      "TextColor: ${1:r g b}"
    ],
    "\\APHidden#c,\\APDollar#c,\\APScore#c": [
      "BorderColor: ${1:r g b}",
      "FillColor: ${1:r g b}"
    ],
    "\\APRight#c,\\APWrong#c": [
      "Message: ${1:text}"
    ],
    "\\APScore#c": [
      "CellHeight: ${1:length}",
      "CellWidth: ${1:length}",
      "AutoPlacement: ${1:true|false}",
      "Score: \"${1:text}\"",
      "Currency: \"${1:symbol}\"",
      "align: ${1:l|c|r}",
      "tpScaledCellWidth: ${1:factor}",
      "tpHighlightBorderColor: ${1:r g b}"
    ],
    "\\setTwoPlayerOptions#c": [
      "player1: ${1:name}",
      "player2: ${1:name}",
      "playerFmt: ${1:font commands}",
      "checkbox: ${1:true|false}"
    ]
  },
