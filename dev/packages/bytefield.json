{
  "deps": [
    {
      "name": "calc"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "bitbox",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:width}}{${2:text}}"
      }
    },
    {
      "name": "bitbox",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:sides}]{${1:width}}{${2:text}}"
      }
    },
    {
      "name": "bitbox",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:width}}[${3:options%keyvals}]{${2:text}}"
      }
    },
    {
      "name": "bitbox",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "[${3:sides}]{${1:width}}[${4:options%keyvals}]{${2:text}}"
      }
    },
    {
      "name": "wordbox",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:width}}{${2:text}}"
      }
    },
    {
      "name": "wordbox",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:sides}]{${1:width}}{${2:text}}"
      }
    },
    {
      "name": "wordbox",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:width}}[${3:options%keyvals}]{${2:text}}"
      }
    },
    {
      "name": "wordbox",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "[${3:sides}]{${1:width}}[${4:options%keyvals}]{${2:text}}"
      }
    },
    {
      "name": "bitboxes",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:width}}{${2:tokens}}"
      }
    },
    {
      "name": "bitboxes",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:sides}]{${1:width}}{${2:tokens}}"
      }
    },
    {
      "name": "bitboxes",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:width}}[${3:options%keyvals}]{${2:tokens}}"
      }
    },
    {
      "name": "bitboxes",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "[${3:sides}]{${1:width}}[${4:options%keyvals}]{${2:tokens}}"
      }
    },
    {
      "name": "bitboxes*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:width}}{${2:tokens}}"
      }
    },
    {
      "name": "bitboxes*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:sides}]{${1:width}}{${2:tokens}}"
      }
    },
    {
      "name": "bitboxes*",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:width}}[${3:options%keyvals}]{${2:tokens}}"
      }
    },
    {
      "name": "bitboxes*",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "[${3:sides}]{${1:width}}[${4:options%keyvals}]{${2:tokens}}"
      }
    },
    {
      "name": "bitheader",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bit-positions}}"
      }
    },
    {
      "name": "bitheader",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:bit-positions}}"
      }
    },
    {
      "name": "skippedwords"
    },
    {
      "name": "bytefieldsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\bytefieldsetup,\\begin{bytefield},\\bitbox,\\wordbox,\\bitboxes,\\bitboxes*,\\usepackage/bytefield#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "amp",
      "unusual": true
    },
    {
      "name": "bitwidth",
      "unusual": true
    },
    {
      "name": "byteheight",
      "unusual": true
    },
    {
      "name": "curlyshrinkage",
      "unusual": true
    },
    {
      "name": "curlyspace",
      "unusual": true
    },
    {
      "name": "labelspace",
      "unusual": true
    },
    {
      "name": "heightunits",
      "unusual": true
    },
    {
      "name": "widthunits",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "bytefield",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bit-width}}"
      }
    },
    {
      "name": "bytefield",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:bit-width}}"
      }
    },
    {
      "name": "rightwordgroup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "rightwordgroup",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}"
      }
    },
    {
      "name": "leftwordgroup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "leftwordgroup",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}"
      }
    }
  ],
  "keys": {
    "\\bitheader": [
      "bitformatting={${1:commands}}\r",
      "endianness=${1|little,big\r|}",
      "lsb=${1:integer}\r"
    ],
    "\\begin{rightwordgroup},\\begin{leftwordgroup}": [
      "curlystyle=${1:command}\r",
      "leftcurlystyle=${1:command}\r",
      "rightcurlystyle=${1:command}\r"
    ],
    "\\bytefieldsetup,\\begin{bytefield},\\bitbox,\\wordbox,\\bitboxes,\\bitboxes*,\\usepackage/bytefield#c": [
      "bitwidth=",
      "bitheight=",
      "endianness=${1|little,big\r|}",
      "bitformatting={${1:commands}}\r",
      "boxformatting={${1:commands}}\r",
      "bgcolor=",
      "leftcurly=${1:delimiter}\r",
      "rightcurly=${1:delimiter}\r",
      "leftcurlyspace=",
      "rightcurlyspace=",
      "curlyspace=",
      "leftlabelspace=",
      "rightlabelspace=",
      "labelspace=",
      "leftcurlyshrinkage=",
      "rightcurlyshrinkage=",
      "curlyshrinkage=",
      "leftcurlystyle=${1:command}\r",
      "rightcurlystyle=${1:command}\r",
      "curlystyle=${1:command}\r",
      "lsb=${1:integer}\r",
      "perword=${1:command}\r"
    ]
  },
  "args": [
    "\\bytefieldsetup,\\begin{bytefield},\\bitbox,\\wordbox,\\bitboxes,\\bitboxes*,\\usepackage/bytefield#c"
  ]
}