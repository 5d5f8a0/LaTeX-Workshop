{
  "deps": [
    {
      "name": "tikzpagenodes"
    }
  ],
  "macros": [
    {
      "name": "snaptodo",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text%todo}}"
      }
    },
    {
      "name": "snaptodo",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text%todo}}"
      }
    },
    {
      "name": "snaptodoset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\snaptodo,\\snaptodoset"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\snaptodo,\\snaptodoset": [
      "call chain/.style={${1:TikZ styles}}\r",
      "margin block/.style={${1:TikZ styles}}\r",
      "block sep=",
      "chain sep=",
      "chain bias=",
      "block rise="
    ]
  },
  "args": []
}