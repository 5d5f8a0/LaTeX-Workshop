{
  "deps": [
    {
      "name": "ifpdf"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "controlbutton",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
      }
    },
    {
      "name": "controlbutton",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:sequence file,start page}]{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
      }
    },
    {
      "name": "controlbutton",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:sequence file,start page,end page}]{${1:caption%text}}{${2:target frame}}{${3:options%keyvals}}"
      }
    }
  ],
  "envs": [
    {
      "name": "animation",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:width}}{${2:height}}"
      }
    },
    {
      "name": "aframe",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:frame name}}{${2:imagefile}}"
      }
    },
    {
      "name": "aframe",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:frame name}}{${2:imagefile,filepage}}"
      }
    }
  ],
  "keys": {
    "\\controlbutton#c": [
      "X=${2|start,center,end,${1:coord}\r|}",
      "Y=${2|start,center,end,${1:coord}\r|}",
      "position=${1|left,right,above,below,center,from\r|}",
      "width=${1:integer}\r",
      "height=${1:integer}\r",
      "scale=${1:factor}\r",
      "hidden\r",
      "transparent\r",
      "span=${1:milliseconds}\r",
      "keep\r"
    ]
  },
  "args": []
}