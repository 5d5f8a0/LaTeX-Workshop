{
  "deps": [
    {
      "name": "pgfkeys"
    },
    {
      "name": "etoolbox"
    }
  ],
  "macros": [
    {
      "name": "CollectArguments",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:argspec}}{${2:next-code}}"
      }
    },
    {
      "name": "CollectArguments",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:argspec}}{${2:next-code}}"
      }
    },
    {
      "name": "CollectArgumentsRaw",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:option-setting code}}{${2:argspec}}{${3:next-code}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\CollectArguments": [
      "caller=${1:control sequence}\r",
      "environment=${1:envname}\r",
      "begin tag",
      "end tag",
      "tags",
      "ignore nesting",
      "ignore other tags",
      "append preprocessor=${1:code}\r",
      "prepend preprocessor=${1:code}\r",
      "append postprocessor=${1:code}\r",
      "prepend postprocessor=${1:code}\r",
      "clear preprocessors\r",
      "clear postprocessors\r",
      "append expandable preprocessor=${1:code}\r",
      "prepend expandable preprocessor=${1:code}\r",
      "append expandable postprocessor=${1:code}\r",
      "prepend expandable postprocessor=${1:code}\r",
      "no delimiters",
      "return=${1|braced,plain,no\r|}",
      "clear args",
      "verbatim\r",
      "verb\r",
      "no verbatim\r",
      "fix from verbatim\r",
      "fix from verb\r",
      "fix from no verbatim\r",
      "braces=${1:<begin-char><end-char>}\r",
      "verbatim ranges={${1:from-to}}\r"
    ]
  },
  "args": []
}