{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "multicol"
    }
  ],
  "macros": [
    {
      "name": "makeindex",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "indexsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\indexsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "splitindexoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:splitindex options}}"
      }
    },
    {
      "name": "index",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:entry}}"
      }
    },
    {
      "name": "indexprologue",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "indexprologue",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:spacing}]{${1:text}}"
      }
    },
    {
      "name": "printindex"
    },
    {
      "name": "printindex",
      "arg": {
        "format": "[]",
        "snippet": "[${1:name}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/imakeidx#c": [
      "xindy\r",
      "texindy\r",
      "truexindy\r",
      "makeindex\r",
      "noautomatic\r",
      "nonewpage\r",
      "splitindex\r",
      "original\r",
      "quiet\r"
    ],
    "\\makeindex": [
      "name=${1:name}\r",
      "title=${1:text}\r",
      "program=${1|makeindex,xindy,texindy,truexindy\r|}",
      "options=${1:program options}\r",
      "noautomatic",
      "intoc",
      "columns=${1:number}\r",
      "columnsep=",
      "columnseprule"
    ],
    "\\indexsetup": [
      "level=${1:section cmd}\r",
      "toclevel=${1:section csname}\r",
      "noclearpage",
      "firstpagestyle=\r",
      "headers={${1:left marks}}{${2:right marks}}\r",
      "othercode=${1:code}\r"
    ]
  },
  "args": [
    "\\usepackage/imakeidx#c"
  ]
}