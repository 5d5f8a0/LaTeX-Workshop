{
  "deps": [
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "setsizes",
      "arg": {
        "format": "{}",
        "snippet": "{${1:chars per line}}"
      }
    },
    {
      "name": "setsizes",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:line height ratio}]{${1:chars per line}}"
      }
    },
    {
      "name": "fonttobox",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:text width}}{${2:text}}"
      }
    },
    {
      "name": "fonttobox",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:line height ratio}]{${1:text width}}{${2:text}}"
      }
    },
    {
      "name": "mediaquery",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:test name}}{${2:true}}{${3:false}}"
      }
    },
    {
      "name": "DeclareMediaQueryMatcher",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:test name}}{${2:LaTeX3 boolean test}}"
      }
    },
    {
      "name": "mediaquerytrue"
    },
    {
      "name": "fixtextheight"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/responsive#c,\\ResponsiveSetup": [
      "characters=${1:integer}\r",
      "noautomatic\r",
      "scale=${1|heptatonic,pentatonic,tetratonic,tritonic,golden\r|}",
      "number=${1:integer}\r",
      "ratio=${1:number}\r",
      "lineratio=${1:number}\r",
      "lineheight=${1:number}\r",
      "boxwidth="
    ]
  },
  "args": [
    "\\usepackage/responsive#c,\\ResponsiveSetup"
  ]
}