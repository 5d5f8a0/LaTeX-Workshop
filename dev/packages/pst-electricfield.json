{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "multido"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psElectricfield"
    },
    {
      "name": "psElectricfield",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "psEquipotential(x1,y1)(x2,y2)"
    },
    {
      "name": "psEquipotential",
      "arg": {
        "format": "[]()()",
        "snippet": "[${1:options%keyvals}](${2:x1},${3:y1})(${4:x2},${5:y2})"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psElectricfield#c,\\psEquipotential#c": [
      "Q={[${1:x1,y1,z1}][${2:x2,y2,z2}]${3:...}}\r",
      "N=${1:integer}\r",
      "linewidth=",
      "linecolor="
    ],
    "\\psElectricfield#c": [
      "points=${1:integer}\r",
      "Pas=${1:number}\r",
      "posArrow=${1:factor}\r",
      "radius=",
      "runit=${1:number}\r"
    ],
    "\\psEquipotential#c": [
      "Vmax=${1:number}\r",
      "Vmin=${1:number}\r",
      "stepV=${1:number}\r",
      "stepFactor=${1:factor}\r"
    ]
  },
  "args": []
}