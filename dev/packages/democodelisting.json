{
  "deps": [
    {
      "name": "listings"
    },
    {
      "name": "scontents"
    }
  ],
  "macros": [
    {
      "name": "DisplayCode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:st-name}}"
      }
    },
    {
      "name": "DisplayCode",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:st-name}}"
      }
    },
    {
      "name": "DemoCode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:st-name}}"
      }
    },
    {
      "name": "DemoCode",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:st-name}}"
      }
    },
    {
      "name": "DemoCode",
      "unusual": true,
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:st-name}}[${2:length}]"
      }
    },
    {
      "name": "DemoCode",
      "unusual": true,
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:options%keyvals}]{${1:st-name}}[${3:length}]"
      }
    },
    {
      "name": "TabbedDemoCode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:st-name}}"
      }
    },
    {
      "name": "TabbedDemoCode",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:st-name}}"
      }
    },
    {
      "name": "TabbedDemoCode",
      "unusual": true,
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:st-name}}[${2:length}]"
      }
    },
    {
      "name": "TabbedDemoCode",
      "unusual": true,
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:options%keyvals}]{${1:st-name}}[${3:length}]"
      }
    },
    {
      "name": "setdclisting",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\DisplayCode,\\DemoCode,\\TabbedDemoCode,\\setdclisting"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "setdcpar",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:options}}"
      }
    },
    {
      "name": "DisplayCodeB",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:st-name}}"
      }
    },
    {
      "name": "DisplayCodeB",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:st-name}}"
      }
    },
    {
      "name": "DemoCodeB",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:st-name}}"
      }
    },
    {
      "name": "DemoCodeB",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:st-name}}"
      }
    },
    {
      "name": "TabbedDemoCodeB",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:st-name}}"
      }
    },
    {
      "name": "TabbedDemoCodeB",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:st-name}}"
      }
    }
  ],
  "envs": [
    {
      "name": "stcode"
    },
    {
      "name": "stcode",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "verbsc",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:options}}"
      }
    }
  ],
  "keys": {
    "\\begin{stcode}": [
      "st=${1:st-name}\r",
      "store-env=${1:st-name}\r",
      "print-env",
      "write-env=${1:file.ext}\r",
      "write-out=${1:file.ext}\r",
      "overwrite",
      "force-eol"
    ],
    "\\DisplayCode,\\DemoCode,\\TabbedDemoCode,\\setdclisting": [
      "settexcs={${1:list of csnames}}\r",
      "texcs={${1:list of csnames}}\r",
      "texcsstyle=${1:font commands}\r",
      "settexcs2={${1:list of csnames}}\r",
      "texcs2={${1:list of csnames}}\r",
      "texcs2style=${1:font commands}\r",
      "settexcs3={${1:list of csnames}}\r",
      "texcs3={${1:list of csnames}}\r",
      "texcs3style=${1:font commands}\r",
      "setkeywd={${1:list of csnames}}\r",
      "keywd={${1:list of keywords}}\r",
      "keywdstyle=${1:font commands}\r",
      "setkeywd2={${1:list of keywords}}\r",
      "keywd2={${1:list of keywords}}\r",
      "keywd2style=${1:font commands}\r",
      "setkeywd3={${1:list of keywords}}\r",
      "keywd3={${1:list of keywords}}\r",
      "keywd3style=${1:font commands}\r",
      "setemph={${1:identifier list}}\r",
      "emph={${1:identifier list}}\r",
      "emphstyle=${1:font commands}\r",
      "setemph2={${1:identifier list}}\r",
      "emph2={${1:identifier list}}\r",
      "emph2style=${1:font commands}\r",
      "setemph3={${1:identifier list}}\r",
      "emph3={${1:identifier list}}\r",
      "emph3style=${1:font commands}\r",
      "stringstyle=${1:font commands}\r",
      "commentstyle=${1:font commands}\r",
      "bckgndcolor=",
      "rulecolor=",
      "numbers=${1|none,left\r|}",
      "numberstyle=${1:font commands}\r",
      "codeprefix=${1:prefix}\r",
      "resultprefix=${1:prefix}\r"
    ]
  },
  "args": []
}