{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "iftex"
    },
    {
      "name": "oblivoir-xl"
    },
    {
      "name": "moreverb",
      "if": "moreverb"
    },
    {
      "name": "lwarp",
      "if": "lwarp"
    },
    {
      "name": "xob-lwarp",
      "if": "lwarp"
    },
    {
      "name": "amsmath",
      "if": "amsmath"
    },
    {
      "name": "xob-amssymb",
      "if": "amsmath"
    },
    {
      "name": "bookmark",
      "if": "bookmark"
    },
    {
      "name": "ob-nokoreanappendix",
      "if": "nokorean"
    },
    {
      "name": "polyglossia",
      "if": "polyglossia"
    },
    {
      "name": "babel",
      "if": "babelhangul"
    }
  ],
  "macros": [
    {
      "name": "ifLuaOrXeTeX",
      "unusual": true
    },
    {
      "name": "MSNormalSize",
      "if": "ms"
    },
    {
      "name": "sethangulfont",
      "if": "polyglossia",
      "arg": {
        "format": "{}",
        "snippet": "{${1:font name}}"
      }
    },
    {
      "name": "sethangulfont",
      "if": "polyglossia",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:font name}}[${2:font features}]"
      }
    },
    {
      "name": "hangulfont",
      "if": "polyglossia",
      "unusual": true
    },
    {
      "name": "hangulfonttt",
      "if": "polyglossia",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/oblivoir#c": [
      "cjk\r",
      "dhucs\r",
      "fontspec={${1:fontspec options}}\r",
      "hyperref={${1:hyperref options}}\r",
      "xcolor={${1:xcolor options}}\r"
    ]
  },
  "args": [
    "\\documentclass/oblivoir#c"
  ]
}