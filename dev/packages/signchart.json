{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "pgfplots"
    },
    {
      "name": "xstring"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "signchart",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:values}}{${2:signs}}"
      }
    },
    {
      "name": "signchart",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:values}}{${2:signs}}"
      }
    },
    {
      "name": "signHeightKey",
      "unusual": true
    },
    {
      "name": "valNorthSouthKey",
      "unusual": true
    },
    {
      "name": "valSepKey",
      "unusual": true
    },
    {
      "name": "signChartWidth",
      "unusual": true
    },
    {
      "name": "signHeight",
      "unusual": true
    },
    {
      "name": "valNorthSouth",
      "unusual": true
    },
    {
      "name": "valSep",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/signchart#c": [
      "signunder\r",
      "valunder\r"
    ],
    "\\signchart": [
      "height=",
      "valanchor=${1:anchor name}\r",
      "valsep=",
      "width=",
      "arrows=${1:arrow style}\r"
    ]
  },
  "args": [
    "\\usepackage/signchart#c"
  ]
}