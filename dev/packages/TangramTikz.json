{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "simplekv"
    },
    {
      "name": "xstring"
    },
    {
      "name": "listofitems"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "tikzlibraryshapes.geometric"
    }
  ],
  "macros": [
    {
      "name": "TangramTikz",
      "arg": {
        "format": "{}",
        "snippet": "{${1:tangram name}}"
      }
    },
    {
      "name": "TangramTikz<TikZ options>",
      "arg": {
        "format": "{}",
        "snippet": "{${1:tangram name}}"
      }
    },
    {
      "name": "TangramTikz",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:tangram name}}"
      }
    },
    {
      "name": "TangramTikz",
      "arg": {
        "format": "[]<>{}",
        "snippet": "[${2:options%keyvals}]<${3:TikZ options}>{${1:tangram name}}"
      }
    },
    {
      "name": "PieceTangram",
      "arg": {
        "format": "[](){}",
        "snippet": "[${2:options%keyvals}](${3:x},${4:y}){${1:piece name}}"
      }
    },
    {
      "name": "PieceTangram",
      "arg": {
        "format": "[]<>(){}",
        "snippet": "[${2:options%keyvals}]<${3:TikZ options}>(${4:x},${5:y}){${1:piece name}}"
      }
    }
  ],
  "envs": [
    {
      "name": "EnvTangramTikz"
    },
    {
      "name": "EnvTangramTikz",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "EnvTangramTikz",
      "arg": {
        "format": "[]<>",
        "snippet": "[${1:options%keyvals}]<${2:TikZ options}>"
      }
    }
  ],
  "keys": {
    "\\begin{EnvTangramTikz},\\TangramTikz": [
      "Puzzle",
      "Correction",
      "Color=",
      "Couleur=",
      "ColorCorrection",
      "CorrectionCouleur",
      "ColorList={${1:color1,color2,...}}\r",
      "ListeCouleurs={${1:couleur1,couleur2,...}}\r",
      "Sep="
    ],
    "\\PieceTangram#c": [
      "TangPuzz\r",
      "TangPuzz={${1:color}}\r",
      "TangSol\r",
      "TangSol={${1:color}}\r"
    ]
  },
  "args": []
}