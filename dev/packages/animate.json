{
  "includes": {
    "ifthen": [],
    "iftex": [],
    "ifdraft": [],
    "calc": [],
    "pdfbase": [],
    "graphics": [],
    "zref-abspage": [],
    "ocgbase": []
  },
  "macros": {
    "animategraphics{}{}{}{}": {
      "snippet": "animategraphics{${1:frame rate}}{${2:file basename}}{${3:first}}{${4:last}}"
    },
    "animategraphics[]{}{}{}{}": {
      "snippet": "animategraphics[${5:options}]{${1:frame rate}}{${2:file basename}}{${3:first}}{${4:last}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "newframe": {},
    "newframe[]": {
      "snippet": "newframe[${1:frame rate}]"
    },
    "newframe*": {},
    "newframe*[]": {
      "snippet": "newframe*[${1:frame rate}]"
    },
    "multiframe{}{}{}": {
      "snippet": "multiframe{${1:number of frames}}{${2:variables}}{${3:content}}"
    },
    "multiframebreak": {}
  },
  "envs": {
    "animateinline{}": {
      "name": "animateinline",
      "snippet": "{${1:frame rate}}"
    },
    "animateinline[]{}": {
      "name": "animateinline",
      "snippet": "[${2:options}]{${1:frame rate}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "options": [
    "dvipdfmx",
    "xetex",
    "dvisvgm",
    "export",
    "autoplay",
    "autopause",
    "autoresume",
    "loop",
    "palindrome",
    "draft",
    "final",
    "controls",
    "controls=${1|all,true,on,none,false,off|}",
    "controlsaligned=${2|left,left+${1:<indent>},center,right,right+${2:<indent>}|}",
    "width=",
    "height=",
    "totalheight=",
    "keepaspectratio",
    "scale=${1:factor}",
    "bb=${1:llx lly urx ury}",
    "viewport=${1:llx lly urx ury}",
    "trim=${1:left bottom right top}",
    "clip",
    "hiresbb",
    "interpolate",
    "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox|}",
    "buttonsize=",
    "buttonbg=${1:color array}",
    "buttonfg=${1:color array}",
    "buttonalpha=${1:opacity}",
    "step",
    "nomouse",
    "type=${1:file ext}",
    "method=${1|icon,widget,ocg|}",
    "poster",
    "poster=${1|first,last,none,${1:number}|}",
    "alttext=${1|none,${1:alt description}|}"
  ],
  "keyvals": [
    [
      "label=${1:label}",
      "type=${1:file ext}",
      "poster",
      "poster=${1|first,last,none,${1:number}|}",
      "every=${1:number}",
      "autopause",
      "autoplay",
      "autoresume",
      "loop",
      "palindrome",
      "step",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}",
      "bb=${1:llx lly urx ury}",
      "viewport=${1:llx lly urx ury}",
      "trim=${1:left bottom right top}",
      "clip",
      "hiresbb",
      "interpolate",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox|}",
      "controls=${1|all,true,on,none,false,off|}",
      "controlsaligned=${2|left,left+${1:<indent>},center,right,right+${2:<indent>}|}",
      "buttonsize=",
      "buttonbg=${1:color array}",
      "buttonfg=${1:color array}",
      "buttonalpha=${1:opacity}",
      "draft",
      "final",
      "nomouse",
      "method=${1|icon,widget,ocg|}",
      "measure",
      "alttext=${1|none,${1:alt description}|}",
      "begin={${1:begin code}}",
      "end={${1:end code}}",
      "timeline=${1:file}"
    ]
  ]
}