{
  "deps": [
    {
      "name": "array"
    },
    {
      "name": "tikz",
      "if": "tikz"
    },
    {
      "name": "etoolbox",
      "if": "lfg"
    },
    {
      "name": "unicode-math",
      "if": "lfg"
    }
  ],
  "macros": [
    {
      "name": "avm",
      "arg": {
        "format": "{}",
        "snippet": "{${1:structure}}"
      }
    },
    {
      "name": "avm",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:structure}}"
      }
    },
    {
      "name": "lframe"
    },
    {
      "name": "rframe"
    },
    {
      "name": "tag",
      "arg": {
        "format": "{}",
        "snippet": "{${1:identifier}}"
      }
    },
    {
      "name": "type",
      "arg": {
        "format": "{}",
        "snippet": "{${1:type}}"
      }
    },
    {
      "name": "type*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:type}}"
      }
    },
    {
      "name": "id",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:id}}{${2:structure}}"
      }
    },
    {
      "name": "punk",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:attribute}}{${2:type}}"
      }
    },
    {
      "name": "shuffle",
      "detail": "â§¢",
      "doc": "Shuffle product"
    },
    {
      "name": "avmsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\avm,\\avmsetup,\\avmdefinestyle"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "avmdefinestyle",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:settings%keyvals}}",
        "keys": [
          "\\avm,\\avmsetup,\\avmdefinestyle"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "avmdefinecommand",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:settings%keyvals}}"
      }
    },
    {
      "name": "avmdefinecommand",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:name}}[${3:label}]{${2:settings%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/langsci-avm#c": [
      "tikz\r",
      "lfg\r"
    ],
    "\\avm,\\avmsetup,\\avmdefinestyle": [
      "framewidth=",
      "framesep=",
      "id align=${1:token}\r",
      "id position=${1|south-west,south-east\r|}",
      "style=${1:name}\r",
      "align",
      "stretch=${1:factor}\r",
      "columnsep=",
      "vectorsep=",
      "delimfactor=${1:factor}\r",
      "delimfall=",
      "extraskip=",
      "attributes=${1:font settings}\r",
      "values=${1:font settings}\r",
      "types=${1:font settings}\r",
      "tags=${1:format settings}\r",
      "switch=${1:token}\r",
      "customise=${1:settings}\r"
    ]
  },
  "args": [
    "\\usepackage/langsci-avm#c"
  ]
}