{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-node"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "pstSetRelativeOrigin(x,y)"
    },
    {
      "name": "pstSetAbsoluteOrigin"
    },
    {
      "name": "pstSetPostScriptOrigin"
    },
    {
      "name": "pstPutAbs(x,y)",
      "arg": {
        "format": "{}",
        "snippet": "{${1:stuff}}"
      }
    },
    {
      "name": "pstPutAbs",
      "arg": {
        "format": "[](){}",
        "snippet": "[${2:options%keyvals}](${3:x},${4:y}){${1:stuff}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pst-abspos#c": [
      "notitlepage\r",
      "absolute\r",
      "lowerleft\r",
      "relative\r",
      "PostScript\r"
    ]
  },
  "args": [
    "\\usepackage/pst-abspos#c"
  ]
}