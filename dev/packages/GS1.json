{
  "deps": [
    {
      "name": "rule-D"
    }
  ],
  "macros": [
    {
      "name": "EANControlDigit",
      "arg": {
        "format": "{}",
        "snippet": "{${1:string}}"
      }
    },
    {
      "name": "EANBarcode",
      "arg": {
        "format": "{}",
        "snippet": "{${1:string}}"
      }
    },
    {
      "name": "EANBarcode",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:string}}"
      }
    },
    {
      "name": "GSSetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\EANBarcode,\\GSSetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\EANBarcode,\\GSSetup": [
      "ocrb",
      "module_width=",
      "module_height=",
      "code=${1|EAN-8,EAN-13\r|}",
      "scale=${1:factor}\r",
      "scale_to_font",
      "add_control"
    ]
  },
  "args": []
}