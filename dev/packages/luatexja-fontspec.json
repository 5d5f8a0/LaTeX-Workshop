      }
    },
    {
      "name": "newjfontfamily",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:cmd}}[${3:font features%keyvals}]{${2:font}}"
      }
    },
    {
      "name": "renewjfontfamily",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:cmd}}{${2:font}}"
      }
    },
    {
      "name": "renewjfontfamily",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:cmd}}[${3:font features%keyvals}]{${2:font}}"
