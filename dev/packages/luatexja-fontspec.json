      "name": "providejfontfamily",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:cmd}}[${3:font features%keyvals}]{${2:font}}"
      }
    },
    {
      "name": "newjfontface",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:cmd}}{${2:font}}"
      }
    },
    {
      "name": "newjfontface",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:cmd}}[${3:font features%keyvals}]{${2:font}}"
      }
    },
    {
      "name": "renewjfontface",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:cmd}}{${2:font}}"
      }
    },
    {
      "name": "renewjfontface",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:cmd}}[${3:font features%keyvals}]{${2:font}}"
      }
    },
    {
      "name": "setjfontface",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:cmd}}{${2:font}}"
      }
    },
    {
      "name": "setjfontface",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:cmd}}[${3:font features%keyvals}]{${2:font}}"
      }
    },
    {
      "name": "providejfontface",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:cmd}}{${2:font}}"
      }
    },
    {
      "name": "providejfontface",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:cmd}}[${3:font features%keyvals}]{${2:font}}"
      }
    },
    {
      "name": "defaultjfontfeatures",
      "arg": {
        "format": "{}",
        "snippet": "{${1:font features%keyvals}}",
        "keys": [
          "\\jfontspec,\\setmainjfont,\\setsansjfont,\\setmonojfont,\\newjfontfamily,\\renewjfontfamily,\\setjfontfamily,\\newjfontface,\\defaultjfontfeatures,\\addjfontfeatures,\\addjfontfeature"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "defaultjfontfeatures",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:font name list}]{${1:font features%keyvals}}",
        "keys": [
          "\\jfontspec,\\setmainjfont,\\setsansjfont,\\setmonojfont,\\newjfontfamily,\\renewjfontfamily,\\setjfontfamily,\\newjfontface,\\defaultjfontfeatures,\\addjfontfeatures,\\addjfontfeature"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "addjfontfeatures",
      "arg": {
        "format": "{}",
        "snippet": "{${1:font features%keyvals}}",
        "keys": [
          "\\jfontspec,\\setmainjfont,\\setsansjfont,\\setmonojfont,\\newjfontfamily,\\renewjfontfamily,\\setjfontfamily,\\newjfontface,\\defaultjfontfeatures,\\addjfontfeatures,\\addjfontfeature"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/luatexja-fontspec#c": [
      "match",
      "scale=${1:factor}\r",
      "tuenc\r",
      "euenc\r",
      "math\r",
      "no-math\r",
      "config\r",
      "no-config\r",
      "quiet\r",
      "silent\r"
    ],
    "\\jfontspec,\\setmainjfont,\\setsansjfont,\\setmonojfont,\\newjfontfamily,\\renewjfontfamily,\\setjfontfamily,\\newjfontface,\\defaultjfontfeatures,\\addjfontfeatures,\\addjfontfeature": [
      "CID=${1:name}\r",
      "JFM=${1:name}\r",
      "JFM-var=${1:name}\r",
      "NoEmbed\r",
      "Kanjiskip",
      "TateFeatures={${1:features}}\r",
      "TateFont=${1:font}\r",
      "YokoFeatures={${1:features}}\r",
      "AltFont={${1:specification}}\r",
      "Alternate=${1:feature}\r",
      "Annotation=\r",
      "AutoFakeBold=${1:number}\r",
      "AutoFakeSlant=${1:number}\r",
      "BoldFeatures={${1:features}}\r",
      "BoldFont=${1:font name}\r",
      "BoldItalicFeatures={${1:features}}\r",
      "BoldItalicFont=${1:font name}\r",
      "BoldSlantedFeatures={${1:features}}\r",
      "BoldSlantedFont=${1:font name}\r",
      "BoldSwashFeatures={${1:features}}\r",
      "BoldSwashFont=${1:font name}\r",
      "CJKShape=${1|Traditional,Simplified,JIS1978,JIS1983,JIS1990,Expert,NLC\r|}",
      "CharacterVariant={${1:glyph num list}:${2:variety num}}\r",
      "CharacterWidth=${1|Proportional,Full,Half,Thurd,Quarter,AlternateProportional,AlternateHalf,ResetAll\r|}",
      "Color=",
      "Colour=",
      "Contextuals=${1|Swash,Alternate,WordInitial,WordFinal,LineFinal,Inner,ResetAll\r|}",
      "Diacritics=${1|MarkToBase,MarkToMark,AboveBase,BelowBase,ResetAll\r|}",
      "Extension=${1:extension}\r",
      "FakeBold=${1:number}\r",
      "FakeSlant=${1:number}\r",
      "FakeStretch=${1:number}\r",
      "Font=${1:font name}\r",
      "FontFace={${1:series}}{${2:shape}}{${3:font name}}\r",
      "Fractions=${1|On,Off,Reset,Alternate,ResetAll\r|}",
      "HyphenChar=${1:character}\r",
      "IgnoreFontspecFile\r",
      "ItalicFeatures={${1:features}}\r",
      "ItalicFont=${1:font name}\r",
      "Kerning=${1|On,Off,Reset,Uppercase,ResetAll\r|}",
      "Language=${1:language}\r",
      "LetterSpace=${1:number}\r",
      "Letters=${1|SmallCaps,PetiteCaps,UppercaseSmallCaps,UppercasePetiteCaps,Unicase,ResetAll\r|}",
      "Ligatures=${1|Required,Common,Contextual,Rare,Discretionary,Historic,TeX\r|}",
      "LocalForms=${1|On,Off,Reset\r|}",
      "NFSSFamily=${1:font family}\r",
      "Numbers=${1|Uppercase,UppercaseOff,Lowercase,LowercaseOff,Lining,LiningOff,OldStyle,OldStyleOff,Proportional,ProportionalOff,Monospaced,MonospacedOff,SlashedZero,SlashedZeroOff,Arabic,ArabicOff,ResetAll\r|}",
      "OpticalSize=${1:size}\r",
      "Ornament=\r",
      "Path=${1:file path}\r",
      "PunctuationSpace=${1:number}\r",
      "RawFeature=${1:feature tag}\r",
      "Scale=${1:number}\r",
      "ScaleAgain=${1:number}\r",
      "Script=${1:name}\r",
      "Size=${1:number}\r",
      "SizeFeatures={${1:features}}\r",
      "SlantedFeatures={${1:features}}\r",
      "SlantedFont=${1:font name}\r",
      "SmallCapsFeatures={${1:features}}\r",
      "SmallCapsFont=${1:font name}\r",
      "Style=${1|Alternate,Italic,Ruby,Swash,Cursive,Historic,Titling,HorizontalKana,VerticalKana,ResetAll\r|}",
      "StylisticAlternates=${1:feature}\r",
      "StylisticSet=${1:set number}\r",
      "SwashFeatures={${1:features}}\r",
      "SwashFont=${1:font name}\r",
      "UprightFeatures={${1:features}}\r",
      "UprightFont=${1:font name}\r",
      "Variant=${1:integer}\r",
      "Vertical=${1|RotatedGlyphs,AlternatesForRotation,Alternates,KanaAlternates,Kerning,AlternateMetrics,HalfMetrics,ProportionalMetrics,ResetAll\r|}",
      "VerticalPosition=${1|Superior,Inferior,Numerator,Denominator,ScientificInferior,Ordinal,ResetAll\r|}",
      "WordSpace=${1:number}\r",
      "Mapping=${1:xetex-only feature}\r",
      "Renderer=${1:renderer}\r"
    ]
  },
  "args": [
    "\\usepackage/luatexja-fontspec#c"
  ]
}