{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "class-scrbook"
    },
    {
      "name": "roboto"
    },
    {
      "name": "sourceserifpro"
    },
    {
      "name": "anyfontsize"
    },
    {
      "name": "geometry"
    },
    {
      "name": "ragged2e"
    },
    {
      "name": "scrlayer-scrpage"
    },
    {
      "name": "csquotes"
    },
    {
      "name": "biblatex-chicago"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "doclicense"
    },
    {
      "name": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "edition",
      "arg": {
        "format": "{}",
        "snippet": "{${1:edition%text}}"
      }
    },
    {
      "name": "insertedition"
    },
    {
      "name": "insertpublishersaddress"
    },
    {
      "name": "insertauthor"
    },
    {
      "name": "insertdate"
    },
    {
      "name": "insertpublishers"
    },
    {
      "name": "Article",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title}}"
      }
    },
    {
      "name": "Article",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:title}}"
      }
    },
    {
      "name": "keywords",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keywords%text}}"
      }
    },
    {
      "name": "listofauthors"
    },
    {
      "name": "HUGE",
      "unusual": true
    },
    {
      "name": "TocAuthorEntry",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "currentarticlelabel",
      "unusual": true
    },
    {
      "name": "citeimprint",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:bibid}}"
      }
    },
    {
      "name": "citearticleauthor",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:bibid}}"
      }
    },
    {
      "name": "publishersaddress",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:address%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "abstract",
      "arg": {
        "format": "[]",
        "snippet": "[${1:language}]"
      }
    }
  ],
  "keys": {
    "\\documentclass/unigrazpub#c": [
      "collection",
      "license-type=${1:type}\r",
      "license-modifier=${1|by,by-sa,by-nd,by-nc,by-nc-sa,by-nc-nd\r|}",
      "license-version=${1|1.0,3.0,4.0\r|}"
    ],
    "\\Article#c": [
      "author=${1:author}\r",
      "authorkeys={${1:author1,author2,...}}\r",
      "head=${1:text}\r",
      "subtitle=${1:text}\r",
      "imprint=${1:text}\r",
      "doi=${1:DOI}\r",
      "license-type=${1:type}\r",
      "license-modifier=${1|by,by-sa,by-nd,by-nc,by-nc-sa,by-nc-nd\r|}",
      "license-version=${1|1.0,3.0,4.0\r|}"
    ]
  },
  "args": [
    "\\documentclass/unigrazpub#c"
  ]
}