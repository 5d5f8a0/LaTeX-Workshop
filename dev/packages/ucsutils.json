{
  "deps": [
    {
      "name": "ucs"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "UnicodeEmbedFont",
      "if": "univerb",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\UnicodeEmbedFont"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "univerb",
      "if": "univerb",
      "arg": {
        "format": "||",
        "snippet": "|${1:text}|"
      }
    },
    {
      "name": "unistring",
      "if": "unistring",
      "arg": {
        "format": "{}",
        "snippet": "{${1:string%definition}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/ucsutils#c": [
      "UnicodeEmbedFont",
      "univerb",
      "unistring"
    ],
    "\\UnicodeEmbedFont": [
      "encoding=${1:encoding}",
      "start=${1:position}",
      "fontstart=${1:position}",
      "num=${1:number}",
      "option=${1:Unicode option}"
    ]
  },
  "args": [
    "\\usepackage/ucsutils#c"
  ]
}