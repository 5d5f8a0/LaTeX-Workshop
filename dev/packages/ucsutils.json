{
  "deps": [
    {
      "name": "ucs"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "UnicodeEmbedFont",
      "if": "univerb",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\UnicodeEmbedFont"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "unistring",
      "if": "unistring",
      "arg": {
        "format": "{}",
        "snippet": "{${1:string%definition}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/ucsutils#c": [
      "UnicodeEmbedFont\r",
      "univerb\r",
      "unistring\r"
    ],
    "\\UnicodeEmbedFont": [
      "encoding=${1:encoding}\r",
      "start=${1:position}\r",
      "fontstart=${1:position}\r",
      "num=${1:number}\r",
      "option=${1:Unicode option}\r"
    ]
  },
  "args": [
    "\\usepackage/ucsutils#c"
  ]
}