{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "stringenc"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "shellesc"
    },
    {
      "name": "ifxetex"
    }
  ],
  "macros": [
    {
      "name": "getmap",
      "arg": {
        "format": "{}",
        "snippet": "{${1:address}}"
      }
    },
    {
      "name": "getmap",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:address}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/getmap#c,\\getmap": [
      "mode=${1|osm,gm,gsv\r|}",
      "inputencoding=${1:encoding}\r",
      "overwrite",
      "file=${1:file name}\r",
      "key=${1:service key}\r",
      "scale=${1:number}\r",
      "zoom=${1:number}\r",
      "xsize=${1:width in pixels}\r",
      "ysize=${1:height in pixels}\r",
      "imagetype=${1|png,png8,png32,jpeg,jpg,jpg-baseline,gif\r|}",
      "type=${1|map,sat,hyb,roadmap,satellite,hybrid,terrain\r|}",
      "color=${1:color}\r",
      "number=${1:number}\r",
      "language=${1:lang code}\r",
      "markers={${1:&markers=...}}\r",
      "visible={${1:loc1|loc2|...}}\r",
      "path={${1:&path=...}}\r",
      "pathfile=${1:file name}\r",
      "heading=${1:degrees}\r",
      "pitch=${1:degrees}\r",
      "fov=${1:degrees}\r"
    ]
  },
  "args": [
    "\\usepackage/getmap#c,\\getmap"
  ]
}