      "expand-amount=${1|n,o,f,x\r|}",
      "expand-unit=${1|n,o,f,x\r|}",
      "set-special-sign={${1:characters}}\r",
      "add-special-sign={${1:characters}}\r",
      "set-unknown-message=${1|error,warning,none\r|}",
      "set-cutext-translation-message=${1|error,warning,none\r|}",
      "print-numerals",
      "use-numerals-below=${1:integer}\r",
      "numeral-function=${1:function}\r",
      "Numeral-function=${1:function}\r",
      "parse-number",
      "range-sign=${1:string}\r",
      "cunum-range-sign=${1:string}\r",
      "cutext-range-sign=${1:string}\r",
      "use-phrases",
      "round-precision=${1:integer}\r",
      "round-to-int",
      "round-half=${1|default,commercial\r|}",
      "eval-fraction",
      "convert-fraction",
      "fraction-inline={${1:input}}\r",
      "mixed-fraction-space=",
      "cutext-space=${1:string}\r",
      "phrase-space=${1:string}\r",
      "amount-unit-space=${1:string}\r",
      "times-space=${1:string}\r",
      "recalculate-amount",
      "set-number-of-persons=${1:integer}\r",
      "label=${1:string}*${2:integer}\r",
      "get-label=${1:label}\r",
      "ref=${1:label}\r",
      "curef-add-forbidden-unit={${1:unit list}}\r",
      "curef-remove-forbidden-unit={${1:unit list}}\r",
      "curef-clear-forbidden-units",
      "check-temperature",
      "add-temperature-to-check={${1:unit=min value,...}}\r",
      "convert-to-eV",
      "add-natural-unit=${1:unit}\r",
      "42",
      "nothing-special",
      "going-bonkers",
      "fully-bonkers",
      "xD-lol"
