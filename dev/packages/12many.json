{
  "deps": [
    {
      "name": "calc"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "nto",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:num1}}{${2:num2}}"
      }
    },
    {
      "name": "ito",
      "arg": {
        "format": "{}",
        "snippet": "{${1:num}}"
      }
    },
    {
      "name": "oto",
      "arg": {
        "format": "{}",
        "snippet": "{${1:num}}"
      }
    },
    {
      "name": "setOTMstyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:style}}"
      }
    },
    {
      "name": "setOTMstyle",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:params%keyvals}]{${1:style}}"
      }
    },
    {
      "name": "newOTMstyle",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "newOTMstyle",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:params}]{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "getOTMparameter",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:param}}"
      }
    },
    {
      "name": "renewOTMstyle",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "newOTMstyle",
      "unusual": true,
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:params}]{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "newOTMparameter",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:style}}{${2:param}}{${3:value}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/12many#c": [
      "set\r",
      "dots\r",
      "nude\r",
      "laue\r"
    ],
    "\\setOTMstyle/set": [
      "var=${1:variable name}\r",
      "naturals=${1:symbol}\r",
      "where=${1:symbol}\r"
    ],
    "\\setOTMstyle/dots": [
      "dots=${1:symbol}\r"
    ],
    "\\setOTMstyle/nude": [
      "dots=${1:symbol}\r"
    ],
    "\\setOTMstyle/laue": [
      "setminus=${1:symbol}\r",
      "setplus=${1:symbol}\r",
      "ybelow=",
      "strokewidth=",
      "innersidegap=",
      "outersidegap="
    ]
  },
  "args": [
    "\\usepackage/12many#c"
  ]
}