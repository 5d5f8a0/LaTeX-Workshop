{
  "deps": [
    {
      "name": "datetime"
    },
    {
      "name": "xstring"
    },
    {
      "name": "stack"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "forloop"
    },
    {
      "name": "calc"
    },
    {
      "name": "etoolbox"
    }
  ],
  "macros": [
    {
      "name": "iplot",
      "arg": {
        "format": "{}",
        "snippet": "{${1:function%definition}}"
      }
    },
    {
      "name": "iplot",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:function%definition}}"
      }
    }
  ],
  "envs": [
    {
      "name": "iplotdd"
    },
    {
      "name": "iplotdd",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "iplotddd"
    },
    {
      "name": "iplotddd",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{iplotdd},\\begin{iplotddd}": [
      "width=${1:number}\r",
      "height=${1:number}\r"
    ],
    "\\iplot": [
      "var={${1:name,start,end}}\r",
      "param={${1:name,value,step,start,end}}\r",
      "frange={${1:name,start,end}}\r",
      "color=${1|black,white,red,green,blue,cyan,magenta,yellow,dark-gray,gray,light-gray\r|}",
      "resolution=${1:resolution}\r"
    ]
  },
  "args": []
}