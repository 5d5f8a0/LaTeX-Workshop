{
  "deps": [
    {
      "name": "luatex"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "py",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code%definition}}"
      }
    },
    {
      "name": "pyq",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code%definition}}"
      }
    },
    {
      "name": "pyc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code%definition}}"
      }
    },
    {
      "name": "pycq",
      "arg": {
        "format": "{}",
        "snippet": "{${1:code%definition}}"
      }
    },
    {
      "name": "pyfile",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "pyfileq",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "pyfilerepl",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "pysession",
      "arg": {
        "format": "{}",
        "snippet": "{${1:session}}"
      }
    },
    {
      "name": "pyoption",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:option%keyvals}}{${2:value}}",
        "keys": [
          "\\pyoption"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pyif",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:test}}{${2:then clause}}{${3:else clause}}"
      }
    },
    {
      "name": "PyLTVerbatimEnv"
    }
  ],
  "envs": [
    {
      "name": "python"
    },
    {
      "name": "pythonq"
    },
    {
      "name": "pythonrepl"
    }
  ],
  "keys": {
    "\\usepackage/pyluatex#c": [
      "executable=${1:path}",
      "ignoreerrors",
      "localimports",
      "shutdown=${1|veryveryend,veryenddocument,off|}",
      "verbose"
    ],
    "\\pyoption": [
      "ignoreerrors",
      "verbose"
    ]
  },
  "args": [
    "\\usepackage/pyluatex#c"
  ]
}