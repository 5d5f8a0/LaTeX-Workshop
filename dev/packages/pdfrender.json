{
  "deps": [
    {
      "name": "iftex"
    },
    {
      "name": "infwarerr"
    },
    {
      "name": "ltxcmds"
    },
    {
      "name": "kvsetkeys"
    }
  ],
  "macros": [
    {
      "name": "pdfrender",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "textpdfrender",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:keyvals}}{${2:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pdfrender,\\textpdfrender": [
      "TextRenderingMode=${1|0,1,2,3,4,5,6,7,Fill,Stroke,FillStroke,Invisible,FillClip,StrokeClip,FillStrokeClip,Clip\r|}",
      "LineWidth=\r",
      "LineCapStyle=${1|0,1,2,Butt,Round,ProjectingSquare\r|}",
      "LineJoinStyle=${1|0,1,2,Miter,Round,Bevel\r|}",
      "MiterLimit=${1:number}\r",
      "Flatness=${1:number}\r",
      "LineDashPattern=\r",
      "RenderingIntent=${1|AbsoluteColorimetric,RelativeColorimetric,Saturation,Perceptual\r|}",
      "FillColor=\r",
      "StrokeColor=\r"
    ]
  },
  "args": []
}