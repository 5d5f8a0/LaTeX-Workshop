{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "polynomial",
      "arg": {
        "format": "{}",
        "snippet": "{${1:coeff1,coeff2,...}}"
      }
    },
    {
      "name": "polynomial",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:coeff1,coeff2,...}}"
      }
    },
    {
      "name": "polynomialfrac",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:numerator coeffs}}{${2:denominator coeffs}}"
      }
    },
    {
      "name": "polynomialfrac",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:numerator coeffs}}{${2:denominator coeffs}}"
      }
    },
    {
      "name": "polynomialstyle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\polynomial,\\polynomialfrac,\\polynomialstyle"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\polynomial,\\polynomialfrac,\\polynomialstyle": [
      "falling",
      "reciprocal",
      "var=${1:variable symbol}\r",
      "start=${1:integer}\r",
      "step=${1:integer}\r",
      "add=${1:addition symbol}\r",
      "sub=${1:subtraction symbol}\r",
      "firstsub=${1:symbol}\r"
    ]
  },
  "args": []
}