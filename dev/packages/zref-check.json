{
  "deps": [
    {
      "name": "zref-user"
    },
    {
      "name": "zref-abspage"
    },
    {
      "name": "ifdraft"
    },
    {
      "name": "zref-hyperref"
    }
  ],
  "macros": [
    {
      "name": "zcheck",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:labellist}}{${2:text}}"
      }
    },
    {
      "name": "zcheck",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:checks%keyvals}]{${1:labellist}}{${2:text}}"
      }
    },
    {
      "name": "zctarget",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:label}}{${2:text}}"
      }
    },
    {
      "name": "zrefchecksetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\zcheck,\\zrefchecksetup,\\usepackage/zref-check#c",
          "\\zrefchecksetup,\\usepackage/zref-check#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [
    {
      "name": "zcregion",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    }
  ],
  "keys": {
    "\\zcheck": [
      "thispage\r",
      "prevpage\r",
      "nextpage\r",
      "otherpage\r",
      "pagegap\r",
      "facing\r",
      "above\r",
      "below\r",
      "pagesbefore\r",
      "ppbefore\r",
      "pagesafter\r",
      "ppafter\r",
      "before\r",
      "after\r",
      "thischap\r",
      "prevchap\r",
      "nextchap\r",
      "chapsbefore\r",
      "chapsafter\r",
      "thissec\r",
      "prevsec\r",
      "nextsec\r",
      "secsbefore\r",
      "secsafter\r",
      "close\r",
      "far\r",
      "manual\r",
      "ignore\r",
      "ok\r"
    ],
    "\\zcheck,\\zrefchecksetup,\\usepackage/zref-check#c": [
      "msglevel=${1|warn,info,none,infoifdraft,warniffinal\r|}",
      "onpage=${1|labelseq,msg,labelseqifdraft,msgiffinal\r|}"
    ],
    "\\zrefchecksetup,\\usepackage/zref-check#c": [
      "hyperref=${1|auto,true,false\r|}",
      "closerange=${1:integer}\r"
    ]
  },
  "args": [
    "\\zcheck,\\zrefchecksetup,\\usepackage/zref-check#c",
    "\\zrefchecksetup,\\usepackage/zref-check#c"
  ]
}