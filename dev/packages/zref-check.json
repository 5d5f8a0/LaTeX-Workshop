{
  "includes": {
    "zref-user": [],
    "zref-abspage": [],
    "ifdraft": [],
    "zref-hyperref": []
  },
  "macros": {
    "zcheck{}{}": {
      "snippet": "zcheck{${1:labellist}}{${2:text}}"
    },
    "zcheck[]{}{}": {
      "snippet": "zcheck[${3:checks}]{${1:labellist}}{${2:text}}"
    },
    "zctarget{}{}": {
      "snippet": "zctarget{${1:label}}{${2:text}}"
    },
    "zrefchecksetup{}": {
      "snippet": "zrefchecksetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
  "envs": {
    "zcregion{}": {
      "name": "zcregion",
      "snippet": "{${1:label}}"
    }
  },
  "options": [
    "msglevel=${1|warn,info,none,infoifdraft,warniffinal|}",
    "onpage=${1|labelseq,msg,labelseqifdraft,msgiffinal|}",
    "hyperref=${1|auto,true,false|}",
    "closerange=${1:integer}"
  ],
  "keyvals": [
    [
      "msglevel=${1|warn,info,none,infoifdraft,warniffinal|}",
      "onpage=${1|labelseq,msg,labelseqifdraft,msgiffinal|}",
      "hyperref=${1|auto,true,false|}",
      "closerange=${1:integer}"
    ]
  ]
}