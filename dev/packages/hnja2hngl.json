{
  "deps": [
    {
      "name": "grruby"
    }
  ],
  "macros": [
    {
      "name": "readhanja",
      "arg": {
        "format": "{}",
        "snippet": "{${1:hanja}}"
      }
    },
    {
      "name": "readhanja",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:hanja}}[${2:number}]"
      }
    },
    {
      "name": "readhanjaword",
      "arg": {
        "format": "{}",
        "snippet": "{${1:hanja}}"
      }
    },
    {
      "name": "rwhanja",
      "arg": {
        "format": "{}",
        "snippet": "{${1:hanja}}"
      }
    },
    {
      "name": "AssignReading",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:hanja}}{${2:reading}}"
      }
    },
    {
      "name": "rpSetReading",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:hanja=reading}}"
      }
    },
    {
      "name": "grrwhanja",
      "arg": {
        "format": "{}",
        "snippet": "{${1:hanja}}"
      }
    },
    {
      "name": "rwhanjachar",
      "arg": {
        "format": "{}",
        "snippet": "{${1:hanja}}"
      }
    },
    {
      "name": "rpRead",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options}}",
        "keys": [
          "\\rpRead#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "rpRead",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:options}}[${2:hanja=reading}]",
        "keys": [
          "\\rpRead#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "viewCodePoint",
      "arg": {
        "format": "{}",
        "snippet": "{${1:unicode char}}"
      }
    },
    {
      "name": "viewCodePoint*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:unicode char}}"
      }
    },
    {
      "name": "showReadings",
      "arg": {
        "format": "{}",
        "snippet": "{${1:hanja}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/hnja2hngl#c": [
      "grruby=false"
    ],
    "\\rpRead#c": [
      "comma",
      "para",
      "at",
      "period"
    ]
  },
  "args": [
    "\\usepackage/hnja2hngl#c"
  ]
}