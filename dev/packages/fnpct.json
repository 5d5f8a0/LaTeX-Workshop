{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "translations"
    }
  ],
  "macros": [
    {
      "name": "footnote*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:number}]{${1:text}}"
      }
    },
    {
      "name": "footnote*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "footnotemark*",
      "unusual": true
    },
    {
      "name": "footnotemark*",
      "unusual": true,
      "arg": {
        "format": "[]",
        "snippet": "[${1:number}]"
      }
    },
    {
      "name": "setfnpct",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\setfnpct"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "multfootsep",
      "unusual": true
    },
    {
      "name": "multfootrange",
      "unusual": true
    },
    {
      "name": "multfootnote",
      "if": "multiple",
      "arg": {
        "format": "{}",
        "snippet": "{${1:fn1;fn2;...}}"
      }
    },
    {
      "name": "multfootnote*",
      "if": "multiple",
      "arg": {
        "format": "{}",
        "snippet": "{${1:fn1;fn2;...}}"
      }
    },
    {
      "name": "AdaptNote",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:cmd}}{${2:args}}{${3:code}}"
      }
    },
    {
      "name": "AdaptNote",
      "unusual": true,
      "arg": {
        "format": "{}{}[]{}",
        "snippet": "{${1:cmd}}{${2:args}}[${4:counter}]{${3:code}}"
      }
    },
    {
      "name": "AdaptNoteName",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:csname}}{${2:args}}{${3:code}}"
      }
    },
    {
      "name": "AdaptNoteName",
      "unusual": true,
      "arg": {
        "format": "{}{}[]{}",
        "snippet": "{${1:csname}}{${2:args}}[${4:counter}]{${3:code}}"
      }
    },
    {
      "name": "MultVariant",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:cmd}}"
      }
    },
    {
      "name": "MultVariantName",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:csname}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\setfnpct": [
      "dont-mess-around",
      "reverse",
      "unspace",
      "ranges",
      "add-trailing-token={${1:token}{${2:name}}}\r",
      "add-trailing-token={[${1:before}]${2:token}[${3:after}]{${4:name}}}\r",
      "activate-trailing-tokens={${1:tokenlist}}\r",
      "deactivate-trailing-tokens={${1:tokenlist}}\r",
      "before-footnote-space=",
      "before-dot-space=",
      "before-comma-space=",
      "before-punct-space=",
      "after-dot-space=",
      "after-comma-space=",
      "after-punct-space=",
      "mult-variant-prefix={${1:letters}}\r",
      "mult-fn-delim={${1:code}}\r",
      "separation-symbol=${1:symbol}\r",
      "range-symbol=${1:symbol}\r",
      "print-separation={${1:code}}\r",
      "print-range={${1:code}}\r",
      "keep-ranges"
    ]
  },
  "args": []
}