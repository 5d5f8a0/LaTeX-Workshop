{
  "deps": [
    {
      "name": "iexec"
    },
    {
      "name": "fancyvrb"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "tikz"
    },
    {
      "name": "pgfopts"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "ifxetex"
    }
  ],
  "macros": [
    {
      "name": "docshotOptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "docshotPrerequisite",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "docshotAfter",
      "arg": {
        "format": "{}",
        "snippet": "{${1:command line}}"
      }
    }
  ],
  "envs": [
    {
      "name": "docshot"
    }
  ],
  "keys": {
    "\\usepackage/docshots#c": [
      "pdflatex=${1:path}\r",
      "gs=${1:path}\r",
      "pdfcrop=${1:path}\r",
      "margin=${1:number}\r",
      "nocrop\r",
      "hspace=",
      "left=",
      "right=",
      "dtx\r",
      "tmpdir=${1:path}\r",
      "runs=${1:integer}\r",
      "small\r",
      "tiny\r",
      "log\r",
      "inputminted=${1:language}\r",
      "lstinputlisting\r"
    ],
    "\\docshotOptions#c": [
      "inputpath=${1:path}\r",
      "aboveskip=",
      "belowskip=",
      "lineskip=",
      "boxpos=${1|b,c,t\r|}",
      "print",
      "showlines",
      "emptylines=${1:number}\r",
      "emptylines=*${1:number}\r",
      "gobble=${1:number}\r",
      "style=${1:style name}\r",
      "language=${1:language}\r",
      "language=[${1:dialect}]${2:language}\r",
      "alsolanguage=${1:language}\r",
      "alsolanguage=[${1:dialect}]${2:language}\r",
      "defaultdialect=[${1:dialect}]${2:language}\r",
      "printpod",
      "usekeywordsintag",
      "tagstyle=${1:style}\r",
      "markfirstintag=${1:style}\r",
      "makemacrouse",
      "basicstyle=${1:style}\r",
      "identifierstyle=${1:style}\r",
      "commentstyle=${1:style}\r",
      "stringstyle=${1:style}\r",
      "keywordstyle=${1:style}\r",
      "keywordstyle=[${1:number}]${2:style}\r",
      "keywordstyle=[${1:number}]*${2:style}\r",
      "ndkeywordstyle=${1:style}\r",
      "classoffset=${1:number}\r",
      "texcsstyle=${1:style}\r",
      "texcsstyle=[${1:class number}]${2:style}\r",
      "texcsstyle=*[${1:class number}]${2:style}\r",
      "directivestyle=${1:style}\r",
      "emph={${1:identifier list}}\r",
      "emph=[${1:number}]{${2:identifier list}}\r",
      "moreemph={${1:identifier list}}\r",
      "moreemph=[${1:number}]{${2:identifier list}}\r",
      "deleteemph={${1:identifier list}}\r",
      "deleteemph=[${1:number}]{${2:identifier list}}\r",
      "emphstyle={${1:style}}\r",
      "emphstyle=[${1:number}]{${2:style}}\r",
      "delim=[${1:type}][${2:style}]${3:delimiters}\r",
      "delim=*[${1:type}][${2:style}]${3:delimiters}\r",
      "delim=**[${1:type}][${2:style}]${3:delimiters}\r",
      "moredelim=[${1:type}][${2:style}]${3:delimiters}\r",
      "moredelim=*[${1:type}][${2:style}]${3:delimiters}\r",
      "moredelim=**[${1:type}][${2:style}]${3:delimiters}\r",
      "deletedelim=[${1:type}][${2:style}]${3:delimiters}\r",
      "deletedelim=*[${1:type}][${2:style}]${3:delimiters}\r",
      "deletedelim=**[${1:type}][${2:style}]${3:delimiters}\r",
      "extendedchars",
      "inputencoding=${1:encoding}\r",
      "upquote",
      "tabsize=${1:number}\r",
      "showtabs",
      "tab=${1:tokens}\r",
      "showspaces",
      "showstringspaces",
      "formfeed=${1:tokens}\r",
      "numbers=${1|none,left,right\r|}",
      "stepnumber=${1:number}\r",
      "numberfirstline",
      "numberstyle=${1:style}\r",
      "numbersep=",
      "numberblanklines",
      "firstnumber=${1:auto|last|<number>}\r",
      "name=${1:name}\r",
      "title=${1:title text}\r",
      "caption={${1:caption text}}\r",
      "caption={[${1:short}]${2:caption text}}\r",
      "label=",
      "nolol",
      "numberbychapter",
      "captionpos=${1:subset of tb}\r",
      "abovecaptionskip=",
      "belowcaptionskip=",
      "linewidth=",
      "xleftmargin=",
      "xrightmargin=",
      "resetmargins",
      "breaklines",
      "breakatwhitespace",
      "prebreak=${1:tokens}\r",
      "postbreak=${1:tokens}\r",
      "breakindent=",
      "breakautoindent",
      "frame=${1:type}\r",
      "frameround=\r",
      "framesep=",
      "rulesep=",
      "framerule=",
      "framexleftmargin=",
      "framexrightmargin=",
      "framextopmargin=",
      "framexbottommargin=",
      "backgroundcolor=",
      "rulecolor=",
      "fillcolor=",
      "rulesepcolor=",
      "frameshape={${1:top shape}}{${2:left shape}}{${3:right shape}}{${4:bottom shape}}\r",
      "index={${1:identifiers}}\r",
      "index=[${1:number}]{${2:identifiers}}\r",
      "index=[${1:number}][${2:keyword classes}]{${3:identifiers}}\r",
      "moreindex={${1:identifiers}}\r",
      "moreindex=[${1:number}]{${2:identifiers}}\r",
      "moreindex=[${1:number}][${2:keyword classes}]{${3:identifiers}}\r",
      "deleteindex={${1:identifiers}}\r",
      "deleteindex=[${1:number}]{${2:identifiers}}\r",
      "deleteindex=[${1:number}][${2:keyword classes}]{${3:identifiers}}\r",
      "indexstyle=${1:one-parameter macro}\r",
      "indexstyle=[${1:number}]${2:one-parameter macro}\r",
      "columns=${1:alignment}\r",
      "columns=[${1:c|l|r}]${2:alignment}\r",
      "flexiblecolumns",
      "keepspaces",
      "basewidth=",
      "fontadjust",
      "texcl",
      "mathescape",
      "escapechar=${1:character}\r",
      "escapeinside=${1:<char1><char2>}\r",
      "escapebegin=${1:tokens}\r",
      "escapeend=${1:tokens}\r",
      "fancyvrb",
      "fvcmdparams=${1:<cmd1> <num1> ...}\r",
      "morefvcmdparams=${1:<cmd1> <num1> ...}\r",
      "literate={${1:replace}}{${2:replacement text}}{${3:length}}${4:...}\r",
      "literate=*{${1:replace}}{${2:replacement text}}{${3:length}}${4:...}\r",
      "rangebeginprefix=${1:prefix}\r",
      "rangebeginsuffix=${1:suffix}\r",
      "rangeendprefix=${1:prefix}\r",
      "rangeendsuffix=${1:suffix}\r",
      "rangeprefix=${1:prefix}\r",
      "rangesuffix=${1:suffix}\r",
      "includerangemarker",
      "multicols=${1:number}\r",
      "float\r",
      "float=${1:subset of tbph}\r",
      "float=*${1:subset of tbph}\r",
      "floatplacement=${1:place specifiers}\r",
      "firstline=${1:number}\r",
      "lastline=${1:number}\r",
      "linerange={${1:first1-last1,first2-last2,...}}\r",
      "consecutivenumbers",
      "keywordsprefix=${1:prefix}\r",
      "keywords={${1:list of keywords}}\r",
      "keywords=[${1:number}]{${2:list of keywords}}\r",
      "morekeywords={${1:list of keywords}}\r",
      "morekeywords=[${1:number}]{${2:list of keywords}}\r",
      "deletekeywords={${1:list of keywords}}\r",
      "deletekeywords=[${1:number}]{${2:list of keywords}}\r",
      "ndkeywords={${1:list of keywords}}\r",
      "moreendkeywords={${1:list of keywords}}\r",
      "deleteendkeywords={${1:list of keywords}}\r",
      "texcs={${1:list of csnames}}\r",
      "texcs=[${1:class number}]{${2:list of csnames}}\r",
      "moretexcs={${1:list of csnames}}\r",
      "moretexcs=[${1:class number}]{${2:list of csnames}}\r",
      "deletetexcs={${1:list of csnames}}\r",
      "deletetexcs=[${1:class number}]{${2:list of csnames}}\r",
      "directives={${1:list of compiler directives}}\r",
      "moredirectives={${1:list of compiler directives}}\r",
      "deletedirectives={${1:list of compiler directives}}\r",
      "sensitive",
      "alsoletter={${1:character sequence}}\r",
      "alsodigit={${1:character sequence}}\r",
      "alsoother={${1:character sequence}}\r",
      "otherkeywords={${1:keywords}}\r",
      "tag=${1:<char1><char2>}\r",
      "string=${1:delimiter}\r",
      "string=[${1:b|d|m|bd|s}]${2:delimiter}\r",
      "morestring=${1:delimiter}\r",
      "morestring=[${1:b|d|m|bd|s}]${2:delimiter}\r",
      "deletestring=${1:delimiter}\r",
      "deletestring=[${1:b|d|m|bd|s}]${2:delimiter}\r",
      "comment=${1:delimiter(s)}\r",
      "comment=[${1:type}]${2:delimiter(s)}\r",
      "morecomment=${1:delimiter(s)}\r",
      "morecomment=[${1:type}]${2:delimiter(s)}\r",
      "deletecomment=${1:delimiter(s)}\r",
      "deletecomment=[${1:type}]${2:delimiter(s)}\r",
      "keywordcomment={${1:keywords}}\r",
      "morekeywordcomment={${1:keywords}}\r",
      "deletekeywordcomment={${1:keywords}}\r",
      "keywordcommentsemicolon={${1:keywords}}{${2:keywords}}{${3:keywords}}\r",
      "podcomment"
    ]
  },
  "args": [
    "\\usepackage/docshots#c"
  ]
}