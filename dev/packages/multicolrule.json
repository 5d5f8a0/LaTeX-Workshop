{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "xpatch"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "scrlfile"
    },
    {
      "name": "multicol"
    },
    {
      "name": "tikz",
      "if": "tikz"
    },
    {
      "name": "tikz",
      "if": "tikz=true"
    }
  ],
  "macros": [
    {
      "name": "SetMCRule",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\SetMCRule,\\DeclareMCRulePattern",
          "\\SetMCRule,\\DeclareMCRulePattern",
          "\\SetMCRule,\\DeclareMCRulePattern"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "DeclareMCRulePattern",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:keyvals}}"
      }
    },
    {
      "name": "columnseprulecolor",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/multicolrule#c": [
      "tikz",
      "twocolumn",
      "paracol"
    ],
    "\\SetMCRule,\\DeclareMCRulePattern": [
      "custom-line={${1:draw command}}\r"
    ]
  },
  "args": [
    "\\usepackage/multicolrule#c"
  ]
}