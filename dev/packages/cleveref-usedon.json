{
  "deps": [
    {
      "name": "cleveref"
    }
  ],
  "macros": [
    {
      "name": "cref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "cref*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "Cref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    },
    {
      "name": "Cref*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:option%keyvals}]{${1:labellist}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/cleveref-usedon#c": [
      "poorman\r",
      "sort\r",
      "compress\r",
      "sort&compress\r",
      "nosort\r",
      "capitalise\r",
      "capitalize\r",
      "nameinlink\r",
      "noabbrev\r",
      "english\r",
      "german\r",
      "ngerman\r",
      "dutch\r",
      "french\r",
      "spanish\r",
      "italian\r",
      "russian\r",
      "ukrainian\r",
      "norsk\r",
      "danish\r",
      "esperanto\r",
      "swedish\r",
      "brazilian\r",
      "catalan\r"
    ],
    "\\cref#c,\\cref*#c,\\Cref#c,\\Cref*#c": [
      "UsedOn\r",
      "UsedBy\r",
      "UsedByAndOn\r"
    ]
  },
  "args": [
    "\\usepackage/cleveref-usedon#c"
  ]
}