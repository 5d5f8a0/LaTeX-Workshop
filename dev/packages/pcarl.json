{
  "deps": [
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "offamily"
    },
    {
      "name": "textof",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "offamilydefault",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pcarl#c": [
      "scaled=${1:factor}\r"
    ]
  },
  "args": [
    "\\usepackage/pcarl#c"
  ]
}