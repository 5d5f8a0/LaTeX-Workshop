{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "geometry"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "pstricks"
    },
    {
      "name": "pst-ovl"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "enumitem"
    },
    {
      "name": "verbatim"
    },
    {
      "name": "powerdot-default"
    },
    {
      "name": "pdfbase",
      "if": "clock"
    },
    {
      "name": "pdfbase",
      "if": "clock=true"
    },
    {
      "name": "powerdot-aggie",
      "if": "style=aggie"
    },
    {
      "name": "powerdot-BerlinFU",
      "if": "style=BerlinFU"
    },
    {
      "name": "powerdot-bframe",
      "if": "style=bframe"
    },
    {
      "name": "powerdot-ciment",
      "if": "style=ciment"
    },
    {
      "name": "powerdot-elcolors",
      "if": "style=elcolors"
    },
    {
      "name": "powerdot-fyma",
      "if": "style=fyma"
    },
    {
      "name": "powerdot-horatio",
      "if": "style=horatio"
    },
    {
      "name": "powerdot-husky",
      "if": "style=husky"
    },
    {
      "name": "powerdot-ikeda",
      "if": "style=ikeda"
    },
    {
      "name": "powerdot-jefka",
      "if": "style=jefka"
    },
    {
      "name": "powerdot-klope",
      "if": "style=klope"
    },
    {
      "name": "powerdot-paintings",
      "if": "style=paintings"
    },
    {
      "name": "powerdot-pazik",
      "if": "style=pazik"
    },
    {
      "name": "powerdot-sailor",
      "if": "style=sailor"
    },
    {
      "name": "powerdot-simple",
      "if": "style=simple"
    },
    {
      "name": "powerdot-tuliplab",
      "if": "style=tuliplab"
    },
    {
      "name": "powerdot-tycja",
      "if": "style=tycja"
    },
    {
      "name": "powerdot-upen",
      "if": "style=upen"
    }
  ],
  "macros": [
    {
      "name": "pdsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\pdsetup",
          "\\pdsetup,\\maketitle,\\begin{titleslide},\\begin{basic},\\begin{wideslide},\\begin{slide},\\begin{sectionslide},\\begin{sectionwideslide},\\section"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "maketitle",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "pause"
    },
    {
      "name": "pause",
      "arg": {
        "format": "[]",
        "snippet": "[${1:number}]"
      }
    },
    {
      "name": "item<overlays>"
    },
    {
      "name": "item",
      "arg": {
        "format": "[]<>",
        "snippet": "[${1:label}]<${2:overlays}>"
      }
    },
    {
      "name": "onslide",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:overlays}}{${2:text}}"
      }
    },
    {
      "name": "onslide*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:overlays}}{${2:text}}"
      }
    },
    {
      "name": "tableofcontents",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "slidewidth"
    },
    {
      "name": "slideheight"
    },
    {
      "name": "thenote",
      "unusual": true
    },
    {
      "name": "theslide",
      "unusual": true
    },
    {
      "name": "twocolumn",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "twocolumn",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:left%text}}{${2:right%text}}"
      }
    },
    {
      "name": "pddefinetemplate",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:envname}}{${2:options}}{${3:commands}}"
      }
    },
    {
      "name": "pddefinetemplate",
      "unusual": true,
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:basis}]{${1:envname}}{${2:options}}{${3:commands}}"
      }
    },
    {
      "name": "pdifsetup",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:desired}}{${2:true}}{${3:false}}"
      }
    },
    {
      "name": "pdbookmark",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}{${3:arg3}}{${4:arg4}}"
      }
    },
    {
      "name": "pdcontentsline",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}{${3:arg3}}"
      }
    }
  ],
  "envs": [
    {
      "name": "enumerate",
      "arg": {
        "format": "[][]",
        "snippet": "[${1:type=0|1,}][${2:enumitem options}]"
      }
    },
    {
      "name": "itemize",
      "arg": {
        "format": "[][]",
        "snippet": "[${1:type=0|1}][${2:enumitem options}]"
      }
    },
    {
      "name": "note",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title}}"
      }
    },
    {
      "name": "emptyslide",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title}}"
      }
    },
    {
      "name": "pauseslide",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:title}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/powerdot#c": [
      "mode=${1|present,print,handout\r|}",
      "nohandoutpagebreaks\r",
      "nohandoutframes\r",
      "paper=${1|screen,a4paper,letterpaper,smartboard,169\r|}",
      "orient=${1|landscape,portrait\r|}",
      "display=${1|slides,slidesnotes,notes\r|}",
      "size=${1|8pt,9pt,10pt,11pt,12pt,14pt,17pt,20pt\r|}",
      "style=${1:style name}\r",
      "fleqn\r",
      "leqno\r",
      "nopsheader\r",
      "hlentries",
      "hlsections",
      "pauseslide\r",
      "clock\r"
    ],
    "\\pdsetup": [
      "theslide=${1:code}\r",
      "thenote=${1:code}\r",
      "counters={${1:counter1,counter2,...}}\r",
      "list={${1:enumitem options}}\r",
      "enumerate={${1:enumitem options}}\r",
      "itemize={${1:enumitem options}}\r",
      "clockformat=${1:format}\r",
      "clockrefresh=${1:milliseconds}\r"
    ],
    "\\pdsetup,\\maketitle,\\begin{titleslide},\\begin{basic},\\begin{wideslide},\\begin{slide},\\begin{sectionslide},\\begin{sectionwideslide},\\section": [
      "palette=${1:palette name}\r",
      "lf=${1:text}\r",
      "cf=${1:text}\r",
      "rf=${1:text}\r",
      "trans=${1:transition}\r",
      "method=${1|normal,direct,file\r|}",
      "logohook=${1|tl,t,tr,r,Br,br,b,bl,Bl,l,B,c\r|}",
      "logopos={${1:width,height}}\r",
      "logocmd={${1:code}}\r",
      "randomdots",
      "dmindots=${1:integer}\r",
      "dmaxdots=${1:integer}\r",
      "dminsize=",
      "dmaxsize=",
      "dminwidth=",
      "dmaxwidth=",
      "dminheight=",
      "dmaxheight=",
      "dbright=${1:integer}\r",
      "dprop={${1:pstricks options}}\r"
    ],
    "\\begin{titleslide},\\begin{basic},\\begin{wideslide},\\begin{slide},\\begin{sectionslide},\\begin{sectionwideslide},\\section": [
      "toc=${1:TOC entry}\r",
      "bm=${1:bookmark entry}\r"
    ],
    "\\section": [
      "tocsection=${1|true,false,hidden\r|}",
      "slide",
      "template=${1:template name}\r"
    ],
    "\\tableofcontents": [
      "type=${1|0,1\r|}",
      "content=${1|all,sections,currentsection,future,futuresections\r|}"
    ],
    "\\twocolumn": [
      "lineheight=",
      "lineprop={${1:pstricks options}}\r",
      "lfrheight=",
      "lfrprop={${1:pstricks options}}\r",
      "rfrheight=",
      "rfrprop={${1:pstricks options}}\r",
      "lcolwidth=",
      "rcolwidth=",
      "frsep=",
      "colsep=",
      "topsep=",
      "bottomsep=",
      "indent="
    ]
  },
  "args": [
    "\\documentclass/powerdot#c"
  ]
}