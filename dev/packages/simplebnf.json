{
  "deps": [
    {
      "name": "tabularray"
    },
    {
      "name": "mathtools"
    }
  ],
  "macros": [
    {
      "name": "SetBNFLayout",
      "arg": {
        "format": "{}",
        "snippet": "{${1:tblr options}}"
      }
    },
    {
      "name": "SetBNFConfig",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\begin{bnf},\\SetBNFConfig"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [
    {
      "name": "bnf"
    },
    {
      "name": "bnf",
      "arg": {
        "format": "[]",
        "snippet": "[${1:tblr options}]"
      }
    },
    {
      "name": "bnf",
      "arg": {
        "format": "()",
        "snippet": "(${1:options%keyvals})"
      }
    },
    {
      "name": "bnf",
      "arg": {
        "format": "()[]",
        "snippet": "(${2:options%keyvals})[${1:tblr options}]"
      }
    }
  ],
  "keys": {
    "\\begin{bnf},\\SetBNFConfig": [
      "prod-delim={${1:characters}}\r",
      "new-line-delim={${1:characters}}\r",
      "single-line-delim={${1:characters}}\r",
      "comment={${1:characters}}\r",
      "relation={${1:characters}}\r",
      "relation-sym-map={${1:specification}}\r",
      "or-sym=${1:symbol}\r",
      "prod-sep=",
      "row-sep="
    ]
  },
  "args": []
}