{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "xstring"
    },
    {
      "name": "iftex"
    },
    {
      "name": "hyperxmp"
    }
  ],
  "macros": [
    {
      "name": "pdfpcsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\pdfpcsetup,\\usepackage/pdfpc#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pdfpcnote",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pdfpcsetup,\\usepackage/pdfpc#c": [
      "duration=\r",
      "starttime=\r",
      "endtime=\r",
      "enduserslide=\r",
      "lastminutes=\r",
      "hidenotes",
      "overridenote",
      "notesposition=${1|right,left,top,bottom,none\r|}",
      "disablemarkdown",
      "defaulttransition=\r"
    ]
  },
  "args": [
    "\\pdfpcsetup,\\usepackage/pdfpc#c"
  ]
}