{
  "deps": [
    {
      "name": "gtl"
    }
  ],
  "macros": [
    {
      "name": "unravel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:content}}"
      }
    },
    {
      "name": "unravel",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:keyvals}]{${1:content}}"
      }
    },
    {
      "name": "unravelsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\unravel,\\unravelsetup": [
      "explicit-prompt",
      "internal-debug",
      "machine",
      "max-action=${1:integer}",
      "max-output=${1:integer}",
      "max-input=${1:integer}",
      "mute",
      "number-steps",
      "online=${1:integer}",
      "prompt-input={${1:item1,item2,...}}",
      "trace-assigns",
      "trace-expansion",
      "trace-other",
      "welcome-message"
    ]
  },
  "args": []
}