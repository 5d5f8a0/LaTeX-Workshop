{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "eforms"
    }
  ],
  "macros": [
    {
      "name": "item",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "puUseMenus",
      "arg": {
        "format": "{}",
        "snippet": "{${1:menu array names}}"
      }
    },
    {
      "name": "popUpMenu(name)"
    },
    {
      "name": "puProcessMenu(name)"
    },
    {
      "name": "urlPath",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:cmd}}{${2:path%URL}}"
      }
    },
    {
      "name": "Esc"
    },
    {
      "name": "cs",
      "arg": {
        "format": "{}",
        "snippet": "{${1:csname}}"
      }
    },
    {
      "name": "itemindex",
      "unusual": true
    },
    {
      "name": "submenuLevel",
      "unusual": true
    },
    {
      "name": "puUseTheseMenus",
      "unusual": true
    },
    {
      "name": "puMenuCmds",
      "unusual": true
    },
    {
      "name": "puNone",
      "unusual": true
    },
    {
      "name": "puTracking",
      "unusual": true
    },
    {
      "name": "iftrackingPU",
      "unusual": true
    },
    {
      "name": "trackingPUfalse",
      "unusual": true
    },
    {
      "name": "trackingPUtrue",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "popupmenu",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%text}}"
      }
    },
    {
      "name": "submenu",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/popupmenu#c": [
      "tracking\r",
      "!tracking\r"
    ],
    "\\item,\\begin{submenu}": [
      "title=${1:string}\r",
      "marked"
    ],
    "\\item": [
      "enabled",
      "return=${1:string}\r"
    ]
  },
  "args": [
    "\\usepackage/popupmenu#c"
  ]
}