{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "ulem"
    },
    {
      "name": "underscore"
    }
  ],
  "macros": [
    {
      "name": "gap",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "gap",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:style%keyvals}]{${1:text}}"
      }
    },
    {
      "name": "gap*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "gap*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:style%keyvals}]{${1:text}}"
      }
    },
    {
      "name": "TeacherModeOn",
      "unusual": true
    },
    {
      "name": "TeacherModeOff",
      "unusual": true
    },
    {
      "name": "dashundergapssetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\dashundergapssetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "thegapnumber",
      "unusual": true
    },
    {
      "name": "thetotalgapnumber",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\gap,\\gap*": [
      "u\r",
      "d\r",
      "w\r",
      "b\r",
      "-\r",
      ".\r"
    ],
    "\\dashundergapssetup": [
      "teacher-mode",
      "gap-mode",
      "teachermode",
      "gap-format=${1|underline,double-underline,dash,dot,wave,blank\r|}",
      "gap-format-adjust",
      "teacher-gap-format=${1|underline,double-underline,dash,dot,wave,blank\r|}",
      "gap-font=\r",
      "dash\r",
      "dot\r",
      "gap-numbers",
      "gap-number-format=\r",
      "numbers",
      "display-total-gaps",
      "displaynbgaps",
      "gap-widen",
      "gap-extend-minimum=",
      "gap-extend-percent=\r",
      "widen"
    ]
  },
  "args": []
}