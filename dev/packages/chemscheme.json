{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "psfrag"
    },
    {
      "name": "floatrow"
    },
    {
      "name": "chemcompounds"
    },
    {
      "name": "bpchem",
      "if": "bpchem"
    },
    {
      "name": "bpchem",
      "if": "tracking=bpchem"
    }
  ],
  "macros": [
    {
      "name": "schemerefsub",
      "if": "bpchem",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:label%keyvals}}{${2:sublabel}}"
      }
    },
    {
      "name": "schemerefsub",
      "if": "bpchem",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:marker}]{${1:label%keyvals}}{${2:sublabel}}"
      }
    },
    {
      "name": "chemschemerefsub",
      "if": "bpchem",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:marker}}{${2:label%keyvals}}{${3:sublabel}}"
      }
    },
    {
      "name": "schemerefsub",
      "if": "tracking=bpchem",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:label}}{${2:sublabel}}"
      }
    },
    {
      "name": "schemerefsub",
      "if": "tracking=bpchem",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:marker}]{${1:label}}{${2:sublabel}}"
      }
    },
    {
      "name": "chemschemerefsub",
      "if": "tracking=bpchem",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:marker}}{${2:label}}{${3:sublabel}}"
      }
    },
    {
      "name": "schemename",
      "unusual": true
    },
    {
      "name": "listofschemes"
    },
    {
      "name": "listschemename",
      "unusual": true
    },
    {
      "name": "schemeref",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label%keyvals}}"
      }
    },
    {
      "name": "schemeref",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:marker}]{${1:label%keyvals}}"
      }
    },
    {
      "name": "chemschemeref",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:marker}}{${2:label%keyvals}}"
      }
    },
    {
      "name": "schemerefmarker",
      "unusual": true
    },
    {
      "name": "schemerefformat",
      "unusual": true
    },
    {
      "name": "floatcontentscenter"
    },
    {
      "name": "floatcontentscentre",
      "unusual": true
    },
    {
      "name": "floatcontentsleft"
    },
    {
      "name": "floatcontentsright"
    }
  ],
  "envs": [
    {
      "name": "scheme"
    },
    {
      "name": "scheme",
      "arg": {
        "format": "[]",
        "snippet": "[${1:placement}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/chemscheme#c": [
      "chemcompounds\r",
      "bpchem\r",
      "floatrow\r",
      "tracking=${1|bpchem,chemcompounds\r|}",
      "floats=${1|float,floatrow,memoir\r|}"
    ],
    "\\schemeref#c,\\chemschemeref#c,\\schemerefsub#c,\\chemschemerefsub#c": [
      "%bpchemlabel\r"
    ],
    "\\schemeref#c,\\chemschemeref#c": [
      "%compound\r"
    ]
  },
  "args": [
    "\\usepackage/chemscheme#c"
  ]
}