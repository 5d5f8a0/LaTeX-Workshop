{
  "deps": [
    {
      "name": "ifthen"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarycalc"
    }
  ],
  "macros": [
    {
      "name": "bcbar",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "bcbar",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:number}}"
      }
    },
    {
      "name": "bcskip",
      "arg": {
        "format": "{}",
        "snippet": "{${1:length}}"
      }
    },
    {
      "name": "bcxlabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "bclabel",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "bcfontstyle",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "bchart",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{bchart}": [
      "max=${1:number}\r",
      "min=${1:number}\r",
      "step=\r",
      "steps=\r",
      "plain\r",
      "unit=\r",
      "width=",
      "scale=\r"
    ],
    "\\bcbar": [
      "text=\r",
      "color=",
      "plain\r",
      "value=\r",
      "label=\r"
    ]
  },
  "args": []
}