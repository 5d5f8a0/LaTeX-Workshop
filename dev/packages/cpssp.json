{
  "deps": [
    {
      "name": "ifthen"
    },
    {
      "name": "calc"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarypositioning"
    },
    {
      "name": "tikzlibrarydecorations.pathmorphing"
    }
  ],
  "macros": [
    {
      "name": "cpsspformat",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/cpssp#c,\\cpsspformat,\\cpsspinput,\\begin{cpsspimage}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cpsspinput",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "cpsspinput",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}",
        "keys": [
          "\\usepackage/cpssp#c,\\cpsspformat,\\cpsspinput,\\begin{cpsspimage}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cpsspGap",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspBridge",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspCoil",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspSheet",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspSheetT",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspThreeTenHelix",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspAlphaHelix",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspPiHelix",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspBend",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspTurn",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x1}}{${3:x2}}"
      }
    },
    {
      "name": "cpsspLabel",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:y}}{${2:text}}"
      }
    },
    {
      "name": "cpsspStartRes",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x}}{${3:text}}"
      }
    },
    {
      "name": "cpsspEndRes",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:y}}{${2:x}}{${3:text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "cpsspimage",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "cpsspimage",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:file}}",
        "keys": [
          "\\usepackage/cpssp#c,\\cpsspformat,\\cpsspinput,\\begin{cpsspimage}"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\usepackage/cpssp#c,\\cpsspformat,\\cpsspinput,\\begin{cpsspimage}": [
      "style=${1|small,large,graylines,pdb|}",
      "labels",
      "numbers",
      "labelformat=${1:font commands}",
      "numberformat=${1:font commands}",
      "threehelixname=${1:name}",
      "pihelixname=${1:name}"
    ]
  },
  "args": [
    "\\usepackage/cpssp#c,\\cpsspformat,\\cpsspinput,\\begin{cpsspimage}"
  ]
}