{
  "deps": [
    {
      "name": "ifluatex"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "inputenc"
    },
    {
      "name": "luatexbase"
    },
    {
      "name": "ucs",
      "if": "utf8x"
    }
  ],
  "macros": [
    {
      "name": "InputNonUtfFile",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "InputUtfFile",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/luainputenc#c": [
      "utf8\r",
      "utf8x\r",
      "unactivate\r"
    ]
  },
  "args": [
    "\\usepackage/luainputenc#c"
  ]
}