{
  "deps": [],
  "macros": [
    {
      "name": "QueryWorkingDirectory",
      "arg": {
        "format": "{}",
        "snippet": "{${1:result cmd%cmd}}"
      }
    },
    {
      "name": "QueryFiles",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:spec}}{${2:function}}"
      }
    },
    {
      "name": "QueryFiles",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:spec}}{${2:function}}"
      }
    },
    {
      "name": "QueryFilesTF",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:spec}}{${2:function}}{${3:pre code}}{${4:empty list code}}"
      }
    },
    {
      "name": "QueryFilesTF",
      "arg": {
        "format": "[]{}{}{}{}",
        "snippet": "[${5:options%keyvals}]{${1:spec}}{${2:function}}{${3:pre code}}{${4:empty list code}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\QueryFiles,\\QueryFilesTF": [
      "recursive\r",
      "ignore-case\r",
      "reverse\r",
      "pattern\r",
      "sort=${1|date,name\r|}",
      "type=${1|d,f\r|}",
      "exclude=${1:Lua pattern}\r"
    ]
  },
  "args": []
}