{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tkz-euclide"
    },
    {
      "name": "simplekv"
    },
    {
      "name": "listofitems"
    },
    {
      "name": "ifthen"
    }
  ],
  "macros": [
    {
      "name": "roueQuestions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:liste}}"
      }
    },
    {
      "name": "roueQuestions",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:liste}}"
      }
    },
    {
      "name": "rouesQuestions",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:listeQuestions}}{${2:listeRéponses}}"
      }
    },
    {
      "name": "rouesQuestions",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:listeQuestions}}{${2:listeRéponses}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\roueQuestions,\\rouesQuestions": [
      "Titre={${1:texte}}\r",
      "Consigne={${1:texte}}\r",
      "LineWidth=",
      "RayonCentral=${1:nombre}\r",
      "RayonTotal=${1:nombre}\r",
      "BordureCouleur=",
      "CasesCouleur=",
      "CentreCouleur=",
      "Image=${1:imagefile}\r",
      "ImageOpacite=${1:factor}\r",
      "Decoupe",
      "DecoupeCouleur=",
      "Pliage",
      "PliageCouleur=",
      "Solution"
    ],
    "\\rouesQuestions": [
      "Conseil={${1:texte}}\r",
      "EspaceRoues="
    ]
  },
  "args": []
}