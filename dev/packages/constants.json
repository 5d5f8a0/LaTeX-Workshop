{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "C"
    },
    {
      "name": "C",
      "arg": {
        "format": "[]",
        "snippet": "[${1:family}]"
      }
    },
    {
      "name": "Cl",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label%specialDef}}"
      }
    },
    {
      "name": "Cl",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:family}]{${1:label}}"
      }
    },
    {
      "name": "Cr",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label%keyvals}}"
      }
    },
    {
      "name": "pagerefconstant",
      "arg": {
        "format": "{}",
        "snippet": "{${1:label%keyvals}}"
      }
    },
    {
      "name": "resetconstant"
    },
    {
      "name": "resetconstant",
      "arg": {
        "format": "[]",
        "snippet": "[${1:family}]"
      }
    },
    {
      "name": "newconstantfamily",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:family name}}{${2:options%keyvals}}",
        "keys": [
          "\\newconstantfamily,\\renewconstantfamily"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "renewconstantfamily",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:family name}}{${2:options%keyvals}}",
        "keys": [
          "\\newconstantfamily,\\renewconstantfamily"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "newlabelconstant",
      "unusual": true,
      "arg": {
        "format": "{}{{}{}{}}",
        "snippet": "{${1:label}}{{${2:number}}{${3:page}}{${4:family}}}"
      }
    },
    {
      "name": "refconstant",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "familyconstant",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "counterconstant",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:label}}"
      }
    },
    {
      "name": "refstepcounterconstant",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:counter}}"
      }
    },
    {
      "name": "labelconstant",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:label}}{${2:family}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\Cr#c,\\pagerefconstant#c": [
      "%constantslabel"
    ],
    "\\newconstantfamily,\\renewconstantfamily": [
      "format=${1:font command}",
      "symbol=${1:symbol}",
      "reset=${1:counter}"
    ]
  },
  "args": []
}