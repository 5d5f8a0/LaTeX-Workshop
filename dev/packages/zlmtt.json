      }
    },
    {
      "name": "proptt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "lctt",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/zlmtt#c": [
      "light\r",
      "l\r",
      "lightcondensed\r",
      "lc\r",
      "med\r",
      "m\r",
      "proportional\r",
      "p\r",
      "scale=${1:factor}\r",
