{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "calc"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "array"
    },
    {
      "name": "multirow"
    },
    {
      "name": "pifont"
    }
  ],
  "macros": [
    {
      "name": "AQmessage",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "AQms(height,depth)",
      "unusual": true
    },
    {
      "name": "InputQuestionList",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:filepath%file}}{${2:prefix}}{${3:num1,num2,...}}"
      }
    },
    {
      "name": "AQannexe",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:start}}{${2:end}}{${3:number of props}}"
      }
    },
    {
      "name": "AQannexe",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:start}}{${2:end}}{${3:number of props}}"
      }
    },
    {
      "name": "AQpoints",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "dingsquare"
    },
    {
      "name": "dingchecksquare"
    },
    {
      "name": "aqlabelforquest",
      "unusual": true
    },
    {
      "name": "aqlabelforrep",
      "unusual": true
    },
    {
      "name": "aqfoottext",
      "unusual": true
    },
    {
      "name": "nextrandom",
      "unusual": true
    },
    {
      "name": "setrannum",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:register}}{${2:min}}{${3:max}}"
      }
    },
    {
      "name": "points",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:points}}"
      }
    },
    {
      "name": "aqheightadvance",
      "unusual": true
    },
    {
      "name": "aqdepthadvance",
      "unusual": true
    },
    {
      "name": "aqpretxt",
      "unusual": true
    },
    {
      "name": "aqpretxtVF",
      "unusual": true
    },
    {
      "name": "aqtextfortrue",
      "unusual": true
    },
    {
      "name": "aqtextforfalse",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "alterqcm"
    },
    {
      "name": "alterqcm",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/alterqcm#c": [
      "french",
      "english",
      "german",
      "greek",
      "russian",
      "italian",
      "chinese",
      "unknown"
    ],
    "\\begin{alterqcm}": [
      "lq=",
      "pq=",
      "bonus=",
      "malus=",
      "numbreak=",
      "points=",
      "symb=",
      "corsymb=",
      "numstyle=",
      "propstyle=",
      "size=${1:fontsize cmd}",
      "afterpreskip=",
      "long",
      "sep",
      "pre",
      "VF",
      "numprop",
      "num",
      "nosquare",
      "title",
      "correction",
      "alea",
      "tone=${1:text}",
      "ttwo=${1:text}",
      "language=${1|english,german,greek,italian,russian,chinese,unknown|}",
      "transparent"
    ],
    "\\AQquestion": [
      "pq=",
      "br="
    ],
    "\\AQannexe": [
      "VF",
      "propstyle="
    ]
  },
  "args": [
    "\\usepackage/alterqcm#c"
  ]
}