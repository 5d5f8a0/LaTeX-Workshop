{
  "deps": [
    {
      "name": "amsmath"
    },
    {
      "name": "calc"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "lcg"
    },
    {
      "name": "stackengine"
    }
  ],
  "macros": [
    {
      "name": "hat",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:type%keyvals}]{${1:arg}}"
      }
    },
    {
      "name": "hatwidth",
      "unusual": true
    },
    {
      "name": "hshif",
      "unusual": true
    },
    {
      "name": "vshif",
      "unusual": true
    },
    {
      "name": "hatused",
      "unusual": true
    },
    {
      "name": "gethat",
      "unusual": true
    },
    {
      "name": "hatn",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg}}{${2:type%keyvals}}"
      }
    },
    {
      "name": "hatnoptions",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:arg}}{${2:type%keyvals}}{${3:hshift%l}}{${4:vshift%l}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\hat,\\usepackage/realhats#c,\\hatn,\\hatnoptions": [
      "beret\r",
      "santa\r",
      "sombrero\r",
      "witch\r",
      "tophat\r",
      "ash\r",
      "fez\r",
      "cowboy\r",
      "crown\r",
      "dunce\r",
      "policeman\r",
      "scottish\r",
      "birthday\r",
      "mortarboard\r",
      "tile-white\r",
      "tile-gray\r",
      "tile-light-blue\r",
      "tile-blue\r"
    ]
  },
  "args": [
    "\\hat,\\usepackage/realhats#c,\\hatn,\\hatnoptions"
  ]
}