{
  "deps": [
    {
      "name": "iexec"
    },
    {
      "name": "xkeyval"
    },
    {
      "name": "anyfontsize"
    },
    {
      "name": "tikz"
    },
    {
      "name": "microtype"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "calc"
    },
    {
      "name": "setspace"
    },
    {
      "name": "geometry"
    },
    {
      "name": "textpos"
    }
  ],
  "macros": [
    {
      "name": "putSpine",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "putSpine",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:imagefile}]{${1:text}}"
      }
    },
    {
      "name": "putPicture",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "putVolume",
      "arg": {
        "format": "{}",
        "snippet": "{${1:number}}"
      }
    },
    {
      "name": "putPrice",
      "arg": {
        "format": "{}",
        "snippet": "{${1:price}}"
      }
    },
    {
      "name": "putBack",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "putTitle",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "putAuthor",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "putTLDR",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "putVersion",
      "arg": {
        "format": "{}",
        "snippet": "{${1:version}}"
      }
    },
    {
      "name": "putCopyright",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:year}}{${2:name}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/kdpcover#c": [
      "6x9\r",
      "7x10\r",
      "8x10\r",
      "pages=${1:integer}\r",
      "barless\r",
      "pdf=${1:PDF file}\r"
    ]
  },
  "args": [
    "\\documentclass/kdpcover#c"
  ]
}