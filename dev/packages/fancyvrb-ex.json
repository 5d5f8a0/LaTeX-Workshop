{
  "deps": [
    {
      "name": "fancyvrb"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "hbaw"
    },
    {
      "name": "pstricks",
      "if": "pstricks"
    }
  ],
  "macros": [
    {
      "name": "showgrid",
      "if": "pstricks"
    }
  ],
  "envs": [
    {
      "name": "PCenterExample",
      "if": "pstricks",
      "arg": {
        "format": "()()",
        "snippet": "(${1:x1},${2:y1})(${3:x2},${4:y2})"
      }
    },
    {
      "name": "PCenterExample",
      "if": "pstricks",
      "arg": {
        "format": "[]()()",
        "snippet": "[${1:options%keyvals}](${2:x1},${3:y1})(${4:x2},${5:y2})"
      }
    },
    {
      "name": "PSideBySideExample",
      "if": "pstricks",
      "arg": {
        "format": "()()",
        "snippet": "(${1:x1},${2:y1})(${3:x2},${4:y2})"
      }
    },
    {
      "name": "PSideBySideExample",
      "if": "pstricks",
      "arg": {
        "format": "[]()()",
        "snippet": "[${1:options%keyvals}](${2:x1},${3:y1})(${4:x2},${5:y2})"
      }
    },
    {
      "name": "Example"
    },
    {
      "name": "Example",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "CenterExample"
    },
    {
      "name": "CenterExample",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "SideBySideExample"
    },
    {
      "name": "SideBySideExample",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/fancyvrb-ex#c": [
      "pstricks\r",
      "color\r"
    ],
    "\\begin{Example},\\begin{CenterExample},\\begin{SideBySideExample},\\begin{PCenterExample},\\begin{PSideBySideExample}": [
      "commentchar=${1:single char}\r",
      "gobble=${1:integer}\r",
      "formatcom=${1:commands}\r",
      "formatcom*=${1:commands}\r",
      "fontfamily=${1:family}\r",
      "fontsize=${1:size macro}\r",
      "fontshape=${1:shape}\r",
      "fontseries=${1:series}\r",
      "frame=${1|none,leftline,topline,bottomline,lines,single\r|}",
      "framerule=",
      "framesep=",
      "rulecolor=${1:color cmd}\r",
      "fillcolor=${1:color cmd}\r",
      "label=${1:label text}\r",
      "labelposition=${1|none,topline,bottomline,all\r|}",
      "numbers=${1|none,left,right\r|}",
      "numbersep=",
      "firstnumber=${1:auto|last|<integer>}\r",
      "stepnumber=${1:integer}\r",
      "numberblanklines",
      "firstline=${1:integer}\r",
      "lastline=${1:integer}\r",
      "showspaces",
      "showtabs",
      "obeytabs",
      "tabsize=${1:integer}\r",
      "baselinestretch=${1:factor}\r",
      "commandchars=${1:three chars}\r",
      "xleftmargin=",
      "xrightmargin=",
      "resetmargins",
      "hfuzz=",
      "samepage",
      "codes={${1:code}}\r",
      "codes*={${1:code}}\r",
      "defineactive={${1:code}}\r",
      "defineactive*={${1:code}}\r",
      "reflabel=",
      "fileext=${1:extension}\r",
      "vspace=",
      "listparameters={${1:code}}\r"
    ]
  },
  "args": [
    "\\usepackage/fancyvrb-ex#c"
  ]
}