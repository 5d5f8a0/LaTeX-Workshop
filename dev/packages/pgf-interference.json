{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "pgfinterferencepattern",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\pgfinterferencepattern,\\pgfinterferenceoptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfinterferenceoptions",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\pgfinterferencepattern,\\pgfinterferenceoptions"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pgf-interference#c": [
      "draft\r"
    ],
    "\\pgfinterferencepattern,\\pgfinterferenceoptions": [
      "wavelength=${1:number}\r",
      "intensity=${1:number}\r",
      "slits=${1:integer}\r",
      "slit-distance=${1:number}\r",
      "slit-width=${1:number}\r",
      "screen-distance=${1:number}\r",
      "screen-width=${1:number}\r",
      "screen-height=${1:number}\r",
      "scale=${1:factor}\r",
      "screen-color=",
      "ruler=${1|above,below,screen,none\r|}"
    ]
  },
  "args": [
    "\\usepackage/pgf-interference#c"
  ]
}