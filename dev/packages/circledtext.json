{
  "deps": [
    {
      "name": "xtemplate"
    },
    {
      "name": "l3keys2e"
    },
    {
      "name": "l3draw"
    }
  ],
  "macros": [
    {
      "name": "circledtext",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "circledtext",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:text}}"
      }
    },
    {
      "name": "circledtextset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\circledtext,\\circledtextset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "charboxwd",
      "unusual": true
    },
    {
      "name": "charboxht",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\circledtext,\\circledtextset": [
      "basechar=${1:base char}\r",
      "charf=${1:format macros}\r",
      "boxtype=${1|o,o+,ox,ox+,x+,O,O+,OX,OX+,X+,oo,Oo,OO\r|}",
      "charshrink=${1:factor}\r",
      "resize=${1|none,real,base\r|}",
      "xscale=${1:factor}\r",
      "yscale=${1:factor}\r",
      "width=",
      "height=",
      "boxlinewidth=",
      "crosslinewidth=",
      "crosscolorratio=${1:percent}\r",
      "boxcolor=",
      "charcolor=",
      "boxfill=",
      "charstroke=${1|none,solid,dashed,invisible\r|}",
      "dashpattern={${1:len1,len2,...}}\r"
    ]
  },
  "args": []
}