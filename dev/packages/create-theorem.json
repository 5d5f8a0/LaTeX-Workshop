{
  "deps": [
    {
      "name": "crefthe"
    },
    {
      "name": "amsfonts"
    }
  ],
  "macros": [
    {
      "name": "NameTheorem",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:keyvals}}"
      }
    },
    {
      "name": "CreateTheorem",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:keyvals}}"
      }
    },
    {
      "name": "SetTheorem",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:keyvals}}"
      }
    },
    {
      "name": "SetTheoremBinding",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:envname to bind with}}"
      }
    },
    {
      "name": "NameTheorems",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:language}}{${2:keyvals}}"
      }
    },
    {
      "name": "CreateTheoremAddLanguage",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:language}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/create-theorem#c": [
      "name as context\r",
      "name as is\r",
      "name in link\r",
      "no preset names\r"
    ],
    "\\NameTheorem#c": [
      "heading=${1:string}\r",
      "heading={${1:lang1=string1,lang2=string2,...}}\r",
      "heading style=${1:font commands}\r",
      "crefname={${1:singular name}}{${2:plural name}}\r",
      "crefname={${1:lang1={name1}{pluralname1},...}}\r",
      "crefname style=${1:font commands}\r",
      "Crefname={${1:singular name}}{${2:plural name}}\r",
      "Crefname={${1:lang1={name1}{pluralname1},...}}\r",
      "Crefname style=${1:font commands}\r",
      "numbering style=${1:font commands}\r",
      "use name={${1:envname1;envname2;...}}\r"
    ],
    "\\CreateTheorem#c": [
      "name={${1:\\NameTheorem keyvals}}\r",
      "use name={${1:envname1;envname2;...}}\r",
      "style=${1:theorem style}\r",
      "qed=${1:symbol}\r",
      "parent counter=${1:counter}\r",
      "shared counter=${1:counter}\r",
      "numberless\r",
      "create starred version\r",
      "copy existed=${1:envname}\r"
    ],
    "\\SetTheorem#c": [
      "name={${1:\\NameTheorem keyvals}}\r",
      "qed=${1:symbol}\r",
      "parent counter=${1:counter}\r",
      "shared counter=${1:counter}\r"
    ],
    "\\NameTheorems#c": [
      "heading={${1:envname}=${2:string}}\r",
      "crefname={${1:envname}={${2:singular}}{${3:plural}}}\r",
      "Crefname={${1:envname}={${2:singular}}{${3:plural}}}\r"
    ]
  },
  "args": [
    "\\usepackage/create-theorem#c"
  ]
}