{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "luatexja"
    },
    {
      "name": "luatexja-fontspec"
    }
  ],
  "macros": [
    {
      "name": "ebdefault",
      "if": "deluxe",
      "unusual": true
    },
    {
      "name": "ebseries",
      "if": "deluxe"
    },
    {
      "name": "gtebfamily",
      "if": "deluxe"
    },
    {
      "name": "ltdefault",
      "if": "deluxe",
      "unusual": true
    },
    {
      "name": "ltseries",
      "if": "deluxe"
    },
    {
      "name": "mgfamily",
      "if": "deluxe"
    },
    {
      "name": "texteb",
      "if": "deluxe",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "textlt",
      "if": "deluxe",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "textmg",
      "if": "deluxe",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "rubyfamily",
      "if": "expert"
    },
    {
      "name": "ltjnewpreset",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name%specialDef}}{${2:specification%keyvals}}"
      }
    },
    {
      "name": "ltjnewpreset*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name%specialDef}}{${2:specification%keyvals}}"
      }
    },
    {
      "name": "ltjapplypreset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%keyvals}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/luatexja-preset#c": [
      "fontspec\r",
      "nfssonly\r",
      "nodeluxe\r",
      "deluxe\r",
      "expert\r",
      "bold\r",
      "jis90\r",
      "jis2004\r",
      "jfm_yoko=${1:jfm}\r",
      "jfm_tate=${1:jfm}\r",
      "jis\r",
      "kozuka-pro\r",
      "kozuka-pr6\r",
      "kozuka-pr6n\r",
      "hiragino-pro\r",
      "hiragino-pron\r",
      "bizud\r",
      "morisawa-pro\r",
      "morisawa-pr6n\r",
      "yu-win\r",
      "yu-win10\r",
      "yu-osx\r",
      "moga-mobo\r",
      "moga-mobo-ex\r",
      "ume\r",
      "sourcehan\r",
      "sourcehan-jp\r",
      "noto-otc\r",
      "noto-otf\r",
      "noto-jp\r",
      "haranoaji\r",
      "noembed\r",
      "ipa\r",
      "ipaex\r",
      "ms\r",
      "ipa-hg\r",
      "ipaex-hg\r",
      "ms-hg\r",
      "match\r",
      "scale=${1:factor}\r",
      "tuenc\r",
      "euenc\r",
      "math\r",
      "no-math\r",
      "config\r",
      "no-config\r",
      "quiet\r",
      "silent\r"
    ],
    "\\ltjnewpreset#c,\\ltjnewpreset*#c": [
      "mc-l=${1:font}\r",
      "mc-m=${1:font}\r",
      "mc-b=${1:font}\r",
      "mc-bx=${1:font}\r",
      "gt-m=${1:font}\r",
      "gt-b=${1:font}\r",
      "gt-bx=${1:font}\r",
      "gt-eb=${1:font}\r",
      "mg-m=${1:font}\r",
      "mc=${1:font}\r",
      "gt=${1:font}\r"
    ],
    "\\ltjnewpreset#c,\\ltjnewpreset*#c,\\ltjapplypreset#c": [
      "%luatexjapreset\r",
      "kozuka-pro\r",
      "kozuka-pr6\r",
      "kozuka-pr6n\r",
      "hiragino-pro\r",
      "hiragino-pron\r",
      "bizud\r",
      "morisawa-pro\r",
      "morisawa-pr6n\r",
      "yu-win\r",
      "yu-win10\r",
      "yu-osx\r",
      "moga-mobo\r",
      "moga-mobo-ex\r",
      "ume\r",
      "sourcehan\r",
      "sourcehan-jp\r",
      "noto-otc\r",
      "noto-otf\r",
      "haranoaji\r",
      "noembed\r",
      "ipa\r",
      "ipaex\r",
      "ms\r",
      "ipa-hg\r",
      "ipaex-hg\r",
      "ms-hg\r"
    ]
  },
  "args": [
    "\\usepackage/luatexja-preset#c"
  ]
}