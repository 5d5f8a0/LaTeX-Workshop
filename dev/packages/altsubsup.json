{
  "deps": [
    {
      "name": "amstext"
    },
    {
      "name": "spbmark",
      "if": "spbmark"
    }
  ],
  "macros": [
    {
      "name": "SetAltSubscriptCommand",
      "arg": {
        "format": "{}",
        "snippet": "{${1:command}}"
      }
    },
    {
      "name": "SetAltSuperscriptCommand",
      "arg": {
        "format": "{}",
        "snippet": "{${1:command}}"
      }
    },
    {
      "name": "SetAltSubSupCommands",
      "arg": {
        "format": "{}",
        "snippet": "{${1:command}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/altsubsup#c": [
      "subscript\r",
      "superscript\r",
      "both\r",
      "spbmark\r"
    ]
  },
  "args": [
    "\\usepackage/altsubsup#c"
  ]
}