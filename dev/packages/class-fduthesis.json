    },
    {
      "name": "natbib"
    },
    {
      "name": "biblatex"
    },
    {
      "name": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "fdusetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}"
      }
    },
    {
      "name": "makecoveri"
    },
    {
      "name": "makecoverii"
    },
    {
      "name": "makecoveriii"
    },
    {
      "name": "newtheorem",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:envname}}{${2:title%plain}}"
      }
    },
    {
      "name": "newtheorem*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:envname}}{${2:title%plain}}"
      }
    },
    {
      "name": "newtheorem*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:envname}}{${2:title%plain}}"
      }
    },
    {
      "name": "DeclareCoverTemplate",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "theoremsymbol",
      "arg": {
        "format": "{}",
        "snippet": "{${1:symbol}}"
      }
    }
  ],
  "envs": [
    {
      "name": "abstract*"
    },
    {
      "name": "notation"
    },
    {
      "name": "notation",
      "arg": {
        "format": "[]",
        "snippet": "[${1:cols}]"
      }
    },
    {
      "name": "acknowledgements"
    },
    {
      "name": "axiom"
    },
    {
      "name": "axiom",
      "arg": {
        "format": "[]",
        "snippet": "[${1:subheading%text}]"
      }
    },
    {
      "name": "corollary"
    },
    {
      "name": "corollary",
      "arg": {
        "format": "[]",
        "snippet": "[${1:subheading%text}]"
      }
    },
    {
      "name": "definition"
    },
    {
      "name": "definition",
      "arg": {
        "format": "[]",
        "snippet": "[${1:subheading%text}]"
      }
    },
    {
      "name": "example"
    },
    {
      "name": "example",
      "arg": {
        "format": "[]",
        "snippet": "[${1:subheading%text}]"
      }
    },
    {
      "name": "lemma"
    },
    {
      "name": "lemma",
      "arg": {
        "format": "[]",
        "snippet": "[${1:subheading%text}]"
      }
    },
    {
      "name": "proof"
    },
    {
      "name": "proof",
      "arg": {
        "format": "[]",
        "snippet": "[${1:subheading%text}]"
      }
    },
    {
      "name": "theorem"
    },
    {
      "name": "theorem",
      "arg": {
        "format": "[]",
        "snippet": "[${1:subheading%text}]"
      }
    }
  ],
  "keys": {
    "\\documentclass/fduthesis#c": [
      "type=${1|doctor,master,bachelor\r|}",
      "oneside\r",
      "twoside\r",
      "draft",
      "config=${1:file}\r"
    ],
    "\\fdusetup#c": [
      "style={${1:options}}\r",
      "style/font=${1|garamond,libertinus,lm,palatino,times,times*,none\r|}",
      "style/cjk-font=${1|adobe,fandol,founder,mac,sinotype,sourcehan,windows,none\r|}",
      "style/font-size=${1|-4,5\r|}",
      "style/fullwidth-stop=${1|catcode,mapping,false\r|}",
      "style/footnote-style=${1|plain,libertinus,libertinus*,libertinus-sans,pifont,pifont*,pifont-sans,pifont-sans*,xits,xits-sans,xits-sans*\r|}",
      "style/hyperlink=${1|border,color,none\r|}",
      "style/hyperlink-color=${1|default,classic,material,graylevel,prl\r|}",
      "style/bib-backend=${1|bibtex,biblatex\r|}",
      "style/bib-style=${2|author-year,numerical,${1:other style}\r|}",
      "style/cite-style=${1:style}\r",
      "style/bib-resource={${1:bib files}}\r",
      "style/logo=${1:imagefile}\r",
      "style/logo-size=",
      "style/logo-size={${1:width,height}}\r",
      "style/auto-make-cover",
      "style/declaration-page=${1:PDF file}\r",
      "info={${1:options}}\r",
      "info/degree=${1|academic,professional\r|}",
      "info/title={${1:title in Chinese}}\r",
      "info/title*={${1:title in English}}\r",
      "info/author={${1:name in Chinese}}\r",
      "info/author*={${1:name in English}}\r",
      "info/supervisor=${1:name}\r",
      "info/department=${1:name}\r",
      "info/major=${1:name}\r",
      "info/student-id=${1:number}\r",
      "info/school-id=${1:number}\r",
      "info/date=${1:date}\r",
      "info/secret-level=${1|none,i,ii,iii\r|}",
      "info/secret-year=${1:year}\r",
      "info/instructors={${1:member1,member2,...}}\r",
      "info/keywords={${1:keywords in Chinese}}\r",
      "info/keywords*={${1:keywords in English}}\r",
      "info/clc={${1:classification codes}}\r",
      "info/jel={${1:classification codes}}\r",
      "theorem={${1:options}}\r",
      "theorem/style=${1|plain,margin,change,break,marginbreak,changebreak\r|}",
      "theorem/header-font=${1:font commands}\r",
      "theorem/body-font=${1:font commands}\r",
      "theorem/qed=${1:symbol}\r",
      "theorem/counter=${1:counter}\r"
    ],
    "\\newtheorem,\\newtheorem*": [
      "style=${1|plain,margin,change,break,marginbreak,changebreak\r|}",
      "header-font=${1:font commands}\r",
      "body-font=${1:font commands}\r",
      "qed=${1:symbol}\r",
      "counter=${1:counter}\r"
    ]
  },
  "args": [
    "\\documentclass/fduthesis#c"
  ]
}