{
  "deps": [
    {
      "name": "xtemplate"
    },
    {
      "name": "l3keys2e"
    },
    {
      "name": "class-ctexbook"
    },
    {
      "name": "xeCJK"
    },
    {
      "name": "amsmath"
    },
    {
      "name": "unicode-math"
    },
    {
      "name": "geometry"
    },
    {
      "name": "fancyhdr"
    },
    {
      "name": "footmisc"
    },
    {
      "name": "ntheorem"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "longtable"
    },
    {
      "name": "caption"
    },
    {
      "name": "xcolor"
    },
    {
