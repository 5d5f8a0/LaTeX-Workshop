{
  "deps": [
    {
      "name": "luatex"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "class-beamer"
    },
    {
      "name": "babel"
    },
    {
      "name": "fontspec"
    }
  ],
  "macros": [
    {
      "name": "localfootnote",
      "if": "layout=footnotes",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "mainfootnote",
      "if": "layout=footnotes",
      "arg": {
        "format": "{}",
        "snippet": "{${1:text}}"
      }
    },
    {
      "name": "localfootnotetext",
      "if": "layout=footnotes",
      "unusual": true
    },
    {
      "name": "mainfootnotetext",
      "if": "layout=footnotes",
      "unusual": true
    },
    {
      "name": "blacktriangleright",
      "detail": "▶ (\"wasysym\" command)",
      "doc": "\\blacktriangleright (fourier -mathabx), (large) right triangle, filled"
    },
    {
      "name": "blacktriangleleft",
      "detail": "◀ (\"wasysym\" command)",
      "doc": "\\blacktriangleleft (fourier -mathabx), (large) left triangle, filled"
    },
    {
      "name": "redefbeamertemplate",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:element}}{${2:option}}"
      }
    }
  ],
  "envs": [],
