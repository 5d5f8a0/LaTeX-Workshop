{
  "deps": [
    {
      "name": "upmethodology-p-common"
    },
    {
      "name": "upmethodology-extension"
    },
    {
      "name": "upmethodology-document"
    }
  ],
  "macros": [
    {
      "name": "setfrontcover",
      "arg": {
        "format": "{}",
        "snippet": "{${1:layout name%keyvals}}"
      }
    },
    {
      "name": "makefrontcover"
    },
    {
      "name": "setfrontillustration",
      "arg": {
        "format": "{}",
        "snippet": "{${1:imagefile}}"
      }
    },
    {
      "name": "setfrontillustration",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:scale}]{${1:imagefile}}"
      }
    },
    {
      "name": "clearfrontillustration"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/upmethodology-frontpage#c": [
      "french\r",
      "francais\r",
      "english\r"
    ],
    "\\setfrontcover#c": [
      "classic\r",
      "modern\r"
    ]
  },
  "args": [
    "\\usepackage/upmethodology-frontpage#c"
  ]
}