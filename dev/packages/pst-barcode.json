{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-xkey"
    },
    {
      "name": "marginnote"
    }
  ],
  "macros": [
    {
      "name": "psbarcode",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:text or filename%file}}{${2:PS options}}{${3:barcode type}}"
      }
    },
    {
      "name": "psbarcode",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:text or filename%file}}{${2:PS options}}{${3:barcode type}}"
      }
    },
    {
      "name": "QR",
      "arg": {
        "format": "{}",
        "snippet": "{${1:URL}}"
      }
    },
    {
      "name": "QR<label>",
      "arg": {
        "format": "{}",
        "snippet": "{${1:URL}}"
      }
    },
    {
      "name": "QR",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:scale}]{${1:URL}}"
      }
    },
    {
      "name": "QR",
      "arg": {
        "format": "[]<>{}",
        "snippet": "[${2:scale}]<${3:label}>{${1:URL}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psbarcode#c": [
      "fontstyle=${1:font commands}\r",
      "transx=${1:number}\r",
      "transy=${1:number}\r",
      "scalex=${1:factor}\r",
      "scaley=${1:factor}\r",
      "rotate=${1:degrees}\r",
      "file",
      "linecolor="
    ]
  },
  "args": []
}