{
  "deps": [],
  "macros": [
    {
      "name": "setupsc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\setupsc,\\usepackage/scontents#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "newenvsc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "newenvsc",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:envname}}[${2:initial keys%keyvals}]"
      }
    },
    {
      "name": "Scontents",
      "arg": {
        "format": "{}",
        "snippet": "{${1:argument}}"
      }
    },
    {
      "name": "Scontents",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:argument}}"
      }
    },
    {
      "name": "Scontents*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:argument}}"
      }
    },
    {
      "name": "Scontents*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:argument}}"
      }
    },
    {
      "name": "getstored",
      "arg": {
        "format": "{}",
        "snippet": "{${1:seq name}}"
      }
    },
    {
      "name": "getstored",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:index}]{${1:seq name}}"
      }
    },
    {
      "name": "foreachsc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:seq name}}"
      }
    },
    {
      "name": "foreachsc",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:seq name}}"
      }
    },
    {
      "name": "typestored",
      "arg": {
        "format": "{}",
        "snippet": "{${1:seq name}}"
      }
    },
    {
      "name": "typestored",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:seq name}}"
      }
    },
    {
      "name": "meaningsc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:seq name}}"
      }
    },
    {
      "name": "meaningsc",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:seq name}}"
      }
    },
    {
      "name": "countsc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:seq name}}"
      }
    },
    {
      "name": "cleanseqsc",
      "arg": {
        "format": "{}",
        "snippet": "{${1:seq name}}"
      }
    }
  ],
  "envs": [
    {
      "name": "scontents"
    },
    {
      "name": "scontents",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "verbatimsc"
    }
  ],
  "keys": {
    "\\setupsc,\\usepackage/scontents#c": [
      "store-env=${1:seq name}\r",
      "store-cmd=${1:seq name}\r",
      "print-env",
      "print-cmd",
      "print-all",
      "store-all=${1:seq name}\r",
      "overwrite",
      "width-tab=${1:integer}\r",
      "force-eol",
      "verb-font=${1:font family}\r"
    ],
    "\\begin{scontents}": [
      "store-env=${1:seq name}\r",
      "print-env",
      "write-env=${1:file.ext}\r",
      "write-out=${1:file.ext}\r",
      "overwrite",
      "force-eol"
    ],
    "\\Scontents": [
      "store-cmd=${1:seq name}\r",
      "print-cmd",
      "force-eol"
    ],
    "\\Scontents*": [
      "store-cmd=${1:seq name}\r",
      "print-cmd",
      "write-cmd=${1:file.ext}\r",
      "write-out=${1:file.ext}\r",
      "overwrite",
      "force-eol"
    ],
    "\\foreachsc": [
      "sep={${1:code}}\r",
      "step=${1:integer}\r",
      "start=${1:integer}\r",
      "stop=${1:integer}\r",
      "before={${1:code}}\r",
      "after={${1:code}}\r",
      "wrapper={${1:code}}\r"
    ],
    "\\typestored#c,\\meaningsc#c": [
      "width-tab=${1:integer}\r",
      "${1:index}\r"
    ],
    "\\mergesc": [
      "typestored\r",
      "meaningsc\r",
      "print-cmd",
      "write-out=${1:file.ext}\r",
      "width-tab=${1:integer}\r",
      "overwrite"
    ]
  },
  "args": [
    "\\setupsc,\\usepackage/scontents#c"
  ]
}