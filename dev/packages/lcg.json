{
  "deps": [
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "rand"
    },
    {
      "name": "reinitrand",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "chgrand",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\reinitrand,\\chgrand,\\usepackage/lcg#c": [
      "counter=${1:counter}\r",
      "first=${1:integer}\r",
      "last=${1:integer}\r",
      "seed=${1:integer}\r",
      "quiet\r"
    ]
  },
  "args": [
    "\\reinitrand,\\chgrand,\\usepackage/lcg#c"
  ]
}