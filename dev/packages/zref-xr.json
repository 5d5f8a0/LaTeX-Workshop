{
  "deps": [
    {
      "name": "zref-base"
    },
    {
      "name": "keyval"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "zexternaldocument",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "zexternaldocument",
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:prefix}]{${1:file}}[${3:url}]"
      }
    },
    {
      "name": "zexternaldocument*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "zexternaldocument*",
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:prefix}]{${1:file}}[${3:url}]"
      }
    },
    {
      "name": "zxrsetup",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\zxrsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\zxrsetup": [
      "ext=${1:extension}\r",
      "tozreflabel",
      "urluse",
      "verbose"
    ],
    "\\zref#c": [
      "url\r",
      "urluse\r",
      "externaldocument\r",
      "anchor\r",
      "theotype\r",
      "title\r"
    ]
  },
  "args": []
}