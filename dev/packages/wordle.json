{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "simplekv"
    },
    {
      "name": "xstring"
    }
  ],
  "macros": [],
  "envs": [
    {
      "name": "WordleGrid",
      "arg": {
        "format": "{}",
        "snippet": "{${1:word%text}}"
      }
    },
    {
      "name": "WordleGrid",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:keyvals}]{${1:word%text}}"
      }
    },
    {
      "name": "GrilleSutom",
      "arg": {
        "format": "{}",
        "snippet": "{${1:mot%text}}"
      }
    },
    {
      "name": "GrilleSutom",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:clés%keyvals}]{${1:mot%text}}"
      }
    }
  ],
  "keys": {
    "\\begin{WordleGrid}": [
      "Colors={${1:back,misplaced,good}}\r",
      "Rounded=${1:number}\r",
      "Unit=${1:number}\r",
      "Font=${1:font commands}\r",
      "ColorLetters=",
      "BorderColor=",
      "Letters",
      "Style=\r",
      "Thickness=${1:number}\r"
    ],
    "\\begin{GrilleSutom}": [
      "Couleurs={${1:fond,mal placée,bien placée}}\r",
      "Arrondi=${1:nombre}\r",
      "Unite=${1:nombre}\r",
      "Police=${1:font commands}\r",
      "CouleurLettres=",
      "CouleurBordure=",
      "Lettres",
      "Style=\r",
      "Epaisseur=${1:number}\r"
    ]
  },
  "args": []
}