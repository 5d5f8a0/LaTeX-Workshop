{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarycommutative-diagrams"
    }
  ],
  "macros": [],
  "envs": [
    {
      "name": "codi"
    },
    {
      "name": "codi",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{codi}#c": [
      "prompter\r",
      "expand=${1|none,once,full\r|}",
      "replace character=${1:character1} with ${2:character2}\r",
      "replace charcode=${1:charcode} with ${2:character}\r",
      "remove characters=${1:characters}\r",
      "remove character=${1:character}\r",
      "remove charcode=${1:charcode}\r",
      "overwrite=${1|false,alias,true\r|}",
      "every diagram/.append style={${1:TikZ options}}\r",
      "every layout/.append style={${1:TikZ options}}\r",
      "every object/.append style={${1:TikZ options}}\r",
      "every arrow/.append style={${1:TikZ options}}\r",
      "every label/.append style={${1:TikZ options}}\r",
      "tetragonal\r",
      "tetragonal=base ${1:<length>} height ${2:<length>}\r",
      "hexagonal\r",
      "hexagonal=${1:<direction>} side ${2:<length>} angle ${3:<angle>}\r",
      "square=",
      "golden=",
      "arrows/crossing over\r",
      "arrows/crossing over/clearance=",
      "arrows/crossing over/color=",
      "arrows/slide=",
      "arrows/shove=",
      "%color\r",
      "x=",
      "y=",
      "node distance="
    ]
  },
  "args": []
}