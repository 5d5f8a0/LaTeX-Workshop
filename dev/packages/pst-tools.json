{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psPrintValue",
      "arg": {
        "format": "{}",
        "snippet": "{${1:PS code}}"
      }
    },
    {
      "name": "psPrintValue",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:PS code}}",
        "keys": [
          "\\psPrintValue,\\psPrintValueNew"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "psPrintValueNew",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:PS code}}"
      }
    },
    {
      "name": "psPrintValueNew",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:PS code}}",
        "keys": [
          "\\psPrintValue,\\psPrintValueNew",
          "\\psPrintValueNew"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "psRegisterList",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:name}}{${2:value list}}"
      }
    },
    {
      "name": "randomi",
      "unusual": true
    },
    {
      "name": "nextrandom",
      "unusual": true
    },
    {
      "name": "setrannum",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:count register%cmd}}{${2:min}}{${3:max}}"
      }
    },
    {
      "name": "setrandim",
      "arg": {
        "format": "{}{}{}",
        "snippet": "{${1:dimen register%cmd}}{${2:min}}{${3:max}}"
      }
    },
    {
      "name": "pointless"
    },
    {
      "name": "PoinTless",
      "unusual": true
    },
    {
      "name": "ranval",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\psPrintValue,\\psPrintValueNew": [
      "printfont=${1:font name}",
      "postString=${1:string}",
      "trimSpaces",
      "fontscale=${1:scale in pts}",
      "valuewidth=${1:number}",
      "decimals=${1:number}",
      "xShift=${1:shift in pts}",
      "algebraic",
      "VarName=${1:string}",
      "comma"
    ],
    "\\psPrintValueNew": [
      "round",
      "science"
    ]
  },
  "args": []
}