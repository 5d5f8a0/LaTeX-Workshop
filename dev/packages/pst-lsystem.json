{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "pslsystem(x,y)"
    },
    {
      "name": "pslsystem",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:options%keyvals}](${2:x},${3:y})"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pslsystem#c": [
      "F=${1:Rule F → (F)}\r",
      "X=${1:Rule X → (F,X,Y)}\r",
      "Y=${1:Rule Y → (F,X,Y)}\r",
      "Start=${1:Startrule S → (F,X,Y)}\r",
      "Angle=${1:degrees}\r",
      "N=${1:number of recursive calls}\r",
      "Ftype=${1|1,2,3,4\r|}",
      "BaseLength=",
      "usecolor=${1|0,1,2,3,4\r|}",
      "linecolor="
    ]
  },
  "args": []
}