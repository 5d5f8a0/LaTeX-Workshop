      "forename=${1:name}\r",
      "surname=${1:name}\r",
      "addname=${1:additional name}\r",
      "note=${1:note}\r"
    ],
    "\\lem": [
      "wit=${1:list of witnesses}\r",
      "source=${1:list of sources}\r",
      "resp=${1:list of scholars}\r",
      "alt=${1:alternate lemma}\r",
      "sep=${1:separator}\r",
      "nosep",
      "nolem",
      "type=${1:type}\r",
      "num\r",
      "nonum\r",
      "pre=${1:words}\r",
      "post=${1:words}\r",
      "prewit=${1:words}\r",
      "postwit=${1:words}\r"
    ],
    "\\rdg": [
      "wit=${1:list of witnesses}\r",
      "source=${1:list of sources}\r",
      "resp=${1:list of scholars}\r",
      "alt=${1:alternate lemma}\r",
      "nordg",
      "type=${1:type}\r",
      "pre=${1:words}\r",
      "post=${1:words}\r",
      "prewit=${1:words}\r",
      "postwit=${1:words}\r",
      "subsep=${1:subseparator}\r",
      "nosubsep\r"
    ],
    "\\note,\\note*": [
      "pre=${1:words}\r",
      "post=${1:words}\r",
      "sep\r",
      "subsep\r"
    ],
    "\\SetCritSymbols": [
      "suppbegin=${1:symbol}\r",
      "suppend=${1:symbol}\r",
      "delbegin=${1:symbol}\r",
      "delend=${1:symbol}\r",
      "sicbegin=${1:symbol}\r",
      "sicend=${1:symbol}\r",
      "gapmark=${1:symbols}\r",
      "keepinapp"
    ],
    "\\gap": [
      "reason=${1:reason}\r",
      "unit=${1:unit}\r",
      "quantity=${1:n}\r",
      "extent=${1:description}\r"
    ],
    "\\begin{alignment},\\SetAlignment": [
      "tcols=${1:number}\r",
      "lcols=${1:number}\r",
      "texts=${1:semicolon-separated values}\r",
      "apparatus=${1:semicolon-separated values}\r",
      "paired",
      "lineation=${1|page,document\r|}",
      "flush",
      "segmentation=${1|auto,noauto\r|}"
    ],
    "\\SetHooks": [
      "appheight=",
      "fitalgorithm=${1|fontsize,hybrid,areasize,squeeze\r|}"
    ],
    "\\SetApparatus,\\DeclareApparatus": [
      "direction=${1|LR,RL\r|}",
      "sep=${1:separator}\r",
      "subsep=${1:subseparator}\r",
      "delim=${1:delimiter}\r",
      "bhook=${1:characters or commands}\r",
      "ehook=${1:characters}\r",
      "rule=${1:line definition}\r",
      "norule\r",
      "lang=${1:language}\r",
      "notelang=${1:language}\r",
      "maxentries=${1:integer}\r"
    ],
    "\\note": [
      "type=${1:type}\r",
      "sep=${1:separator}\r",
      "nosep",
      "lem=${1:lemma text}\r",
      "num\r",
      "nonum\r",
      "labelb=${1:label}\r",
      "labele=${1:label}\r"
    ],
    "\\begin{ekdverse}": [
      "width=",
      "type=${1:type}\r"
    ],
    "\\begin{ekdstanza}": [
      "type=${1:type}\r"
    ],
    "\\SetLineation": [
      "vlineation=${1|page,document\r|}",
      "vmodulo=${1:integer}\r",
      "vnumbrokenlines",
      "vmargin=${1|right,left\r|}",
      "continuousvnum\r"
    ],
    "\\ekddiv": [
      "type=${1:type}\r",
      "n=${1:value}\r",
      "head=${1:name}\r",
      "barehead=${1:name}\r",
      "depth=${1:n}\r",
      "toc=${1|book,part,chapter,section,subsection,subsubsection,paragraph,subparagraph\r|}",
      "mark=${1:signpost}\r"
    ],
    "\\SetTEIxmlExport": [
      "autopar"
    ]
  },
