{
  "deps": [
    {
      "name": "xtemplate"
    },
    {
      "name": "l3keys2e"
    },
    {
      "name": "fontawesome5"
    },
    {
      "name": "tcolorbox"
    },
    {
      "name": "tcolorboxlibraryskins"
    },
    {
      "name": "tcolorboxlibrarybreakable"
    },
    {
      "name": "tcolorboxlibraryminted"
    },
    {
      "name": "tcolorboxlibrarylistings"
    },
    {
      "name": "tikzlibraryshapes.geometric"
    },
    {
      "name": "varwidth"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "etoolbox"
    }
  ],
  "macros": [
    {
      "name": "codefile",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:title%text}}{${2:file}}"
      }
    },
    {
      "name": "codefile",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:title%text}}{${2:file}}"
      }
    },
    {
      "name": "codefile*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:title%text}}{${2:file}}"
      }
    },
    {
      "name": "codefile*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:title%text}}{${2:file}}"
      }
    },
    {
      "name": "cvfile",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:title%text}}{${2:file}}"
      }
    },
    {
      "name": "cvfile",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:title%text}}{${2:file}}"
      }
    },
    {
      "name": "cvfile*",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:title%text}}{${2:file}}"
      }
    },
    {
      "name": "cvfile*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:title%text}}{${2:file}}"
      }
    },
    {
      "name": "codeset",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}"
      }
    },
    {
      "name": "thecvcounter"
    }
  ],
  "envs": [
    {
      "name": "codebox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "codebox",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:title%text}}"
      }
    },
    {
      "name": "codebox*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "codebox*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:title%text}}"
      }
    },
    {
      "name": "codeview",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "codeview",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:title%text}}"
      }
    },
    {
      "name": "codeview*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "codeview*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:title%text}}"
      }
    }
  ],
  "keys": {
    "\\begin{codebox},\\begin{codebox*},\\codefile,\\codefile*,\\begin{codeview},\\begin{codeview*},\\cvfile,\\cvfile*": [
      "minted",
      "lang=${1:source code lang}\r",
      "pretitle=${1:title prefix}\r",
      "codestyle=${1:highlight style}\r",
      "codesize=${1:fontsize macro}\r",
      "comments=${1:text}\r",
      "commentf=${1:format macros}\r",
      "codestretch=${1:factor}\r",
      "linenumsep=${1:number}\r"
    ],
    "\\begin{codeview},\\begin{codeview*},\\cvfile,\\cvfile*": [
      "label="
    ]
  },
  "args": []
}