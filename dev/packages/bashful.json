{
  "deps": [
    {
      "name": "catchfile"
    },
    {
      "name": "listings"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "bash",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "END"
    },
    {
      "name": "bashStdout"
    },
    {
      "name": "bashStderr"
    },
    {
      "name": "splice",
      "arg": {
        "format": "{}",
        "snippet": "{${1:shell command%definition}}"
      }
    },
    {
      "name": "bashI",
      "unusual": true,
      "arg": {
        "format": "[]",
        "snippet": "[${1:options}]"
      }
    },
    {
      "name": "bashII",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "bashIII",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "bashIV",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:shell command%definition}}"
      }
    },
    {
      "name": "bashV",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "logBL",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:message%text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/bashful#c": [
      "unique",
      "hide",
      "dir=${1:directory}\r",
      "verbose"
    ],
    "\\bash": [
      "scriptFile=${1:file name}\r",
      "stdoutFile=${1:file name}\r",
      "stderrFile=${1:file name}\r",
      "exitCodeFile=${1:file name}\r",
      "script",
      "stdout",
      "stderr",
      "ignoreExitCode",
      "ignoreStderr",
      "prefix=${1:tokens}\r",
      "environment=${1:envname}\r",
      "verbose"
    ],
    "\\lstset,\\lstinline,\\begin{lstlisting},\\lstinputlisting,\\lstMakeShortInline": [
      "style=bashfulScript\r",
      "style=bashfulStdout\r",
      "style=bashfulStderr\r"
    ]
  },
  "args": [
    "\\usepackage/bashful#c"
  ]
}