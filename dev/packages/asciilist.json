{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "trimspaces"
    }
  ],
  "macros": [
    {
      "name": "AsciiListFromFile",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:item chars}}{${2:file}}"
      }
    },
    {
      "name": "AsciiListFromFile",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:environments%keyvals}]{${1:item chars}}{${2:file}}",
        "keys": [
          "\\begin{AsciiList}#c,\\AsciiListFromFile#c,\\AsciiListFromFiles#c,\\AsciiListSetEnvironments#c,\\NewAsciiListEnv#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "AsciiListFromFiles",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:item chars}}{${2:file list%file}}"
      }
    },
    {
      "name": "AsciiListFromFiles",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:environments%keyvals}]{${1:item chars}}{${2:file list%file}}",
        "keys": [
          "\\begin{AsciiList}#c,\\AsciiListFromFile#c,\\AsciiListFromFiles#c,\\AsciiListSetEnvironments#c,\\NewAsciiListEnv#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "AsciiListSetAutochars",
      "arg": {
        "format": "{}",
        "snippet": "{${1:chars}}"
      }
    },
    {
      "name": "UP"
    },
    {
      "name": "UP",
      "arg": {
        "format": "[]",
        "snippet": "[${1:integer}]"
      }
    },
    {
      "name": "UPTO",
      "arg": {
        "format": "{}",
        "snippet": "{${1:integer}}"
      }
    },
    {
      "name": "AsciiDocListFromFile",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "AsciiDocListFromFile",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:char1=env1,...%keyvals}]{${1:file}}",
        "keys": [
          "\\begin{AsciiDocList}#c,\\AsciiDocListFromFile#c,\\AsciiDocListFromFiles#c,\\AsciiDocListSetEnvironments#c,\\NewAsciiDocListEnv#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "AsciiDocListFromFiles",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file list%file}}"
      }
    },
    {
      "name": "AsciiDocListFromFiles",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:char1=env1,...%keyvals}]{${1:file list%file}}",
        "keys": [
          "\\begin{AsciiDocList}#c,\\AsciiDocListFromFile#c,\\AsciiDocListFromFiles#c,\\AsciiDocListSetEnvironments#c,\\NewAsciiDocListEnv#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "AsciiListRegisterEnv",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "{${1:envname%specialDef}}{${2:begindef%definition}}{${3:enddef%definition}}{${4:itemdef%definition}}"
      }
    },
    {
      "name": "AsciiListRegisterDescEnv",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname%specialDef}}"
      }
    },
    {
      "name": "AsciiListEndArg",
      "arg": {
        "format": "{}",
        "snippet": "{${1:command}}"
      }
    },
    {
      "name": "AsciiListEndOArg",
      "arg": {
        "format": "{}",
        "snippet": "{${1:command}}"
      }
    },
    {
      "name": "AsciiListSetEnvironments",
      "arg": {
        "format": "{}",
        "snippet": "{${1:environments%keyvals}}"
      }
    },
    {
      "name": "NewAsciiListEnv",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:item chars}}{${2:envname}}"
      }
    },
    {
      "name": "NewAsciiListEnv",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:environments%keyvals}]{${1:item chars}}{${2:envname}}"
      }
    },
    {
      "name": "AsciiDocListSetEnvironments",
      "arg": {
        "format": "{}",
        "snippet": "{${1:char1=env1,...%keyvals}}"
      }
    },
    {
      "name": "NewAsciiDocListEnv",
      "arg": {
        "format": "{}",
        "snippet": "{${1:envname}}"
      }
    },
    {
      "name": "NewAsciiDocListEnv",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:char1=env1,...%keyvals}]{${1:envname}}"
      }
    }
  ],
  "envs": [
    {
      "name": "AsciiList",
      "arg": {
        "format": "{}",
        "snippet": "{${1:item chars}}"
      }
    },
    {
      "name": "AsciiList",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:environments%keyvals}]{${1:item chars}}",
        "keys": [
          "\\begin{AsciiList}#c,\\AsciiListFromFile#c,\\AsciiListFromFiles#c,\\AsciiListSetEnvironments#c,\\NewAsciiListEnv#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "AsciiDocList"
    },
    {
      "name": "AsciiDocList",
      "arg": {
        "format": "[]",
        "snippet": "[${1:char1=env1,...%keyvals}]",
        "keys": [
          "\\begin{AsciiDocList}#c,\\AsciiDocListFromFile#c,\\AsciiDocListFromFiles#c,\\AsciiDocListSetEnvironments#c,\\NewAsciiDocListEnv#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\begin{AsciiList}#c,\\AsciiListFromFile#c,\\AsciiListFromFiles#c,\\AsciiListSetEnvironments#c,\\NewAsciiListEnv#c": [
      "chapter",
      "section",
      "subsection",
      "subsubsection",
      "section*",
      "subsection*",
      "subsubsection*",
      "paragraph",
      "Heading",
      "Heading*",
      "description",
      "Description",
      "itemize",
      "enumerate",
      "compactenum",
      "compactenum<${1:label}>",
      "compactitem",
      "compactitem<${1:label}>",
      "compactdesc",
      "CompactDesc",
      "Heading",
      "Heading<${1:level}>",
      "Heading*",
      "Heading*<${1:level}>",
      "%asciiregisteredenv"
    ],
    "\\begin{AsciiDocList}#c,\\AsciiDocListFromFile#c,\\AsciiDocListFromFiles#c,\\AsciiDocListSetEnvironments#c,\\NewAsciiDocListEnv#c": [
      "*=${1:envname}",
      "-=${1:envname}",
      "!=${1:envname}",
      "/=${1:envname}",
      "+=${1:envname}",
      "?=${1:envname}",
      ";=${1:envname}"
    ]
  },
  "args": []
}