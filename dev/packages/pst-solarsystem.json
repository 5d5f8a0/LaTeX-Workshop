    {
      "name": "pstricks"
    },
    {
      "name": "pst-node"
    },
    {
      "name": "pst-plot"
    },
    {
      "name": "pst-3d"
    },
    {
      "name": "pst-grad"
    },
    {
      "name": "pst-tools"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "SolarSystem"
    },
    {
      "name": "SolarSystem",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    },
    {
      "name": "Jupiter",
      "unusual": true
    },
    {
      "name": "Saturn",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\SolarSystem#c": [
      "Day=${1:day}\r",
      "Month=${1:month}\r",
      "Year=${1:year}\r",
      "Hour=${1:hour}\r",
      "Minute=${1:minute}\r",
      "Second=${1:second}\r",
      "solarValues",
      "viewpoint=\r",
      "language=${1|DE,FR,EN\r|}"
    ]
  },
  "args": []
}