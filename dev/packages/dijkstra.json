{
  "deps": [
    {
      "name": "simplekv"
    }
  ],
  "macros": [
    {
      "name": "readgraph",
      "arg": {
        "format": "{}",
        "snippet": "{${1:graph specs}}"
      }
    },
    {
      "name": "readgraph*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:graph specs}}"
      }
    },
    {
      "name": "dijkstra",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:vertex1}}{${2:vertex2}}"
      }
    },
    {
      "name": "dijkstra",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:vertex1}}{${2:vertex2}}"
      }
    },
    {
      "name": "setdijk",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\dijkstra,\\setdijk,\\setdijkdefault"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "setdijkdefault",
      "arg": {
        "format": "{}",
        "snippet": "{${1:options%keyvals}}",
        "keys": [
          "\\dijkstra,\\setdijk,\\setdijkdefault"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "dijkdist"
    },
    {
      "name": "dijkpath"
    },
    {
      "name": "initdijk"
    },
    {
      "name": "formatnodewithprev",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "highlightfirstnode",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:arg}}"
      }
    },
    {
      "name": "highlightnode",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:arg1}}{${2:arg2}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\dijkstra,\\setdijk,\\setdijkdefault": [
      "show-tab",
      "v-position=\r",
      "pre-tab=${1:code}\r",
      "post-tab=${1:code}\r",
      "col-type=\r",
      "infinity-code=${1:code}\r",
      "norevisit-code=${1:code}\r",
      "h-rules",
      "show-lastcol",
      "lastcol-type=\r",
      "lastcol-label=\r",
      "nopath-string=\r",
      "path-sep=\r"
    ]
  },
  "args": []
}