  "macros": {
    "attachfile{}": {
      "snippet": "attachfile{${1:file}}"
    },
    "attachfile[]{}": {
      "snippet": "attachfile[${2:options}]{${1:file}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "noattachfile": {},
    "noattachfile[]": {
      "snippet": "noattachfile[${1:options}]",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "notextattachfile{}": {
      "snippet": "notextattachfile{${1:text}}"
    },
    "notextattachfile[]{}": {
      "snippet": "notextattachfile[${2:options}]{${1:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "textattachfile{}{}": {
      "snippet": "textattachfile{${1:file}}{${2:text}}"
    },
    "textattachfile[]{}{}": {
      "snippet": "textattachfile[${3:options}]{${1:file}}{${2:text}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    },
    "attachfilesetup{}": {
      "snippet": "attachfilesetup{${1:options}}",
      "keyvalindex": 0,
      "keyvalpos": 0
    }
  },
