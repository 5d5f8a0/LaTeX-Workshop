{
  "cmds": {
    "so{}": {
      "command": "so",
      "snippet": "so{${1:text to add letter spacing}}"
    },
    "caps{}": {
      "command": "caps",
      "snippet": "caps{${1:small caps text}}"
    },
    "ul{}": {
      "command": "ul",
      "snippet": "ul{${1:text to underline}}"
    },
    "st{}": {
      "command": "st",
      "snippet": "st{${1:text to strikeout}}"
    },
    "hl{}": {
      "command": "hl",
      "snippet": "hl{${1:text to highlight}}"
    },
    "soulaccent{}": {
      "command": "soulaccent",
      "snippet": "soulaccent{${1:accent}}"
    },
    "soulregister{}{}": {
      "command": "soulregister",
      "snippet": "soulregister{${1:cmd}}{${2:text}}"
    },
    "sloppyword{}": {
      "command": "sloppyword",
      "snippet": "sloppyword{${1:text}}"
    },
    "sodef{}{}{}{}{}": {
      "command": "sodef",
      "snippet": "sodef{${1:cmd}}{${2:font}}{${3:inner letter space}}{${4:inner space}}{${5:outer space}}"
    },
    "resetso": {},
    "capsdef{}{}{}{}": {
      "command": "capsdef",
      "snippet": "capsdef{${1:cmd}}{${2:inner letter space}}{${3:inner space}}{${4:outer space}}"
    },
    "capssave{}": {
      "command": "capssave",
      "snippet": "capssave{${1:name}}"
    },
    "capsselect{}": {
      "command": "capsselect",
      "snippet": "capsselect{${1:name}}"
    },
    "capsreset": {},
    "setul{}{}": {
      "command": "setul",
      "snippet": "setul{${1:underline depth}}{${2:underline thickness}}"
    },
    "resetul": {},
    "setuldepth{}": {
      "command": "setuldepth",
      "snippet": "setuldepth{${1:characters}}"
    },
    "setuloverlap{}": {
      "command": "setuloverlap",
      "snippet": "setuloverlap{${1:underline overlap}}"
    },
    "setulcolor{}": {
      "command": "setulcolor",
      "snippet": "setulcolor{${1:named color}}"
    },
    "setstcolor{}": {
      "command": "setstcolor",
      "snippet": "setstcolor{${1:named color}}"
    },
    "sethlcolor{}": {
      "command": "sethlcolor",
      "snippet": "sethlcolor{${1:named color}}"
    },
    "soulomit{}": {
      "command": "soulomit",
      "snippet": "soulomit{${1:text to ignore}}"
    }
  },
  "envs": {}
}