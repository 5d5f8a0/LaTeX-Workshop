{
  "includes": [
    "expl3",
    "l3keys2e",
    "calc",
    "amsmath",
    "chemgreek",
    "graphics",
    "ifthen",
    "keyval",
    "pgf",
    "tikz",
    "pgf",
    "tikz"
  ],
  "cmds": {
    "mhchemoptions{}": {
      "snippet": "mhchemoptions{${1:options}}",
      "keyvals": 0,
      "keyvalindex": 0
    },
    "ce{}": {
      "snippet": "ce{${1:formula}}"
    },
    "bond{}": {
      "snippet": "bond{${1:-=~.<${2:}> etc.}}"
    },
    "cesplit{}{}": {
      "snippet": "cesplit{${1:search-and-replace list}}{${2:string}}"
    },
    "hyphen": {},
    "cee{}": {
      "snippet": "cee{${1:equation}}",
      "option": "version=3"
    },
    "cf{}": {
      "snippet": "cf{${1:arg}}",
      "option": "version=3"
    },
    "cf[]{}": {
      "snippet": "cf[${2:opt}]{${1:arg}}",
      "option": "version=3"
    },
    "cmath{}": {
      "snippet": "cmath{${1:arg}}",
      "option": "version=3"
    },
    "sbond": {
      "option": "version=3"
    },
    "dbond": {
      "option": "version=3"
    },
    "tbond": {
      "option": "version=3"
    }
  },
  "envs": {},
  "options": [
    "version=${1:version}",
    "textfontname=${1:font csname}",
    "mathfontname=${1:font csname}",
    "font=",
    "text-greek=",
    "math-greek=",
    "arrows=",
    "layout=",
    "minus-sidebearing-left=",
    "minus-sidebearing-right=",
    "minus-math-sidebearing-left=",
    "minus-math-sidebearing-right=",
    "minus-text-sidebearing-left=",
    "minus-text-sidebearing-right="
  ],
  "keyvals": [
    [
      "textfontcommand=${1:font command}",
      "mathfontcommand=${1:font command}",
      "textminus=${1:symbol}",
      "cdot=${1:symbol}",
      "textelectrondot=${1:symbol}"
    ]
  ]
}