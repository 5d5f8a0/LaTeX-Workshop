      "justification=Justified",
      "justification=Centering",
      "justification=RaggedRight",
      "justification=RaggedLeft"
    ]
  },
  "macros": {
    "subfigure{}": {
      "snippet": "subfigure{${1:body}}",
      "option": "config=altsf"
    },
    "subfigure[]{}": {
      "snippet": "subfigure[${2:subcaption}]{${1:body}}",
      "option": "config=altsf"
    },
    "subfigure[][]{}": {
      "snippet": "subfigure[${2:listentry}][${3:subcaption}]{${1:body}}",
      "option": "config=altsf"
    },
    "subtable{}": {
      "snippet": "subtable{${1:body}}",
      "option": "config=altsf"
    },
    "subtable[]{}": {
      "snippet": "subtable[${2:subcaption}]{${1:body}}",
      "option": "config=altsf"
    },
    "subtable[][]{}": {
      "snippet": "subtable[${2:listentry}][${3:subcaption}]{${1:body}}",
      "option": "config=altsf"
    },
    "Subref{}": {
      "snippet": "Subref{${1:label}}",
      "option": "config=altsf"
    },
    "newsubfloat{}": {
      "snippet": "newsubfloat{${1:float name}}"
    },
    "newsubfloat[]{}": {
      "snippet": "newsubfloat[${2:kv-list}]{${1:float name}}"
    },
    "DeclareCaptionListOfFormat{}{}": {
      "snippet": "DeclareCaptionListOfFormat{${1:keyword}}{${2:code}}"
    },
    "DeclareSubrefFormat{}{}": {
      "snippet": "DeclareSubrefFormat{${1:keyword}}{${2:code}}"
    },
    "subfloat{}": {
      "snippet": "subfloat{${1:body}}"
    },
    "subfloat[]{}": {
      "snippet": "subfloat[${2:subcaption}]{${1:body}}"
    },
    "subfloat[][]{}": {
      "snippet": "subfloat[${2:listentry}][${3:subcaption}]{${1:body}}"
    },
    "subref{}": {
      "snippet": "subref{${1:label}}"
    },
    "subref*{}": {
      "snippet": "subref*{${1:label}}"
    },
    "listsubcaptions": {},
    "thesubfigure": {},
    "thesubtable": {},
    "subfigurename": {},
    "subtablename": {},
