{
  "includes": [
    "array",
    "varwidth",
    "delarray",
    "linegoal"
  ],
  "cmds": {
    "tabulinestyle{}": {
      "snippet": "tabulinestyle{${1:style}}"
    },
    "usetabu{}": {
      "snippet": "usetabu{${1:username}}"
    },
    "tabucline{}": {
      "snippet": "tabucline{${1:%<from%>-%<to%>}}"
    },
    "tabucline[]{}": {
      "snippet": "tabucline[${2:%<spec%>}]{${1:%<from%>-%<to%>}}"
    },
    "savetabu{}": {
      "snippet": "savetabu{${1:username}}"
    },
    "preamble{}": {
      "snippet": "preamble{${1:username}}"
    },
    "tabuphantomline": {},
    "tabulinesep=%<dimen%>": {
      "snippet": "tabulinesep = ${1:dimen}"
    },
    "extrarowdepth": {},
    "abovetabulinesep": {},
    "belowtabulinesep": {},
    "tabustrutrule": {},
    "extrarowsep=%<dimen%>": {
      "snippet": "extrarowsep =%<dimen%>"
    },
    "extrarowsep=^%<dimen%>": {
      "snippet": "extrarowsep =^${1:dimen}"
    },
    "extrarowsep=_%<dimen%>": {
      "snippet": "extrarowsep =_${1:dimen}"
    },
    "extrarowsep=_%<dimen%>^%<dimen%>": {
      "snippet": "extrarowsep =_${1:dimen}^${2:dimen}"
    },
    "taburulecolor{}": {
      "snippet": "taburulecolor|${2:doubleRuleSepColor}|{${1:%<color%>}}"
    },
    "tabureset": {},
    "newtabulinestyle{}": {
      "snippet": "newtabulinestyle{${1:style}}"
    },
    "everyrow{}": {
      "snippet": "everyrow{${1:code}}"
    },
    "taburowcolors[]%<number%>{}": {
      "snippet": "taburowcolors [${2:%<first line%>}] ${3:number} {${1:%<first%> .. %<last%>}}"
    },
    "taburowcolors%<number%>{}": {
      "snippet": "taburowcolors ${2:number} {${1:%<first%> .. %<last%>}}"
    },
    "rowfont[]{}": {
      "snippet": "rowfont[${2:alignment}]{${1:fontSpec}}"
    },
    "tabudecimal{}": {
      "snippet": "tabudecimal{${1:code}}"
    },
    "firstline": {},
    "lastline": {},
    "iftabuscantokens": {},
    "tabuscantokenstrue": {},
    "tabuscantokensfalse": {},
    "tabucolumn{}": {
      "snippet": "tabucolumn{${1:arg}}"
    },
    "tabucolX": {},
    "tabudefaulttarget": {},
    "tabuDisableCommands": {},
    "tabuendlongtrial": {},
    "tabulineoff": {},
    "tabulineon": {},
    "tabuthepreamble": {},
    "thetaburow": {},
    "tracingtabu": {}
  },
  "envs": {
    "tabu{}": {
      "name": "tabu",
      "snippet": "{${1:preamble}}"
    },
    "tabuto%<width%>{}": {
      "name": "tabu",
      "snippet": " to ${2:width} {${1:%<preamble%>}}"
    },
    "tabuspread%<width%>{}": {
      "name": "tabu",
      "snippet": " spread ${2:width} {${1:%<preamble%>}}"
    },
    "longtabu{}": {
      "name": "longtabu",
      "snippet": "{${1:preamble}}"
    },
    "longtabuto%<width%>{}": {
      "name": "longtabu",
      "snippet": " to ${2:width} {${1:%<preamble%>}}"
    },
    "longtabuspread%<width%>{}": {
      "name": "longtabu",
      "snippet": " spread ${2:width} {${1:%<preamble%>}}"
    }
  },
  "options": [
    "delarray",
    "linegoal",
    "scantokens",
    "debugshow"
  ]
}